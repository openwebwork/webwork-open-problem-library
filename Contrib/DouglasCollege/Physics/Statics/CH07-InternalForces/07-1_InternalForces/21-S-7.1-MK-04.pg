#Setup

TEXT(beginproblem());

Context("Matrix");		# or Complex

#---------------Tolerance------------------#
Context()->flags->set(
tolerance=>.02,
tolType=>'relative');  #relative or absolute

#-------------- Image Scale---------------#
$imgScale =0.35;


#-------------- Image Aspect Ratio---------------#
$width = $imgScale*2200;
$height = $imgScale*1100;


#---- Provided --------#
# random(lower, upper, stepsize)

$f1 = random(1,5,1);
$d1 = random(3,6,1);
$d2 = random(3.5, 4.5, 0.5);
$d3 = random(3.5, 4.5, 0.5);

#---Reaction Forces---
$fd2 = $f1 * $d1;
$By = $f1 * $d1 * $d1/2 / ($d2 + $d3);
$Nc = 0;
$Vc = $By;
$Mc = -$By * $d3;

######################################################
#PGML

BEGIN_PGML

