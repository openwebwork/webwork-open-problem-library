#Setup

TEXT(beginproblem());

Context("Matrix");		# or Complex

#---------------Tolerance------------------#
Context()->flags->set(
tolerance=>.02,
tolType=>'relative');  #relative or absolute

#-------------- Image Scale---------------#
$imgScale =0.35;


#-------------- Image Aspect Ratio---------------#
$width = $imgScale*2200;
$height = $imgScale*1100;


#---- Provided --------#
# random(lower, upper, stepsize)
$f1 = random(5,15,1);
$f2 = random(5,10,1);
$d1 = random(1,2,0.1);
$d2 = random(1,2,0.1);
$d3 = random(1,2,0.1);
$d4 = random(1,2,0.1);
$d = $d3 + $d4;

#---Reaction Forces---
$fd2 = $f2 * $d;
$Ay = ($fd2 * ($d3+$d4)/2 + $f1 * ($d2 + $d3 + $d4)) / ($d1 + $d2 + $d3 + $d4);
$Nc = 0;
$Vc = $Ay - $f2*$d3 - $f1;
$Mc = $Ay*($d1+$d2+$d3) - $f1*($d2+$d3) - $f2 * ($d3) * ($d3/2);

