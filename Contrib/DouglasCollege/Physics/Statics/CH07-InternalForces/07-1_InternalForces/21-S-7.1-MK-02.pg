[`N_{C}`] = [_____]{$Nc} [`\textrm{kN}`]

 
[`V_{C}`] = [_____]{$Vc} [`\textrm{kN}`]


[`M_{C}`] = [_____]{$Mc} [`\textrm{kN-m}`]
  

