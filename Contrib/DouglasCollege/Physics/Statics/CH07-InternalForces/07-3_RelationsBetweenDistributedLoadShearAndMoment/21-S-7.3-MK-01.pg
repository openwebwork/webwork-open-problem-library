#Setup

TEXT(beginproblem());

Context("Matrix");		# or Complex

#---------------Tolerance------------------#
Context()->flags->set(
tolerance=>.02,
tolType=>'relative');  #relative or absolute

#-------------- Image Scale---------------#
$imgScale =0.35;


#-------------- Image Aspect Ratio---------------#
$width = $imgScale*2863;
$height = $imgScale*1100;


#---- Provided --------#
# random(lower, upper, stepsize)
$f = random(7,12,1); 
$d1 = random(3.5,6,0.4);
$d2 = random(1.5,2,0.1);
$d3 = random(1.5,2,0.1);
$d4 = random(1.5,2,0.1);
$d5 = random(3,5,1);

#---triangle force---
$ft = $f * $d1 / 2;

#---Reaction Forces---
$Ey= $f * $d5/2;
$Fy = $Ey;
$Ay = $ft * $d1 * (1/3) / $d1;
$By = $ft * $d1 * (2/3) / $d1;
$Dy = ($f * ($d2 + $d3 + $d4) * (($d2 + $d3 + $d4)/2 - $d2) + $Ey * ($d3 + $d4) - $By * $d2)/$d3; 
$Cy = $By + $Ey + $f * ($d2 + $d3 + $d4) - $Dy;

# ---Shear Forces---
$Vb = $Ay - $f / $d1 /2 * $d1**2;
$Vc = $Vb - $f * $d2;
$Vd = $Vc + $Cy - $f * $d3;
$Ve = $Vd + $Dy - $f * $d4;
$Vf = $Ve - $f * $d5 + $Fy;

$x1 = sqrt($Ay * 2 / ($f/$d1));
$x2 = ($Vc + $Cy) / $f + $d1 + $d2;
$x3 = ($Vd + $Dy) / $f + $d1 + $d2 + $d3;

$Mb = $Ay * $d1 - $f / $d1 /2 * $d1**2 * $d1 / 3;
$Mc = $Mb + $Vb * $d2 - $f * $d2 * $d2 /2;
$Md = $Mc + $Vc * $d3 + $Cy * $d3 - $f * $d3 * $d3 / 2;
$Me = $Md + ($Vd + $Dy) * $d4 - $f * $d4 * $d4/2;
$Mf = $Me + $Ve * $d5 - $f * $d5 * $d5 /2;

$Mx1 = $Ay * $x1 - $f / $d1 /2 * $x1**3/3;
$Mx2 = $Mc + $Vc * ($x2-$d1-$d2) + $Cy * ($x2-$d1-$d2) - $f * ($x2-$d1-$d2) * ($x2-$d1-$d2) / 2;
$Mx3 = $Me + $Ve * ($x3 - ($d1+$d2+$d3+$d4)) - $f * ($x3 - ($d1+$d2+$d3+$d4)) * ($x3 - ($d1+$d2+$d3+$d4)) /2;


