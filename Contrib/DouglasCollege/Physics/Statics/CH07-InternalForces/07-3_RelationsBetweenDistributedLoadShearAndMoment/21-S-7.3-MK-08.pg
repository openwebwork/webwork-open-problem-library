#Setup

TEXT(beginproblem());

Context("Matrix");		# or Complex

#---------------Tolerance------------------#
Context()->flags->set(
tolerance=>.02,
tolType=>'relative');  #relative or absolute

#-------------- Image Scale---------------#
$imgScale =0.35;


#-------------- Image Aspect Ratio---------------#
$width = $imgScale*1680;
$height = $imgScale*1000;


#---- Provided --------#
# random(lower, upper, stepsize)
$p1 = random(200,300,25);
$p2 = random(200,400,25);
$f = random(1500,2000,100);
$d1 = random(3,4,1);
$d2 = random(4,5,1);
$d3 = random(6,8,1);

#---Calculations---
$m1 = $p1/$d1;
$m2 = $p2/$d2;
$By = ($p1*$d1*0.5*$d1*2/3 + $p2*$d2*0.5*($d2*2/3+$d1) + $f*($d1+$d2+$d3))/ ($d1+$d2);
$Ay = -$By + $f + $p1*$d1*0.5 + $p2*$d2*0.5;


#---Shear force
$Va = $Ay;
$V1 = $Ay - $m1 * $d1 * ($d1/2);
$Vbl = $V1 - $m2 * $d2 * ($d2/2);
$Vbr = $Vbl + $By;

#---Internal Bending Moment
$M1 = $Ay * $d1 - $m1 * $d1 * ($d1/2) * $d1/3;
$Mb = $M1 + $V1* $d2 - $m2 * $d2 * ($d2/2) * $d2/3;


if(abs($Vbl)<abs($Vbr)){
	$Vmax = abs($Vbr);
}else{
	$Vmax = abs($Vbl);
}

$Mmax = abs($Mb);


