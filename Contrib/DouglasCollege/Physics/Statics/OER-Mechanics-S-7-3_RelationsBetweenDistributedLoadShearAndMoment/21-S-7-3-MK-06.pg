#Setup

TEXT(beginproblem());

Context("Matrix");		# or Complex

#---------------Tolerance------------------#
Context()->flags->set(
tolerance=>.02,
tolType=>'relative');  #relative or absolute

#-------------- Image Scale---------------#
$imgScale =0.35;


#-------------- Image Aspect Ratio---------------#
$width = $imgScale*1600;
$height = $imgScale*1000;


#---- Provided --------#
# random(lower, upper, stepsize)
$p1 = random(2,4,1);
$p2 = $p1/2;
$d1 = random(5,15,1);

#---Calculations---
$m1 = $p1/$d1;
$Fy = $p2*$d1 - $p1/$d1*($d1**2/2);
$M =  $p2*$d1*$d1/2 - $p1/$d1*($d1**2/2)*$d1/3;


#---At max stress
$d = $p2*$d1/$p1;
$Vd = $p2*$d - $p1/$d1*($d**2/2);
$Md = $p2*$d*$d/2 - $p1/$d1*($d**2/2)*$d/3;

