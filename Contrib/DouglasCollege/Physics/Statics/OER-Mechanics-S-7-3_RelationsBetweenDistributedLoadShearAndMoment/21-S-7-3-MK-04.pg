#Setup

TEXT(beginproblem());

Context("Matrix");		# or Complex

#---------------Tolerance------------------#
Context()->flags->set(
tolerance=>.02,
tolType=>'relative');  #relative or absolute

#-------------- Image Scale---------------#
$imgScale =0.35;


#-------------- Image Aspect Ratio---------------#
$width = $imgScale*2000;
$height = $imgScale*1100;


#---- Provided --------#
# random(lower, upper, stepsize)
$p1 = random(4,9,1);
$p2 = random(4,9,1);
$d1 = random(1,3,0.5);
$d2 = random(1,3,0.5);

#---Calculations---
$m1 = $p1/$d1;
$m2 = $p2/$d2;
$M = 0.5*$p1*$d1*$d1/3 + 0.5*$p2*$d2*($d2*2/3+$d1);
$Fy = 0.5*$p1*$d1 + 0.5*$p2*$d2;

#---Shear Stress---
$Va = $Fy;
$Vb = $Va - $p1*($d1) + ($m1*$d1**2)/2;
$Vc = $Vb - $m2 * ($d2**2)/2;

#---Internal Bending Moment---
$Ma = -$M;
$Mb = $Ma + $Va*$d1 - $p1*($d1**2)/2 + ($m1*$d1**3)/6;
$Mc = $Mb + $Vb*$d2 - $m2*($d2**3)/6;

#---Shear
$d = $d1*0.735;
$Vd = $Va - $p1*($d) + ($m1*$d**2)/2;
$Md = $Ma + $Va*$d - $p1*($d**2)/2 + ($m1*$d**3)/6;

