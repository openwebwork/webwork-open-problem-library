#Setup

TEXT(beginproblem());

Context("Matrix");		# or Complex

#---------------Tolerance------------------#
Context()->flags->set(
tolerance=>.02,
tolType=>'relative');  #relative or absolute

#-------------- Image Scale---------------#
$imgScale =0.35;


#-------------- Image Aspect Ratio---------------#
$width = $imgScale*2000;
$height = $imgScale*1100;


#---- Provided --------#
# random(lower, upper, stepsize)
$p1 = random(4,9,1);
$p2 = random(4,9,1);
$d1 = random(1,3,0.5);
$d2 = random(1,3,0.5);

#---Calculations---
$m1 = $p1/$d1;
$m2 = $p2/$d2;
$M = 0.5*$p1*$d1*$d1/3 + 0.5*$p2*$d2*($d2*2/3+$d1);
$Fy = 0.5*$p1*$d1 + 0.5*$p2*$d2;

