#---------------Tolerance------------------#
Context()->flags->set(
tolerance=>.02,
tolType=>'relative');  #relative or absolute

#-------------- Image Scale---------------#
$imgScale =0.35;


#-------------- Image Aspect Ratio---------------#
$width = $imgScale*1300;
$height = $imgScale*1056;


#---- Provided --------#
# random(lower, upper, stepsize)

$d1 = random(1.5,2,0.1);
$d2 = random(1.5,2,0.1);
$theta1 = 0;
$rad1 = $theta1 * pi / 180;
$theta2 = 90;
$rad2 = $theta2 * pi / 180;
$F = random(50,200,25);

#-----Calculations------#

$Mxs = sin($rad1) * $F * $d2; 
$Mys = sin($rad1) * -$F * $d1;
$Mzs = cos($rad1) * $F * $d1;
$Ms = sqrt($Mxs**2 + $Mys**2 + $Mzs**2);

$Mxl = sin($rad2) * $F * $d2; 
$Myl = sin($rad2) * -$F * $d1;
$Mzl = cos($rad2) * $F *$d1;
$Ml = sqrt($Mxl**2 + $Myl**2 + $Mzl**2);


