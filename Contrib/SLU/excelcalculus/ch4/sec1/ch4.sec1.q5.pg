DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGcourse.pl"
);

TEXT(beginproblem());

Context("Numeric");

$a = random(1,30,1);

$formula = Compute("$a")->reduce;

$deriv = $formula->D;


Context()->texStrings;
BEGIN_TEXT

Use the symbolic rules to find the derivative of the function, \(k(x)=$a\)
$BR
\(k'(x)=\) \{ ans_rule() \} 

END_TEXT
Context()->normalStrings;

$showPartialCorrectAnswers = 1;


ANS( $deriv->cmp());


Context()->texStrings;
BEGIN_SOLUTION
${PAR}SOLUTION:${PAR}

\(k'(x)=$deriv\)

END_SOLUTION
Context()->normalStrings;

COMMENT('MathObject version.');

ENDDOCUMENT();  