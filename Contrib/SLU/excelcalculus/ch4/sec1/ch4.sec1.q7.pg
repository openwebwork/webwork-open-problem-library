## Author('Megan Nigg');
## DBsubject(Calculus - single variable)
## DBchapter(Symbolic Differentiation)
## DBsection(Elementary Derivatives)
## Institution(Saint Louis University)
## Level(2)
## MO(1)
## TitleText1('Business Calculus with Excel')
## AuthorText1('May')
## Chapter('4')
## Section('1').
## Problem('7')
## KEYWORDS('calculus','linear','functions')

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGcourse.pl"
);

TEXT(beginproblem());

Context("Numeric");

$a = random(1,5,1);
$a2=Compute("$a+0.5");

$formula = Compute("x^{$a}*sqrt(x)")->reduce;
$formula2 = Compute("x^$a2")->reduce;

$deriv = $formula2->D;


Context()->texStrings;
BEGIN_TEXT

Use the symbolic rules to find the derivative of the function, \(f(x)=x^{$a}*\sqrt{x}\)
$BR
\(f'(x)=\) \{ ans_rule() \} 

END_TEXT
Context()->normalStrings;

$showPartialCorrectAnswers = 1;


ANS( $deriv->cmp());


Context()->texStrings;
BEGIN_SOLUTION
${PAR}SOLUTION:${PAR}
\(f(x)=$formula=$formula2\)
$BR
\(f'(x)=$deriv\)

END_SOLUTION
Context()->normalStrings;

COMMENT('MathObject version.');

ENDDOCUMENT();  