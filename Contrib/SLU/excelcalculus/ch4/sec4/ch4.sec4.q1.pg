DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGcourse.pl"
);

TEXT(beginproblem());

Context("Numeric");

$formula = Compute("x*ln(x)")->reduce;

$deriv = $formula->D;


Context()->texStrings;
BEGIN_TEXT
Find the derivative of the following function.
$BR
\(f(x)=$formula\)
$BR
\(f'(x)=\) \{ ans_rule() \} 

END_TEXT
Context()->normalStrings;

$showPartialCorrectAnswers = 1;


ANS( $deriv->cmp());


Context()->texStrings;
BEGIN_SOLUTION
${PAR}SOLUTION:${PAR}

\(f'(x)=$deriv\)

END_SOLUTION
Context()->normalStrings;

COMMENT('MathObject version.');

ENDDOCUMENT();  
