## Section('4').
## Problem('9')
## KEYWORDS('calculus','linear','functions')

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGcourse.pl",
"parserFormulaUpToConstant.pl",
);

TEXT(beginproblem());

Context("Vector");
Context()->variables->add(t=>'Real');

$a = random(1,10,1);
$b = random(1,10,1);
$c = random(1,10,1);
$d = random(1,10,1);


$fx = Compute("($a*2*x+$b)*($a*x^2+$b*x+$c)^$d")->reduce;
$answer = FormulaUpToConstant("(($a*x^2+$b*x+$d)^($c+1))/($c+1)")->reduce;

$u = Compute("$a*x^2+$b*x+$c");
$du =$u->D;


Context()->texStrings;
BEGIN_TEXT

For the integral \(\int $fx dx\). Identify the term that will be treated as \(u\) and then evaluated the integral.
$BR
\(u=\)\{ans_rule()\}
$BR
\(\int $fx dx=\)\{ans_rule()\}

END_TEXT
Context()->normalStrings;

$showPartialCorrectAnswers = 1;

ANS( $u->cmp()); 
ANS( $answer->cmp()); 



Context()->texStrings;
BEGIN_SOLUTION
${PAR}SOLUTION:${PAR}

Let \(u=$u\). Then \(du=$du dx\).
$BR
$BCENTER
\(\int $fx dx=$answer\)

END_SOLUTION
Context()->normalStrings;

COMMENT('MathObject version.');

ENDDOCUMENT();
