## Section('4').
## Problem('17')
## KEYWORDS('calculus','linear','functions')

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGcourse.pl",
"parserFormulaUpToConstant.pl",
);

TEXT(beginproblem());

Context("Vector");
Context()->variables->add(u=>'Real');


$a = random(1,10,1);
$b = random(1,10,1);

$upper = random(1,10,1);
$lower = 1;


$fx = Compute("x*sqrt($a*x^2+$b)")->reduce;
$Fx = Compute("(($a*x^2+$b)^(3/2))/3*$a")->reduce;

$u = Compute("($a*x^2+$b)")->reduce;
$du =$u->D;

$fu = Compute("u^(0.5)");

$newupper = $u->eval(x=>$upper);
$newlower = $u->eval(x=>$lower);

$Fupper = $Fx->eval(x=>$upper);
$Flower = $Fx->eval(x=>$lower);

$answer=Compute("$Fupper-$Flower");


Context()->texStrings;
BEGIN_TEXT

For the integral \(\int_{$lower}^{$upper} $fx dx\). Identify the term that will be treated as \(u\) and then evaluated the integral.
$BR
\(u=\)\{ans_rule()\}
$BR
\(\int_{$lower}^{$upper} $fx dx=\)\{ans_rule()\}

END_TEXT
Context()->normalStrings;

$showPartialCorrectAnswers = 1;

ANS( $u->cmp()); 
ANS( $answer->cmp()); 



Context()->texStrings;
BEGIN_SOLUTION
${PAR}SOLUTION:${PAR}

Let \(u=$u\). Then \(du=$du dx\).
$BR
$BCENTER
\(\int_{$lower}^{$upper} $fx dx=\int_{$newlower}^{$newupper} $fu du=$answer\)

END_SOLUTION
Context()->normalStrings;

COMMENT('MathObject version.');

ENDDOCUMENT();
