## KEYWORDS('calculus','linear','functions')

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGcourse.pl",
"parserFormulaUpToConstant.pl",
);

TEXT(beginproblem());

Context("Vector");

$a = random(100000,300000,1);
$b = random(1,10,1);
$c = random(100,500,1);
$d = random(100,500,1);
$e = random(-300000,-100000,1);

$fx = Compute("x^{$a}+$b*x^{$c}+$d*x^{$e}")->reduce;
$answer = Compute("(1/$a)*x^{$a+1}+($b/$c)*x^{$c+1}+($d/$e)*x^{$e+1}")->reduce;


Context()->texStrings;
BEGIN_TEXT

Find an antiderivative for the function \(f(x)=$fx\)\{ans_rule()\}

END_TEXT
Context()->normalStrings;

$showPartialCorrectAnswers = 1;

ANS( $answer->cmp(upToConstant=>1)); 



Context()->texStrings;
BEGIN_SOLUTION
${PAR}SOLUTION:${PAR}

\(F(x)=$answer+Constant\)

END_SOLUTION
Context()->normalStrings;

COMMENT('MathObject version.');

ENDDOCUMENT();
