DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGcourse.pl",
"parserFormulaUpToConstant.pl",
);

TEXT(beginproblem());

Context("Vector");


$fx = Compute("x*ln(x)")->reduce;

$answer = FormulaUpToConstant("(1/4)*x^2*(2*ln(x)-1)")->reduce;


Context()->texStrings;
BEGIN_TEXT

Find the antiderivative of the given funciton \(f(x)=$fx\).
$BR
\{ans_rule()\}

END_TEXT
Context()->normalStrings;

$showPartialCorrectAnswers = 1;

ANS( $answer->cmp()); 



Context()->texStrings;
BEGIN_SOLUTION
${PAR}SOLUTION:${PAR}

\(\int $fx dx=$answer\)

END_SOLUTION
Context()->normalStrings;

COMMENT('MathObject version.');

ENDDOCUMENT();