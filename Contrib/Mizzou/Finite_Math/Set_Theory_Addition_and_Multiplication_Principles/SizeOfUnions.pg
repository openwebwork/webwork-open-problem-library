##DESCRIPTION
##  Introduction to Set Theory: Inclusion-Exclusion
##ENDDESCRIPTION

##KEYWORDS('set theory', 'union', 'inclusion exclusion')

## DBsubject('Discrete Mathematics')
## DBchapter('Set Theory')
## DBsection('The Inclusion Exclusion Principle')
## Date('Spring 2012')
## Author1('David Covert')
## Author2('Jason Aubrey')
## Institution('University of Missouri')

DOCUMENT();      

loadMacros(
   "PGstandard.pl",
   "MathObjects.pl",
);

TEXT(beginproblem());

# Show which answers are correct and which ones are incorrect
$showPartialCorrectAnswers = 1;

##############################################################
#
#  Setup
#
#

Context("Numeric");
$A = random(100,200,1);
$B = random(100,200,1);
$AnB = random(50,75,1);
$U = random(500,600,1);
$AUB = $A + $B - $AnB;
$AUBcomplement = $U - $AUB;
$AnotB = $A - $AnB;
$BnotA = $B - $AnB;
$notA = $U - $A;
$notB = $U - $B;

##############################################################
#
#  Text
#

Context()->texStrings;
BEGIN_TEXT
Suppose that \(A\) and \(B\) are sets with 
$PAR
\[
n(A') = $notA, n(B') = $notB, n(A' \cap B') = $AUBcomplement, \text{ and  } n(U) = $U.
\]
$PAR  Find the following:

$PAR
\(n(A) = \)  \{ANS_RULE(10)\}
$PAR
\(n(B) =\) \{ANS_RULE(10)\}
$PAR
\(n(A \cup B) = \) \{ANS_RULE(10)\}
$PAR
\(n(A \cap B) = \)  \{ANS_RULE(10)\}
$PAR
\(n(A \cap B') = \) \{ANS_RULE(10)\}
$PAR
\( n(A' \cap B) = \) \{ANS_RULE(10)\}
END_TEXT
Context()->normalStrings;


##############################################################
#
#  Answers
#

ANS(Compute($A)->cmp);
ANS(Compute($B)->cmp);
ANS(Compute($AUB)->cmp);
ANS(Compute($AnB)->cmp);
ANS(Compute($AnotB)->cmp);
ANS(Compute($BnotA)->cmp);

COMMENT('MathObject version');
ENDDOCUMENT();