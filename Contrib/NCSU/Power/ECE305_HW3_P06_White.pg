$emf2->{test_points} = [0,[$k],[2*$k],[3*$k],[5*$k],[7*$k],[8*$k],[11*$k]];
$emf3->{test_points} = [0,[$k],[2*$k],[3*$k],[5*$k],[7*$k],[8*$k],[11*$k]];

