#Setup

Context("Numeric");

#Tolerance#

Context()->flags->set(
tolerance=>.03,
tolType=>'relative');  #relative or absolute

#- Random variables for this problem --#

$mc = new_checkbox_multiple_choice();
$mc -> qa (
"
Consider the following scenario:
$BR
The hook depicted in the image above has two ropes pulling on it. What is the magnitude of the resultant force acting on the hook due to the two ropes?


",
"Hook supporting by two ropes",
"Angles as shown",
"\(F_1\)=100N",
"\(F_2\)=150N",
"Coordinate axes as shown",
);
$mc -> makeLast("Hook supporting by two ropes",
"Angles as shown",
"See figure",
"\(\overrightarrow{F_1}\)=100N",
"\(F_1\)=100N",
"\(F_2\)=150N",
"\(f_2\)=150N",
"Coordinate axes as shown",
);

