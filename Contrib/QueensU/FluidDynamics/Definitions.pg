$mc = new_checkbox_multiple_choice();
$mc->qa(
	"Choose the correct definitions.",
	"Pathlines, streamlines, and streaklines are the same for steady flows.", 
	"A streamline is a line that is everywhere tangent to the velocity vector.", 
	"For a steady incompressible flow, velocity field can be given as \(u = x^2+y^2\), \(v = xy+z\), and \(w = -3xz+x/y\)."
);
$mc->extra(
	"In steady state flow, velocity at a given point in space varies with time."
);

BEGIN_PGML



[@ $mc->print_q() @]*
[@ $mc->print_a() @]*

END_PGML

ANS(checkbox_cmp($mc->correct_ans()));

