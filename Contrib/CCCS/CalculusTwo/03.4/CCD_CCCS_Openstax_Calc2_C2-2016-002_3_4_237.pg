#  Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"AnswerFormatHelp.pl",
"PGML.pl",
"PGcourse.pl",
);



TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

###########################
#  Setup
$a=random(1,5);
$h=random(1,5);
$b=$a+$h;
$x1=random(1,10);
$k=random(1,10);
$x2=$x1+$k;

$f=Formula("1/(x^3+($a+$b)x^2+($a*$b)x)")->reduce();
$ans=Formula("(2*pi/$h)ln(($x2+$a)($x1+$b)/(($x1+$a)($x2+$b)))")->reduce();


###########################
#  Main text


BEGIN_PGML
Find the volume generated by revolving the area
bounded by [``y=[$f]``], [`x=[$x1]`], [`x=[$x2]`] and [`y=0`] around the [`y`]-axis.


[``V=``][_____________]{$ans }[@ AnswerFormatHelp("numbers") @]*



END_PGML




