$f=Formula("$b/((x-$a)^2(x+$a))")->reduce();
$ans=FormulaUpToConstant("($b/((2*$a)^2))[ln(abs(x+$a))-ln(abs(x-$a))]-$b/((2*$a)(x-$a))+C")->reduce();



