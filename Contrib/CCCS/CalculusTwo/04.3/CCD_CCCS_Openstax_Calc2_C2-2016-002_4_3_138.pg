Context()->variables->add(y=>"Real");

$a=random(1,6);
$b=random(1,6);
$k=random(1,10);
$f=Formula(" ($a*x*y)($b+y) ")->reduce();

$ans=Formula("$k*$b/[($b+$k)*e^(-0.5*$a*$b*x^2)-$k]")->reduce; 



###########################
