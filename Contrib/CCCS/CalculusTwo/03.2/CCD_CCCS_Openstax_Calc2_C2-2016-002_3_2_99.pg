#  Setup

Context("Numeric");


$a = random(2, 6, 1); #coefficient on x
$b = random(1, 6, 1); #constant multiple
$f = Formula("$b*cos^2($a*x)")->reduce;  


$F=FormulaUpToConstant("$b*(x/2 +1/(4*$a)*sin (2*$a*x)) + C")->reduce;
$ans=$F;

