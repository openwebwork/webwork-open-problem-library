###########################
#  Setup

Context("Numeric");
Context()->variables->add(y=>"Real");

$a = random(1, 3, 1); #coefficient on x
$f = Formula("$a*x")->reduce;  


$F=FormulaUpToConstant("1/(2*$a)*sec($a*x)*tan($a*x) - 1/(2*$a)*ln|sec($a*x) + tan($a*x)| + C")->reduce;
$ans=$F;

