Context("Numeric");
Context()->variables->add(a => "Real");


$a = random(2,6,1);
$b = random(3,6,1);
do {$c = random(2,5,1);} until ($a*$c != $b);


$f=Formula("$a*a^2+($a*$c-$b)*a-$b*$c");


$ans=Formula(" ($a*a-$b)(a+$c)");


###########################
