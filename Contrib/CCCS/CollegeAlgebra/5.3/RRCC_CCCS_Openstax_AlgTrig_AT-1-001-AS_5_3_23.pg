Context("Numeric");

$b=list_random(1,4);
do {$c=random(1,4);} until ($b != $c);


$f=Formula("x**5-($b+$c)*x**3+$b*$c*x")->reduce;

Context("LimitedPoint");

$xint = List(Point("(0,0)"),Point("(sqrt($b),0)"), Point("(-sqrt($b),0)"),Point("(sqrt($c),0)"),Point("(-sqrt($c),0)"));
