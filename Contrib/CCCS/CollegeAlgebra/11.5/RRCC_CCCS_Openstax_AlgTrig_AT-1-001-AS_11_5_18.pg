##################
#  Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"AnswerFormatHelp.pl",
"PGML.pl",
"PGcourse.pl",
);

TEXT(beginproblem());

$showPartialCorrectAnswers = 1;

##################
#  Setup

Context("Matrix");

$A = Matrix([
[random(-5,5,1),random(-5,5,1)],
[random(-5,5,1),random(-5,5,1)],
]);

$B = Matrix([
[random(-5,5,1),random(-5,5,1)],
[random(-5,5,1),random(-5,5,1)],
]);

$answer = $A * $B;

$video = MODES(
HTML=> 
'<iframe width="420" height="315" src="//www.youtube.com/embed/RqA4HLtJBBs"frameborder="0" allowfullscreen></iframe>', 
TeX =>
"An embedded YouTube video."
);

##################
#  Main text

BEGIN_PGML
Suppose
>> [``A = [$A] \ \ \mbox{and} \ \ B = [$B]. ``] <<

Find the product [`AB`].

[` AB = `][_____]*{$answer} [@ AnswerFormatHelp("matrices") @]*

[$video]*
END_PGML


BEGIN_TEXT
$BR

If the video does not work, \{ htmlLink ("https://www.youtube.com/watch?v=RqA4HLtJBBs", "click here to watch on YouTube.", "TARGET='_blank'")\}
$BR
END_TEXT


