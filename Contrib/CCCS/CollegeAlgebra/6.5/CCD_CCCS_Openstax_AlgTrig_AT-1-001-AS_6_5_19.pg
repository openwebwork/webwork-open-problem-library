Context("Numeric");
Context()->operators->undefine("^");

Context()->variables->add(y => 'Real');
Context()->variables->add(z => 'Real');

$expX = random(3,9,1); 
$expY = random(5,7,1); 
$expX2 = random(3,9,1); 
$expY2 = random(5,7,1); 
$root = random(3,9,1);


$expXA = Formula($expX+$expX2/$root);
$expYA = Formula($expY+$expY2/$root);



$ans = Formula("$expXA*log(x)+$expYA*log(y)");
$ans->{test_points} = [[1,1,1], [2,2,2]];


