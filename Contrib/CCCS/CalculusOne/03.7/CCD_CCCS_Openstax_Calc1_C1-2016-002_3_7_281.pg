$a=non_zero_random(-5,5,1);
$b=non_zero_random(1,5,1);
$f=Formula("sec^(-1)(($a)/(x**$b))")->reduce;
 
$ans1=Formula("($a*$b*(-1))/(x**($b+1))/((abs(($a)/(x**$b)))sqrt((($a)/(x**$b))**2-1))")->reduce;
