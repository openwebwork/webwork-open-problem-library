#  Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"AnswerFormatHelp.pl",
"PGML.pl",
"PGcourse.pl",
"parserRadioButtons.pl",
"parserPopUp.pl",
);
loadMacros("parserFormulaUpToConstant.pl");

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

###########################
#  Setup
$k=non_zero_random(-5,5,.5);
$p=random(1,5);
$a=non_zero_random(-5,5,.5);
$b=non_zero_random(-5,5,.5);
$f=Formula("$a*e^{$k*x}-$b*sin($p*x)")->reduce;
$F=FormulaUpToConstant("{$a/$k}*e^{$k*x}+{$b/$p}*cos($p*x)+C")->reduce;
$ans=$F;

###########################
#  Main text

BEGIN_PGML
Find the family of antiderivatives of the function

[``f(x)=[$f]``].   



[`` F(x) =``][__________________________]{$ans }[@ AnswerFormatHelp("formulas") @]*



END_PGML


