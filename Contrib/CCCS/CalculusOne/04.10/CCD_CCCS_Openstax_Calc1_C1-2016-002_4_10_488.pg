###########################
#  Setup
$k=random(1,10);
$a=random(-5,5);
$b=random(-5,5);
$f=Formula("$a*sec($k*x)[sec($k*x)-$b*tan($k*x)]")->reduce;
$F=FormulaUpToConstant("{$a }/{$k }[tan($k*x)-$b*sec($k*x)]+C")->reduce;
$ans=$F;

