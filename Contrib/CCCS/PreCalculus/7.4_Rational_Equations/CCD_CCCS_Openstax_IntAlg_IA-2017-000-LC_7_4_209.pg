###########################
#  Setup

Context("Numeric");


$a = random(6,8,1);
$b = random(2,4,1);
$c = random(5,12,1);
$d = $c*$c;

$ans=(5-$a*$c-$b*$c)/($a-$b);


if  ($ans == -$c) {$ans1 = NONE;
} 

elsif ($ans == $c) {$ans1 = NONE;
}

elsif ( $ans=(5-$a*$c-$b*$c)/($a-$b))  {$ans1=(5-$a*$c-$b*$c)/($a-$b);
}

$ans2=($ans1);

$video = MODES(
HTML=> 
'<iframe width="420" height="315" src="//www.youtube.com/embed/6eqgIZyXgK8"frameborder="0" allowfullscreen></iframe>', 
