#  Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"AnswerFormatHelp.pl",
"PGML.pl",
"PGcourse.pl",
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

###########################
#  Setup

$a=random(2,10,1);
do { $b = random(5,15,1); } until ( $b != $a );
do { $c = random(2,6,1); } until ( $c != $a ) and ($c != $b);

$ans=List(" 1/$c*ln($b/$a) ");

$video = MODES(
HTML=> 
'<iframe width="560" height="315" src="https://www.youtube.com/embed/XBpg0PN6ZG4" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>', 
TeX =>
"An embedded YouTube video."
);


###########################
#  Main text

BEGIN_PGML

Use logarithms to solve [` [$a] e^{[$c]*x}=[$b] `]. If there is more than one solution, enter them as a comma separated list. If there is no solution, enter "NONE".

[`x=`] [________]{$ans} [@ AnswerFormatHelp("numbers") @]*


[$video]* 
END_PGML

BEGIN_TEXT
$BR

If the video does not work, \{ htmlLink ("https://youtu.be/XBpg0PN6ZG4", "click here to watch on YouTube.", "TARGET='_blank'")\}
$BR
END_TEXT


