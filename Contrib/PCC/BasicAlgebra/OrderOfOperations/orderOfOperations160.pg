#
# Template
#
# Evaluate these expressions:
#
#    [`` -[$a]^{2}-[$b][ [$c]-( [$d]-[$e]^{3} ) ] =  ``]  [________]{$ans} 
#
# 
# Last updated: Hughes, 6/26/13
# ENDDESCRIPTION

## DBsubject('Algebra')
## DBchapter('Basic Algebra')
## DBsection('Algebraic Expressions')
## KEYWORDS('integer','order of operations','integer exponent','subtract','evaluate')
## DBCCSS('6.EE.1')
## TitleText1('')
## EditionText1('')
## AuthorText1('')
## Section1('')
## Problem1('')
## Author('Alex Jordan, Carl Yao, Chris Hughes')
## Institution('PCC')

##############################################



DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGML.pl",
  "PGcourse.pl",
);

##############################################

Context("LimitedNumeric");

$a=random(2,10,2);
$b=random(3,9,2);
$c=random(1,10,1);
$d=random(1,7,1);
$e=random(2,4,1);

$ans= Compute(-($a**2)-$b*($c-($d-($e**3))));

##############################################

TEXT(beginproblem());

BEGIN_PGML

Evaluate this expression:

    [`` -[$a]^{2}-[$b][ [$c]-( [$d]-[$e]^{3} ) ] =  ``]  [________]{$ans} 

END_PGML

##############################################

$step1 = $a**2;
$step2 = ($e)**3;
$step3 = $d-$step2;
$step4 = abs($step3);
$step5 = $c+$step4;
$step6 = $b*$step5;

BEGIN_PGML_SOLUTION

    [`
\begin{aligned} 
-[$a]^{2}-[$b][ [$c]-( [$d]-[$e]^{3} ) ]  & = -[$step1]-[$b][ [$c]-( [$d]-[$step2] ) ] \\
& = -[$step1]-[$b][ [$c]-( [$step3] ) ] \\
& = -[$step1]-[$b][ [$c]+ [$step4] ] \\
& = -[$step1]-[$b]\cdot[$step5] \\
& = -[$step1]-[$step6] \\
& = [$ans] 
\end{aligned}`]

END_PGML_SOLUTION

ENDDOCUMENT();


