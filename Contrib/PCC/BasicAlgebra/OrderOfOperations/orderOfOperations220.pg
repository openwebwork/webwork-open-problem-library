# WeBWorK problem written by Carl Yao
# Portland Community College
#
# Template
#
# Evaluate these expressions:
#
#    [`` [$a]-[$b]\left\lvert [$c]+([$d]-[$e])^{3}\right\rvert  =  ``]  [________]{$ans} 
# 
# Last updated: Hughes, 6/26/13
# ENDDESCRIPTION

## DBsubject('Algebra')
## DBchapter('Basic Algebra')
## DBsection('Algebraic Expressions')
## KEYWORDS('integer','order of operations','integer exponent','add','subtract','evaluate','absolute value')
## DBCCSS('6.EE.1','6.NS.7')
## TitleText1('')
## EditionText1('')
## AuthorText1('')
## Section1('')
## Problem1('')
## Author('Alex Jordan, Carl Yao, Chris Hughes')
## Institution('PCC')

##############################################

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGML.pl",
  "PGcourse.pl",
);

##############################################

Context("LimitedNumeric");

$a=random(1,10,1);
$b=random(2,8,2);
$c=random(-9,-1,2);
$d=random(2,4,1);
$e=random(5,7,1);

$ans=Compute($a-$b*abs($c+($d-$e)**3));

##############################################

TEXT(beginproblem());

BEGIN_PGML

Evaluate this expression:

    [`` [$a]-[$b]\left\lvert [$c]+([$d]-[$e])^{3}\right\rvert  =  ``]  [________]{$ans} 
END_PGML

##############################################

$step1 = $d-$e;
$step2 = ($step1)**3;
$step3 = $c+$step2;
$step4 = abs($step3);
$step5 = $b*$step4;

BEGIN_PGML_SOLUTION

    [`
\begin{aligned}
 [$a]-[$b]\left\lvert [$c]+([$d]-[$e])^{3}\right\rvert  & = [$a]-[$b]\left\lvert [$c]+([$step1])^{3}\right\rvert  \\
& = [$a]-[$b]\left\lvert [$c]+([$step2])\right\rvert  \\
& = [$a]-[$b]\left\lvert [$step3]\right\rvert  \\
& = [$a]-[$b]\cdot[$step4] \\
& = [$a]-[$step5] \\
& = [$ans] 
\end{aligned}`]

END_PGML_SOLUTION

ENDDOCUMENT();


