# WeBWorK problem written by Carl Yao
# Portland Community College
#
# Template
#
# Evaluate these expressions:
#
#    [`` [$a][[$b]-[$c]([$d]+[$e])] =  ``]  [________]{$ans} 
#
# 
# Last updated: Hughes, 6/26/13
# ENDDESCRIPTION

## DBsubject('Algebra')
## DBchapter('Basic Algebra')
## DBsection('Algebraic Expressions')
## KEYWORDS('integer','order of operations','add','subtract','multiply','evaluate')
## DBCCSS('6.EE.1')
## TitleText1('')
## EditionText1('')
## AuthorText1('')
## Section1('')
## Problem1('')
## Author('Alex Jordan, Carl Yao, Chris Hughes')
## Institution('PCC')

##############################################



DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGML.pl",
  "PGcourse.pl",
);

##############################################

Context("LimitedNumeric");

$a=random(2,6,2);
$b=random(15,20,1);
$c=random(3,7,2);
$d=random(1,7,2);
$e=random(2,8,2);

$ans=Compute($a*($b-$c*($d+$e)));

##############################################

TEXT(beginproblem());

BEGIN_PGML

Evaluate this expression:

    [`` [$a][[$b]-[$c]([$d]+[$e])] =  ``]  [________]{$ans} 

END_PGML

##############################################

$step1 = $d+$e;
$step2 = $c*$step1;
$step3 = $b-$step2;

BEGIN_PGML_SOLUTION

    [`
      \begin{aligned}
    [$a][[$b]-[$c]([$d]+[$e])] &  = [$a][[$b]-[$c]\cdot[$step1]]  \\
& = [$a][[$b]-[$step2]] \\
& = [$a]\cdot[[$step3]] \\
& = [$ans] \end{aligned}`]

END_PGML_SOLUTION

ENDDOCUMENT();


