TEXT(beginproblem());

#
# Now we do the randomization of variables, and other computations
# as needed for this problem.  Sometimes we compute the answers here.
#
$h = random(20,120,1);
$a = ceil($h*random(.1,.2,.01));
$b = floor($h*random(.75,1.25,.1));
$c = ceil($a*random(.75,1.25,.1));
$d = floor($b*random(.2,.4,.01));
$w = ceil($c*random(.75,1.25,.01));

$n = 3;

@y_Ci = (
  $c/2,
  $c + $h/2,
  $c + $h + $a/2);

@A_i = (
  $c * $d,
  $w * $h,
  $a * $b);

$Ai_yCi = 0;
$A = 0;

for($i=0; $i<$n; $i++){
$Ai_yCi = $Ai_yCi + $A_i[$i]*$y_Ci[$i];
$A = $A + $A_i[$i];
}
$y_C = $Ai_yCi/$A;

@I_xCi = (
  $d*($c**3)/12,
  $w*($h**3)/12,
  $b*($a**3)/12);

for($i=0; $i<$n; $i++){
  $I_NA = $I_NA + $I_xCi[$i] + $A_i[$i]*($y_C - $y_Ci[$i])**2;
}

$I_x = $I_NA + $A*$y_C**2;

@I_yCi = (
  $c*($d**3)/12,
  $h*($w**3)/12,
  $a*($b**3)/12);

for($i=0; $i<$n; $i++){
  $I_y = $I_y + $I_yCi[$i];
}

BEGIN_TEXT
MOI I unequal flange 01 $A_i[0], $y_Ci[0]
$PAR
