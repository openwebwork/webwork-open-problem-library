$showPartialCorrectAnswers = 1;
ANS(num_cmp($RA*1000, units=>"N"));
ANS(num_cmp($RE*1000, units=>"N"));

ANS(num_cmp($M_max*1000, units=>"N*m"));

ANS(num_cmp($S_req, units=>"m^3"));

ANS(str_cmp($good_Sbeams[$i]));

