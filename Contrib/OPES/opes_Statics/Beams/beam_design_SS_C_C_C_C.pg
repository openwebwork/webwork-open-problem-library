\{ row("$BCENTER$SPACE $a m $SPACE$ECENTER",
       "$BCENTER$SPACE $b m $SPACE$ECENTER",
       "$BCENTER$SPACE $c m $SPACE$ECENTER",
       "$BCENTER$SPACE $d m $SPACE$ECENTER") \}
\{ row("","","","") \}
\{ row("$BCENTER \(F_1\) $ECENTER",
