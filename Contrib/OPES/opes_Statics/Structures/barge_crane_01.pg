#
# Now we do the randomization of variables, and other computations
# as needed for this problem.  Sometimes we compute the answers here.
#

$mass = random(10,30,.1);
$F = $mass*1000*9.81;
$h1 = random(13,21,.1);
$h2 = floor($h1*10*random(.25,.33,.02)+.5)/10;
$h3 = floor($h1*10*random(.55,.75,.02)+.5)/10;
$h4 = floor($h1*10*random(.30,.42,.02)+.5)/10; 
$h5 = floor($h1*10*random(.13,.23,.02)+.5)/10;
$h6 = floor($h1*10*random(.35,.39,.02)+.5)/10;
$w1 = floor($h2*10*random(1,1.3,.02)+.5)/10;
$w2 = random(70,140,1);
$w3 = floor($h1*10*random(.12,.22,.02)+.5)/10;
$w4 = floor($h1*10*random(.43,.53,.02)+.5)/10;
$w5 = floor($h1*10*random(.76,.96,.02)+.5)/10;
$theta = random(68,80,1);

$ABy = $h4*($w3+$w4)/($w3+$w4+$w5);
$BGx = ($w2/100)+$w3+$w4;
$BGy = $h4+$h5 - $ABy;
$BG = sqrt($BGx**2 + $BGy**2);
$CEx = $w4+$w5;
$CEy = $h5+$h6;
$CE = sqrt($CEx**2 + $CEy**2);
$DEx = $w3;
$DEy = $h3+$h4+$h5+$h6-$h1;
$DE = sqrt($DEx**2 + $DEy**2);
$EIx = $w1 + 2*($w2/100) + $w3;
$EIy = $h3+$h4+$h5+$h6-($h1+$h2);
$EI = sqrt($EIx**2 + $EIy**2);
$HIx = $w1;
$HIy = $h2;
$HI = sqrt($HIx**2 + $HIy**2);
$FHdir = atan2(-$HIy,$HIx);

$TBG = $F;
$TGK = $F;
$FBx = -$TBG*$BGx/$BG;
$FBy = $TBG*$BGy/$BG - $F;
$FB = sqrt($FBx**2 + $FBy**2);
$FBdir = atan2($FBy,$FBx);
$TCE = (-$FBy*($w3+$w4) + $FBx*$ABy)/($h4*$CEx/$CE + ($w3+$w4+$w5)*$CEy/$CE);
$FCdir = atan2($CEy,-$CEx);
$FAx = -$FBx + $TCE*$CEx/$CE;
$FAy = -$FBy - $TCE*$CEy/$CE;
$FA = sqrt($FAx**2 + $FAy**2);
$FAdir = atan2($FAy,$FAx);
$TDE = $TCE*($CEx*$EIy/($CE*$EIx) + $CEy/$CE)/($DEx*$EIy/($DE*$EIx) - $DEy/$DE);
$TEI = ($TCE*$CEx/$CE - $TDE*$DEx/$DE)*$EI/$EIx;
$THI = $TEI*($HI*($EIy + $EIx*tan($theta*pi/180)))/
            ($EI*($HIy - $HIx*tan($theta*pi/180)));
$TIL = (-$TEI*$EIx/$EI - $THI*$HIx/$HI)/cos($theta*pi/180);
$FDx = $TDE*$DEx/$DE;
$FDy = $TDE*$DEy/$DE;
$FHx = -$THI*$HIx/$HI;
$FHy = $THI*$HIy/$HI;
$FDdir = atan2(-$DEy,-$DEx);
$FGx = $TBG*$BGx/$BG;
$FGy = -$TGK - $TBG*$BGy/$BG;
$FG = sqrt($FGx**2 + $FGy**2);
$FGdir = atan2($FGy,$FGx);
$FJx = $FAx-$FDx-$FGx-$FHx;
$FJy = $FAy-$FDy-$FGy-$FHy;
$FJ = sqrt($FJx**2 + $FJy**2);
$FJdir = atan2($FJy,$FJx);
$MJ =  $FGx*($h3+$h4+$h5)  
     + $FHx*$h1 + $FHy*($w2/100) 
     + $FDx*$h1 - $FDy*($w2/100)
     - $FAx*$h3 + $FAy*($w2/100);

$check = -$FBy*($w3+$w4);

