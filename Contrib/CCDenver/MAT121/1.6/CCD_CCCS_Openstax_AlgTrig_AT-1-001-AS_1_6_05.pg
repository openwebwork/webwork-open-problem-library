"contextForm.pl",
"PGcourse.pl",
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

###########################
#  Setup

Context("Form");
Context()->variables->add(y => "Real");


$a = random(2,6,1);
$b = random(3,6,1);
$c = random(3,6,1);



$f=Formula("(y^2+($a+$b)y+$a*$b)/( y^2+($a+$c)*y+$a*$c)")->reduce;


$ans=Formula(" (y+$b)/(y+$c)");


###########################
#  Main text

BEGIN_PGML

Simplify the following rational expression. 

 [`` [$f] ``]

 [_______________]{$ans} [@ AnswerFormatHelp("equations") @]*


END_PGML

