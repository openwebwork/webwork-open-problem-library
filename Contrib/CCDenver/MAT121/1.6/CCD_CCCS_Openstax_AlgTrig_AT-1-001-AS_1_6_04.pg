Context("Form");

$a = random(2,6,1);
$b = random(3,6,1);

$f=Formula("(x^2-$a*$a)/( x^2-($a+$b)*x+$a*$b)")->reduce;


$ans=Formula(" (x+$a)/(x-$b)");


###########################
