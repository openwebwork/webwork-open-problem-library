DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGcourse.pl"
);


$a = random(1,9,1);
$b = random(1,9,1);


TEXT(beginproblem() );
BEGIN_TEXT
Let \( y = y(x)\) be implicitly defined by the equation \( x = $a x y + $b ln(y) \).
$PAR
\( \displaystyle \frac{dy}{dx} \) = \{ans_rule(40)\}
END_TEXT


$ans = "y*(1 - $a*y)/($a*x*y + $b)";

ANS(fun_cmp($ans, var=>2) );


ENDDOCUMENT();
