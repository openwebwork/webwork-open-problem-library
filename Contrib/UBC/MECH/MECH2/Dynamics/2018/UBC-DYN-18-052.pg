DOCUMENT();

#image scale
$imgScale = .2;

#image aspect ratio
$width = $imgScale*2188;
$height = $imgScale*1827;

loadMacros(   
	"PGstandard.pl",     # Standard macros for PG language
	"MathObjects.pl",
	"PGML.pl",
	"parserPopUp.pl",
	"parserMultiAnswer.pl",
	"parserRadioButtons.pl",
	#"source.pl",        # allows code to be displayed on certain sites.
	#"PGcourse.pl", 	 # Customization file for the course
   	);

##############################################################
#
#  Setup
#
#

Context("Numeric");
#given
$k2 = random (10,100,1); #N/m
$k1 = 300; #N/m
$b0 = 0.5; #m  
$m = random(10,30,0.6); #kg
$a = 0.5; #m
$b = 0.8; #m
$c = random(0.1,5,0.6); #kg/s

#computation
$a1 = $m;
$a2 = $c;
$a3 = ($k1 + $k2*$b**2/$a**2);
$a4 = $k2 *$b / $a * $b0;


#set tolerance
Context()->flags->set(
tolerance=>.5,
tolType=>'absolute'
);

