Context("Numeric");
#given
$m = random(4000,5000,1);


#computation 
$p = -($m/3600)*(2000-2500 + ((5500/60)**2-(2500/60)**2)/2000 + (1-5)*9.81/1000)-1500/3600;

#set tolerance
Context()->flags->set(
tolerance=>.01);

