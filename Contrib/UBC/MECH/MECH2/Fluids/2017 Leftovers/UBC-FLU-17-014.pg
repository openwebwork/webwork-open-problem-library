Context("Numeric");
#given
$A = random(2,3,.1);
$B = random(2,4,.1);

#image scale
$imgScale =0.2;

#image aspect ratio
$width = $imgScale*1466;
$height = $imgScale*917;

#computation
$v = 8*$B*998*(1.8205*(10**(-5)))/(1.001*(10**(-3)));

#set tolerance
Context()->flags->set(
tolerance=>.01);

