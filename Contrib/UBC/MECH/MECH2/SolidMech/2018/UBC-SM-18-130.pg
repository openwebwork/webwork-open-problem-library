   	);

TEXT(beginproblem());

##############################################################
#
#  Setup
#
#

Context("Complex");

do{
	#given
	$L1 = random(2,2.5,.05);
	$L2 = random(2.5,3,.05);
	$load = random(5,10,.1);
	$gamma = random(5,10,.1);
	$bend = random(10,25,.1);

	#computation 
	$q1 = $load*$L2/2;
	$q0 = 4 * $gamma / 3;
	$a = $bend * 8 * 1000000;
	$b = -$q0 * .5 * $L1**2* 1000 * 27;
	$c = 0;
	$d = -$q1 * $L1**2 * .5* 1000 * 27;

	$p = -$b / (3*$a);
	$q = $p**3 - ($p *  $c + $d)/(2 * $a);
	$r = $c / (3 * $a);
	$Q = sqrt($q**2 + ($r - $p**2)**3);

	$B = $p + ($q - $Q)**(1/3) + ($q + $Q)**(1/3)*1000;
	$H = 4*$B/3;
}while($B <= 0 or $H <= 0);

#set tolerance
Context()->flags->set(
tolerance=>.05);

