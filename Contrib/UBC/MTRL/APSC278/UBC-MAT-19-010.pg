#image scale
$imgScale = .3;

#image aspect ratio
$width = $imgScale*3827;
$height = $imgScale*2033;

loadMacros(   
	"PGstandard.pl",     # Standard macros for PG language
	"MathObjects.pl",
	"PGML.pl",
	"parserPopUp.pl",
	"parserMultiAnswer.pl",
	"parserRadioButtons.pl",
