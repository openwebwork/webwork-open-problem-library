##DESCRIPTION
##KEYWORDS('derivatives', 'inverse trig functions')
##  differentiation of function a sin(b x)* arcsin(x)
##ENDDESCRIPTION

##KEYWORDS('Derivatives')
##Tagged by ynw2d

##DBsubject('Calculus')
##DBchapter('Differentiation')
##DBsection('Derivatives of Trigonometric Functions')
## TitleText1('Calculus: Early Transcendentals')
## EditionText1('1')
## AuthorText1('Rogawski')
## Section1('3.9')
## Problem1('23')

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

Context("Numeric");

$a = random(2,8,1);
$tag = random(0,1,1);

@funct = ('\sin x ','\cos x ');
@dfunct = ('\cos x ','-\sin x');
@g = (Formula("sin(x)"),Formula("cos(x)"));

$f = Formula("$a*$g[$tag]*arcsin(x)");
$df = $f->D->reduce;

Context()->texStrings;
BEGIN_TEXT
Let \[ f(x) = $a $funct[$tag] \sin^{-1}(x)  \]
$PAR
\( f'( x ) = \) \{ans_rule(40) \}
$BR
NOTE:  The webwork system will accept \( \arcsin(x) \) and not \( \sin ^{-1}(x) \) as the inverse of \( \sin (x) \).
END_TEXT
Context()->normalStrings;

ANS($df->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION') );
$PAR SOLUTION $PAR

\(f'(x) = $a( \frac{$funct[$tag]}{\sqrt{1-x^2}} + $dfunct[$tag] sin^{-1} x)\)
END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();        # This should be the last executable line in the problem.

