## KEYWORDS('Integration')
## DBsubject('Calculus')
## DBchapter('Principles of Integral Evaluation')
## DBsection('Integrating Rational Functions by Partial Fractions')
## TitleText1('Calculus')
## EditionText1('9') 
## AuthorText1('Anton')
## Section1('7.5')
## Problem1('50')
## Author('Brian Burns')
## Institution('John Wiley & Sons')


DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",            
  "MathObjects.pl","littleneck.pl","contextFraction.pl"          
);
# allow the student to change the seed for this problem.

rand_button();


TEXT(beginproblem());

###################################
# Setup
Context()->variables->add(u=>'Real',t=>'Real',A=>'Real',B=>'Real',C=>'Real');

$a=non_zero_random(-8,8,1);
$F1=Formula("A")->reduce;
$F2=Formula("2*$a*A+B")->reduce;
$F3=Formula("$a**2*A+$a*B+C")->reduce;
$l1=Formula("[(x+$a)]")->reduce;
($n,$d)=reduce($a**2,2);
$B=Compute("$a*(-2)");
$A=Compute("1");
$c=Compute("$a*$a")->reduce;
$f=Formula("$A*[1/$l1]+$B/$l1**2+$c/$l1**3")->reduce;
$G=Formula("(x**2)/[$l1]^3")->reduce;
$ansd=Formula("$A*ln|$l1|-$B/($l1)-$n/($d*$l1**2)")->reduce;
$ans=Compute("$ansd");
###################################
# Main text
Context()->texStrings;
BEGIN_TEXT
Evaluate the integral.
$PAR
\( \displaystyle  \int $G \;dx\) \(=\) \{ans_rule(20) \} \(+C\)
END_TEXT

###################################
# Answers

#$showPartialCorrectAnswers = 0;

ANS($ans->cmp);

###################################
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR
Using partial fraction, \[$G=\frac{A}{($l1)}+\frac{B}{($l1)^2}+\frac{C}{($l1)^3}\] this gives \[\begin{cases}
$F1=1\\
$F2=0\\
$F3=0
\end{cases}\]giving \[A=$A,\;B=$B,\;C=$c\] therefore
\[ \int  $G \;dx= \int \left( $f\right) \;dx=$ansd+C\]

END_SOLUTION
Context()->normalStrings;

###################################
ENDDOCUMENT();