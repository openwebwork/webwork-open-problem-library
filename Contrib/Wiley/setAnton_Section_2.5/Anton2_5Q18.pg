DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "littleneck.pl",
  "PGcourse.pl"
);
# allow the student to change the seed for this problem.


rand_button();


TEXT(beginproblem());

###################################
# Setup
$a=random(1,5,1);
$b=random(1,4,1);
$c=2*$b;
$d=random(1,5,1);
##
## f(x)= $a (x^2 +$d) cot(x) / ($b-cot(x))
##
$f1a= Formula("x**2+$d");
$f1b= Formula("cot(x)");
$f1=$f1a*$f1b;
$f2= Formula("$b-cot(x)");
$f2alt=Formula("$b-cos(x)*csc(x)");
$f=($a*$f1)/$f2alt->reduce;
$falt=($a*$f1)/$f2->reduce;
$df1b=$f1b->D->reduce;
$df1a=$f1a->D->reduce;
$df1=$f1->D->reduce;
$df2=$f2->D->reduce;
$ans=Formula("($a*($c*x*cot(x)-$b*(x**2+$d)*csc^2(x)-2*x*(cot(x))**2)/($b-cot(x))**2)")->reduce;

###################################
# Main text
Context()->texStrings;
BEGIN_TEXT
Find \(f'(x)\) for \[f(x)=$f\]
$PAR 
\(f'(x)  =\) \{ans_rule(70) \}
END_TEXT
###################################
# Answers

$showPartialCorrectAnswers = 0;

ANS($ans->cmp);

###################################
Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR
First note \[f(x)=$f=$falt\]
Using the quotient rule to differentiate we get,
\[f'(x)=\frac{d}{dx}($f)=$a\frac{\frac{d}{dx}[$f1]\cdot ($f2)-\frac{d}{dx}[$f2]\cdot $f1}{($f2)^2}\]
\[=$a\frac{[$df1a\cdot $f1b+$df1b \cdot ($f1a)]\cdot  ($f2)-[$df2]\cdot $f1}{($f2)^2}\]
\[ =$ans\]
END_SOLUTION
Context()->normalStrings;

###################################
ENDDOCUMENT();
