## KEYWORDS('derivative')
## DBsubject('Calculus')
## DBchapter('Principles of Integral Evaluation')
## DBsection('Integrating Trigonometric Functions')
## TitleText1('Calculus')
## EditionText1('9') 
## AuthorText1('Anton')
## Section1('7.3')
## Problem1('17')
## Author('Brian Burns')
## Institution('John Wiley & Sons')


DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "littleneck.pl",
  "contextFraction.pl",
  "PGcourse.pl"
);
# allow the student to change the seed for this problem.

rand_button();


TEXT(beginproblem());

###################################
# Setup
Context()->variables->add(u=>'Real',t=>'Real');
$a=random(2,9,1);
$B=Formula("pi/(2*$a)")->reduce;
$A=Formula("pi/(2)")->reduce;
$fu=Formula("1-u^2")->reduce;
$u=Formula("sin($a*t)")->reduce;
$v=Formula("($a*t)")->reduce;
$dx=Formula("cos($a*t)")->reduce;
$f=$fu->substitute(u=>$u);
$g=Formula("cos^3($v)")->reduce;
$Fu=Formula("u-u^3/3")->reduce;
Context("Fraction");
Context()->variables->add(u=>'Real',t=>'Real');
if($a==1){$du=""}
else{$du=Formula("1/$a")->reduce;}
$F=Formula("2/(3*$a)")->reduce;
$ans=Compute("$F");
Context("Numeric");
Context()->variables->add(u=>'Real',t=>'Real');
###################################
# Main text
Context()->texStrings;
BEGIN_TEXT
Evaluate the integral.
$PAR
\( \displaystyle \int_{0}^{$B}  $g \;dt\) \(=\) \{ans_rule(50) \}
END_TEXT

###################################
# Answers

#$showPartialCorrectAnswers = 0;

ANS($ans->cmp);

###################################
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR


For \(u=$u,\;$du \;du=$dx\;dt\)
\[ \int_{0}^{$B}  \left($f\right)$dx \;dt= $du \int_{u(0)}^{u($B)} \left($fu\right) \;du=$du\left[$Fu\right]_{0}^{1}=$F\]

END_SOLUTION
Context()->normalStrings;

###################################
ENDDOCUMENT();
