## DESCRIPTION
## Calculus
## ENDDESCRIPTION

## KEYWORDS('derivative')
## DBsubject('Calculus')
## DBchapter('Principles of Integral Evaluation')
## DBsection('Integrating Trigonometric Functions')
## TitleText1('Calculus')
## EditionText1('9') 
## AuthorText1('Anton')
## Section1('7.3')
## Problem1('46')
## Author('Brian Burns')
## Institution('John Wiley & Sons')


DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",            
  "MathObjects.pl","littleneck.pl","contextFraction.pl"          
);
# allow the student to change the seed for this problem.

rand_button();


TEXT(beginproblem());

###################################
# Setup
Context()->variables->add(u=>'Real',t=>'Real');
$a=random(2,9,1); $ta=3*$a;
$B=Formula("pi/(3*$a)")->reduce;
$fu=Formula("u^2")->reduce;
$u=Formula("sec($a*t)")->reduce;
$v=Formula("($a*t)")->reduce;
$dx=Formula("sec($a*t)*tan($a*t)")->reduce;
$f=$fu->substitute(u=>$u);
$g=Formula("sec^3($v)*tan($v)")->reduce;
$Fu=Formula("u^3/3")->reduce;
Context("Fraction");
Context()->variables->add(u=>'Real',t=>'Real');
$du=Formula("1/$a")->reduce;
$ans=Compute("7/$ta")->reduce;
Context("Numeric");
Context()->variables->add(u=>'Real',t=>'Real');
###################################
# Main text
Context()->texStrings;
BEGIN_TEXT
Evaluate the integral.
$PAR
\( \displaystyle \int _{0}^{$B} $g \;dt\) \(=\) \{ans_rule(50) \}
END_TEXT

###################################
# Answers

#$showPartialCorrectAnswers = 0;

ANS($ans->cmp);

###################################
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR


For \(u=$u,\;$du \;du=$dx\;dt\)
\[ \int _{0}^{$B}  \left($f\right)$dx \;dt= $du\int _{u(0)}^{u($B)}  \left($fu\right) \;du=$du\left[$Fu\right]_{1}^{2}=$ans\]

END_SOLUTION
Context()->normalStrings;

###################################
ENDDOCUMENT();
