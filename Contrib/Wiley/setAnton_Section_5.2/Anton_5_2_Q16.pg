DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "littleneck.pl",
  "contextFraction.pl",
  "PGcourse.pl"
);
# allow the student to change the seed for this problem.


rand_button();


TEXT(beginproblem());

###################################
# Setup
# f(y)= ($a  + $b y^2)^2
# 
Context()->variables->add(y=>'Real');
$a=random(1,8,1);
$b=random(1,4,1);
$f1=Formula("$a+$b*y**2")->reduce;
$f2=Formula("($a)**2+2*$a*$b*y**2+($b)**2*y**4")->reduce;
$f=Formula("($f1)**2")->reduce;
$df=$f2->D->reduce;
Context("Fraction");
$A=($a)**2;
$B=Compute("(2*$a*$b)/3");
$C=Compute("(($b)**2)/5");
Context("Numeric");
Context()->variables->add(y=>'Real');
$ans=Compute("$A*y+$B*y**3+$C*y**5");
$Ay=Formula("$A*y")->reduce;
###################################
# Main text
Context()->texStrings;
BEGIN_TEXT
Evaluate the integral and check your answer by differentiating.
$PAR
\( \displaystyle \int $f dy\) \(=\) \{ans_rule(40) \} \(+\) \(C\)
END_TEXT

###################################
# Answers

#$showPartialCorrectAnswers = 0;

ANS($ans->cmp(upToConstant=>1));

###################################
Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

$PAR
\[\displaystyle \int  $f dy= \int \left[ $f2\right]dy=$Ay+$B y^3+$C y^5 +C\]

$BR
\[\frac{d}{dy}\left[$Ay+$B y^3+$C y^5+C\right]=$f2=$f\]
END_SOLUTION
Context()->normalStrings;

###################################
ENDDOCUMENT();
