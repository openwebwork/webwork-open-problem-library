# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Derivative Rules")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"If \( u \), \( v \), and \( w \) are nonzero differentiable functions, then the derivative of \( \frac{uv}{w} \) is",
	"\( \frac{uv'w+u'vw-uvw'}{w^{2}} \)"

);

$radio -> extra(

	"\( \frac{uv'+u'v}{w} \)",
	"\( \frac{u'v'w-uvw'}{w^{2}} \)",
	"\( \frac{uvw'-uv'w-u'vw}{w^{2}} \)",
	"\( \frac{u'vw+uv'w+uvw'}{w^{2}} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
