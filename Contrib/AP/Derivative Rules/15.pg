# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Derivative Rules")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"If \( f(x)=\sin{e^{-x}} \), then \( f'(x) \)=",
	"\( -e^{-x}\cos{e^{-x}} \)"

);

$radio -> extra(

	"\( -\cos{e^{-x}} \)",
	"\( \cos{e^{-x}}+e^{-x} \)",
	"\( \cos{e^{-x}}-e^{-x} \)",
	"\( e^{-x}\cos{e^{-x}} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
