# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Derivative Rules")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"If \( y=10^{x^{2}-1} \), then \( \frac{dy}{dx} \)=",
	"\( 2x(\ln{10})10^{x^{2}-1} \)"

);

$radio -> extra(

	"\( (\ln{10})10^{x^{2}-1} \)",
	"\( (2x)10^{x^{2}-1} \)",
	"\( (x^{2}-1)10^{x^{2}-2} \)",
	"\( x^{2}(\ln{10})10^{x^{2}-1} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
