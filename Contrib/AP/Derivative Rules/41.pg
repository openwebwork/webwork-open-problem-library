## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Derivative Rules")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"\( \frac{d}{dx}(\arcsin{2x})= \)",
	"\( \frac{2}{\sqrt{1-4x^{2}}} \)"

);

$radio -> extra(

	"\( \frac{-1}{2\sqrt{1-4x^{2}}} \)",
	"\( \frac{-2}{\sqrt{4x^{2}-1}} \)",
	"\( \frac{1}{2\sqrt{1-4x^{2}}} \)",
	"\( \frac{2}{\sqrt{4x^{2}-1}} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
