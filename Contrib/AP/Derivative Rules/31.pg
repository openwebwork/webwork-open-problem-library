# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Derivative Rules")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"If \( f(x)=x\ln{x^{2}} \), then \( f(x) \)=",
	"\( \ln{x^{2}}+2 \)"

);

$radio -> extra(

	"\( \ln{x^{2}}+1 \)",
	"\( \ln{x^{2}}+\frac{1}{x} \)",
	"\( \frac{1}{x^{2}} \)",
	"\( \frac{1}{x} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
