## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Derivative Rules")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"If \( f(x)=e^{\tan^2{x}} \), then \( f'(x) \)=",
	"\( 2\tan{x}\sec^2{x}e^{\tan^2{x}} \)"

);

$radio -> extra(

	"\( e^{\tan^2{x}} \)",
	"\( \sec^2{x}e^{\tan^2{x}} \)",
	"\( \tan^2{x}e^{\tan^2{x}-1} \)",
	"\( 2\tan{x}e^{\tan^2{x}} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
