# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Derivative Rules")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"If \( f(x)=e^{\frac{1}{x}} \), then \( f'(x) \)=",
	"\( -\frac{e^{\frac{1}{x}}}{x^{2}} \)"

);

$radio -> extra(

	"\( -e^{\frac{1}{x}} \)",
	"\( \frac{e^{\frac{1}{x}}}{x} \)",
	"\( \frac{e^{\frac{1}{x}}}{x^{2}} \)",
	"\( \frac{1}{x}e^{\frac{1}{x}-1} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
