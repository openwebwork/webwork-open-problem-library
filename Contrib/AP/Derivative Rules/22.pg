# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Derivative Rules")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"If \( f(x)=e^{3\ln{x^{2}}} \), then \( f'(x) \)=",
	"\( 6x^{5} \)"

);

$radio -> extra(

	"\( e^{3\ln{x^{2}}} \)",
	"\( \frac{3}{x^{2}}e^{3\ln{x^{2}}} \)",
	"\( 6(\ln{x})e^{3\ln{x^{2}}} \)",
	"\( 5x^{4} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
