# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Motion")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"A point moves in a straight line so that its distance at time \( t \) from a fixed point of the line is \( 8t-3t^{2} \). What is the total distance covered by the point between \( t=1 \) and \( t=2 \)?",
	"\( 1 \)"

);

$radio -> extra(

	"\( \frac{4}{3} \)",
	"\( \frac{5}{3} \)",
	"\( 2 \)",
	"\( 5 \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
