# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Motion")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"A particle with velocity at any time \( t \) given by \( v(t)=e^{t} \) moves in a strait line. How far does the particle move from \( t=0 \) to \( t=2 \)?",
	"\( e^{2}-1 \)"

);

$radio -> extra(

	"\( e-1 \)",
	"\( 2e \)",
	"\( e^{2} \)",
	"\( \frac{e^{3}}{3} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
