# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Motion")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"The position of an object attached to a sprint is given by \( y(t)=\frac{1}{6}\cos{5t}-\frac{1}{4}\sin{5t} \), where \( t \) is time in seconds. In the first 4 seconds, how many times is the velocity of the object equal to 0?",
	"Five"

);

$radio -> extra(

	"Zero",
	"Three",
	"Six",
	"Seven"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
