# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Motion")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"A point moves on the x-axis in such a way that its velocity at time \( t (t>0) \) is given by \( v=\frac{\ln{t}}{t} \). At what value of \( t \) does \( v \) attain its maximum?",
	"\( e \)"

);

$radio -> extra(

	"\( 1 \)",
	"\( e^{\frac{1}{2}} \)",
	"\( e^{\frac{3}{2}} \)"

);

$radio -> makeLast(

	"There is no maximum value for \( v \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
