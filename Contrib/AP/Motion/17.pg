# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Motion")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"The acceleration of a particle moving along the x-axis at time \( t \) is given by \( a(t)=6t-2 \). If the velocity is 25 when \( t=3 \) and the position is 10 when \( t=1 \), then the position \( x(t)= \)",
	"\( t^{3}-t^{2}+4t+6 \)"

);

$radio -> extra(

	"\( 9t^{2}+1 \)",
	"\( 3t^{2}-2t+4 \)",
	"\( t^{3}-t^{2}+9t-20 \)",
	"\( 36t^{3}-4t^{2}-77t+55 \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
