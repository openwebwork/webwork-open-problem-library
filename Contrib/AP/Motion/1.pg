# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Motion")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"A particle moves along the x-axis so that at any time \( t \geq 0 \) its position is given by \( x(t)=t^{3}-3t^{2}-9t+1 \). For what values of \( t \) is the particle at rest?",
	"\( 3 \) only"

);

$radio -> extra(

	"\( 1 \) only",
	"\( 5 \) only",
	"\( 1 \) and \( 3 \)"

);

$radio -> makeLast(

	"No values"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
