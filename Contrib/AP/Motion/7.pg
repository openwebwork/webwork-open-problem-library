# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Motion")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"A particle moves along a line so that at time \( t \), where \( 0 \leq t \leq \pi \), its position is given by \( s(t)=-4\cos{t}-\frac{t^{2}}{2}+10 \). What is the velocity of the particle when its acceleration is zero?",
	"\( -5.19 \)"

);

$radio -> extra(

	"\( 0.74 \)",
	"\( 1.32 \)",
	"\( 2.55 \)",
	"\( 8.13 \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
