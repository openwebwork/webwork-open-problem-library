# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Motion")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"At \( t=0 \) a particle starts at rest and moves along a line in such a way that at time \( t \) its acceleration is \( 24t^{2} \) feet per second per second. Through how many feet does the particle move during the first 2 seconds?",
	"\( 32 \)"

);

$radio -> extra(

	"\( 48 \)",
	"\( 64 \)",
	"\( 96 \)",
	"\( 192 \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
