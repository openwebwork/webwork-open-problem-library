# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Motion")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"The velocity of a particle moving on a line at time \( t \) is \( v=3t^{\frac{1}{2}}+5t^{\frac{3}{2}} \) meters per second. How many meters dis the particle travel from \( t=0 \) to \( t=4 \)?",
	"\( 80 \)"

);

$radio -> extra(

	"\( 32 \)",
	"\( 40 \)",
	"\( 64 \)",
	"\( 184 \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
