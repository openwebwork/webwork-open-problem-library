# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Definition of a Derivative")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"What is \( \lim_{h\to0}{\frac{\frac{1}{2}(2+h)^{6}-\frac{1}{2}(2)^{6}}{h}} \)?",
	"\( 96 \)"

);

$radio -> extra(

	"\( 16 \)",
	"\( 48 \)",
	"\( 64 \)"

);

$radio -> makeLast(

	"none of these"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
