# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Limits")
## DBsection("Limits")
## KEYWORDS("limits")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"\( \lim_{x\to\infty}{\frac{3n^{3}-5n}{n^{3}-2n^{2}+1}} \) is",
	"\( 3 \)"

);

$radio -> extra(

	"\( -5 \)",
	"\( -2 \)",
	"\( 1 \)"

);

$radio -> makeLast(

	"nonexistent"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
