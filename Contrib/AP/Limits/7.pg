);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"\( \lim_{x\to1}{\frac{x}{\ln{x}}} \) is",
	"nonexistent"

);

$radio -> extra(

	"\( 0 \)",
	"\( 1 \)",
	"\( e \)",
	"\( \frac{1}{e} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

