# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Limits")
## DBsection("Limits")
## KEYWORDS("limits")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"\( \lim_{x\to\infty}{(1+5e^{x})^{\frac{1}{x}}} \) is",
	"\( e \)"

);

$radio -> extra(

	"\( 0 \)",
	"\( 1 \)",
	"\( e^{5} \)"

);

$radio -> makeLast(

	"nonexistent"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
