# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Implicit Differentiation")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"If \( x^{2}+xy+y^{3}=0 \), then in terms of \( x \) and \( y \), \( \frac{dy}{dx}= \)",
	"\( -\frac{x+3y^{2}}{2x+y} \)"

);

$radio -> extra(

	"\( -\frac{2x+y}{x+3y^{2}} \)",
	"\( \frac{-2x}{1+3y^{2}} \)",
	"\( -\frac{2x}{x+3y^{2}} \)",
	"\( -\frac{2x+y}{x+3y^{2}-1} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
