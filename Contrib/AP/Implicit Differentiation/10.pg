$radio -> qa(

	"If \( x^{2}+xy=10 \), then when \( x=2 \), \( \frac{dy}{dx}= \)",
	"\( -\frac{7}{2} \)"

);

$radio -> extra(

	"\( -2 \)",
	"\( \frac{2}{7} \)",
	"\( \frac{3}{2} \)",
	"\( \frac{7}{2} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

