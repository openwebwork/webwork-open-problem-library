# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Implicit Differentiation")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"If \( x^{2}+y^{2}=25 \), what is the value of \( \frac{d^{2}y}{dx^{2}} \) at the point \( \frac{4}{3} \)?",
	"\( -\frac{25}{27} \)"

);

$radio -> extra(

	"\( -\frac{7}{27} \)",
	"\( \frac{7}{27} \)",
	"\( \frac{3}{4} \)",
	"\( \frac{25}{27} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
