$radio -> qa(

	"If \( y^{2}-2xy=16 \), then \( \frac{dy}{dx}= \)",
	"\( \frac{y}{y-x} \)"

);

$radio -> extra(

	"\( \frac{x}{y-x} \)",
	"\( \frac{y}{x-y} \)",
	"\( \frac{y}{2y-x} \)",
	"\( \frac{2y}{x-y} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

