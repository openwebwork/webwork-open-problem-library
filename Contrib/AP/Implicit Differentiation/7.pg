# DESCRIPTION
# AP Question for Limits
# ENDDESCRIPTION

## DBsubject("Calculus")
## DBchapter("Derivatives")
## DBsection("Implicit Differentiation")
## KEYWORDS("derivative")

# Init

DOCUMENT();

loadMacros(

	"PGstandard.pl",
	"MathObjects.pl",
	"PGchoicemacros.pl"

);

# Setup

Context("Numeric");

$radio = new_multiple_choice();

$radio -> qa(

	"If \( x^{3}+3xy+2y^{3}=17 \), then in terms of \( x \) and \( y \), \( \frac{dy}{dx}= \)",
	"\( -\frac{x^{2}+y}{x+2y^{2}} \)"

);

$radio -> extra(

	"\( -\frac{x^{2}+y}{x+y^{2}} \)",
	"\( -\frac{x^{2}+y}{x+2y} \)",
	"\( -\frac{x^{2}+y}{2y^{2}} \)",
	"\( -\frac{x^{2}}{1+2y^{2}} \)"

);

# Main Text

TEXT(beginproblem());
Context() -> texStrings;

BEGIN_TEXT

\{ $radio -> print_q() \}
\{ $radio -> print_a() \}

END_TEXT

Context() -> normalStrings;

# Answer

ANS(radio_cmp($radio -> correct_ans()));

ENDDOCUMENT();
