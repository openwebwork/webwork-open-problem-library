# Print problem number and point value (weight) for the problem
TEXT(beginproblem());

# Show which answers are correct and which ones are incorrect
$showPartialCorrectAnswers = 1;


##############################################################
#  Setup

Context("Numeric");
# Context()->flags->set(tolerance => 0.01);
Context()->variables->add(y => 'Real');
# See http://webwork.maa.org/wiki/Introduction_to_contexts for more options



## VERY easy with ln(x), after simplifying (easy version?)
## product rule with other fns:
$g = list_random('sin','e^','cos');
$n = random(2,4,1);
$m = non_zero_random(2,4,1);
$a = non_zero_random(1,1,1);
$b = non_zero_random(2,3,1);

$h = Compute("$b*y^$m")->reduce();

#$f = Compute("$g($a x^$n y^$m)")->reduce();
$f = Compute("$a*x^$n*$g($h)")->reduce();
$fx = $f->D('x');
$fy = $f->D('y');

$fxx = $fx->D('x');
$fyy = $fy->D('y');
$fxy = $fx->D('y');
$fyx = $fy->D('x');


#################################################
# Write Problem 
#
# PGML syntax at 
# 	http://webwork.maa.org/wiki/Introduction_to_PGML#.V8CReFQko2w
# 	http://webwork.maa.org/wiki/Category:PGML_Syntax#.V8DHHVQko2w


BEGIN_PGML

Let [`f(x) = [$f] `]. Compute the following mixed partial derivatives.

[`f_x = `][________________________________________________]{$fx}

[`f_y = `][________________________________________________]{"$fy"}

[`f_{xx} = `][________________________________________________]{"$fxx"}

[`f_{xy} = `][________________________________________________]{"$fxy"}

[`f_{yx} = `][________________________________________________]{"$fyx"}

[`f_{yy} = `][________________________________________________]{"$fyy"}

END_PGML


ENDDOCUMENT();
