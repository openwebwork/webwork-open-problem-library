## EditionText1('7')
## Section1('14.7')
## Problem1('')
## KEYWORDS('calculus', 'partial derivatives', 'functions of two variables', 'critical points')

########################################################################

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl",
  "parserVectorUtils.pl"
);
########################################################################

$showPartialCorrectAnswers = 1;

TEXT(beginproblem);

##############################################################

Context("Numeric");
Context()->strings->add(none=>{},EmptySet=>{alias=>"none"});
Context()->variables->add(y =>'Real', z =>'Real');

$a = random(3,9,3);
$b = random(3,9,3);
$c = random(1,5,1);

Context("Point");

$P1 = Point(0,0);
$P2 = Point(0,$b/3);
$P3 = Point(2*$a/3,0);
$P4 = Point(2*$a/3,$b/3);

$ans = List($P1,$P2,$P3,$P4);

##############################################################

Context()->texStrings;
BEGIN_TEXT
Find the critical points of the function \(f(x,y) = x^3 - $a x^2 - 2 y^3 + $b y^2 + $c.\)$BBOLD List your answers as points in the form \((a,b)\). $EBOLD $PAR
Answer (separate by commas): \{ans_rule(30)\} $PAR
END_TEXT
Context()->normalStrings;

##############################################################

ANS( $ans->cmp() );

########################################################################

ENDDOCUMENT();        # This should be the last executable line in the problem.