##DESCRIPTION
##  Algebra problem: Solve a system of 2 linear equations in 2 variables by graphing
##ENDDESCRIPTION

## DBsubject(Algebra)
## DBchapter(Systems of equations and inequalities)
## DBsection(Linear systems)
## Level(2)
## KEYWORDS('linear systems', 'system of linear equations')
## Author(K. Andrew Parker)
## Institution(CUNY City Tech)
## Language(en)

########################################################################

DOCUMENT();      

loadMacros(
   "PGstandard.pl",     # Standard macros for PG language
   "MathObjects.pl",
   "parserAssignment.pl",
   "parserImplicitEquation.pl",
   "contextFraction.pl",
   "AppletObjects.pl");

# Print problem number and point value (weight) for the problem
TEXT(beginproblem());

# Show which answers are correct and which ones are incorrect
$showPartialCorrectAnswers = 1;

##############################################################
#
#  Setup
#
#

Context("Fraction");
parser::Assignment->Allow;
Context()->variables->are(x=>["Real", limits=>[-6,6]], y=>["Real",limits=>[-10,10]]);
#Context()->flags->set(reduceConstants=>0);
Context()->noreduce('(-x)+y', '(-x)-y');

#determine y-intercepts
$a = non_zero_random(-5,5,1);
do { $b = non_zero_random(-5,5,1); } until ($a != $b);

#generate integer point of intersection
$intX = random(-1,1,2)*random(2,5,1);
do { $intY = non_zero_random(-5,5,1); } until ( $intY != $a && $intY != $b);

$mA = Fraction("$intY-$a",$intX)->reduce;
($dyA, $dxA) = $mA->value;
$mB = Fraction("$intY-$b",$intX)->reduce;
($dyB, $dxB) = $mB->value;

Context()->parens->set("("=>{type=>"Point"});
$int = Point($intX, $intY);

Context("ImplicitEquation");
Context()->variables->are(x=>["Real", limits=>[-6,6]], y=>["Real",limits=>[-10,10]]);
#Context()->flags->set(reduceConstants=>0);
Context()->noreduce('(-x)+y', '(-x)-y');
#Context()->{error}{msg}{"Your formula doesn't look like an implicit equation"} 
#  = "It looks like you did not enter an equation. <BR> Double check your entry and try again.";

$LHSA = Formula("$dyA x - $dxA y")->reduce;
$RHSA = Formula("-$dxA*$a")->reduce;
$LHSB = Formula("$dyB x - $dxB y")->reduce;
$RHSB = Formula("-$dxB*$b")->reduce;

$answerA = ImplicitEquation("$LHSA = $RHSA", solutions=>[[$intX,$intY],[0,$a]]);
$answerB = ImplicitEquation("$LHSB = $RHSB", solutions=>[[$intX,$intY],[0,$b]]);

$ansList = List(Real($dxA), Real($dyA), Real($a), Real($dxB), Real($dyB), Real($b));

$custom = sub {
      my ( $correct, $student, $ansHash ) = @_;
      my @errors = ();
      
      my ($dxAc, $dyAc, $intAc, $dxBc, $dyBc, $intBc) = @$correct;
      $LHSAc = Formula("$dyAc x - $dxAc y")->reduce;
      $RHSAc = Formula("-$dxAc*$intAc")->reduce;
      $LHSBc = Formula("$dyBc x - $dxBc y")->reduce;
      $RHSBc = Formula("-$dxBc*$intBc")->reduce;
      my $ansA = ImplicitEquation("$LHSAc = $RHSAc");
      my $ansB = ImplicitEquation("$LHSBc = $RHSBc");

      my ($dxAs, $dyAs, $intAs, $dxBs, $dyBs, $intBs) = @$student; 
      my $sLHSA = Formula("$dyAs x - $dxAs y")->reduce;
      my $sLHSB = Formula("$dyBs x - $dxBs y")->reduce;
      my $sRHSA = Formula("-$dxAs*$intAs")->reduce;
      my $sRHSB = Formula("-$dxBs*$intBs")->reduce;
      my $stuA = ImplicitEquation("$sLHSA = $sRHSA")->reduce;#, solutions=>[[0,$intAs]]);
      my $stuB = ImplicitEquation("$sLHSB = $sRHSB")->reduce;#, solutions=>[[0,$intBs]]);
      
      $ansHash->{preview_latex_string} = "\text{you graphed: }".$stuA->TeX."\text{ and }".$stuB->TeX;
      return 0 if $ansHash->{isPreview};
      my $matchA = ($stuA == $ansA || $stuB == $ansA) ? 1 : 0;
      my $matchB = ($stuA == $ansB || $stuB == $ansB) ? 1 : 0;
      my $duplicate = ($stuA == $stuB) ? 2 : 1;
      push(@errors, "Your graph of \(".$ansA->TeX."\) is incorrect.") unless $matchA;
      push(@errors, "Your graph of \(".$ansB->TeX."\) is incorrect.") unless $matchB;
      push(@errors, "You've graphed the same line twice.") if ($duplicate == 2);
      return (3*($matchA+$matchB)/$duplicate, @errors);
};

######################################
