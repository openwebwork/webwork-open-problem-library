############################
# Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGchoicemacros.pl"
);

TEXT(beginproblem());

############################
# Setup

$showPartialCorrectAnswers = 1;

$radio1 = new_multiple_choice();
$radio1->qa("True or False?","True");
$radio1->makeLast("False");

$radio2 = new_multiple_choice();
$radio2->qa("True or False?","False");
$radio2->extra("True");
$radio2->makeLast("False");

$a = random(-1, 3, 1);
$b = random(4, 7, 1);

############################
# Main Text

Context()->texStrings;
BEGIN_TEXT

\{$radio1->print_q()\} $BR 
\( ($a, $b) \subset [$a, $b]\)
\{$radio1->print_a()\} 

$BR 
\{$radio2->print_q()\} $BR 
\( ($a, $b] \subset [$a, $b) \)
\{$radio2->print_a()\} 

$BR $BR
\{$radio2->print_q()\} $BR 
\( \lbrace $a \rbrace \subset ($a, $b) \)
\{$radio2->print_a()\} 

$BR $BR
\{$radio2->print_q()\} $BR 
\( [$a, $b] \subset \lbrace $a, $b \rbrace \)
\{$radio2->print_a()\} 


END_TEXT
Context()->normalStrings;

############################
# Answer Evaluation

ANS(radio_cmp($radio1->correct_ans()));
ANS(radio_cmp($radio2->correct_ans()));
ANS(radio_cmp($radio2->correct_ans()));
ANS(radio_cmp($radio2->correct_ans()));


ENDDOCUMENT();