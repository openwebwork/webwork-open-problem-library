DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGML.pl",
  "MathObjects.pl",
  "parserPopUp.pl",
  "PGcourse.pl"
);

$showPartialCorrectAnswers = 1;
TEXT(beginproblem());

Context("Interval");

$a = random(1, 6, 1);
$b = random(1, 5, 1);
$c = random(1, 5, 1);
$d = random(1, 5, 1);
$e = $a + $b;
$f = $e+$c;
$g = $f+$d;

BEGIN_PGML
What is [`([$a], [$f]) \cap ([$e], [$g]]`]? [____________]{Compute("($e, $f)")}

What is [`([$a], [$f]) \cup ([$e], [$g]]`]? [____________]{Compute("($a, $g]")}
END_PGML

    ENDDOCUMENT();
