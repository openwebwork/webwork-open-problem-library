DOCUMENT();

    loadMacros(
      "PGstandard.pl",
      "PGML.pl",
      "MathObjects.pl",
      "parserPopUp.pl",
      "PGcourse.pl",
    );
 
$showPartialCorrectAnswers = 1;
TEXT(beginproblem());

Context("Interval"); 

$a = random(-7, -3, 1);
$b = random(-5, -2, 1); 
$c = random(1, 6, 1);
$d = $a*$b;
$e = $a*$c;
$f = $b/$a;
$g = $c/$a;

BEGIN_PGML
Let [`f(x) = [$a]x`].  Let [`S = [[$b], [$c])`].  

What is [`f (S)`]? [____________]{Compute("($e, $d]")}

What is [` f^{-1} (S)`]? [____________]{Compute("($g, $f]")}

END_PGML

    ENDDOCUMENT();