############################
# Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGchoicemacros.pl"
);

TEXT(beginproblem());

############################
# Setup

$showPartialCorrectAnswers = 1;

$radio1 = new_multiple_choice();
$radio1->qa("True or False?","True");
$radio1->makeLast("False");

$radio2 = new_multiple_choice();
$radio2->qa("True or False?","False");
$radio2->extra("True");
$radio2->makeLast("False");

$a = random(-1, 1, 1);
$b = random(2, 5, 1);
$c = random(6, 8, 1);
$d = $b + 1;
$e = $b + 2;

############################
# Main Text

Context()->texStrings;
BEGIN_TEXT

\{$radio1->print_q()\} $BR 
\( \lbrace $a, $b, $c \rbrace = \lbrace $c, $b, $a \rbrace \)
\{$radio1->print_a()\} 

$BR 
\{$radio2->print_q()\} $BR 
\( \lbrace $b, $d, $e \rbrace = [$b, $e] \)
\{$radio2->print_a()\} 

$BR $BR
\{$radio1->print_q()\} $BR 
\( \lbrace x : $a \leq x \leq $c \rbrace = [$a, $c] \)
\{$radio1->print_a()\} 

$BR $BR
\{$radio2->print_q()\} $BR 
\( \lbrace n : $a \leq n \leq $c \rbrace = [$a, $c] \)
\{$radio2->print_a()\} 


END_TEXT
Context()->normalStrings;

############################
# Answer Evaluation

ANS(radio_cmp($radio1->correct_ans()));
ANS(radio_cmp($radio2->correct_ans()));
ANS(radio_cmp($radio1->correct_ans()));
ANS(radio_cmp($radio2->correct_ans()));


############################
# Solution

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR Solution: $PAR
Order doesn't matter in sets.   Conventionally, \(x\) is used to denote real numbers, but \( n\) is used to denote integers.
 $BR
END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();