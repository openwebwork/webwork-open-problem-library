############################
# Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGchoicemacros.pl"
);

$a = random(1, 5, 1);
$b = random(1, 3, 1);
$c = $a+$b;
$d = $a-$b;
$e = random(.1, .5, .1);
$f = $a+$e;
$g = $a-$e;

TEXT(beginproblem());

$mc = new_checkbox_multiple_choice();

$mc -> qa ("For which of the following sets is $a an interior point?", 
"\( ($d, $c) \)$BR",
"\( [$g, $f] \)$BR",
);

$mc -> extra(
"\( [$a,$c] \)$BR",
"\( \lbrace $a \rbrace \)$BR",
"\( \mathbb{Z}\)$BR"
);
$mc -> makeLast("None of the above");

BEGIN_TEXT

\{ $mc -> print_q() \}
$BR
\{ $mc -> print_a() \}

END_TEXT

install_problem_grader(~~&std_problem_grader);

$showPartialCorrectAnswers = 1;

ANS( checkbox_cmp( $mc->correct_ans() ) );

ENDDOCUMENT();
