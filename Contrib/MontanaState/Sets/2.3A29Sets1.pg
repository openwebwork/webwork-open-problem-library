# Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGchoicemacros.pl"
);

TEXT(beginproblem());

$mc = new_checkbox_multiple_choice();

$mc -> qa ("For which of the following sets is 35 a bound?", 
"\( \lbrace x : x^{2} < 144 \rbrace \)$BR",
"\( [-35, 35]\)$BR",
);

$mc -> extra(
"\( (-50, 12] \)$BR",
"\( (-\infty, 22) \)$BR",
"\( (-25, 40] \)$BR",
"\( \lbrace x : \sqrt{x} < 10 \rbrace \)$BR",
);
$mc -> makeLast("None of the above");

BEGIN_TEXT

\{ $mc -> print_q() \}
$BR
\{ $mc -> print_a() \}

END_TEXT

install_problem_grader(~~&std_problem_grader);

$showPartialCorrectAnswers = 1;

ANS( checkbox_cmp( $mc->correct_ans() ) );

ENDDOCUMENT();