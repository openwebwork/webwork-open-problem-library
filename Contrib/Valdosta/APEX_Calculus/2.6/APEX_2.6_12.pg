#Problem APEX 2.6.12

DOCUMENT();

# Load whatever macros you need for the problem
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

## DBsubject(Calculus - single variable)
## DBchapter(Differentiation)
## DBsection(Derivatives involving multiple rules (all rules))
## Institution('Valdosta State University')
## Author('S. V. Ault')
## TitleText1('APEX Calculus')
## AuthorText1('Hartman')
## EditionText1('3.0')
## Section1('2.6')
## Problem1('12')

$showPartialCorrectAnswers = 1;

$a = random(3,8,1);

$f = "\sqrt[$a]{t}";
$f_ans = "t^(1/$a)";

$a1 = 1 - $a;
$fp = "\frac{1}{$a}t^{$a1/$a}";
$fp_ans = "(1/$a)t^($a1/$a)";

$b = random(2,8,1)*random(-1,1,2);
$c = random(0.1,0.9,0.1)*random(-1,1,2);
$bc = $b*$c;

$choice = random(1,4,1);
if ($choice==1) {
  $g = "\sec t + $b e^{$c t}";
  $gp = "\sec t \tan t + $bc e^{$c t}";
  $g_ans = "sec(t) + $b e^{$c t}";
  $gp_ans = "sec(t)*tan(t) + $bc e^{$c t}";
} elsif ($choice==2) {
  $g = "\csc t + $b e^{$c t}";
  $gp = "-\csc t \cot t + $bc e^{$c t}";
  $g_ans = "csc(t) + $b e^{$c t}";
  $gp_ans = "-csc(t)*cot(t) + $bc e^{$c t}";
} elsif ($choice==3) {
  $g = "\tan t + $b e^{$c t}";
  $gp = "\sec^2 t + $bc e^{$c t}";
  $g_ans = "tan(t) + $b e^{$c t}";
  $gp_ans = "(sec(t))^2 + $bc e^{$c t}";
} else {
  $g = "\cot t + $b e^{$c t}";
  $gp = "-\csc^2 t + $bc e^{$c t}";
  $g_ans = "cot(t) + $b e^{$c t}";
  $gp_ans = "-(csc(t))^2 + $bc e^{$c t}";
}

$ans = "($f_ans)*($gp_ans) + ($fp_ans)*($g_ans)";

TEXT(beginproblem());

BEGIN_TEXT
Compute the derivative of the given function.
\[
  f(t) = $f($g).
\]
$BR
\( f'(t) = \)\{ ans_rule(25) \}.
END_TEXT

ANS( fun_cmp( $ans, vars=>['t'] ));

SOLUTION(EV3(<<'END_SOLUTION'));
$BR$BBOLD Solution:$EBOLD 
$PAR
Use the Product Rule.
\[
  \begin{array}{rcl}
  f'(t) &=&\displaystyle $f \frac{d}{dx}[ $g ]
  + \frac{d}{dx}[$f] ($g) \\
  &&\\
  &=&\displaystyle $f ($gp) + $fp ($g) 
  \end{array}
\]
END_SOLUTION

ENDDOCUMENT();    



