DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGgraphmacros.pl",
"PGmatrixmacros.pl",
"PGnumericalmacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$NO_SPACE = '@{}';

$r11 = non_zero_random(-5,5,1);
$r21 = non_zero_random(-5,5,1);
$r31 = non_zero_random(-5,5,1);
$r41 = non_zero_random(-5,5,1);
$mult = random(2,3,1)*random(-1,1,2);
$r12 = $r11*$mult;
$r22 = $r21*$mult;
$r32 = $r31*$mult;
$r42 = $r41*$mult;

$s1 = non_zero_random(-9,9,1);
$s2 = non_zero_random(-9,9,1);
$s3 = random(-9,9,1);
$s4 = random(-9,9,1);
$s5 = non_zero_random(-9,9,1);

$t1 = random(6,9,1);
$t2 = non_zero_random(-5,5,2);
$t3 = non_zero_random(-4,4,2);

BEGIN_TEXT

\{mbox( 'Let \(A= \)', display_matrix([[$r11, $r12],[$r21, $r22],[$r31, $r32], [$r41, $r42]]), '.' )\}
$BR
Find \(k\) such that \(Ker A\) is a subspace of \( {\mathbb R}^k\) \{ans_rule(10)\} 
$BR
$BR
Find \(k\) such that \(Im A\) is a subspace of \( {\mathbb R}^k\) \{ans_rule(10)\}
$BR
$BR
$BR
\{mbox( 'Let \(B= \)', display_matrix([[$s1, $s2, $s3, $s4, $s5]]), '.' )\}
$BR
Find \(k\) such that the rowspace of \(B\) is a subspace of \( {\mathbb R}^k\) \{ans_rule(10)\} 
$BR
$BR
Find \(k\) such that \(Ker B\) is a subspace of \( {\mathbb R}^k\) \{ans_rule(10)\}
$BR
$BR
$BR
\{mbox( 'Let \(C= \)', display_matrix([[$t1,$t2,0],[$t2,0,$t3],[0,$t3,$t1], [$t3,$t1,$t2]]), '.' )\}
$BR
Find \(k\) such that \(Im C\) is a subspace of \( {\mathbb R}^k\) \{ans_rule(10)\} 
$BR
$BR
Find \(k\) such that the rowspace of \(C\) is a subspace of \( {\mathbb R}^k\) \{ans_rule(10)\}

END_TEXT

ANS(num_cmp(2));
ANS(num_cmp(4));

ANS(num_cmp(5));
ANS(num_cmp(5));

ANS(num_cmp(4));
ANS(num_cmp(3));

ENDDOCUMENT();       # This should be the last executable line in the problem.
