TEXT("$BR$BR",ans_rule(30),"$BR$BR");
ANS( CAPA_ans( $workweight   , 'format' => "%0.2e" , 'sig' => '3 PLUS 13', 'reltol' => 1   ,  'wgt' => $prob_val    ,  'tries' => $prob_try    ,  'unit' => 'J'    ) ); 
