ANS( CAPA_ans( $dv0   , 'format' => "%0.2e" , 'sig' => '3 PLUS 13', 'reltol' => 5   ,  'wgt' => $prob_val    ,  'tries' => $prob_try    ,  'unit' => 'm/s'    ) ); 
