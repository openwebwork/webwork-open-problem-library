$I = random( 10.0   , 80.0   , 1.0   )   ;  
$x0 = random( 2.0   , 8.0   , 0.1   )   ;  
$y0 = random( 2.0   , 8.0   , 0.1   )   ;  
$x1 = floor( 10.0 * $x0 /  random( 2.1   , 2.9   , 0.1   )   ) /  10.0  + 0.0   ;  
$y1 = floor( 10.0 * $y0 /  random( 2.1   , 2.9   , 0.1   )   ) /  10.0  + 0.0   ;  
$Fx = random( 20.0   , 80.0   , 1.0   )   ;  
$torque_0 = $y0 * $Fx   ;  
$torque_1 = abs( $y0  - $y1   ) * $Fx   ;  
$alpha = $torque_0 /  $I   ;  
## 
CAPA_import( " ${CAPA_Tools}Problem " );   
TEXT(CAPA_EV (<<'END_OF_TEXT'));
A force \{ CAPA_tex( '{}/*{}\bf F{}/*{}'  , 'F'  )  \} = \{  spf( $Fx    , "%0.0f" ) \} \{ $N_u \} \{ CAPA_tex( '{}/*{}\bf i{}/*{}'  , 'i'  )  \} acts on an object at a point (\{ CAPA_web( 'x0'  , '{}/*{}x_0{}/*{}'  , 'x<sub>0</sub>'  )  \}, \{ CAPA_web( 'y0'  , '{}/*{}y_0{}/*{}'  , 'y<sub>0</sub>'  )  \}) = (\{  spf( $x0    , "%0.1f" ) \} \{ $m_u \}, \{  spf( $y0    , "%0.1f" ) \} \{ $m_u \}) as shown in the figure.  $BR
\{ CAPA_web( ''  , '\noindent {\centerline{\epsfxsize = 1.4 in \epsffile{${CAPA_GraphicsDirectory}Gtype20/prob14a.eps}}}'  , '<img src = ${CAPA_Graphics_URL}Gtype20/prob14a.gif align=center width=500>'  )  \}
$BR$BR What is the magnitude of the torque generated by this force about the origin?
END_OF_TEXT

## 

TEXT("$BR",ans_rule(30),"$BR");
ANS( CAPA_ans( $torque_0   , 'format' => "%0.2e" , 'sig' => '3 plus 13', 'reltol' => 1   ,  'wgt' => $prob_val    ,  'tries' => $prob_try    ,  'unit' => 'N*m'    ) ); 
