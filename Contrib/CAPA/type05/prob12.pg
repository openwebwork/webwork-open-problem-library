$dist = random( 110.0   , 160.0   , 1.0   )   ;  
$speed = pow( 2. * $smallg * $dist   , 0.5   )   ;  
$time = pow( 2.0 * $dist /  $smallg   , 0.5   )   ;  
## 
CAPA_import( " ${CAPA_Tools}Problem " );   
## 
TEXT(CAPA_EV (<<'END_OF_TEXT'));
The single cable supporting an unoccupied elevator breaks when the elevator is at rest at the top of a \{  spf( $dist    , "%0.0f" ) \} \{ $m_u \} high building.  With what speed does the elevator hit the ground?
END_OF_TEXT

## 

TEXT("$BR$BR",ans_rule(30),"$BR$BR");
ANS( CAPA_ans( $speed   , 'format' => "%0.2e" , 'sig' => '3 PLUS 13', 'reltol' => 1   ,  'wgt' => $prob_val    ,  'tries' => $prob_try    ,  'unit' => 'm/s'    ) ); 
