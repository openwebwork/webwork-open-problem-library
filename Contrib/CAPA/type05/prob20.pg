ANS( CAPA_ans( $a_avg   , 'format' => "%0.2e" , 'sig' => '3 plus 13', 'tol' => $sig_a   ,  'wgt' => $prob_val    ,  'tries' => $prob_try    ,  'unit' => 'm/s^2'    ) ); 
