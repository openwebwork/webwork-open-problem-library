TEXT("$BR$BR",ans_rule(30),"$BR");
ANS( CAPA_ans( $vm   , 'format' => "%0.2e" , 'sig' => '3 PLUS 13', 'tol' => $sig_v   ,  'wgt' => $prob_val    ,  'tries' => $prob_try    ,  'unit' => 'm/s'    ) ); 
