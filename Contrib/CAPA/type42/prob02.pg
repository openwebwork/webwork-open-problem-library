TEXT("$BR",ans_rule(50),"$BR");
ANS( CAPA_ans( $matcondb   , 'format' => "%0.2e" , 'sig' => '3 PLUS 13', 'reltol' => 1   ,  'wgt' => $prob_val    ,  'tries' => $prob_try    ,  'unit' => 'W/m*degC'    ) ); 
