TEXT("$BR$BR",ans_rule(50),"$BR");
ANS( CAPA_ans( $l_fund   , 'format' => "%0.2e" , 'sig' => '3 plus 13', 'reltol' => 1   ,  'wgt' => $prob_val    ,  'tries' => $prob_try    ,  'unit' => 'm'    ) ); 
