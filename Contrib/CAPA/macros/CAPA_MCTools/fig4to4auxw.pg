#   READING FROM fig4to4auxw

## This is the auxilary file for the fig4to4 tool
## 
$seed = random( 1   , 300   , 1   )   ;  
TEXT(CAPA_EV (<<'END_OF_TEXT'));
\{CAPA_map(    $seed     , [ 'lb1', 'lb2', 'lb3', 'lb4' ] , [  $sT1   , $sT2   , $sT3   , $sT4    ]  )\}

\{ CAPA_tex( '\put(' . $cX1  . ',' . $cY1  . '){\large ' . $lb1  . '}'  , ''  )  \}\{ CAPA_tex( '\put(' . $cX2  . ',' . $cY2  . '){\large ' . $lb2  . '}'  , ''  )  \}\{ CAPA_tex( '\put(' . $cX3  . ',' . $cY3  . '){\large ' . $lb3  . '}'  , ''  )  \}\{ CAPA_tex( '\put(' . $cX4  . ',' . $cY4  . '){\large ' . $lb4  . '}'  , ''  )  \}\{ CAPA_tex( '\end{picture}'  , ''  )  \} 
END_OF_TEXT

## 
$slopex = ( $WX2  - $WX1    ) /  ( $TX2  - $TX1    )   ;  
$slopey = ( $WY2  - $WY1    ) /  ( $TY2  - $TY1    )   ;  
$cstx = $WX2  - $slopex * ( $TX2    )   ;  
$csty = $WY2  - $slopey * ( $TY2    )   ;  
## 
$WcX1 = floor( ( $cX1    ) * $slopex  + $cstx   )   ;  
$WcX2 = floor( ( $cX2    ) * $slopex  + $cstx   )   ;  
$WcX3 = floor( ( $cX3    ) * $slopex  + $cstx   )   ;  
$WcX4 = floor( ( $cX4    ) * $slopex  + $cstx   )   ;  
$WcY1 = floor( ( $cY1    ) * $slopey  + $csty   )   ;  
$WcY2 = floor( ( $cY2    ) * $slopey  + $csty   )   ;  
$WcY3 = floor( ( $cY3    ) * $slopey  + $csty   )   ;  
$WcY4 = floor( ( $cY4    ) * $slopey  + $csty   )   ;  
## 
TEXT(CAPA_EV (<<'END_OF_TEXT'));
\{ CAPA_html( '<TABLE><tr><TD>'  )  \} \{ CAPA_html( $AppCode   )  \} \{ CAPA_html( $FigID   )  \} \{ CAPA_html( $WebGrids   )  \} /DIS(html("<PARAM NAME=\"COUNT\" VALUE=4>")) /DIS(html("<PARAM NAME=\"LB0\" VALUE=\"" + lb1 + "\">")) /DIS(html("<PARAM NAME=\"X0\" VALUE="+WcX1+"> <PARAM NAME=\"Y0\" VALUE="+WcY1+">")) /DIS(html("<PARAM NAME=\"LB1\" VALUE=\"" + lb2 + "\">"))/DIS(html("<PARAM NAME=\"X1\" VALUE="+WcX2+"> <PARAM NAME=\"Y1\" VALUE="+WcY2+">"))/DIS(html("<PARAM NAME=\"LB2\" VALUE=\"" + lb3 + "\">"))/DIS(html("<PARAM NAME=\"X2\" VALUE="+WcX3+"> <PARAM NAME=\"Y2\" VALUE="+WcY3+">")) /DIS(html("<PARAM NAME=\"LB3\" VALUE=\"" + lb4 + "\">"))/DIS(html("<PARAM NAME=\"X3\" VALUE="+WcX4+"> <PARAM NAME=\"Y3\" VALUE="+WcY4+">"))  \{ CAPA_html( '</APPLET>'  )  \} \{ CAPA_html( '</TD>'  )  \} 
END_OF_TEXT

## 
## 
$seed = random( 1   , 300   , 1   )   ;  
TEXT(CAPA_EV (<<'END_OF_TEXT'));
\{CAPA_map(    $seed     , [ 'M1', 'M2', 'M3', 'M4' ] , [  $s1   , $s2   , $s3   , $s4    ]  )\}

END_OF_TEXT

$it = CAPA_tex( '\item[] '  , ''  )   ;  
TEXT(CAPA_EV (<<'END_OF_TEXT'));
\{ CAPA_tex( '\vspace*{-4mm}'  , ''  )  \} \{ CAPA_tex( '\begin{choicelist}'  , ''  )  \}\{ CAPA_html( '<TD>'  )  \} 
\{ $it \} 1) \{ $M1 \} 
\{ $it \} 2) \{ $M2 \} 
\{ $it \} 3) \{ $M3 \} 
\{ $it \} 4) \{ $M4 \} 
\{ CAPA_tex( '\end{choicelist}'  , ''  )  \}\{ CAPA_html( '</TD></tr></table>'  )  \} 
\{CAPA_map(    $seed     , [ 'b1', 'b2', 'b3', 'b4' ] , [  $a1   , $a2   , $a3   , $a4    ]  )\}

END_OF_TEXT

$f1 = choose( $b1   , $lb1   , $lb2   , $lb3   , $lb4   )   ;  
$f2 = choose( $b2   , $lb1   , $lb2   , $lb3   , $lb4   )   ;  
$f3 = choose( $b3   , $lb1   , $lb2   , $lb3   , $lb4   )   ;  
$f4 = choose( $b4   , $lb1   , $lb2   , $lb3   , $lb4   )   ;  
$fig4to4right = $f1  + $f2  + $f3  + $f4   ;  
TEXT(CAPA_EV (<<'END_OF_TEXT'));












END_OF_TEXT

