#   READING FROM SM4aux

## This is the auxilary file for the SM4 tool, it maps and scrambles the 
## statements and answers
## 
$m = choose( $mix1   , $s1a   , $s1b   , $s1c   , $s1d   )   ;  
$a = choose( $mix1   , $a1a   , $a1b   , $a1c   , $a1d   )   ;  
$n = choose( $mix2   , $s2a   , $s2b   , $s2c   , $s2d   )   ;  
$b = choose( $mix2   , $a2a   , $a2b   , $a2c   , $a2d   )   ;  
$o = choose( $mix3   , $s3a   , $s3b   , $s3c   , $s3d   )   ;  
$c = choose( $mix3   , $a3a   , $a3b   , $a3c   , $a3d   )   ;  
$p = choose( $mix4   , $s4a   , $s4b   , $s4c   , $s4d   )   ;  
$d = choose( $mix4   , $a4a   , $a4b   , $a4c   , $a4d   )   ;  
$seed = random( 1   , 300   , 1   )   ;  
TEXT(CAPA_EV (<<'END_OF_TEXT'));
\{CAPA_map(    $seed     , [ 'M1', 'M2', 'M3', 'M4' ] , [  $m   , $n   , $o   , $p    ]  )\}

END_OF_TEXT

$it = CAPA_tex( '\item[] '  , ''  )   ;  
TEXT(CAPA_EV (<<'END_OF_TEXT'));
\{ CAPA_tex( '\begin{choicelist}'  , ''  )  \} 
\{ $it \} A) \{ $M1 \} 
\{ $it \} B) \{ $M2 \} 
\{ $it \} C) \{ $M3 \} 
\{ $it \} D) \{ $M4 \} 
\{ CAPA_tex( '\end{choicelist}'  , ''  )  \} 
\{CAPA_map(    $seed     , [ 'b1', 'b2', 'b3', 'b4' ] , [  $a   , $b   , $c   , $d    ]  )\}

END_OF_TEXT

$f1 = choose( $b1   , $sT1   , $sT2   , $sT3   , $sT4   , $sT5   )   ;  
$f2 = choose( $b2   , $sT1   , $sT2   , $sT3   , $sT4   , $sT5   )   ;  
$f3 = choose( $b3   , $sT1   , $sT2   , $sT3   , $sT4   , $sT5   )   ;  
$f4 = choose( $b4   , $sT1   , $sT2   , $sT3   , $sT4   , $sT5   )   ;  
$SM4right = $f1  . $f2  . $f3  . $f4   ;  
