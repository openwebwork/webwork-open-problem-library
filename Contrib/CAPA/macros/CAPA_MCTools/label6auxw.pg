#   READING FROM label6auxw

## This is the auxilary file for the label6w tool
## 
$seed = random( 1   , 300   , 1   )   ;  
TEXT(CAPA_EV (<<'END_OF_TEXT'));
\{CAPA_map(    $seed     , [ 'lb1', 'lb2', 'lb3', 'lb4', 'lb5', 'lb6' ] , [  $sT1   , $sT2   , $sT3   , $sT4   , $sT5   , $sT6    ]  )\}

\{ CAPA_tex( '\put(' . $cX1  . ',' . $cY1  . '){\large ' . $lb1  . '}'  , ''  )  \}\{ CAPA_tex( '\put(' . $cX2  . ',' . $cY2  . '){\large ' . $lb2  . '}'  , ''  )  \}\{ CAPA_tex( '\put(' . $cX3  . ',' . $cY3  . '){\large ' . $lb3  . '}'  , ''  )  \}\{ CAPA_tex( '\put(' . $cX4  . ',' . $cY4  . '){\large ' . $lb4  . '}'  , ''  )  \}\{ CAPA_tex( '\put(' . $cX5  . ',' . $cY5  . '){\large ' . $lb5  . '}'  , ''  )  \}\{ CAPA_tex( '\put(' . $cX6  . ',' . $cY6  . '){\large ' . $lb6  . '}'  , ''  )  \}\{ CAPA_tex( '\end{picture}'  , ''  )  \} 
END_OF_TEXT

## 
$slopex = ( $WX2  - $WX1    ) /  ( $TX2  - $TX1    )   ;  
$slopey = ( $WY2  - $WY1    ) /  ( $TY2  - ( $TY1  - 1.0    )    )   ;  
$cstx = $WX2  - $slopex * ( $TX2    )   ;  
$csty = $WY2  - $slopey * ( $TY2    )   ;  
## 
$WcX1 = floor( ( $cX1    ) * $slopex  + $cstx   )   ;  
$WcX2 = floor( ( $cX2    ) * $slopex  + $cstx   )   ;  
$WcX3 = floor( ( $cX3    ) * $slopex  + $cstx   )   ;  
$WcX4 = floor( ( $cX4    ) * $slopex  + $cstx   )   ;  
$WcX5 = floor( ( $cX5    ) * $slopex  + $cstx   )   ;  
$WcX6 = floor( ( $cX6    ) * $slopex  + $cstx   )   ;  
$WcY1 = floor( ( $cY1    ) * $slopey  + $csty   )   ;  
$WcY2 = floor( ( $cY2    ) * $slopey  + $csty   )   ;  
$WcY3 = floor( ( $cY3    ) * $slopey  + $csty   )   ;  
$WcY4 = floor( ( $cY4    ) * $slopey  + $csty   )   ;  
$WcY5 = floor( ( $cY5    ) * $slopey  + $csty   )   ;  
$WcY6 = floor( ( $cY6    ) * $slopey  + $csty   )   ;  
## 
TEXT(CAPA_EV (<<'END_OF_TEXT'));

\{ CAPA_html( $AppCode   )  \} \{ CAPA_html( $FigID   )  \} \{ CAPA_html( $WebGrids   )  \} /DIS(html("<PARAM NAME=\"COUNT\" VALUE=6>")) /DIS(html("<PARAM NAME=\"LB0\" VALUE=\"" + lb1 + "\">")) /DIS(html("<PARAM NAME=\"X0\" VALUE="+WcX1+"> <PARAM NAME=\"Y0\" VALUE="+WcY1+">")) /DIS(html("<PARAM NAME=\"LB1\" VALUE=\"" + lb2 + "\">"))/DIS(html("<PARAM NAME=\"X1\" VALUE="+WcX2+"> <PARAM NAME=\"Y1\" VALUE="+WcY2+">"))/DIS(html("<PARAM NAME=\"LB2\" VALUE=\"" + lb3 + "\">"))/DIS(html("<PARAM NAME=\"X2\" VALUE="+WcX3+"> <PARAM NAME=\"Y2\" VALUE="+WcY3+">")) /DIS(html("<PARAM NAME=\"LB3\" VALUE=\"" + lb4 + "\">"))/DIS(html("<PARAM NAME=\"X3\" VALUE="+WcX4+"> <PARAM NAME=\"Y3\" VALUE="+WcY4+">")) /DIS(html("<PARAM NAME=\"LB4\" VALUE=\"" + lb5 + "\">"))/DIS(html("<PARAM NAME=\"X4\" VALUE="+WcX5+"> <PARAM NAME=\"Y4\" VALUE="+WcY5+">")) /DIS(html("<PARAM NAME=\"LB5\" VALUE=\"" + lb6 + "\">"))/DIS(html("<PARAM NAME=\"X5\" VALUE="+WcX6+"> <PARAM NAME=\"Y5\" VALUE="+WcY6+">")) \{ CAPA_html( '</APPLET>'  )  \} 
END_OF_TEXT

