\{ CAPA_web( ''  , '\noindent {\centerline{\epsfxsize=3.4in \epsffile{${CAPA_GraphicsDirectory}Gtype09/prob81a.eps}}}'  , '<img src = ${CAPA_Graphics_URL}Gtype09/prob81a.gif align=center width=300>'  )  \}
$BR$BR
END_OF_TEXT

