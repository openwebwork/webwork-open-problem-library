$a = random( 3.00   , 4.05   , 0.10   )   ;  # in g
$mass = random( 65.00   , 75.05   , 0.10   )   ;  # in kg
$force = $mass * ($a+1) * $smallg   ;  
## 
CAPA_import( " ${CAPA_Tools}Problem " );   
## 
TEXT(CAPA_EV (<<'END_OF_TEXT'));
A parachutist of mass \{  spf( $mass    , "%0.1f" ) \} \{ $kg_u \} lands with legs bent, coming to rest with an upwardly directed average acceleration of \{  spf( $a    , "%0.1f" ) \} \{ $g_u \}.  What is the magnitude of the average force exerted on him by the ground? 
END_OF_TEXT

## 

TEXT("$BR$BR",ans_rule(30),"$BR");
ANS( CAPA_ans( $force   , 'format' => "%0.2e" , 'sig' => '3 PLUS 13', 'reltol' => 1   ,  'wgt' => $prob_val    ,  'tries' => $prob_try    ,  'unit' => 'N'    ) ); 
