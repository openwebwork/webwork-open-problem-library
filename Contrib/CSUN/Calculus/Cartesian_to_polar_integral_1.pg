# DESCRIPTION
# Convert an iterated integral in Cartesian coordinates 
# to an iterated integral in polar coordinates.
# ENDDESCRIPTION

## DBsubject('Calculus - multivariable')
## DBchapter('Multiple Integrals')
## DBsection('Double Integrals in Polar Coordinates')
## KEYWORDS('polar','integral,'Cartesian')
## Author('David Protas')
## Institution('CSUN')

DOCUMENT();
loadMacros(
"PGstandard.pl",
"MathObjects.pl",
);

Context("Numeric");
Context()->variables->add(y=>'Real');

$a = random(1,9,1);
$b = 8*$a;
$c = random(1,6,1);
$cs = $c*$c;
$d = $a*$cs;
$Func = Compute("e^{$a x^2 + $a y^2}")->reduce;

TEXT(beginproblem());

Context()->texStrings;
BEGIN_TEXT
Convert the integral
\[ I = \int_0^{$c/\sqrt{2}} \int_y^{\sqrt{$cs - y^2}} $Func \,dx\,dy \]
to polar coordinates, getting
\[\int_C^D \int_A^B h(r,\theta)\,dr\,d\theta, \]
where
$PAR
\(h(r,\theta) = \) \{ans_rule(30) \}
$PAR
\(A =\) \{ans_rule(10) \}
$PAR
\(B =\) \{ans_rule(10) \}
$PAR
\(C =\) \{ans_rule(10) \}
$PAR
\(D =\) \{ans_rule(10) \}
$BR$BR
and then evaluate the resulting integral to get
$PAR
\(I =\) \{ans_rule(30) \}.

END_TEXT
Context()->normalStrings;
$showPartialCorrectAnswers = 1;

ANS(fun_cmp("r*e^($a*r^2)",var=>['r']));
ANS(num_cmp(0));
ANS(num_cmp($c));
ANS(num_cmp(0));
ANS(num_cmp("pi/4"));
ANS(num_cmp("(pi/$b)*(e^($d) - 1)"));

ENDDOCUMENT();