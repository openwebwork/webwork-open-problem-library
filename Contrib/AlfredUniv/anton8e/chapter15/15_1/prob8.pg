DOCUMENT();

# Load whatever macros you need for the problem
loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl",
           "MathObjects.pl",
          );
 
## Do NOT show partial correct answers
$showPartialCorrectAnswers = 0;
#TEXT(beginproblem());
Context("Numeric")->variables->are(x=>'Real',y=>'Real');
Context()->{format}{number} = "";
$x1 = random(1,5);
$deltax = random(1,6);
$x2 = $x1+$deltax;
$y1 = random(1,5);
$deltay = random(1,6);
$y2 = $y1 + $deltay;

$c = random(1,10);
$n = random(3,10);

$f = Formula("x/(x*y+$c)^$n");
$power = $n-2;
$ans1 = ((1/($x2*$y2+$c)**($power))-(1/($x1*$y2+$c)**($power)))/$y2;
$ans2 = ((1/($x2*$y1+$c)**($power))-(1/($x1*$y1+$c)**($power)))/$y1; 
$ans = Compute("1/((-$n+1)*(-$n+2))*($ans1-$ans2)");
$a = 1/((-$n+1)*(-$n+2))*($ans1-$ans2);
Context()->texStrings;
BEGIN_TEXT 

$BR
\(\int_{$x1}^{$x2}\int_{$y1}^{$y2}$f dydx =\)\{ans_rule(20)\}
$BR

Now try the integral \(\int_{$y1}^{$y2}\int_{$x1}^{$x2}$f dxdy\) to see why the order of integration is important.
END_TEXT
Context()->normalStrings;

ANS(num_cmp($a));

ENDDOCUMENT();
