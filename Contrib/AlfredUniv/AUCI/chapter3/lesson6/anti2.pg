## DESCRIPTION
## Determine an Indefinite Integral
## ENDDESCRIPTION

## KEYWORDS('calculus', 'integral', 'indefinite')
## Tagged by YJ

## DBsubject('Calculus')
## DBchapter('Integrals')
## DBsection('Indefinite Integrals')
## Date('9/1/2012')
## Author('Darwyn Cook')
## Institution('Alfred University')
## TitleText1('AUCI')
## EditionText1('1')
## AuthorText1('Petrillo')
## Section1('3.5')
## Problem1(' 1')

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "parserFormulaUpToConstant.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = Real(random(6,18,2));
$b = Real(random(3,5,1));

Context()->texStrings;
BEGIN_TEXT
Evaluate the indefinite integral:
$BR
$PAR
   \( \displaystyle{ \int $a x^{-$b} \,dx} = \)
   \{ans_rule(25) \}.
$BR
$BR
END_TEXT
Context()->normalStrings;

$ans = FormulaUpToConstant("($a*x**(-$b+1))/(-$b+1)");
ANS($ans->cmp());

ENDDOCUMENT();
