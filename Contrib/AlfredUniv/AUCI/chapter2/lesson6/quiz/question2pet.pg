DOCUMENT();


# Load whatever macros you need for the problem
loadMacros(
"MathObjects.pl",
"PG.pl",
"PGbasicmacros.pl",
"parserFormulaUpToConstant.pl"
);
 
## Show partial correct answers
Context()->variables->are(t=>"Real");
$showPartialCorrectAnswers = 0;

## Display the problem information
TEXT(beginproblem());

$a2 = Real(non_zero_random(-9,9));
$a1 = Real(non_zero_random(-9,9));
$a0 = Real(non_zero_random(-9,9));


Context()->texStrings;
BEGIN_TEXT
Evaluate the following indefinite integrals:
$BR
$BR
1.  \(\displaystyle\int $a0 \hskip 2pt dt\) = \{ans_rule\}
$BR
$BR
2.  \(\displaystyle\int $a1 t+$a0 \hskip 2pt dt\) = \{ans_rule\}
$BR
$BR
3.  \(\displaystyle\int $a2 t^2+$a1 t+$a0 \hskip 2pt dt\) = \{ans_rule\}


END_TEXT
Context()->normalStrings;
ANS(FormulaUpToConstant("$a0*t")->cmp() );
ANS(FormulaUpToConstant("$a1*t^2/2+$a0*t")->cmp() );
ANS(FormulaUpToConstant("$a2*t^3/3+$a1*t^2/2+$a0*t")->cmp() );
ENDDOCUMENT();
