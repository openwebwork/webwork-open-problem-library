DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "parserFormulaUpToConstant.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
Context("Numeric");
Context()->variables->add(t=>"Real");

$showPartialCorrectAnswers = 1;

$a = random(3,8,1);
$b = random(2,6,1);
$c = random(-8,-5,1);
$d = random(-4,-2,1);


BEGIN_TEXT
Evaluate each indefinite integral:
$BR
$PAR
(a)   \( \displaystyle{ \int $a e^{$b t} \,dt} = \)
   \{ans_rule(35) \}.
$BR
$PAR
(b)   \( \displaystyle{ \int $b e^{$d t} \,dt} = \)
   \{ans_rule(35) \}.
$BR
$PAR
(c)   \( \displaystyle{ \int $c e^{$a t} \,dt} = \)
   \{ans_rule(35) \}.
$BR
$PAR
(d)   \( \displaystyle{ \int $a e^{-t} \,dt} = \)
   \{ans_rule(35) \}.

$BR
$BR
END_TEXT

$ans1 = FormulaUpToConstant("$a/$b*e^($b*t)");
ANS($ans1->cmp());
$ans2 = FormulaUpToConstant("$b/$d*e^($d*t)");
ANS($ans2->cmp());
$ans3 = FormulaUpToConstant("$c/$a*e^($a*t)");
ANS($ans3->cmp());
$ans4 = FormulaUpToConstant("-$a*e^(-t)");
ANS($ans4->cmp());



ENDDOCUMENT();
