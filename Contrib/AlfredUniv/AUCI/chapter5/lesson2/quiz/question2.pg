DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "parserFormulaUpToConstant.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
Context("Numeric");
Context()->variables->add(t=>"Real");

$showPartialCorrectAnswers = 1;

$a = random(3,8,1);
$b = random(2,6,1);

BEGIN_TEXT
Evaluate the indefinite integral:
$BR
$PAR
   \( \displaystyle{ \int $a e^{$b t} \,dt} = \)
   \{ans_rule(35) \}.
$BR
$BR
END_TEXT

$ans = FormulaUpToConstant("$a/$b*exp($b*t)");
ANS($ans->cmp());

ENDDOCUMENT();
