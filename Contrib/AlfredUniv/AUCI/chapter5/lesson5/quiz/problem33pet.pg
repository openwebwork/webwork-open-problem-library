## DESCRIPTION
## Determine an Indefinite Integral
## ENDDESCRIPTION

## KEYWORDS('calculus', 'integral', 'indefinite')

## DBsubject('Calculus')
## DBchapter('Integrals')
## DBsection('Indefinite Integrals')
## Date('7/23/2012')
## Author('Darwyn Cook')
## Institution('Alfred University')
## TitleText1('AUCI')
## EditionText1('1')
## AuthorText1('Petrillo')
## Section1('5.5')
## Problem1('1')


DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "parserFormulaUpToConstant.pl",
  "answerHints.pl",
  "PGcourse.pl"
);
TEXT(beginproblem());

Context()->variables->add(T=>"Real");

$a = random(2,10,1);
$b = random(2,10,1);
$c = random(2,10,1);
$f = FormulaUpToConstant("$a T + $b ln(abs(T)) - $c/T");
$f->{test_at} = [[-3],[-2],[-1]];
$fx = Formula("$a + $b/T + $c/T^2");

$noabs = FormulaUpToConstant("ln(T)");
$noabs->{test_points} = [[1.3],[2.2],[3.2],[13.9],[40.67]];

Context()->texStrings;
BEGIN_TEXT;
$BR
\(\displaystyle\int \left($fx \right) dT = \)
\{$f->ans_rule(20) \}
$BR
$BR
END_TEXT
Context()->normalStrings;
ANS($f->cmp()->withPostFilter(AnswerHints( 
    $noabs => "Absolute value!!!!", 
))
);


ENDDOCUMENT();
