DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "parserFormulaUpToConstant.pl",
  "answerHints.pl",
  "PGcourse.pl"
);
TEXT(beginproblem());

$f = FormulaUpToConstant("ln(abs(x))");
$f->{test_at} = [[-3],[-2],[-1]];
$fx = Formula("1/x");

$noabs = FormulaUpToConstant("ln(x)");
$noabs->{test_points} = [[1.3],[2.2],[3.2],[13.9],[40.67]];

Context()->texStrings;
BEGIN_TEXT;

$BR
\(\int $fx dx = \)
\{$f->ans_rule(10) \}
$BR
END_TEXT
Context()->normalStrings;
ANS($f->cmp()->withPostFilter(AnswerHints( 
    $noabs => "Absolute value!!!!", 
))
);


ENDDOCUMENT();
