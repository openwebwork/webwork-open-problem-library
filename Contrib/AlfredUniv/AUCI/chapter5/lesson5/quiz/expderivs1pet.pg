## DESCRIPTION
## Determine an Indefinite Integral
## ENDDESCRIPTION

## KEYWORDS('calculus', 'derivative')

## DBsubject('Calculus')
## DBchapter('Differentiation')
## DBsection('Derivatives of Polynomials and Exponential Functions')
## Date('7/23/2012')
## Author('Darwyn Cook')
## Institution('Alfred University')
## TitleText1('AUCI')
## EditionText1('1')
## AuthorText1('Petrillo')
## Section1('5.5')
## Problem1('1')


DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$a = Real(random(2,9));
$b = Real(random(2,9));
$g = Formula("e^x");
$dg = $g->D('x');
$f = Formula("$b^x");
$df = $f->D('x');
$a2 = Real(random(-9,-2));
$b2 = Real(random(2,9));
$f2 = Formula("$a2*$b2^x");
$df2 = $f2->D('x');
$f3 = Formula("1000*(1.03)^x");
$df3 = $f3->D('x');

Context()->texStrings;
BEGIN_TEXT;
$BR
Practice differentiating exponential functions:
$BR$BR
(a) If \(y = $g\), then \(\frac{dy}{dx} = \) \{$dg->ans_rule(15) \}.

$BR
$BR 
(b) If \(y = $f\), then \(\frac{dy}{dx} = \) \{$df->ans_rule(15) \}.

END_TEXT
ANS($dg->cmp());
ANS($df->cmp());

BEGIN_TEXT;
$BR
$BR
(c) If \(y = $f3\), then \(\frac{dy}{dx} = \) \{$df3->ans_rule(15) \}.

END_TEXT

ANS($df3->cmp());

Context("Numeric");

$c = random(2,4,2);
$d = random(3,5,2);

$ans1 = Compute("e^($d*x^2)*2*$d*x");
$ans2 = Compute("$c^($d*x^2)*ln($c)*2*$d*x");

Context()->texStrings;
BEGIN_TEXT;
$BR
$BR
(d)  If \( y = e^{$d x^{2}}\), then \(\frac{dy}{dx} = \) \{ans_rule(20)\}.
$BR
$BR
(e)  If \( y = $c^{$d x^{2}}\), then \(\frac{dy}{dx} = \) \{ans_rule(20)\}.
END_TEXT
Context()->normalStrings;

#####################################################################

ANS($ans1->cmp());
ANS($ans2->cmp());


ENDDOCUMENT();
