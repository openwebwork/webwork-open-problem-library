DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "parserFormulaUpToConstant.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$b = Real(random(2,9));
$f = Formula("$b^x");
$intf = FormulaUpToConstant("$b^x/ln($b)");

Context()->texStrings;
BEGIN_TEXT;
\(\int $f dx= \) \{$intf->ans_rule(20) \}.
END_TEXT
Context()->normalStrings;
ANS($intf->cmp());


ENDDOCUMENT();
