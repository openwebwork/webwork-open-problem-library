## DESCRIPTION
## Determine an Indefinite Integral
## ENDDESCRIPTION

## KEYWORDS('calculus', 'derivative')

## DBsubject('Calculus')
## DBchapter('Differentiation')
## DBsection('Derivatives of Polynomials and Exponential Functions')
## Date('7/23/2012')
## Author('Darwyn Cook')
## Institution('Alfred University')
## TitleText1('AUCI')
## EditionText1('1')
## AuthorText1('Petrillo')
## Section1('5.5')
## Problem1('1')


DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "parserFormulaUpToConstant.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$a = Real(random(2,9));
$b = Real(random(2,9));
$f = Formula("$b^x");
$intf = FormulaUpToConstant("$b^x/ln($b)");
$b2 = Real(random(2,9));
$f2 = Formula("$a $b2^x");
$intf2 = FormulaUpToConstant("$a $b2^x/ln($b2)");
$inte = FormulaUpToConstant("e^x");

Context()->texStrings;
BEGIN_TEXT;
$BR
Practice integrating exponential functions:
$BR
$BR
(a)  \(\displaystyle\int e^x dx= \) \{$inte->ans_rule(20) \} 
$BR
$BR
(b)  \(\displaystyle\int $f dx= \) \{$intf->ans_rule(20) \}
$BR
$BR
(c) \(\displaystyle\int $f2 dx= \) \{$intf2->ans_rule(20) \}

END_TEXT
Context()->normalStrings;
ANS($inte->cmp());
ANS($intf->cmp());
ANS($intf2->cmp());


ENDDOCUMENT();
