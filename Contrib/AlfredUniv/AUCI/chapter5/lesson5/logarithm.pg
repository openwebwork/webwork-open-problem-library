DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "parserFormulaUpToConstant.pl",
  "answerHints.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = Real(random(2,8,1));
$b = Real(random(2,8,1));
$c = Real(random(2,9,1));

Context()->texStrings;
BEGIN_TEXT
Evaluate the indefinite integral:
$BR
$PAR
   \( \displaystyle{ \int \frac{$b - $a x e^{$c x}}{x} \,dx} = \)
   \{ans_rule(35) \}.
END_TEXT
Context()->normalStrings;

$f = FormulaUpToConstant("- $a*exp($c*x)/$c + $b*ln(abs(x))");
$f->{test_at} = [[-3],[-2],[-1]];
$g = FormulaUpToConstant("- $a*exp($c*x)/$c + $b*ln(x)");
$g->{test_points} = [[1],[2],[3],[4],[5],[6],[7]];

ANS($f->cmp()->withPostFilter(AnswerHints( 
  $g => "The antiderivative of 1/x is ln(|x|)+C",
)));

ENDDOCUMENT();


