DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"MathObjects.pl",
"parserFormulaUpToConstant.pl"
);

TEXT(beginproblem());

$a = random(2,9,1);
$b = random(-9,-1,1);
$F = FormulaUpToConstant("(1/$a)tan($a x + $b)");
$f = "sec^2($a x + $b)";
Context()->texStrings;
BEGIN_TEXT;
Evaluate the integral.  (HINT: The integrand is a composition in which the inside is linear.)
$BR
$BR
\(\displaystyle\int \$f dx = \)
\{$F->ans_rule(40) \}.
END_TEXT
Context()->normalStrings;
ANS($F->cmp());

ENDDOCUMENT();
