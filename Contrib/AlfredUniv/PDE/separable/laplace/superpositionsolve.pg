##DESCRIPTION
##  Algebra problem: true or false for inequality 
##ENDDESCRIPTION

##KEYWORDS('PDE', 'laplace', 'equation', 'superposition')

## DBsubject('Calculus')
## DBchapter('Partial Differential Equations')
## DBsection('Laplace's Equation')
## Date('7/29/2011')
## Author('Darwyn Cook')
## Institution('Alfred University')
## TitleText1('Differential Equations')
## EditionText1('6')
## AuthorText1('Zill')
## Section1('12.5')
## Problem1('')
########################################################################

DOCUMENT();      

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "contextFraction.pl",
  "answerHints.pl",
  "parserMultiAnswer.pl",
  "unionTables.pl",
  "PGcourse.pl"
);

# Print problem number and point value (weight) for the problem
TEXT(beginproblem());

# Show which answers are correct and which ones are incorrect
$showPartialCorrectAnswers = 1;

##############################################################
#
#  Setup
#
#
Context("Numeric");
Context()->variables->are(a=>'Real',b=>'Real',n => ['Real', limits=>[1,20], 
    resolution=>1]);
Context()->functions->undefine("cos","sin");
$coefficient = Context()->copy;

Context("Numeric");
Context()->variables->add(a=>'Real',b=>'Real',n => ['Real', limits=>[1,20], 
    resolution=>1],y=>'Real');
$f = Formula("x/a");
$g = Formula("1");
$F = Formula("(y/b)^2");
$G = Formula("1");

$cna = Formula("cos(n*pi*x/a)");
$sna = Formula("sin(n*pi*x/a)");
$cnb = Formula("cos(n*pi*y/b)");
$snb = Formula("sin(n*pi*y/b)");

$cosh = Formula("cosh(n*pi*x/b)");
$sinh = Formula("sinh(n*pi*x/b)");
$cosha = Formula("cosh(n*pi*a/b)");
$sinha = Formula("sinh(n*pi*a/b)");
$sinhb = Formula("sinh(n*pi*b/a)");
$coshb = Formula("cosh(n*pi*b/a)");

$intsin = Formula($coefficient,"2*(1+(-1)^(n+1))/(n*pi)");
$intsin->{test_points}=[[1,1,1],[1,1,-1],[1,1,-2],[1,1,2],[1,1,-3],[1,1,3],[1,1,4],[2,1,1],[2,1,-1],[2,1,-2],[2,1,2],[2,1,-3],[-2,1,3],[-2,1,4],[-1,1,1],[3,1,-1],[4,1,-2],[-4,1,2],[-3,1,-3],[-2,1,3],[-4,1,4]];
$An = Formula($coefficient,"2*(-1)^(n+1)/(n*pi)");
$An->{test_points}=[[1,1,1],[1,1,-1],[1,1,-2],[1,1,2],[1,1,-3],[1,1,3],[1,1,4],[2,1,1],[2,1,-1],[2,1,-2],[2,1,2],[2,1,-3],[-2,1,3],[-2,1,4],[-1,1,1],[3,1,-1],[4,1,-2],[-4,1,2],[-3,1,-3],[-2,1,3],[-4,1,4]];
$Bn = Formula($coefficient,"1/$sinhb($intsin - $An*$coshb)");
$Bn->{test_points}=[[1,1,1],[1,1,-1],[1,1,-2],[1,1,2],[1,1,-3],[1,1,3],[1,1,4],[2,1,1],[2,1,-1],[2,1,-2],[2,1,2],[2,1,-3],[-2,1,3],[-2,1,4],[-1,1,1],[3,1,-1],[4,1,-2],[-4,1,2],[-3,1,-3],[-2,1,3],[-4,1,4]];


$Anprime = Formula($coefficient,"(2*(-2+(2-(n*pi)^2)(-1)^n))/(n*pi)^3");
$Anprime->{test_points}=[[1,1,1],[1,1,-1],[1,1,-2],[1,1,2],[1,1,-3],[1,1,3],[1,1,4],[2,1,1],[2,1,-1],[2,1,-2],[2,1,2],[2,1,-3],[-2,1,3],[-2,1,4],[-1,1,1],[3,1,-1],[4,1,-2],[-4,1,2],[-3,1,-3],[-2,1,3],[-4,1,4]];
$Bnprime = Formula($coefficient,"1/$sinha*($intsin-$Anprime*$cosha)");
$Bnprime->{test_points}=[[1,1,1],[1,1,-1],[1,1,-2],[1,1,2],[1,1,-3],[1,1,3],[1,1,4],[2,1,1],[2,1,-1],[2,1,-2],[2,1,2],[2,1,-3],[-2,1,3],[-2,1,4],[-1,1,1],[3,1,-1],[4,1,-2],[-4,1,2],[-3,1,-3],[-2,1,3],[-4,1,4]];
##############################################################
#
#  Text
#
#

Context()->texStrings;
BEGIN_TEXT
Solve
$BR
$BR
$BCENTER
\(\displaystyle\frac{\partial^2 u}{\partial x^2} +\frac{\partial^2 u}{\partial y^2}=0, 0< x < a, 0 < y < b\)
$BR
$BR
\(\displaystyle
u(0,y)=$F, \hskip 10pt u(a,y)=$G, 0 < y < b\)
$BR
$BR
\(\displaystyle
u(x,0) = $f, \hskip 10pt u(x,b) = $g,0<x<a\)
$ECENTER
$BR
$BR
using superposition \(u = u_1+u_2\) and
$BR
$BR
\(u_1(x,y) = \displaystyle\sum\limits_{n=1}^\infty \left( A_n \cosh\left(\frac{n\pi}{a}y\right)+B_n \sinh\left(\frac{n\pi}{a}y\right) \right) \sin\left( \frac{n\pi}{a}x\right) \)
$BR
\(u_2(x,y) = \displaystyle\sum\limits_{n=1}^\infty \left( A_n^\prime \cosh\left(\frac{n\pi}{b}x\right)+B_n^\prime \sinh\left(\frac{n\pi}{b}x\right) \right) \sin\left( \frac{n\pi}{b}y\right) \)
$BR
$BR
\{   BeginTable(center=>0).
     Row(['\(\displaystyle A_n = \)','\(\hskip 20pt\)'.ans_rule(1).$BR.'---------------------------'.$BR.ans_rule(13),ans_rule(1).$BR.'\(\displaystyle\int\)'.$BR.ans_rule(1),ans_rule,'\(dx=\)',ans_rule(40),
     ],separation=>2).
   EndTable();
\}
$BR
$BR
\{   BeginTable(center=>0).
     Row(['\(\displaystyle A_n^\prime = \)','\(\hskip 20pt\)'.ans_rule(1).$BR.'---------------------------'.$BR.ans_rule(13),ans_rule(1).$BR.'\(\displaystyle\int\)'.$BR.ans_rule(1),ans_rule,'\(dy=\)',ans_rule(40),
     ],separation=>2).
   EndTable();
\}
$BR
$BR
\{   BeginTable(center=>0).
     Row(['\(B_n= \)','\(\hskip 40pt 1\)'.$BR.'-----------------------------------'.$BR.ans_rule(),'\(\Biggl(\)',ans_rule(1).$BR.'----------'.$BR.ans_rule(1),ans_rule(1).$BR.'\(\displaystyle\int\)'.$BR.ans_rule(1),ans_rule(15),'\(dx -\)',ans_rule(40),'\(\Biggr)\)',
     ],separation=>2).
   EndTable();
\}
$BR
\(\hskip 15pt =\) \{ans_rule(90)\}
$BR
$BR
\{   BeginTable(center=>0).
     Row(['\(B_n^\prime= \)','\(\hskip 40pt 1\)'.$BR.'-----------------------------------'.$BR.ans_rule(),'\(\Biggl(\)',ans_rule(1).$BR.'----------'.$BR.ans_rule(1),ans_rule(1).$BR.'\(\displaystyle\int\)'.$BR.ans_rule(1),ans_rule(15),'\(dy -\)',ans_rule(40),'\(\Biggr)\)',
     ],separation=>2).
   EndTable();
\}
$BR
\(\hskip 15pt =\) \{ans_rule(90)\}
END_TEXT
Context()->normalStrings;

##############################################################
#
#  Answers
#
#

# Compute A_n
ANS(Compute("2")->cmp); 
ANS(Formula("a")->cmp); 
ANS(Formula("a")->cmp); 
ANS(Compute("0")->cmp); 
ANS(Formula("$f*$sna")->cmp); 
ANS($An->cmp);

#Compute A_nprime
ANS(Compute("2")->cmp); 
ANS(Formula("b")->cmp); 
ANS(Formula("b")->cmp);
ANS(Compute("0")->cmp); 
ANS(Formula("$F*$snb")->cmp); 
ANS($Anprime->cmp);

#Compute B_n
ANS($sinhb->cmp);
ANS(Compute("2")->cmp); 
ANS(Formula("a")->cmp); 
ANS(Formula("a")->cmp); 
ANS(Compute("0")->cmp); 
ANS(Formula("$g*$sna")->cmp);

ANS(Formula($coefficient,"$An*$coshb")->cmp);
ANS($Bn->cmp);

#Compute B_nprime
ANS($sinha->cmp);
ANS(Compute("2")->cmp); 
ANS(Formula("b")->cmp); 
ANS(Formula("b")->cmp); 
ANS(Compute("0")->cmp); 
ANS(Formula("$G*$snb")->cmp);

ANS(Formula("$Anprime*$cosha")->cmp);
ANS($Bnprime->cmp);
ENDDOCUMENT();        
