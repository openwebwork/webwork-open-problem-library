ANS(Compute("c")->cmp() );
ANS(Compute("$X*$Y")->cmp() );
#Second IC
ANS(Compute("$alpha*Bmn*$X*$Y")->cmp());
#Fourier integral for Bmn
ANS(Compute("4")->cmp() );
ANS(Compute("b*c*$alpha")->cmp() );
ANS(Compute("b")->cmp() );
ANS(Compute("c")->cmp() );
ANS(Compute("$X*$Y")->cmp() );

