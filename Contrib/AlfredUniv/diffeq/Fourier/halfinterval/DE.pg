#

ANS($IC->cmp);
ANS($IC->cmp);
ANS($IC->cmp);
ANS($IC->cmp);
ANS( $popup->cmp() );
ANS($d1->cmp);
ANS($d2->cmp);
ANS($d3->cmp);
ANS($d4->cmp);
ANS(Compute("E*I*$d4")->cmp);
ANS(Compute("0")->cmp);
ANS(Formula("L")->cmp);
ANS(Compute("2/(L*E*I*$d4)*$sn")->cmp);
ENDDOCUMENT();        
