########################################################################

DOCUMENT();      

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGchoicemacros.pl",
  "contextFraction.pl",
  "PGcourse.pl"
);

# Print problem number and point value (weight) for the problem
TEXT(beginproblem());

# Show which answers are correct and which ones are incorrect
$showPartialCorrectAnswers = 1;

##############################################################
#
#  Setup
#
#
Context("Fraction");
$alpha = random(2,9);
$nu = random(2,9);

if ($alpha == $nu){$alpha++;};

$asqrd = $alpha**2;
$nsqrd = $nu**2;

$mc = new_multiple_choice();
$mc->qa(
"The general solution of the differential equation \(x^2y^{\prime\prime}+ xy^\prime+($asqrd x^2-$nsqrd)y=0\) is?", 
"\( c_1J_{$nu}($alpha x)+c_2Y_{$nu}($alpha x) \)$BR"
);
$mc->extra(
"\( c_1J_{$alpha}($nu x)+c_2Y_{$alpha}($nu x) \)$BR",
"\( c_1J_{-$alpha}($nu x)+c_2Y_{$alpha}($nu x) \)$BR",
"\( c_1J_{-$nu}($alpha x)+c_2J_{$nu}($alpha x) \)$BR",
);

##############################################################
#
#  Text
#
#

Context()->texStrings;
BEGIN_TEXT
\{ $mc->print_q() \}
$BR
\{ $mc->print_a() \}
END_TEXT
Context()->normalStrings;

##############################################################
#
#  Answers
#
#
install_problem_grader(~~&std_problem_grader);

$showPartialCorrectAnswers = 0;

ANS( checkbox_cmp( $mc->correct_ans() ) );
ENDDOCUMENT();        
