DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGnumericalmacros.pl",
"PGstatisticsmacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;
install_problem_grader(~~&std_problem_grader);

$mc[1] = new_multiple_choice();
$mc[1]->qa('The coefficient of determination \(R^2\) measures the amount of:',
    'variation in y that is explained by variation in x'
);
$mc[1]->extra(
    'variation in x that is explained by variation in y',
    'variation in y that is unexplained by variation in x',
    'variation in x that is unexplained by variation in y'
);


$mc[2] = new_multiple_choice();
$mc[2]->qa('If we are interested in determining whether two 
variables are linearly related, it is necessary to:',
    'either A or B since they are identical'
);
$mc[2]->extra(
    'perform the t-test of the slope \(\beta_1\)',
    'perform the t-test of the coefficient of correlation \(\rho\)'
);

$mc[2]->makeLast(
    'either A or B since they are identical',
    'calculate the standard error of estimate, \(s_\epsilon\)'
);


$mc[3] = new_multiple_choice();
$mc[3]->qa('In the first-order linear regression model, the population 
parameters of the y-intercept and the slope are:',
    '\(\beta_0 \mbox{ and } \beta_1\)'
);
$mc[3]->extra(
    '\(b_0 \mbox{ and } b_1\)',
    '\(\beta_1 \mbox{ and } b_0\)'
);

$mc[3]->makeLast(
    '\(b_1 \mbox{ and } \beta_0\)'
);

$mc[4] = new_multiple_choice();
$mc[4]->qa('The standard error of estimate \(s_\epsilon\) is a measure of:',
    'variation of y around the regression line'
);
$mc[4]->extra(
    'variation of y around the mean \(\bar{y}\)',
    'variation of x around the regression line'
);

$mc[4]->makeLast(
    'variation of x around the mean \(\bar{x}\)'
);


$mc[5] = new_multiple_choice();
$mc[5]->qa('In the simple linear regression model, the y-intercept 
represents the:',
    'value of y when x = 0'
);
$mc[5]->extra(
    'change in y per unit change in x',
    'change in x per unit change in y',
    'value of x when y = 0'
);

$a = random(1,5,1);
$b = random(1,5,1);
while ($a==$b){
     $b=random(1,5,1);
}

BEGIN_TEXT
$PAR
\{ $mc[$a]->print_q() \}

\{ $mc[$a]->print_a() \}
$PAR
\{ $mc[$b]->print_q() \}

\{ $mc[$b]->print_a() \}
$PAR


END_TEXT

ANS(radio_cmp($mc[$a]->correct_ans));
ANS(radio_cmp($mc[$b]->correct_ans));

ENDDOCUMENT();       # This should be the last executable line in the problem.
