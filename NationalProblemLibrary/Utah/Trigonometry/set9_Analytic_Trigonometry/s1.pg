DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);


TEXT(&beginproblem);



$C=4/5;
$S=3/5;

TEXT(EV2(<<EOT));



Suppose \[\sin(u) = \frac{3}{5}\]
and \(\cos(u)\) is $BBOLD positive$EBOLD.
Then

$BR
\(\cos(u) = \)  \{ ans_rule(12) \}
EOT

&ANS( num_cmp($C));

TEXT(EV2(<<EOT));
$BR
\(\tan(u) = \)  \{ ans_rule(12) \}
EOT
&ANS( num_cmp($S/$C));

TEXT(EV2(<<EOT));
$BR
\(\sin(-u) = \)  \{ ans_rule(12) \}
EOT
&ANS( num_cmp(-$S));

TEXT(EV2(<<EOT));
$BR
\(\cos(-u) = \)  \{ ans_rule(12) \}
EOT
&ANS( num_cmp($C));

TEXT(EV2(<<EOT));
$BR
\(\tan(-u) = \)  \{ ans_rule(12) \}
EOT
&ANS( num_cmp(-$S/$C));

TEXT(EV2(<<EOT));
$BR
\(\sin(u+\pi) = \)  \{ ans_rule(12) \}
EOT
&ANS( num_cmp(-$S));

TEXT(EV2(<<EOT));
$BR
\(\cos(u+\pi) = \)  \{ ans_rule(12) \}
EOT
&ANS( num_cmp(-$C));

TEXT(EV2(<<EOT));
$BR
\(\tan(u+\pi) = \)  \{ ans_rule(12) \}
EOT
&ANS( num_cmp($S/$C));

TEXT(EV2(<<EOT));
$BR
\(\sin(u+\frac{\pi}{2}) = \)  \{ ans_rule(12) \}
EOT
&ANS( num_cmp($C));

TEXT(EV2(<<EOT));
$BR
\(\cos(u+\frac{\pi}{2}) = \)  \{ ans_rule(12) \}
EOT
&ANS( num_cmp(-$S));

TEXT(EV2(<<EOT));
$BR
\(\tan(u+\frac{\pi}{2}) = \)  \{ ans_rule(12) \}
EOT
&ANS( num_cmp(-$C/$S));










ENDDOCUMENT();        # This should be the last executable line in the problem.
