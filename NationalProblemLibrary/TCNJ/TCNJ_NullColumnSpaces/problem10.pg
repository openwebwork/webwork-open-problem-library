##DESCRIPTION
##KEYWORDS('null space', 'column space')
##
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGgraphmacros.pl",
"PGmatrixmacros.pl",
"PGnumericalmacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$NO_SPACE = '@{}';

$r11 = non_zero_random(-5,5,1);
$r21 = non_zero_random(-5,5,1);
$r31 = non_zero_random(-5,5,1);

$mult1 = non_zero_random(-2,2,2);
$a = random(-3,3,2);
$b = random(-3,3,2);
$r12 = $mult1*$r11+$a;
$r22 = $mult1*$r21;
$r32 = $mult1*$r31+$b;

$mult2 = random(2,4,1);
$mult3 = random(-4,2,1);
$r13 = $mult2*$r11+$mult3*$r12;
$r23 = $mult2*$r21+$mult3*$r22;
$r33 = $mult2*$r31+$mult3*$r32;

$w1 = - $mult2;
$w2 = - $mult3;
$w3 = 1;

$x1 = random(-5,5,1);
$x2 = non_zero_random(-5,5,1);
$x3 = random(2,4,1);

$v1 = non_zero_random(-5,5,1);
$v2 = non_zero_random(-5,5,1);
$v3 = random(5,7,1);

BEGIN_TEXT

\{mbox( 'Let \(A= \)', display_matrix([[$r11, $r12, $r13],[$r21, $r22, $r23], [$r31, $r32, $r33]]), ', \( v= \)', display_matrix([[$v1],[$v2],[$v3]]), ', \( w= \)', display_matrix([[$w1],[$w2],[$w3]]), 'and \(x=\)', display_matrix([[$x1],[$x2],[$x3]]), '.' )\}
$BR
Is \(v\) in \(Nul A\)? Type "yes" or "no". \{ans_rule(10)\}
$BR
$BR
Is \(w\) in \(Nul A\)? Type "yes" or "no". \{ans_rule(10)\} 
$BR
$BR
Is \(x\) in \(Nul A\)? Type "yes" or "no". \{ans_rule(10)\}

END_TEXT

ANS(str_cmp("no"));
ANS(str_cmp("yes"));
ANS(str_cmp("no"));

ENDDOCUMENT();       # This should be the last executable line in the problem.