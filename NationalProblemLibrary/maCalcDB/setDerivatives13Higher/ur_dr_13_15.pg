DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGnumericalmacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(4,9,1);
$b = random(2,3,1);
$c = random(4,8,1);
if ($a == $c) {$c = $c+1;}

BEGIN_TEXT

If \( f(x) = $a x^{$b} \ln ($c x) \), then $BR
\( f'(x)= \) \{ ans_rule(20) \} $BR
\( f''(x)= \) \{ ans_rule(20) \} $BR
\( f'''(x)= \) \{ ans_rule(20) \} $BR
\( f^{(4)}(x)= \) \{ ans_rule(20) \} $BR
\( f^{(5)}(x)= \) \{ ans_rule(20) \} $BR

END_TEXT

if ($b == 2) {
$ans1 = "2*$a*x*ln($c*x)+$a*x";
$ans2 = "2*$a*ln($c*x)+3*$a";
$ans3 = "2*$a/x";
$ans4 = "-2*$a/(x**2)";
$ans5 = "4*$a/(x**3)";
}

if ($b == 3) {
$ans1 = "3*$a*(x**2)*ln($c*x)+$a*(x**2)";
$ans2 = "6*$a*x*ln($c*x)+5*$a*x";
$ans3 = "6*$a*ln($c*x)+11*$a";
$ans4 = "6*$a/x";
$ans5 = "-6*$a/(x**2)";
}

ANS(fun_cmp($ans1));
ANS(fun_cmp($ans2));
ANS(fun_cmp($ans3));
ANS(fun_cmp($ans4));
ANS(fun_cmp($ans5));

ENDDOCUMENT();       # This should be the last executable line in the problem.

