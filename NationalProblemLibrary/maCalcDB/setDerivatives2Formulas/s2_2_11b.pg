## Section1('3.3')
## Problem1('35')

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

$an = random(2,8,1);
$as = random(-1,1,2);
$a  = $an * $as;

$bn = random(2,8,1);
$bs = random(-1,1,2);
$b  = $bn * $bs;

$m = random(4,6,1);
$n = random(2,3,1);

$funct = "$a*($m + 1/2)*x**($m - 1/2) - $b*($n + 1/2)/x**($n+3/2)";

TEXT(EV2(<<EOT));
$BR
$PAR
Let \( \displaystyle f(x) =  $a x^{$m} \sqrt{x} + \frac{$b} {x^{$n} \sqrt{x} } \).
$BR
$PAR
\( f'( x ) = \) \{ans_rule(30) \}
$BR
$BR
EOT

$ans = $funct;
&ANS(function_cmp($ans));


ENDDOCUMENT();        # This should be the last executable line in the problem.
