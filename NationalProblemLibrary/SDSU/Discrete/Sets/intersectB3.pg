# DESCRIPTION
# Determine the intersection of two sets
# 
# WeBWorK problem written by Michael E. O'Sullivan
# and Thomas Schmidt
# ENDDESCRIPTION

## DBsubject('Discrete Mathematics')
## DBchapter('Set Theory')
## DBsection('Basic Operations')
## KEYWORDS('')
## TitleText1('')
## EditionText1('')
## AuthorText1('')
## Section1('')
## Problem1('')
## Author('Thomas Schmidt','Michael E. O'Sullivan')
## Institution('SDSU')


############################
# Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGchoicemacros.pl"
);

TEXT(beginproblem());

############################
# Setup

$var1 = "A";
$var2 = "B";
$set1 = "\mathbb{R}";
$set2 = "\mathbb{N}";

$radio = new_multiple_choice();
$radio->qa("Determine \($var1\cap $var2\)","\($set2\)");
$radio->extra("\($set1\)","\(\lbrace\rbrace\)","\(\lbrace 0\rbrace\)");

############################
# Main Text

Context()->texStrings;
BEGIN_TEXT
$BR
For help with these problems, please see
\{ htmlLink( "http://www-rohan.sdsu.edu/~mosulliv/Courses/discrete12f/sets.pdf", "this page" ) \}. $PAR
Let \($var1 = $set1\), \($var2 = $set2\)
$BR
$BR
\{$radio->print_q()\}
\{$radio->print_a()\}
END_TEXT
Context()->normalStrings;

############################
# Answer Evaluation

$showPartialCorrectAnswers = 0;

ANS(radio_cmp($radio->correct_ans()));

############################
# Solution

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR Solution: $PAR
\($var1 = $set1\) $BR
\($var2 = $set2\)
$PAR
\($var1\cap $var2\), or \($var1\text{ "intersect" } $var2\), is defined as the set of elements that are contained in both \($var1\) and \($var2\). $BR
In this case, the answer is $BR
\($set2\) $BR
since \($set2\subset $set1\) $BR
END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
