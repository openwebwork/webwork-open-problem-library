# DESCRIPTION
# Determine the complement of a set
# 
# WeBWorK problem written by Michael E. O'Sullivan
# and Thomas Schmidt
# ENDDESCRIPTION

## DBsubject('Discrete Mathematics')
## DBchapter('Set Theory')
## DBsection('Basic Operations')
## KEYWORDS('')
## TitleText1('')
## EditionText1('')
## AuthorText1('')
## Section1('')
## Problem1('')
## Author('Thomas Schmidt','Michael E. O'Sullivan')
## Institution('SDSU')


############################
# Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGchoicemacros.pl"
);

TEXT(beginproblem());

############################
# Setup

$showPartialCorrectAnswers = 0;

# image width and height
$width = 200;
$height = 200;

@images = (image("venn4.gif",width=>$width,height=>$height),
           image("venn15.gif",width=>$width,height=>$height),
           image("venn26.gif",width=>$width,height=>$height),
           image("venn35.gif",width=>$width,height=>$height));
           
$answer = $images[0];

$expr = "(A\cap B)-C";

$radio = new_multiple_choice();
$radio->qa("Which of the following Venn diagrams corresponds to \($expr\)?",$answer);
$radio->extra($images[1],$images[2],$images[3]);

############################
# Main Text

Context()->texStrings;
BEGIN_TEXT
$BR
For help with these problems, please see
\{ htmlLink( "http://www-rohan.sdsu.edu/~mosulliv/Courses/discrete12f/sets.pdf", "this page" ) \}. $PAR
$BR
$BR
\{$radio->print_q()\} $BR
\{$radio->print_a()\}
END_TEXT
Context()->normalStrings;

############################
# Answer Evaluation

ANS(radio_cmp($radio->correct_ans()));

############################
# Solution

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR Solution: $PAR
The Venn diagram for \($expr\) is $PAR
$answer
END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
