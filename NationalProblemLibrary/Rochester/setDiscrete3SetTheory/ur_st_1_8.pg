##DESCRIPTION
##KEYWORDS('Multiple Choice question')
##This problem is also in the directory setSetTheory1
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$inter = new_checkbox_multiple_choice();
$union = new_checkbox_multiple_choice();
$diff  = new_checkbox_multiple_choice();

#$tf = new_pop_up_select_list();   #For popups!

#Question and matching correct answers

$inter -> qa (
"(a) \( A \cap B \)",
"3"
);

#Some incorrect answers

$inter -> extra(
"1",
"2",
"4",
"5"
);

$union -> qa(
"(b) \( A \cup B \)",
"1",
"2",
"3",
"5"
);

$union -> extra(
"4"
);

$diff -> qa(
"(c) \( A \setminus B \)",
"1",
"5"
);

$diff -> extra(
"2",
"3",
"4"
);

BEGIN_TEXT
\( A = \lbrace 1,3,5 \rbrace, B = \lbrace 2,3 \rbrace \)
$PAR
Check ALL elements of the following sets: $BR
\{ $inter -> print_q \}
$PAR
\{ $inter -> print_a \}
$PAR
\{ $union -> print_q \}
$PAR
\{ $union -> print_a \}
$PAR
\{ $diff -> print_q \}
$PAR
\{ $diff -> print_a \}
$PAR
END_TEXT

ANS(checkbox_cmp( $inter -> correct_ans ) );
ANS(checkbox_cmp( $union -> correct_ans ) );
ANS(checkbox_cmp( $diff -> correct_ans ) );

ENDDOCUMENT();        # This should be the last executable line in the problem.;
