##DESCRIPTION
##KEYWORDS('integral', 'hyperbolic')
##  Find the integral derivative of a function involving hyperbolic trigonometric function(s).
##ENDDESCRIPTION

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$c = random(2,4,1);
$c2 = $c*$c;
$a = random($c+1,6,1);
$a2 = $a*$a;
$b = random(($a+1),9,1);
$b2 = $b*$b;
$ans = ln($b/$c+sqrt($b2/$c2-1))-ln($a/$c+sqrt($a2/$c2-1));

BEGIN_TEXT
Evaluate the integral. $PAR
 
\( \displaystyle \int_$a^$b \frac{1}{\sqrt{t^2-$c2}}  dt = \) \{ans_rule(50) \}.
$BR
END_TEXT

ANS(num_cmp($ans));

ENDDOCUMENT();        # This should be the last executable line in the problem.
