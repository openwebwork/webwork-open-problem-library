#DESCRIPTION
##  Algebra problem: Systems of linear equations 
##ENDDESCRIPTION

##KEYWORDS('algebra', 'echelon form', 'linear equations')

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGmatrixmacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$a11=random(-5,5,1);
$a12=non_zero_random(-5,5,1);
$a13=random(-5,5,1);
$a21=random(-5,5,1);
$a22=random(-5,5,1);
$a23=random(-5,5,1);
$a31=random(-5,5,1);
$a32=random(-5,5,1);
$a33=random(-5,5,1);
$b1=random(-5,5,1);
$b2=random(-5,5,1);
$b3=random(-5,5,1);

BEGIN_TEXT
Given the matrix
\{ mbox( "\(A=\)", display_matrix([[1, $a12, $a13], [0, 1, $a23], [0, 0, 0]]), "," ) \}  
$BR
(a) determine whether the matrix \(A\) is in echelon form;
$BR
Your answer is (input Yes or No) \{ans_rule(15) \}
$BR
(b) determine whether the matrix \(A\) is in reduced echelon form;
$BR
Your answer is (input Yes or No) \{ans_rule(15) \}
$BR
END_TEXT

$ans1 = "Yes";
$ans2 = "No";
ANS(str_cmp($ans1));
ANS(str_cmp($ans2));

ENDDOCUMENT();        # This should be the last executable line in the problem.
