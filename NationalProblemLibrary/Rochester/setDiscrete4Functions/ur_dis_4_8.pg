DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$mcone = new_multiple_choice();
$mctwo = new_multiple_choice();
$mcthree = new_multiple_choice();
$mcfour = new_multiple_choice();
$mcfive = new_multiple_choice();


#Question and correct answer

$mcone -> qa (
"\( f(x) = 10 \) is",
"\( O(x) \)",
);

$mcone -> extra(
"\( \Omega(x) \)",
"\( \Theta(x) \)"
);

$mctwo -> qa (
"\( f(x) = 3x+7 \) is",
"\( \Theta(x) \)",
);

$mctwo -> extra(
"\( O(x) \)",
"\( \Omega(x) \)"
);

$mcthree -> qa (
"\( f(x) = x^2 + x + 1 \) is",
"\( \Omega(x) \)",
);

$mcthree -> extra(
"\( O(x) \)",
"\( \Theta(x) \)"
);

$mcfour -> qa (
"\( f(x) = 5 \log(x) \) is",
"\( O(x) \)",
);

$mcfour -> extra(
"\( \Omega(x) \)",
"\( \Theta(x) \)"
);

$mcfive -> qa (
"\( f(x) = \lfloor x \rfloor \) is",
"\( \Theta(x) \)",
);

$mcfive -> extra(
"\( \Omega(x) \)",
"\( O(x) \)"
);

BEGIN_TEXT
$PAR

Check the answer that best describes the relationship between f(x) and x.
$BR
(For example if f(x) is \( \Theta(x) \) check that as your answer and
not \( O(x) \) or \( \Omega(x) \) even though these are true also in
this case.) $PAR 
(a) \{ $mcone -> print_q \}
$PAR
\{ $mcone -> print_a \}
$PAR
(b) \{ $mctwo -> print_q \}
$PAR
\{ $mctwo -> print_a \}
$PAR
(c) \{ $mcthree -> print_q \}
$PAR
\{ $mcthree -> print_a \}
$PAR
(d) \{ $mcfour -> print_q \}
$PAR
\{ $mcfour -> print_a \}
$PAR
(e) \{ $mcfive -> print_q \}
$PAR
\{ $mcfive -> print_a \}
$PAR
END_TEXT

ANS(radio_cmp( $mcone -> correct_ans ) );
ANS(radio_cmp( $mctwo -> correct_ans ) );
ANS(radio_cmp( $mcthree -> correct_ans ) );
ANS(radio_cmp( $mcfour -> correct_ans ) );
ANS(radio_cmp( $mcfive -> correct_ans ) );

ENDDOCUMENT();        # This should be the last executable line in the problem.;
