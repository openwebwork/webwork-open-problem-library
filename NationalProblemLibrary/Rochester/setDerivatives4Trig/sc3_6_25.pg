DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(2,4,1);
$b1 = random(2,8,1);
$x1 = random(.3,.8,.1);
$ab1=$b1*$a1;
$a2=$a1-1;
$a3=2*$a1;
$deriv1 = ($b1*$a1*$x1**($a1-1))/sqrt(1-$x1**(2*$a1));
$funct1 = "($ab1*x^$a2)/sqrt(1-x^$a3)";

TEXT(EV2(<<EOT));
If \( f(x) = $b1 \arcsin(x^{$a1}) \), find \( f'( x ) \).
$BR $BR \{ans_rule(30) \}
$BR
EOT

$ans = $funct1;
ANS(fun_cmp($ans));

TEXT(EV2(<<EOT));
Find \( f'( $x1 ) \).
$BR $BR \{ans_rule(30) \}
$BR
EOT

$ans = $deriv1;
ANS(num_cmp($ans));

ENDDOCUMENT();        # This should be the last executable line in the problem.
