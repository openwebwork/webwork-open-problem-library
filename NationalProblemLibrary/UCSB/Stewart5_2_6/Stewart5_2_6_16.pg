DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

$a=random(1,10,1);
$b=random(1,10,1);

BEGIN_TEXT

$PAR
Evaluate the following limit. If the answer is positive infinite, type "I"; if negative infinite, type "N"; and if it does not exist, type "D". $PAR

\[\displaystyle\lim_{y \to \infty} \frac{$a-3y^2}{5y^2+ $b y}\]
$PAR

\{ans_rule(10)\}

END_TEXT

ANS(fun_cmp("-3/5", var=>["I","N","D"]));

ENDDOCUMENT();
