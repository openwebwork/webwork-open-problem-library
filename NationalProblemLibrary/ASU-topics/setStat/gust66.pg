##DESCRIPTION
##   Evaluation of a simple algebraic expression
##ENDDESCRIPTION

##KEYWORDS('percent')

#
# First comes some stuff that appears at the beginning of every problem
#

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGasu.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 0;


@event = ("\(\mu_1\)", "\(\sigma_1\)", "\(\overline{x}_1\)", "\(s_1\)", "\(\mu_2\)", "\(\sigma_2\)", "\(\overline{x}_2\)", "\(s_2\)");
@ans = ("f", "f", "v", "v", "f", "f", "v", "v"); 

BEGIN_TEXT
$PAR
Consider the following quantities.  If the quantity is a fixed number, type "f".  If the quantity is a variable, type "v". $BR  

$PAR
a)  \( \) $event[0] $BR
answer: \{ans_rule(20)\} $PAR
b)  \( \) $event[1] $BR
answer: \{ans_rule(20)\} $PAR
c)  \( \) $event[2] $BR
answer: \{ans_rule(20)\} $PAR
d)  \( \) $event[3] $BR
answer: \{ans_rule(20)\} $PAR
e)  \( \) $event[4] $BR
answer: \{ans_rule(20)\} $PAR
f)  \( \) $event[5] $BR
answer: \{ans_rule(20)\} $PAR
g)  \( \) $event[6] $BR
answer: \{ans_rule(20)\} $PAR
h)  \( \) $event[7] $BR
answer: \{ans_rule(20)\} $PAR

END_TEXT

ANS(std_str_cmp($ans[0]));
ANS(std_str_cmp($ans[1]));
ANS(std_str_cmp($ans[2]));
ANS(std_str_cmp($ans[3]));
ANS(std_str_cmp($ans[4]));
ANS(std_str_cmp($ans[5]));
ANS(std_str_cmp($ans[6]));
ANS(std_str_cmp($ans[7]));
ENDDOCUMENT();        # This should be the last executable line in the problem.

