DOCUMENT();        # This should be the first executable line in the problem.
       
loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGgraphmacros.pl",
);

TEXT(beginproblem()); 

$showPartialCorrectAnswers = 0;

$mc = new_multiple_choice();

$mc->qa('The function \( x^2 \) is:', 
'quadratic'
);

$mc->extra(
'cubic',
'logarithmic',
'exponential'
);

$mc->makeLast(
'All of the above',
'None of the above'
);

BEGIN_TEXT
$PAR
\{ $mc->print_q() \}
$PAR
\{ $mc->print_a() \}
END_TEXT
ANS(radio_cmp($mc->correct_ans));


