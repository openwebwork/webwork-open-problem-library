## Date('')
## Author('Gavin LaRose')
## Institution('University of Michigan')
## TitleText1('Calculus')
## EditionText1('5')
## AuthorText1('Hughes-Hallett')
## Section1('14.7')
## Problem1('42')

## Textbook tags
## HHChapter1('Differentiating Functions of Several Variables')
## HHSection1('Second-Order Partial Derivatives')

DOCUMENT();

loadMacros(
"PGstandard.pl",
"PGchoicemacros.pl",
"MathObjects.pl",
# "parserNumberWithUnits.pl",
# "parserFormulaWithUnits.pl", 
# "parserFormulaUpToConstant.pl",
# "PGcourse.pl",
);

Context("Numeric");
Context()->variables->add( y=>'Real' );
$showPartialCorrectAnswers = 1;

$a = non_zero_random(-6,6,1);
$whichVar = random(0,1,1);
if ( $whichVar ) {
    $zxy = Compute( "$a*y" );
} else {
    $zxy = Compute( "$a*x" );
}
$zxyx = $zxy->D('x');
$zxyy = $zxy->D('y');

Context()->texStrings;
TEXT(beginproblem());
BEGIN_TEXT

If \( z_{xy} = $zxy \), what can you say about each of the following?
$PAR
${BBOLD}(a)$EBOLD
\( z_{yx} \) = \{ ans_rule(35) \}

$PAR
${BBOLD}(b)$EBOLD
\( z_{xyx} \) = \{ ans_rule(35) \}

$PAR
${BBOLD}(c)$EBOLD
\( z_{xyy} \) = \{ ans_rule(35) \}

END_TEXT
Context()->normalStrings;

ANS($zxy->cmp() );
ANS($zxyx->cmp() );
ANS($zxyy->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

${BBOLD}(a)$EBOLD
\( z_{yx} = z_{xy} = $zxy \).
$PAR

${BBOLD}(b)$EBOLD
\( z_{xyx} = \frac{\partial}{\partial x}(z_{xy}) =
   \frac{\partial}{\partial x}($zxy) = $zxyx \).
$PAR

${BBOLD}(c)$EBOLD
\( z_{xyy} = \frac{\partial}{\partial y}(z_{xy}) =
   \frac{\partial}{\partial y}($zxy) = $zxyy \).

END_SOLUTION
Context()->normalStrings;


COMMENT('MathObject version');
ENDDOCUMENT();
