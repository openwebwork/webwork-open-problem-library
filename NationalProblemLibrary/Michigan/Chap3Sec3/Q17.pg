DOCUMENT();
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"MathObjects.pl",
);
 
Context("Numeric");
Context()->variables->add( t=>'Real' );

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);
$b = random(2,9,1);
$c = random(1,15,1);
$d = random(1,15,1);


Context()->texStrings;
BEGIN_TEXT

Find the derivative of the function \( z \), below.  It may be to your
advantage to simplify  first.
$BR
\(\displaystyle z = \frac{$a t + $d}{$b t + $c} \)

$PAR
\( {dz\over dt} = \) \{ ans_rule(55) \}

END_TEXT
Context()->normalStrings;


ANS( Compute( "($a*($b*t+$c) - $b*($a*t+$d))/($b*t+$c)^2" )->cmp() );

$num = $a*$c - $b*$d;

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\[ {dz\over dt} = \frac{$a($b t+$c) - $b ($a t+$d)}{($b t+$c)^2}
                = \frac{$num}{($b t+$c)^2}. \]

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
