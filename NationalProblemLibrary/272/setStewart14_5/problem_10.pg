## DBsubject('Calculus')
## DBchapter('Partial Derivatives')
## DBsection('Partial Derivatives')
## Date('')
## Author('')
## Institution('ASU')
## TitleText1('Calculus')
## EditionText1('')
## AuthorText1('Stewart')
## Section1('14.5')
## Problem1('')

DOCUMENT();
loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl",
           "PGgraphmacros.pl",
           "Dartmouthmacros.pl");
 

## Do show partial correct answers
$showPartialCorrectAnswers = 1;

## Lots of set up goes here
$l = random(2,9,1);
$m = random(2,9,1);
$n = random(2,9,1);

$c = non_zero_random(-10,10,1);

## Ok, we are ready to begin the problem...
##
TEXT(beginproblem());


BEGIN_TEXT
$BR
    Consider the surface 
\( F(x,y,z) = x^{$l}z^{$n} + \sin(y^{$m}z^{$n}) + $c = 0. \)
$BR
Find the following partial derivatives

$BR
\(\frac{\partial z}{\partial x} = \) \{ans_rule(60)\}
$BR
\(\frac{\partial z}{\partial y} = \) \{ans_rule(60)\}
$BR
$PAR
END_TEXT

$zx = "-$l*x^{$l-1}z/($n*(x^{$l}+y^{$m}*cos(y^{$m}*z^{$n})))";
$zy = "-$m*y^{$m-1}*z*cos(y^{$m}z^{$n})/($n*(x^{$l}+y^{$m}*cos(y^{$m}*z^{$n})))";
ANS(fun_cmp($zx, vars=>3));
ANS(fun_cmp($zy, vars=>3));


ENDDOCUMENT();




