DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

$a = random(9, 13, 1);
$r = random(1,5,1);
$b = $a - $r;

TEXT(EV3(<<'EOT'));
Find \(\displaystyle{\frac{d}{dx} x^{$a/$b}}\).
$PAR
\(\displaystyle{\frac{d}{dx} x^{$a/$b}}\) = \{ans_rule(30) \}
$BR
EOT

$ans = "($a/$b)*(x**(($a/$b)-1))";
ANS(fun_cmp($ans));

ENDDOCUMENT();
