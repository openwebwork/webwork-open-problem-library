## Problem1('')
## TitleText2('Calculus: Early Transcendentals')
## AuthorText2('Stewart')
## EditionText2('6')
## Section2('3.1')
## Problem2('')
## KEYWORDS('derivative', 'polynomial')

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

$a1 = random(1,6,2);
$b1 = random(7,9,1);
$a2 = random(1,6,2);
$b2 = random(7,9,1);
$d = random(1,4,1);
$f = random(-3,-1,1);

TEXT(EV2(<<EOT));
Suppose that
\( \displaystyle{f(x) = \frac{$a1}{$b1 x} - \frac{$a2}{$b2 x^3}}\).
Evaluate each of the following:
$BR
$BR
\( f'($d) \) = \{ans_rule(20) \}
$BR
EOT

$ans = -$a1/($b1*($d)**(2)) + 3*$a2/($b2*($d)**(4));
ANS(num_cmp($ans));

TEXT(EV2(<<EOT));
\( f'($f) \) = \{ans_rule(20) \}
$BR
EOT

$ans = -$a1/($b1*($f)**(2)) + 3*$a2/($b2*($f)**(4));
ANS(num_cmp($ans));


ENDDOCUMENT();
