DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

$a = random(3,6,1);
$b = random(3,8,1);

TEXT(EV2(<<EOT));
Evaluate
\(\displaystyle{\frac{d}{dx}e^{$a x^2 + $b x}}\).
$BR
$BR
\(\displaystyle{\frac{d}{dx}e^{$a x^2 + $b x}}\) = \{ans_rule(20) \}
$BR
EOT

$ans = "(2*$a*x + $b)*exp($a*x**2 + $b*x)";
ANS(fun_cmp($ans));

ENDDOCUMENT();
