DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

$a = random(3,6,1);
$b = random(3,12,1);

TEXT(EV2(<<EOT));
Evaluate
\(\displaystyle{\frac{d}{dx}\ln((x + $a)^{$b})}\).
$BR
$BR
\(\displaystyle{\frac{d}{dx}\ln((x + $a)^{$b})}\) = \{ans_rule(20) \}
$BR
EOT

$ans = "$b/(x + $a)";
ANS(fun_cmp($ans));

ENDDOCUMENT();
