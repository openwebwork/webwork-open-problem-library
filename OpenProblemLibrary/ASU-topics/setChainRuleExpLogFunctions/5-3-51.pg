DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

$a = random(3,5,1);
$b = random(3,12,1);

TEXT(EV2(<<EOT));
Evaluate
\(\displaystyle{\frac{d}{dx}\sqrt[$a]{\ln($b - x^2)}}\) at \(x=1\).
$BR
$BR
\(\displaystyle{\frac{d}{dx}\sqrt[$a]{\ln($b - x^2)}}\) at \(x=1\) = \{ans_rule(20) \}
$BR
EOT

$ans = (2*(ln($b-1))**(-1+1/$a))/($a - $a*$b);
ANS(num_cmp($ans));

ENDDOCUMENT();
