DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

$a = random(3,6,1);
$b = random(3,12,1);

TEXT(EV2(<<EOT));
Evaluate
\(\displaystyle{\frac{d}{dt}$a e^{-$b t}}\).
$BR
$BR
\(\displaystyle{\frac{d}{dt}$a e^{-$b t}}\) = \{ans_rule(20) \}
$BR
EOT

$ans = "-$b*$a*exp(-$b*t)";
ANS(fun_cmp($ans, vars=>"t"));

ENDDOCUMENT();
