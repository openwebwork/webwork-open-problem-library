DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

$a = random(3,6,1);
$b = random(3,8,1);

TEXT(EV2(<<EOT));
Evaluate
\(\displaystyle{\frac{d}{dt}(\ln(t^2 + $a))^{$b}}\).
$BR
$BR
\(\displaystyle{\frac{d}{dt}(\ln(t^2 + $a))^{$b}}\) = \{ans_rule(30) \}
$BR
EOT

$ans = "(2*t*$b*(ln(t**2 + $a))**($b-1))/(t**2 + $a)";
ANS(fun_cmp($ans, vars=>"t"));

ENDDOCUMENT();
