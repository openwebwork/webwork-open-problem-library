DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGgraphmacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(10,20,1);
$a2 = $a1;
$a3 = $a1;
$a4 = random(5,25,1);
$b1 = $a1+1;
$c1 = $a1+3;
$a5 = random($b1,$c1,1);
$b2 = $a1-3;
$c2 = $a1-1;
$a6 = random($b2,$c2,1);
$b3 = $a1+4;
$c3 = $a1+6;
$a7 = random($b3,$c3,1);
$b4 = $a1-6;
$c4 = $a1-4;
$a8 = random($b4,$c4,1);

@data = ($a1, $a2, $a3, $a4, $a5, $a6, $a7, $a8);
@slice = NchooseK(8,8);
@sdata = @data[@slice];

$sum = ($a1 + $a2 + $a3 + $a4 + $a5 + $a6 + $a7 + $a8);
$mean = ($a1 + $a2 + $a3 + $a4 + $a5 + $a6 + $a7 + $a8)/8;
$n=8;

BEGIN_TEXT
For the given data, find \(  \Sigma x \),  \(n\), and \( \overline{x}\):
$BR $BR

\[ x_1=$sdata[0], \ x_2=$sdata[1], \ x_3=$sdata[2], \ x_4=$sdata[3], \ x_5=$sdata[4], \ x_6=$sdata[5], \ x_7=$sdata[6], \ x_8=$sdata[7] \]

$BR $BR 
\(\Sigma x=\)\{ans_rule(10)\} $BR 
\(n =\) \{ans_rule(10)\} $BR 
\( \overline{x} = \) \{ans_rule(10)\} $BR

$BR 

END_TEXT

ANS(num_cmp($sum));
ANS(num_cmp($n));
ANS(num_cmp($mean));

ENDDOCUMENT();       # This should be the last executable line in the problem.

