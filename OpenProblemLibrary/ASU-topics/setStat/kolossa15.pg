DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGgraphmacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a[0] = random(10,15,1);
$a[1] = random(16,25,1);
$a[2] = $a[1];
$c1 = $a[2]+1;
$c2 = $a[2]+3;
$a[3] = random($c1,$c2,1);
$d1 = $a[3]+1;
$d2 = $a[3]+3;
$a[4] = random($d1,$d2,1);
$e1 = $a[4]+1;
$e2 = $a[4]+3;
$a[5] = random($e1,$e2,1);
$f1 = $a[5]+3;
$f2 = $a[5]+6;
$a[6] = random($f1,$f2,1);
$g1 = $a[6]+3;
$g2 = $a[6]+6;
$a[7] = random($g1,$g2,1);
$h1 = $a[7]+3;
$h2 = $a[7]+6;
$a[8] = random($h1,$h2,1);
$n1 = $a[8]+5;
$n2 = $a[8]+8;
$a[9] = random($n1,$n2,1);
$m1 = $a[9]+5;
$m2 = $a[9]+8;
$a[10] = random($m1,$m2,1);

$mean = ($a[0]+$a[1]+$a[2]+$a[3]+$a[4]+$a[5]+$a[6]+$a[7]+$a[8]+$a[9]+$a[10])/11;

@set = ($a[0], $a[1], $a[2], $a[3], $a[4], $a[5], $a[6], $a[7], $a[8], $a[9], $a[10]);
@slice = NchooseK(11,11);
@sset = @set[@slice];

$var = 0;
for ($i=0; $i<11; $i+=1) {
	$b = ($a[$i] - $mean)**2;
	$var = $var+$b;
}


$range =$a[10]-$a[0] ;
$variance = $var/10;
$sd = sqrt($variance);

BEGIN_TEXT
The length (pgs) of a sample of math research projects is given below.
Using this information, calculate the range, variance, and standard deviation.
$PAR
\[ $sset[0], \ $sset[1], \ $sset[2], \ $sset[3], \ $sset[4], \ $sset[5], \ $sset[6], \ $sset[7], \ $sset[8], \ 
$sset[9], \ $sset[10] \]$PAR

range \(=\) \{ans_rule(20)\}$BR
variance \(=\) \{ans_rule(20)\}$BR
standard deviation \(=\) \{ans_rule(20)\}
END_TEXT
ANS(num_cmp($range, mode=>"arith"));
ANS(num_cmp($variance, mode=>"arith"));
ANS(num_cmp($sd, mode=>"arith"));

ENDDOCUMENT();       # This should be the last executable line in the problem.
