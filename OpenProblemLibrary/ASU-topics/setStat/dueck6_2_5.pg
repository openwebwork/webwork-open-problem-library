#

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGasu.pl",
"PGnumericalmacros.pl",
"PGstatisticsmacros.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$p[0] = .5;
$p[1] = random(.9826,.8212,.9854);
$p[2] = random(.1423,.0078,.0749);
$p[3] = random(.9394,.9987,.9952);

$a = normal_distr($p[0]-0.5);
$b = normal_distr($p[1]-0.5);
$c = normal_distr(0.5-$p[2]);
$d = -1*normal_distr($p[3]-0.5);

$aa = $p[0];
$bb = $p[1];
$cc = $p[2];
$dd = $p[3];

#$aa = .5+$p[0];
#$bb = .5+$p[1];
#$cc = .5-$p[2];
#$dd = .5-$p[3];

BEGIN_TEXT
Find the z-score such that: $PAR
(a) The area under the standard normal curve to its left is $aa  $BR
z = \{ans_rule(10)\} $PAR

(b) The area under the standard normal curve to its left is $bb  $BR
z = \{ans_rule(10)\} $PAR

(c) The area under the standard normal curve to its right is $cc  $BR
z = \{ans_rule(10)\} $PAR

(d) The area under the standard normal curve to its right is $dd  $BR
z = \{ans_rule(10)\} $PAR



END_TEXT

ANS(num_cmp($a, tol=>0.01));
ANS(num_cmp($b, tol=>0.01));
ANS(num_cmp($c, tol=>0.01));
ANS(num_cmp($d, tol=>0.01));

ENDDOCUMENT();       # This should be the last executable line in the problem.
