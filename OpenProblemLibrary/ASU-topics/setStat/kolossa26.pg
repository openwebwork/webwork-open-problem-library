BEGIN_TEXT
\{begintable(9)\}
\{row("rooms", "1", "2", "3", "4", "5","6","7","8+")\} 
\{row("no. of units (in thousands)", "47", "140", "1170", "2350", "2450","2130","1370","1560")\} 
