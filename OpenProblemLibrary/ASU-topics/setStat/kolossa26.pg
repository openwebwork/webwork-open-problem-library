$showPartialCorrectAnswers = 1;

@event = ("not  A",
	  "not B", "not D", "not E",
	  "C and D", "B  and E",
	  "A or D",
	  "not C",
	  "A and D",
	  "B and C",
	  "A or C");

$a = 7510;
$b = 47;
$c = 9657;
$d = 11030;
$e = 0;
$f = 140;
$g = 5267;
$h = 5950;

@ans = ($a, $b, $c, $d, $e, $f, $g, $g, $e,$h,$c);
 
@slice = NchooseK(11,5);
