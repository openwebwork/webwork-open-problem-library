$cmc=new_checkbox_multiple_choice();

$cmc -> qa (
"$LQ\( [p \rightarrow q] \vee [ p \rightarrow r] \)$RQ and $LQ\( p \rightarrow [q \vee r] 
 \)$RQ are",
"logically equivalent"
);

$cmc -> extra(
"not logically equivalent",
"not logically comparable"
);


BEGIN_TEXT
$PAR
\{ $cmc -> print_q \}
$PAR
\{ $cmc -> print_a \}
END_TEXT

