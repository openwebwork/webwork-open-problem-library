$showPartialCorrectAnswers = 1;

$a = random(2,10);
$pi = arccos(-1);

TEXT(beginproblem());


BEGIN_TEXT
$BR
Evaluate the triple integral 

\(\displaystyle \iiint_{E} x \, dV \) where \(E\) is the solid
bounded by the paraboloid  \( x=$a y^2+ $a z^2 \) and \( x = $a \).
  $PAR
\{ans_rule(40)\}

$PAR
END_TEXT


ANS(num_cmp($a**2*$pi/3));

