## Do NOT show partial correct answers
$showPartialCorrectAnswers = 0;

@questions = (
              "\(   x^2 + 4 y^2 + 9 z^2 =1  \)",
              "\(  9 x^2 + 4 y^2 + z^2 = 1  \)",
              "\(  x^2 - y^2 +z^2 = 1  \)",
              "\( - x^2 + y^2 - z^2 = 1  \)",
              "\(  y = 2 x^2  + z^2   \)",
              "\(  y^2 = x^2 + 2 z^2   \)",
              "\(  x^2 + 2  z^2  =1  \)",
              "\( y =  x^2 - z^2\)"
              );

@images = (
            "stewart12_6a.gif",
            "stewart12_6b.gif",
            "stewart12_6c.gif",
            "stewart12_6d.gif",
            "stewart12_6e.gif",
            "stewart12_6f.gif",
            "stewart12_6g.gif",
            "stewart12_6h.gif"
             );

@subset = NchooseK(8,6);

@subset_of_questions = @questions[@subset];
@subset_of_images = @images[@subset];

@permutation = NchooseK(6,6);
