##### 6. #####
ANS(num_cmp(0)); ANS(num_cmp($n*$a));
ANS(fun_cmp("0", vars=>"z")); ANS(fun_cmp("$n-z/$a", vars=>"z"));
ANS(fun_cmp("$a*x+z", vars=>["x","z"]));
ANS(fun_cmp("$n*$a", vars=>["x","z"]));




