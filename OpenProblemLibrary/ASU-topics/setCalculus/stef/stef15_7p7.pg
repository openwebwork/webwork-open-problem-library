## Do NOT show partial correct answers
$showPartialCorrectAnswers = 1;

$a = random(1,9);

$p = nicestring([$a,$a],["y^2","z^2"]);
$pi = arccos(-1);

TEXT(beginproblem());


BEGIN_TEXT
$BR
Use a triple integral to find the volume of the solid enclosed by
the paraboloid  \(  x = $p \) and the plane
\( x = $a \).
  $PAR
\{ans_rule(40)\}

$PAR
END_TEXT


ANS(num_cmp($a*$pi/2));

ENDDOCUMENT();
