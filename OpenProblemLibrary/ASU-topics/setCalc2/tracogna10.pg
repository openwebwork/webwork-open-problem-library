DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,5,1);

$funct = "tan($a*x)/$a - x";

BEGIN_TEXT;

Evaluate the indefinite integral.
$BR \[ \int \tan^2 ($a x)\,dx \]
$BR $BR Answer = \{ans_rule( 45) \} \(+C\)
END_TEXT

$ans = $funct;
ANS(fun_cmp($ans, mode=>"antider"));

