## DBsubject(Calculus - single variable)
## DBchapter(Differentiation)
## DBsection(Quotient rule (without trigonometric functions))
## Institution(ASU)
## MLT(quotRulenoTrigRatFun)
## Level(2)
## KEYWORDS('Differentiation' 'Product Rule' 'Quotient Rule')


DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

$a = random(2, 13, 1);
$b = random(2,7,1);
$c = random(2,7,1);
$d = random(3,9,1);
$r1 = random(1,12,1);
while ($c*$r1 - $d == 0)
{
$r1 = random(1,12,1);
}
$r2 = random(-12,0,1);
while ($c*$r2 - $d == 0)
{
$r2 = random(1,12,1);
}

TEXT(EV2(<<EOT));
Let
\[ f(x) = \frac{$a x^2 + $b x}{$c x - $d}. \]
Evaluate \( f'(x) \) at the following points:
$BR
(A)\( f'($r1)\) = \{ans_rule(40) \}
$BR
EOT

$ans = ((2*$a*($r1) + $b)*($c*($r1) - $d) - ($a*(($r1)**2) + $b*($r1))*($c))/($c*$r1 - $d)**2;
ANS(num_cmp($ans));

TEXT(EV2(<<EOT));
(B) \( f'($r2) \) = \{ ans_rule(40) \}
$BR
EOT

$ans = ((2*$a*($r2) + $b)*($c*($r2) - $d) - ($a*(($r2)**2) + $b*($r2))*($c))/($c*$r2 - $d)**2;
ANS(num_cmp($ans));

ENDDOCUMENT();
