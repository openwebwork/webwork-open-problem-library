## DBsubject(Calculus - single variable)
## DBchapter(Differentiation)
## DBsection(Quotient rule (without trigonometric functions))
## Institution(ASU)
## MLT(quotRuleNoTrigxoverLinear)
## Level(2)
## KEYWORDS('Differentiation' 'Product Rule' 'Quotient Rule')


DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;

$a = random(2, 13, 1);
$b = random(2,7,1);
$c = random(2,7,1);
$r1 = random(1,12,1);
while ($c*($r1) - $b == 0)
{
$r1 = random(1,12,1);
}
$r2 = random(-12,-1,1);
while ($c*($r2) - $b == 0)
{
$r2 = random(-12,-1,1);
}

TEXT(EV2(<<EOT));
Let
\[ f(x) = \frac{$a x}{$c x - $b}. \]
Evaluate \( f'(x) \) at the following points:
$BR
(A) \( f'($r1)\) = \{ans_rule(40) \}
$BR
$BR
EOT

$ans = ($a*($c*($r1) - $b) - $c*$a*($r1))/(($c*($r1) - $b)**2);
ANS(num_cmp($ans));

TEXT(EV2(<<EOT));
(B) \( f'($r2) \) = \{ans_rule(40) \}
$BR
EOT

$ans = ($a*($c*($r2) - $b) - $c*$a*($r2))/(($c*($r2) - $b)**2);
ANS(num_cmp($ans));

ENDDOCUMENT();
