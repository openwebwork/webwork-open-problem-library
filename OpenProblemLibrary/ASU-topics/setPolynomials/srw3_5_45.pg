DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcomplexmacros.pl",
  "PGasu.pl",
  "extraAnswerEvaluators.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = non_zero_random(1,4,1);
$b = non_zero_random(1,3,1);
$c = non_zero_random(3,5,1);
## (x+a)(x^2+bx+c)
$b1 = $a+$b;
$c1 = $a*$b+$c;
$d1 = $a*$c;

$p=nicestring([1,$b1,$c1,$d1]);


TEXT(EV2(<<EOT));
Give $BBOLD all $EBOLD the zeros of \[ P(x)=$p \] as
a comma separated list.
$BR  
\{ ans_rule(40) \}
$BR  

EOT


$delta=4*$c-$b*$b;
$nb=-$b;
$ans1 = -$a;
$ans2 = "$nb";
$deltas = "sqrt($delta)";

ANS(number_list_cmp("$ans1,($ans2-$deltas*i)/2,($ans2+$deltas*i)/2",complex=>'ok'));

ENDDOCUMENT();        # This should be the last executable line in the problem.

