"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,6,1);
$b = random(2,6,1);
$c = random(2,6,1);

TEXT(EV2(<<EOT));

Evaluate the definite integral:
$BR
$PAR
   \( \displaystyle{ \int_{-1}^{$a} x^2 - $b x \,dx} = \)
   \{ans_rule(35) \} 
$BR
$BR

EOT

$ans = 1/3 + ($a**3)/3 + $b/2 - ($a**2)*$b/2;
ANS(num_cmp($ans));

ENDDOCUMENT();
