DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

$showPartialCorrectAnswers = 1;

$a = random(1,6,1);
$b = $a + 1;
$a1 = -$a + 1;
$b1 = $a + 2;
$b3 = random(2,6,1);

BEGIN_TEXT
Which of the points  \(A=($a,$b)\) or \(B=($a1,$b1)\)
is closer to the origin?
\{ pop_up_list(['?','A','B']) \}
END_TEXT

$ans1 = "A";

ANS(str_cmp($ans1));

