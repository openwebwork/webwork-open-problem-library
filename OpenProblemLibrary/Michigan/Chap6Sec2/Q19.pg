## MLT(antideriv_poly_find_AN_antideriv)
## Level(2)
## MO(1)
## TitleText1('Calculus')
## AuthorText1('Hughes-Hallett')
## EditionText1('4')
## Section1('6.2')
## Problem1('19')
## TitleText2('Calculus')
## AuthorText2('Hughes-Hallett')
## EditionText2('5')
## Section2('7.')
## Problem2('1')
## TitleText3('Calculus')
## AuthorText3('Hughes-Hallett')
## EditionText3('6')
## Section3('7.')
## Problem3('1')

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGchoicemacros.pl",
# "parserNumberWithUnits.pl",
# "parserFormulaWithUnits.pl", 
# "parserFormulaUpToConstant.pl",
# "PGcourse.pl",
);

Context("Numeric");
Context()->variables->add(t=>"Real");
$showPartialCorrectAnswers = 1;

$a = random(1,9,1);
$b = random(2,5,1);
$b1 = $b+1;
$func = Compute("(t + $a)^($b)");
$afunc = Compute("(1/$b1)*(t + $a)^($b1)");
 
TEXT(beginproblem());
Context()->texStrings;
BEGIN_TEXT

Find an antiderivative of
\( q(t) = $func \)

$PAR
\( Q(t) = \) \{ ans_rule(35) \}

END_TEXT
Context()->normalStrings;

ANS($afunc->cmp( upToConstant=>1 ) );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\[ Q(t) = {1\over $b1}(t + $a)^{$b1} \]

END_SOLUTION
Context()->normalStrings;


;
ENDDOCUMENT();
