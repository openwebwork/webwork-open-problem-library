Context("Numeric");
Context()->variables->set(x=>{limits=>[0.25,0.75]});

$r = list_random("3","4");
$r = 4;
$a = random(2,8,1);
$func = Formula("x^$r e^($a x)");

$asq = $a*$a;
$acu = $asq*$a;
$aqu = $acu*$a;
$aqi = $aqu*$a;

