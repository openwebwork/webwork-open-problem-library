DOCUMENT();
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"MathObjects.pl",
);
 
Context("Numeric");
Context()->variables->add( q=>'Real' );

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(1,19,1);

Context()->texStrings;
BEGIN_TEXT

Find the derivative of 
\( s(q) = $a \cos q \sin q \).

$PAR
\( s'(q) = \) \{ ans_rule(65) \}

END_TEXT
Context()->normalStrings;


ANS(Compute( "$a*(-1*sin(q)*sin(q) + cos(q)*cos(q))" )->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\( s'(q) = $a (-\sin(q) \sin(q) + \cos(q) \cos(q)) \)
$BR
\( \phantom{s'(q)} = $a (-\sin^2(q) + \cos^2(q)). \)

END_SOLUTION
Context()->normalStrings;


;
ENDDOCUMENT();
