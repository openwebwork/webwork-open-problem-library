DOCUMENT();
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"MathObjects.pl",
);

Context("Numeric");
Context()->variables->add( z=>'Real' );

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);

Context()->texStrings;
BEGIN_TEXT

Find the derivative of 
\( h(z) = (\ln$a)^z \).

$PAR
\( h'(z) = \) \{ ans_rule(35) \}

END_TEXT
Context()->normalStrings;


ANS(Compute( "ln(ln($a))*ln($a)^z" )->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\[ h'(z) = \ln(\ln($a)) \cdot (\ln($a))^z \]
END_SOLUTION
Context()->normalStrings;


COMMENT('MathObject version');
ENDDOCUMENT();
