DOCUMENT();
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"MathObjects.pl",
);
 
Context("Numeric");
Context()->variables->add( t=>'Real' );

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(1,9,1);
$b = random(2,9,1);

Context()->texStrings;
BEGIN_TEXT

Find the derivative of
$BR
\( f(t)=t e^{$a-$b t} \)

$PAR
\( f'(t) = \) \{ ans_rule(35) \}

END_TEXT
Context()->normalStrings;


ANS(Compute( "(1 - $b*t)*e^($a - $b*t)" )->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\[ f'(t) = e^{$a - $b t} - $b t e^{$a - $b t} = (1 - $b t) e^{$a - $b t} \]

END_SOLUTION
Context()->normalStrings;


COMMENT('MathObject version');
ENDDOCUMENT();
