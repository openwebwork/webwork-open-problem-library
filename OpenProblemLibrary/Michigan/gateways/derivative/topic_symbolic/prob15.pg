DOCUMENT();
loadMacros(
           "PGbasicmacros.pl",
           "PGanswermacros.pl",
           "PGchoicemacros.pl",
           "PGauxiliaryFunctions.pl",
	   "MathObjects.pl",
           );

TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are( a=>'Real',  s=>'Real',  x=>'Real');
Context()->variables->set( a=>{limits=>[-10,10]}, s=>{limits=>[1,10]}, x=>{limits=>[1,10]});

# Context()->flags->set( limits=>[[-10,10],[1,10],[1,10]] );

$ans = "(a*(0.5*s))*(s*x)^(-0.5)";

# for information about tutorial cgi, contact the UMich repository owner
# $tut = htmlLink("http://[local location of tutorial cgi]" .
# 		"gwderivtut?pnum=721","rule for constant multiples,"");

Context()->texStrings;
BEGIN_TEXT
Differentiate \(z = a \sqrt{s x}\), where \(a\) and \(s\) are constants
$BR
Answer: \( {dz\over dx} = \) \{ans_rule(55)\}
END_TEXT
Context()->normalStrings;

# ANS(fun_cmp($ans, 'var'=>['a', 's', 'x'], 'limits'=>[[0,10],[0.1,10],[0.1,10]]));
ANS( Compute("$ans")->cmp() );

$bsbox = MODES('TeX' => '\par{\bf Solution outline:} ',
	       'Latex2HTML' => '\par{\bf Solution outline:} ',
	       'HTML' => '<div class="gwSoln"><b>Solution outline:</b>');
$esbox = MODES('TeX' => '\par',
	       'Latex2HTML' => '\par',
	       'HTML' => '</div>');
# SOLUTION($bsbox,"The solution to this problem is obtained with the ", $tut,
SOLUTION($bsbox,"The solution to this problem is obtained with the rule for constant multiples",
	 ".", $esbox);

ENDDOCUMENT();
