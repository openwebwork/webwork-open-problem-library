DOCUMENT();
loadMacros(
           "PGbasicmacros.pl",
           "PGanswermacros.pl",
           "PGchoicemacros.pl",
           "PGauxiliaryFunctions.pl",
	   "MathObjects.pl",
           );

TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are( a=>'Real',  b=>'Real',  t=>'Real');
Context()->variables->set( a=>{limits=>[10,20]}, b=>{limits=>[-2,-1]}, t=>{limits=>[0,9]});

# Context()->flags->set( limits=>[[10,20],[-2,-1],[0,9]] );

$ans = "(5/2)*(-1/b)*(a - t/b)^(3/2)";

# for information about tutorial cgi, contact the UMich repository owner
# $tut = htmlLink("http://[local location of tutorial cgi]" .
# 		"gwderivtut?pnum=775","chain rule,"");

Context()->texStrings;
BEGIN_TEXT
Differentiate \(f(t) = (\sqrt{a - {t\over b}})^5\), where \(a\) and
\(b\) are constants.
$BR
Answer: \( f'(t) = \) \{ans_rule(55)\}
END_TEXT
Context()->normalStrings;

# ANS(fun_cmp($ans, 'var'=>['a', 'b', 't'], 'limits'=>[[1,10],[-5,-1],[1,10]]));
ANS( Compute("$ans")->cmp() );

$bsbox = MODES('TeX' => '\par{\bf Solution outline:} ',
	       'Latex2HTML' => '\par{\bf Solution outline:} ',
	       'HTML' => '<div class="gwSoln"><b>Solution outline:</b>');
$esbox = MODES('TeX' => '\par',
	       'Latex2HTML' => '\par',
	       'HTML' => '</div>');
# SOLUTION($bsbox,"The solution to this problem is obtained with the ", $tut,
SOLUTION($bsbox,"The solution to this problem is obtained with the chain rule",
	 ".", $esbox);

ENDDOCUMENT();
