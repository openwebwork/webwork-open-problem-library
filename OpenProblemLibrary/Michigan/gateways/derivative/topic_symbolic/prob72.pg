DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are( u=>'Real',  w=>'Real',  x=>'Real');

$ans = "u*w*cos(w*x)";

# for information about tutorial cgi, contact the UMich repository owner
