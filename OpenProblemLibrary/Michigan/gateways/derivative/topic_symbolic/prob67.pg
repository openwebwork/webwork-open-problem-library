TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are( A=>'Real',  B=>'Real',  z=>'Real');
Context()->variables->set( A=>{limits=>[-10,10]}, B=>{limits=>[1,5]}, z=>{limits=>[0.1,10]});

# Context()->flags->set( limits=>[[-10,10],[1,5],[0.1,10]] );

$ans = "2*A*ln(B*z)/z";

