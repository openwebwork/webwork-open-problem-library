Context()->texStrings;
BEGIN_TEXT
Differentiate \(B(s) = K - \ln({s\over C})\), where \(K\) and \(C\) are constants
$BR
Answer: \( B'(s) = \) \{ans_rule(55)\}
END_TEXT
Context()->normalStrings;

# ANS(fun_cmp($ans, 'var'=>['s','K','C'], 'limits'=>[[0.1, 10],[1,10],[1,10]]));
ANS( Compute("$ans")->cmp() );

$bsbox = MODES('TeX' => '\par{\bf Solution outline:} ',
	       'Latex2HTML' => '\par{\bf Solution outline:} ',
