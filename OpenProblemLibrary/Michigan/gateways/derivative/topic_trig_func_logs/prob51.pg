TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are( z=>'Real');
Context()->variables->set( z=>{limits=>[1.2,1.95]} );

$ans = "(-4*(sqrt(2)))*(cos(4*z))^(sqrt(2)-1)*sin(4*z)";

