TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are( y=>'Real');
Context()->variables->set( y=>{limits=>[0.001,0.21]} );

$ans = "3.5*(tan(7*y))^(-0.5)*(1 / (cos(7*y))^(2))";

