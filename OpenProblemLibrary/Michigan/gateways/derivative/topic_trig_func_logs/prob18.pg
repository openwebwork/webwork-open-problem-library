TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are( t=>'Real');
Context()->variables->set( t=>{limits=>[0, 10]} );

$ans = "-1*(9*t^(3) + 3*t^(2) + sqrt(2))^(-1)*(27*t^2 + 6*t)";

