TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are( y=>'Real');
Context()->variables->set( y=>{limits=>[3.2,15]} );

$ans = "(-1/(y^3 - pi^3))*3*y^2";

