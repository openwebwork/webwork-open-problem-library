Context()->texStrings;
BEGIN_TEXT
Differentiate \(R =\displaystyle{\tan(s) + 5 \over \cos(7s) + 5}\)
$BR
Answer: \( {dR\over ds} = \) \{ans_rule(55)\}
END_TEXT
Context()->normalStrings;

# ANS(fun_cmp($ans, 'var'=>['s'], 'limits'=>[4.8,7.8]));
ANS( Compute("$ans")->cmp() );

$bsbox = MODES('TeX' => '\par{\bf Solution outline:} ',
	       'Latex2HTML' => '\par{\bf Solution outline:} ',
