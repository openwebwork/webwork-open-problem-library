TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->are( s=>'Real');
Context()->variables->set( s=>{limits=>[4.8,7.8]});

# Context()->flags->set( limits=>[4.8,7.8] );

$ans = "((1/(cos(s))^2)*(cos(7*s) + 5) + 7*(tan(s) + 5)*sin(7*s)) / (cos(7*s) + 5)^2";

# for information about tutorial cgi, contact the UMich repository owner
