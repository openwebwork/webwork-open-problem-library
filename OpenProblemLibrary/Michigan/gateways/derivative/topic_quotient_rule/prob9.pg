	   "MathObjects.pl",
           );

TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->set( x=>{limits=>[-9,0]} );
# Context()->flags->set( limits=>[-9,0] );

$ans = "-(sqrt(1 - x)/(10 + x)^2) - 1/(2*sqrt(1 - x)*(10 + x))";

# for information about tutorial cgi, contact the UMich repository owner
