TEXT();
$showPartialCorrectAnswers = 1;

Context('Numeric');
Context()->variables->set( x=>{limits=>[1.1,10]} );
# Context()->flags->set( limits=>[1.1,10] );

$ans = "(3*(6*x + sin(6))^(-0.5)*ln(x) - x^(-1)*sqrt(6*x + sin(6))) / (ln(x))^2";

