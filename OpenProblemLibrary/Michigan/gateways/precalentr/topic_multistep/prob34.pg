TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\({5\over3} - {8\over3}k\)";
$gwprob = new_multiple_choice();
$gwprob->qa('If \((c, 4k)\) lies on the line \(3x + 2y = 5\) then what is \(c\)?
',
	    $ans);
$gwprob->extra("\({5\over2} - 6k\)",
	"\({5\over8} - {3\over8}k\)",
	"\({5\over8} - {3\over 8}c\)",
	"\({5\over3} + {8\over3}k\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

