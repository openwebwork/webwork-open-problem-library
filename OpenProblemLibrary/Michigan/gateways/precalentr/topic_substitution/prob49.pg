	" \(-6x^4\) ",
	" \(-12x^4\) ",
	" \(-18x^4\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

