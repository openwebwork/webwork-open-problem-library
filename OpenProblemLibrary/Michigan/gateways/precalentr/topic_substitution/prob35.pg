	   "extraAnswerEvaluators.pl", # get equation_cmp
           );

TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(15s^2\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('If \(n=r^2-s^2\) and \(r=-4s\) express \(n\) in terms of \(s\).',
	    $ans);
$gwprob->extra(" \(-17s^2\) ",
	" \(16\) ",
	" \(-5s^2\) ",
	"One cannot express \(n\) in terms of \(s\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

