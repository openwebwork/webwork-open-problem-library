TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(y^4-1\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('If \(a=y^2\) express \((a-1)(a+1)\) in terms of \(y\).',
	    $ans);
$gwprob->extra("\( y^2+2y+1\) ",
	"\( y^2-2y-1\) ",
	"\( y^2+1\) ",
	"\( y^2-1\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

