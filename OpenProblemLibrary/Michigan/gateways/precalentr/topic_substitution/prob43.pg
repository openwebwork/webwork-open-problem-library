TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(4z^2+4z\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('If \(b=-2z\) express \(b^2-2b\) in terms of \(z\).',
	    $ans);
$gwprob->extra(" \(0\) ",
	" \(-2z^2+4z\) ",
	" \(-4z^2+4z\) ",
	" \(2\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

