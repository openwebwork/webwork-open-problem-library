DOCUMENT();
loadMacros(
           "PGbasicmacros.pl",
           "PGanswermacros.pl",
           "PGchoicemacros.pl",
           "PGauxiliaryFunctions.pl",
	   "extraAnswerEvaluators.pl", # get equation_cmp
           );

TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(21\)";
$gwprob = new_multiple_choice();
$gwprob->qa(' The point \((5,q)\) is on the graph of \(3x+6=y\). What ' .
	    'is the value of \(q\)?',
	    $ans);
$gwprob->extra("\(-\frac{1}{3}\)",
	"\(\frac{11}{3}\)",
	"\(3x+6\)");
$gwprob->makeLast("It cannot be determined from the given information.");


BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

