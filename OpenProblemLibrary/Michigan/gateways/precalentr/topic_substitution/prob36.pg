TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(-2k^4\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('If \(q={3m^3\over k^2}-k^2m\) and \(m=-k^2\) express \(q\) in terms
of \(k\).',
	    $ans);
$gwprob->extra(" \(-3+k^4\) ",
	" \(-4k^4\) ",
	" \(-3k^3+k^4\) ",
	"One cannot express \(q\) in terms of \(k\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

