TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(-12w^2-3\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('If \(v=4y-3\) and \(y=-3w^2\) express \(v\) in terms of \(w\).',
	    $ans);
$gwprob->extra(" \(12w^2-3\) ",
	" \({1\over2}\) ",
	" \(-3w^2+3\over 4\) ",
	"One cannot express \(v\) in terms of \(w\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

