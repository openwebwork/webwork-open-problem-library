DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "extraAnswerEvaluators.pl",
  "PGcourse.pl"
);

TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(8d^2-6d-2\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('If \(p=(2u-1)(2+u)\) and \(u=-2d\) express \(p\) in terms of \(d\).
',
	    $ans);
$gwprob->extra(" \(4d^2-3d-1\) ",
	" \(16d^2-1\) ",
	" \(-8d^2-6d-2\) ",
	"One cannot express \(p\) in terms of \(d\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

