TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(4d-d^2\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('If \(y=(2-z)(2+z)\) and \(z=2-d\) express \(y\) in terms of \(d\).
',
	    $ans);
$gwprob->extra(" \(-4d+d^2\) ",
	" \(-4d-d^2\) ",
	" \(4\) ",
	"One cannot express \(y\) in terms of \(d\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

