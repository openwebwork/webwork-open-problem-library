TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(\frac{(-2)^u}{Y^u}\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((\frac{-2}{Y})^u\)?',
	    $ans);
$gwprob->extra("\(\frac{2^u}{Y^u}\)",
	"\((Y-2)^u\)",
	"\((-2)^uY^u\)",
	"None of these expressions are equivalent");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

