TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(\frac{2^Jd^{3J}}{h^{J}}\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((\frac{2d^3}{h})^J\)?',
	    $ans);
$gwprob->extra("\(2\frac{d^{3J}}{h^J}\)",
	"\(2(\frac{d}{h})^{3-J}\)",
	"\(2d^3h^{-J}\)",
	"None of these expressions are equivalent");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

