	    $ans);
$gwprob->extra(" \( z^{P}\, U \) ",
	" \( z\, U^{P} \) ",
	" \( z^{P/2}\, U^{P/2} \) ",
	" \( P\, z^{P-1}\, U^{P-1} \) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

