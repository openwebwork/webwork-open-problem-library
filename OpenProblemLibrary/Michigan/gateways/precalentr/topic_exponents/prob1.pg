TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(5^{x+y}\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \(5^x5^y\)?',
	    $ans);
$gwprob->extra("\(25^{xy}\)",
	"\(5^{xy}\)",
	"\(25^{x+y}\)");
$gwprob->makeLast("None of these expressions are equivalent");


BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

