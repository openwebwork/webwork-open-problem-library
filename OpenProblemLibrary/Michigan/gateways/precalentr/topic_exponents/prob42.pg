TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \( {2x^2 z\over y} \) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \( \left({x^{-2}\,y\over 2z}\right)^{-1} \)?',
	    $ans);
$gwprob->extra(" \( {1\over 2x^2 y z} \) ",
	" \( {2z\over x^2 y} \) ",
	" \( {2z\over x^3 y} \) ",
	" None of these expressions are equivalent ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

