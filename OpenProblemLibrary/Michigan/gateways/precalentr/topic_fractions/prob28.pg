TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(9 + u^2\over 1 + 3u\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \({u + {9\over u}\over 3 +
{1\over u}}\)?',
	    $ans);
$gwprob->extra("\(u^2 + {u\over 3} + {3\over u} + 9\)",
	"\((u^2+9)(3u+1)\over u^2\)",
	"\(u + 3\)",
	"\(u^2 + 9\over 3\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

