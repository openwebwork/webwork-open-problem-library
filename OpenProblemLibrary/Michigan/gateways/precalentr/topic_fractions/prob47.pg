TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(15\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \(3t\over t/5\)?',
	    $ans);
$gwprob->extra("\(3t^2\over 5\)",
	"\(3\over 5\)",
	"\(15t^2\)",
	"\(15\over t\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

