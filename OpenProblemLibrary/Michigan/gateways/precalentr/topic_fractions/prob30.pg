TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(8c + 5w\over w^2\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \({ {8\over w} + {5\over c}
\over  {w\over c} }\)?',
	    $ans);
$gwprob->extra("\(8w + 5c\over w^2\)",
	"\({8\over c} + 5w\)",
	"\(8 c + 5 w^2\over w(w+c)\)",
	"\(13\over w\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

