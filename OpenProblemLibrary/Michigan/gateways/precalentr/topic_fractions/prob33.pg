DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "extraAnswerEvaluators.pl",
  "PGcourse.pl"
);

TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(1\over 8z\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \({ {1\over z} + {1\over 8}
\over  z + 8 }  \)?',
	    $ans);
$gwprob->extra("\(1\)",
	"\(1\over (z + 8)^2\)",
	"\(z + 8\)",
	"\((z+8)^2\over 8z\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

