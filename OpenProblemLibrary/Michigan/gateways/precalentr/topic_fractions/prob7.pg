	   "extraAnswerEvaluators.pl", # get equation_cmp
           );

TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(\frac{I^3}{1-I}\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \(\frac{I^2}{\frac {1}{I}
- 1}\)?',
	    $ans);
$gwprob->extra("\(I^3 - 1\)",
	"\(I^3\)",
	"\(\frac{I^2}{\frac {1}{I}}\)",
	"\(I-1\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

