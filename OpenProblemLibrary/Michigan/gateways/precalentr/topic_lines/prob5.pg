TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(-1\)";
$gwprob = new_multiple_choice();
$gwprob->qa('The slope of the line having equation \(-5y-5x=1\) is',
	    $ans);
$gwprob->extra("\(1\)",
	"\(-{1\over5}\)",
	"\(1\over5\)",
	"\(1\over\sqrt5\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

