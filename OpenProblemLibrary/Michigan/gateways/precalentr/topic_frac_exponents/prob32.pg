TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \( W^{8/3}\, m^{16} \) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \( (W\, m^{6})^{8/3} \)?',
	    $ans);
$gwprob->extra(" \( W\, m^{16} \)  ",
	" \( W^{11/3} \, m^{24/3} \)  ",
	" \( W^{3/8}\, m^{9/4} \)  ",
	" None of these expressions are equivalent ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

