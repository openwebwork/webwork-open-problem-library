TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(-3, 1\) ";
$gwprob = new_multiple_choice();
$gwprob->qa(' If \((Q+1)(Q+2) = Q+5\), what are all possible values of \(Q\)?',
	    $ans);
$gwprob->extra(" \(3, 1\) ",
	" \(-1, -2, -5\) ",
	" \(-1, -2\) ",
	" \(1, 2, 5\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

