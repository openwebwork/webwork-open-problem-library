$gwprob->extra(" \( - 2, - 1 \) ",
	" There are no real solutions to this equation ",
	" \( - 2 \) ",
	" \( - 3, - 2 \) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

