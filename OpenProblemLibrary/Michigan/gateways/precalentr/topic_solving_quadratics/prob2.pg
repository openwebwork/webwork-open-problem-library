DOCUMENT();
loadMacros(
           "PGbasicmacros.pl",
           "PGanswermacros.pl",
           "PGchoicemacros.pl",
           "PGauxiliaryFunctions.pl",
	   "extraAnswerEvaluators.pl", # get equation_cmp
           );

TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(2, -5\)";
$gwprob = new_multiple_choice();
$gwprob->qa('If \(10y^2-10 = 9y^2-3y\), what are all possible values of \(y\)?',
	    $ans);
$gwprob->extra("\(1, -1\)",
	"\(0, {1\over3}\)",
	"\({1\over3}\)",
	"There is no \(y\) which satisfies this equation");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

