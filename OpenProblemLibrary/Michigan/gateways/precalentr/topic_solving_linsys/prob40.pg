intersect? ',
	    $ans);
$gwprob->extra(" \( (  5, - 24 ) \) ",
	" \( (  24,  5 ) \) ",
	" \( ( - 3,  {1\over 3} ) \) ",
	" \( (  8, - {19\over 9} ) \) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

