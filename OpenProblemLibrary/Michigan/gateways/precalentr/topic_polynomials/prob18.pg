DOCUMENT();
loadMacros(
           "PGbasicmacros.pl",
           "PGanswermacros.pl",
           "PGchoicemacros.pl",
           "PGauxiliaryFunctions.pl",
	   "extraAnswerEvaluators.pl", # get equation_cmp
           );

TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(r^2 - 2rt^3 + t^6\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((r - t^3)^2\)?',
	    $ans);
$gwprob->extra(" \(r^2 + t^6\) ",
	" \(r^2 - t^6\) ",
	" \(r^2 - t^5\) ",
	" \(r^2 - 2rt^3 - t^6\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

