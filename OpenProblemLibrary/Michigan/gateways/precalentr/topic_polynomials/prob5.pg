',
	    $ans);
$gwprob->extra(" \(-15 - 2y\) ",
	" \(10y - 15\) ",
	" \(10xy - 15x\) ",
	" None of these expressions are equivalent. ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

