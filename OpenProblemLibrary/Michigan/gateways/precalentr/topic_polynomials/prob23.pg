TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(p^2 + 2pq - 2pq^{-1} - 4\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((p + 2q)(p - {2\over q})\)?
',
	    $ans);
$gwprob->extra(" \(p^2 - 4\) ",
	" \(p^2 - 4p - 4\) ",
	" \(p^2 + 4pq - 4\) ",
	" \(p^2 - 2pq + 2pq^{-1} + 4\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

