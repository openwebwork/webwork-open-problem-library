TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(a^4 + 2a^2 b + b^2\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((a^2 + b)^2\)?',
	    $ans);
$gwprob->extra(" \(a^4 + b^2\) ",
	" \(a^4 + a^2 b + b^2\) ",
	" \(a^2 + 2ab + b^2\) ",
	" \(a^4 + 2ab + b^2\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

