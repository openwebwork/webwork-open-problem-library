TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \((x)(x-2)(x+3)\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \(x^3+x^2-6x\)?',
	    $ans);
$gwprob->extra(" \((x)(x+2)(x-3)\) ",
	" \((x-1)(x-2)(x+3)\) ",
	" \((x-2)(x^2+3)\) ",
	" \((x-\sqrt2)(x+\sqrt2)(x+3)\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

