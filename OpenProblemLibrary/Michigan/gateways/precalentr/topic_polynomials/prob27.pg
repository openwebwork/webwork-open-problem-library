DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "extraAnswerEvaluators.pl",
  "PGcourse.pl"
);

TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \((M-2)(M-3)\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \(M^2-5M+6\)?',
	    $ans);
$gwprob->extra(" \((M-2)(M+3)\) ",
	" \((M-6)(M+1)\) ",
	" \((M+6)(M-1)\) ",
	" None of these expressions are equivalent. ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

