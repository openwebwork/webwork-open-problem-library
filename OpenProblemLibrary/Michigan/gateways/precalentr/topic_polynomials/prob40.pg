TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(T(T+4)\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((T+2)^2-4\)?',
	    $ans);
$gwprob->extra(" \(((T+2)-2)^2\) ",
	" \(T^2\) ",
	" \((T+6)(T-2)\) ",
	" \((T+2)(T-2)\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

