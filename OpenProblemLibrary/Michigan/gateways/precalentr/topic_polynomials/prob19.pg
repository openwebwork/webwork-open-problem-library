TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(x^4 + 2x^2\sqrt{y} + y\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((x^2 + \sqrt{y})^2\)?',
	    $ans);
$gwprob->extra(" \(x^4 + y\) ",
	" \(x^4 + 2x^2 y + y\) ",
	" \(x^2 + 2x\sqrt{y} + y\) ",
	" \(x^4 + y^2\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

