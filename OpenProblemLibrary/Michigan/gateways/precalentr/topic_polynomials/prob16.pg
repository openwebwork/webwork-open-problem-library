TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(p^6 - 2p^3 q^3 + q^6\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((p^3 - q^3)^2\)?',
	    $ans);
$gwprob->extra(" \(p^6 + q^6\) ",
	" \(p^6 - q^6\) ",
	" \(p^5 + q^5\) ",
	" \(p^6 - 2p^3 q^3 - q^6\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

