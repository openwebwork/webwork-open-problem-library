TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(-12x^2 - 5x + 2\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((1-4x)(3x+2)\)?',
	    $ans);
$gwprob->extra("\(-12x^2 + 2x + 2\)",
	"\(-9x -6\)",
	"\(-17x + 2\)",
	"\(12x^2 + 5x -2\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

