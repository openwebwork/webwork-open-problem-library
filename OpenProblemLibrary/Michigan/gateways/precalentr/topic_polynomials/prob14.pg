	    $ans);
$gwprob->extra(" \(1 - 4x^4\) ",
	" \(1 + 4x^4\) ",
	" \(-4x^4 - 4x^2 + 1\) ",
	" \(4x^2 - 4x + 1\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

