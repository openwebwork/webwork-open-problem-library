TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\((5-A)(A-1)\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \(4 - (A-3)^2\)?',
	    $ans);
$gwprob->extra("\((2-(A-3))^2\)",
	"\(-(A+5)(A+1)\)",
	"\((-A+6)(A-5)\)",
	"\((A-5)(A-1)\)");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

