TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(L^4 - 2L^2 Q^3 + Q^6\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \((L^2 - Q^3)^2\)?',
	    $ans);
$gwprob->extra(" \(L^4 - Q^6\) ",
	" \(L^4 + Q^6\) ",
	" \(L^4 - 2L^2 Q^3 + Q^5\) ",
	" \(L^4 - L^2 Q^3 + Q^6\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

