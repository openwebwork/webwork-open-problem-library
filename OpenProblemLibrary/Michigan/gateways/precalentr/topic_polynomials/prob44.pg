TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \((y^2+1)^2\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following is equivalent to \(y^4+2y^2+1\)?',
	    $ans);
$gwprob->extra(" \((y+1)^4\) ",
	" \((y+1)^2(y-1)^2\) ",
	" \((y^2+1)(y+1)(y-1)\) ",
	" \((y+1)^3(y-1)\) ");



BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

