DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "extraAnswerEvaluators.pl",
  "PGcourse.pl"
);

TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(0.1 > r > 0\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following inequalities describes all of ' .
	    'the positive values of \(r\) which are less than 0.1?',
	    $ans);
$gwprob->extra(" \(r = 0\) ",
	" \(0 \le r < 0.1\) ",
	" \(0.1 > r < 0\) ",
	" \(0 \le r \le 0.1\) ");

BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

