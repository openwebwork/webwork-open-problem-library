DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "extraAnswerEvaluators.pl",
  "PGcourse.pl"
);

TEXT();
$showPartialCorrectAnswers = 1;

$ans = " \(4 > t > -5\) ";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following inequalities describes all of ' .
	    'values of \(t\) which are between -5 and 4?',
	    $ans);
$gwprob->extra(" \(-4 \le t\le 3\) ",
	" \(4 > t < -5\) ",
	" \(4 \ge t\ge -5\)",
	" \(-4 \le t \ge 3\) ");

BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

