DOCUMENT();
loadMacros(
           "PGbasicmacros.pl",
           "PGanswermacros.pl",
           "PGchoicemacros.pl",
           "PGauxiliaryFunctions.pl",
	   "extraAnswerEvaluators.pl", # get equation_cmp
           );

TEXT();
$showPartialCorrectAnswers = 1;

$ans = "\(0<p<100\)";
$gwprob = new_multiple_choice();
$gwprob->qa('Which of the following inequalities describes all ' .
	    'of the positive values of \(p\) which are less than 100?',
	    $ans);
$gwprob->extra("\(0\le p<100\)",
	"\(0<p>100\)",
	"\(0>p>100\)",
	"\(0\le p\le100\)");

BEGIN_TEXT
\{ $gwprob->print_q() \}
\{ $gwprob->print_a() \}

END_TEXT

ANS(radio_cmp( $gwprob->correct_ans() ));

