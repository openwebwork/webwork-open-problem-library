########################################################################

DOCUMENT();      

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

########################################################################

TEXT(beginproblem());

$showPartialCorrectAnswers = 1;

########################################################################

Context("Complex");
Context()->strings->add(none=>{},EmptySet=>{alias=>"none"});
Context()->variables->are(t=>'Complex');

$a = random(2,6,1);

$b = Compute("(3*$a)");

$lhs = Compute("4*$a/t + 6*$a/(t+8)");

$ans = List(
    Compute("4"), 
    Compute("-16/3") 
);
$an1 = Compute("4");
$an2 = Compute("-16/3");

$ans = List("$an1, $an2");

########################################################################

Context()->texStrings;
BEGIN_TEXT
Solve the equation.
\[
$lhs = \frac{$b}{2}
\]
$PAR
Solutions (separate by commas): \(t =\) \{ ans_rule(30) \}
END_TEXT
Context()->normalStrings;

########################################################################

ANS( $ans->cmp() );

ENDDOCUMENT();
