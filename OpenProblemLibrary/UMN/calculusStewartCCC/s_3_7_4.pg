TEXT(beginproblem());

$showPartialCorrectAnswers = 1;

#####################################################################

Context("Numeric");

$a = random(1,2,1);

if ($a==1) {
$b = "\sin";
$ans = Compute("2*cot(x)");
}
if ($a==2) {
$b = "\cos";
$ans = Compute("-2*tan(x)");
}

#####################################################################
Context()->texStrings;
BEGIN_TEXT
Differentiate \(f(x) = \ln($b^2 x).\)
$PAR
Answer: \(\displaystyle f'(x) = \) \{ans_rule(30)\}
END_TEXT
Context()->normalStrings;

#####################################################################

ANS($ans->cmp());

