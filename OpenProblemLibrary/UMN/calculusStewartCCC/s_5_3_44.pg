#DESCRIPTION
##  Calculus: Indefinite integrals
##ENDDESCRIPTION


## DBsubject(Calculus - single variable)
## DBchapter(Integrals)
## DBsection(Indefinite integrals (without trigonometric functions))
## Date(02/22/2011)
## Institution(University of Minnesota)
## Author(Justin Sukiennik)
## MLT(indefinite_simplify_expression_first)
## Level(3)
## MO(1)
## TitleText1('Calculus: Concepts and Contexts')
## AuthorText1('Stewart')
## EditionText1('4 Custom UMTYMP Ed.')
## Section1('5.3')
## Problem1('44')
## KEYWORDS('calculus', 'integration', 'indefinite integrals')

#####################################################################

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "parserFormulaUpToConstant.pl",
  "PGcourse.pl"
);

#####################################################################

$showPartialCorrectAnswers = 0;

TEXT(beginproblem());

#####################################################################

Context("Numeric");
Context()->variables->are(v =>'Real');

$a = random(2,3,1);
$b = random(2,6,1);

$c = Compute("2*$a+2");
$d = Compute("2*$b");
$e = Compute("$a+2");

$ans = FormulaUpToConstant("(1/$c)*v^($c)+($d/$e)*v^($e)+($b^2/2)*v^2+C");

#####################################################################

Context()->texStrings;
BEGIN_TEXT
Find the general indefinite integral \(\displaystyle \int \! v(v^{$a}+$b)^2\, dv. \)
$PAR
Answer: \{ans_rule(40) \}
END_TEXT
Context()->normalStrings;

#####################################################################

ANS( $ans->cmp() );

ENDDOCUMENT();        # This should be the last executable line in the problem.
