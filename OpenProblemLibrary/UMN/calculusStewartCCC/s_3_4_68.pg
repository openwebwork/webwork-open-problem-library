#####################################################################

Context("Numeric");

$a = random(100,1000,100);

$ans = Compute("x*e^(-x)-$a*e^(-x)");

#####################################################################
Context()->texStrings;
BEGIN_TEXT
Find the \($a\text{th}\) derivative of \(f(x) = x e^{-x}.\)
$PAR
Answer: \(f^{($a)}(x) = \) \{ans_rule(30)\}
END_TEXT
Context()->normalStrings;

#####################################################################

ANS($ans->cmp());

