TEXT(beginproblem());

$showPartialCorrectAnswers = 1;

#####################################################################

Context("Numeric");
Context()->variables->are(z=>'Real');

$a = random(2,6,1);

$a2 = Compute("$a^2");

$ans = Compute("-z/($a2-z^2)-z/($a2+z^2)");

#####################################################################
Context()->texStrings;
BEGIN_TEXT
Differentiate \(H(z) = \ln\sqrt{\displaystyle \frac{$a2-z^2}{$a2+z^2}}.\)
$PAR
Answer: \(\displaystyle H'(z) = \) \{ans_rule(30)\}
END_TEXT
Context()->normalStrings;

#####################################################################

ANS($ans->cmp());

