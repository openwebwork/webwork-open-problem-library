## Institution(University of Minnesota)
## Author(Justin Sukiennik)
## MLT(int_sin_sqrt)
## Level(3)
## MO(1)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('6')
## Section1('7.1')
## Problem1('33')
## KEYWORDS('calculus', 'integration', 'integration by parts')

#####################################################################
DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "parserFormulaUpToConstant.pl",
  "PGcourse.pl"
);

#####################################################################

$showPartialCorrectAnswers = 0;

TEXT(beginproblem());

#####################################################################

Context("Numeric");
Context()->variables->set(x=>{limits=>[0,2]});

$a = random(1,2,1);

if ($a==1){
$b = random(2,6,1);

$f = Formula("$b*cos(sqrt(x))");

$ans = FormulaUpToConstant("2*$b*(sqrt(x)*sin(sqrt(x))+cos(sqrt(x)))");
}
if ($a==2){
$b = random(2,6,1);

$f = Formula("$b*sin(sqrt(x))");

$ans = FormulaUpToConstant("2*$b*(sin(sqrt(x))-sqrt(x)*cos(sqrt(x)))");
}


#####################################################################

Context()->texStrings;
BEGIN_TEXT
First make a substitution and then use integration by parts to evaluate the integral
\[ 
\int $f\, dx.
\]
Answer: \{ans_rule(40) \}
END_TEXT
Context()->normalStrings;

#####################################################################

ANS( $ans->cmp() );

ENDDOCUMENT();        # This should be the last executable line in the problem.
