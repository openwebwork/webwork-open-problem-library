);

#####################################################################

$showPartialCorrectAnswers = 0;

TEXT(beginproblem());

#####################################################################

Context("Numeric");

$b = random(2,15,1);
$a = random(2,20,1);

$a2 = $a*$a;
$b2 = $b*$b;

$ans = Compute("ln(sqrt($a^2+$b^2)+$b)-ln($a)");

#####################################################################

Context()->texStrings;
BEGIN_TEXT
Evaluate
\[ 
\int_{0}^{$b}  \frac{dx}{\sqrt{$a2 +x^2}}.
\]
Answer: \{ans_rule(20) \}
END_TEXT
Context()->normalStrings;

#####################################################################

ANS( $ans->cmp() );

