########################################################################

DOCUMENT();      

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

##############################################################

TEXT(beginproblem());

$showPartialCorrectAnswers = 1;

##############################################################

Context("Numeric");
Context()->variables->add(y =>'Real', z =>'Real' );

$a = random(8,24,2);
$b = random(-8,-2,1);
$c = random(-8,-2,1);
$d = random(2,10,2);

$ans1 = Compute("1^3*2^2*3*sqrt($a-1-2-3)");
$ans2 = Compute("($b)^3*($c)^2*($d)*sqrt($a-($b)-($c)-($d))");

##############################################################

Context()->texStrings;
BEGIN_TEXT
Let \(g(x,y,z) = x^3 y^2 z \sqrt{$a-x-y-z}.\)
$HR
$PAR
$BBOLD 1.$EBOLD Evaluate \(g(1,2,3).\)
$BR 
Answer: \(g(1,2,3) = \) \{ans_rule(20)\} $PAR
$BBOLD 2.$EBOLD Evaluate \(g($b,$c,$d).\)
$BR 
Answer: \(g($b,$c,$d) = \) \{ans_rule(20)\} $PAR
$HR
END_TEXT
Context()->normalStrings;

##############################################################

ANS( $ans1->cmp() );
ANS( $ans2->cmp() );

ENDDOCUMENT();        
