DOCUMENT();


loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGchoicemacros.pl",
  "parserNumberWithUnits.pl",
  "parserPopUp.pl",
  "parserAutoStrings.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
Context("Numeric");
$num = random(5,20,9);
$num2 = 3*$num;
$num3 = 2*$num;
$num4 = $num**2/4;


# Create and use pop up lists 
#$tf = new_select_list();
#$tf->rf_print_q(~~&pop_up_list_print_q);

# Specify choices presented to students
$NumB = PopUp( [ "?", "A", "B", "C", "D", "E", "F" ], "B" );
$NumC = PopUp( [ "?", "A", "B", "C", "D", "E", "F" ], "C" );
$NumF = PopUp( [ "?", "A", "B", "C", "D", "E", "F" ], "F" );
$NumE = PopUp( [ "?", "A", "B", "C", "D", "E", "F" ], "E" );
$NumD = PopUp( [ "?", "A", "B", "C", "D", "E", "F" ], "D" );
# Questions and answers

BEGIN_TEXT
Match each of the following logarithmic expressions to one of the following:
$BR
$BBOLD (A) $EBOLD \($num/10\)
$BR
$BBOLD (B) $EBOLD \($num2\)
$BR
$BBOLD (C) $EBOLD \($num3\)
$BR
$BBOLD (E) $EBOLD \($num4\)
$BR
$BBOLD (F) $EBOLD \($num\)

$BR
$BR
$BR

\($num\log(1000)=\)\{ $NumB->menu() \}
$BR
$BR
\(\frac{$num}{2}\log(10000)=\)\{ $NumC->menu() \}
$BR
$BR
\(\log\left(100^{$num/2}\right)=\)\{ $NumF->menu() \}
$BR
$BR
\( 100^{\log\left($num/2\right)}=\)\{ $NumE->menu() \}
$BR
$BR
\( 100^{\log($num)/2}=\)\{ $NumF->menu() \}
$BR



END_TEXT
ANS( $NumB->cmp() );
ANS( $NumC->cmp() );
ANS( $NumF->cmp() );
ANS( $NumE->cmp() );
ANS( $NumF->cmp() );

ENDDOCUMENT();
