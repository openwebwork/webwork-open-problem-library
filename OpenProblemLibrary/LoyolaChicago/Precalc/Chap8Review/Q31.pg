## KEYWORDS('decomposition','inverse','composition','combinations','function')

DOCUMENT();

loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGgraphmacros.pl",
           "PGauxiliaryFunctions.pl",
           "extraAnswerEvaluators.pl",
           "MathObjects.pl",
           "PGcourse.pl"
);

TEXT(beginproblem());

Context("Numeric");


$showPartialCorrectAnswers = 1;

$m = random(1,3,1);

for ($i = 0; $i <= 4; $i++){$num[$i] = $m*$i };

$u[0] = $num[2];
$u[1] = $num[3];
$u[2] = $num[1];
$u[3] = $num[4];
$u[4] = $num[0];


$v[0] = $num[3];
$v[1] = $num[4];
$v[2] = $num[1];
$v[3] = $num[2];
$v[4] = $num[0];


$w[0] = $num[1];
$w[1] = $num[2];
$w[2] = $num[4];
$w[3] = $num[0];
$w[4] = $num[3];


Context()->texStrings;
BEGIN_TEXT

Fill in the missing values in the tables below given that
\( w(t) = v \big( u(t) \big) \) .
$BR
$BCENTER
\{ begintable(4) \}
\{ row("\( t \)", "\(u(t)\)", "\(v(t)\)", "\(w(t)\)" ) \}
\{ row( "\( $num[0] \)" , " $u[0]", " $v[0]" , ans_rule(3) ) \}
\{ row("\( $num[1] \)", ans_rule(3) , ans_rule(3), " $w[1]" ) \}
\{ row(" \( $num[2] \)", " $u[2]" , " $v[2]", " $w[2]" ) \}
\{ row(" \( $num[3] \)", ans_rule(3), " $v[3]" , " $w[3]" ) \}
\{ row(" \( $num[4] \)", " $u[4]", " $v[4]", ans_rule(3) ) \}
\{ endtable() \}
$ECENTER

END_TEXT
Context()->normalStrings;

# ANS(num_cmp( $w[0] ) );
# ANS(num_cmp( $u[1] ) );
# ANS(num_cmp( $v[1] ) );
# ANS(num_cmp( $u[3] ) );
# ANS(num_cmp( $w[4] ) );

ANS( Compute($w[0])->cmp() );
ANS( Compute($u[1])->cmp() );
ANS( Compute($v[1])->cmp() );
ANS( Compute($u[3])->cmp() );
ANS( Compute($w[4])->cmp() );

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR
$BBOLD  SOLUTION $EBOLD
$PAR
$BCENTER
\{ begintable(4) \}
\{ row(" \( t \)", "\(u(t)\)", "\(v(t)\)", "\(w(t)\)" ) \}
\{ row( " \( $num[0] \)" , "  $u[0]", "  $v[0]" , "  $w[0]" ) \}
\{ row(" \( $num[1] \)", "  $u[1]" , "  $v[1]", "  $w[1]" ) \}
\{ row(" \( $num[2] \)", "  $u[2]" , "  $v[2]", "  $w[2]" ) \}
\{ row(" \( $num[3] \)", "  $u[3]", "  $v[3]" , "  $w[3]" ) \}
\{ row(" \( $num[4] \)", "  $u[4]", "  $v[4]", "  $w[4]" ) \}
\{ endtable() \}
$ECENTER

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
