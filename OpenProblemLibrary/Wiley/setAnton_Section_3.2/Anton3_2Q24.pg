## DBsubject(Calculus - single variable)
## DBchapter(Differentiation)
## DBsection(Derivatives of logarithmic functions)
## Institution(John Wiley & Sons)
## Author(Brian Burns)
## Level(3)
## MO(1)
## TitleText1('Calculus')
## AuthorText1('Anton')
## EditionText1('9')
## Section1('3.2')
## Problem1('24')
## KEYWORDS('derivative')


DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "littleneck.pl",
  "PGcourse.pl"
);
# allow the student to change the seed for this problem.

rand_button();

TEXT(beginproblem());

###################################
# Setup
     

$a=random(-9,10,1);
if($a==0){$a=-10}
$i=random(2,5,1);
$j=random(0,1,1);
if($j==0){$b=$i}
else{$b=-$i}
$A=$b*$a;
$B=$b-1;
$C=-$B;
$f=Formula("$a*(sin(ln(x)))**$b")->reduce;
$df=$f->D->reduce;
if($B>0){
$ans=Formula("($A*(sin(ln(x)))**$B*cos(ln(x)))/x")->reduce;
}
else{
$ans=Formula("($A*cos(ln(x)))/(x*(sin(ln(x)))**$C)")->reduce;
}
###################################
# Main text
Context()->texStrings;
BEGIN_TEXT
Find \(\frac{dy}{dx}\) when \[y=$f\]
 
$PAR
\(\frac{dy}{dx}=\) \{ans_rule(20) \}
END_TEXT
###################################
# Answers

$showPartialCorrectAnswers = 0;

ANS($ans->cmp(limits=>[.1,4]));

###################################
Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR
Differentiating gives  \[\frac{dy}{dx}=$df=$ans\]
END_SOLUTION
Context()->normalStrings;

###################################
ENDDOCUMENT();
