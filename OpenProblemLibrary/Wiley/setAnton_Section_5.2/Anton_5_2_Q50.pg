DOCUMENT();        # This should be the first executable line in the problem.
loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "littleneck.pl",
  "contextFraction.pl",
  "PGcourse.pl"
);
# allow the student to change the seed for this problem.


rand_button();


TEXT(beginproblem());

###################################
# Setup
# 
Context()->variables->add(t=>'Real',C=>'Real');

Context()->reduction->set('(-x)+y'=>0);
Context()->reduction->set('(-x)-y'=>0);
$a=non_zero_random(-8,8,1);
$na=-$a;
$f=Formula("$a*e^t")->reduce;
$F=$f;

$ans=Compute("$F-$a *e")->reduce;
###################################
# Main text
Context()->texStrings;
BEGIN_TEXT
A particle moves along an \(s\)-axis with position function \(s = s(t)\) and velocity function \(v(t) = s'(t)\). Use the given information
to find \(s(t)\).
$PAR
\[v(t)=$a e^t,\; s(1)=0\]
$PAR
$PAR\(s(t)=\) \{ans_rule(40) \} 
END_TEXT

###################################
# Answers

#$showPartialCorrectAnswers = 0;

ANS($ans->cmp);

###################################
Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

$PAR
\[s(t)=\displaystyle \int \left [$f \right] dt= $F+C\]

$BR
\[0=$a e^1+C\]\[C=$na e\]
$BR
Hence, \[y=$F+$na e\]

END_SOLUTION
Context()->normalStrings;

###################################
ENDDOCUMENT();
