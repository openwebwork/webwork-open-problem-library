## DESCRIPTION
## Calculus
## ENDDESCRIPTION

## DBsubject(Calculus - single variable)
## DBchapter(Integrals)
## DBsection(Indefinite integrals (with trigonometric functions))
## Institution(Little Neck West)
## Author(Brian Burns)
## Level(2)
## MO(1)
## TitleText1('Calculus')
## AuthorText1('Anton')
## EditionText1('9')
## Section1('5.2')
## Problem1('30')
## KEYWORDS('derivative')


DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "littleneck.pl",
  "contextFraction.pl",
  "PGcourse.pl"
);
# allow the student to change the seed for this problem.

rand_button();


TEXT(beginproblem());

###################################
# Setup
# 
Context()->variables->add(s=>'Real');
$a=random(1,12,1);$ad=$a;
$b=random(1,8,1)*random(-1,1,2);$bd=$b;
if($a==1){$ad=""}
if($b==1){$bd=""}
if($b==-1){$bd="-"}
$f1=Formula("s")->reduce;
$f2=Formula("$b/sin(s)**2")->reduce;
$f2b=Formula("csc(s)**2")->reduce;
$f=Formula("$a*$f1+$f2")->reduce;
Context("Fraction");
$A=Compute("$a/2");
$Ad=$A;
if($A==1){$Ad=""}

Context("Numeric");
Context()->variables->add(s=>'Real');
$ans=Compute("$A*s**2-$b*cot(s)");
$F=Formula("s**2-$b*cot(s)")->reduce;
###################################
# Main text
Context()->texStrings;
BEGIN_TEXT
Evaluate the integral and check your answer by differentiating.
$PAR
\( \displaystyle \int\left [$f\right] ds \) \(=\) \{ans_rule(40) \} \(+\) \(C\)
END_TEXT

###################################
# Answers

#$showPartialCorrectAnswers = 0;

ANS($ans->cmp(upToConstant=>1));

###################################
Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

$PAR
\[\displaystyle \int \left [$f \right] ds= $ad\int  $f1  \;ds+$bd\int  $f2b  \;ds= $Ad$F+C\]

$BR
\[\frac{d}{ds}\left[$Ad$F+C\right]=$f\]
END_SOLUTION
Context()->normalStrings;

###################################
ENDDOCUMENT();
