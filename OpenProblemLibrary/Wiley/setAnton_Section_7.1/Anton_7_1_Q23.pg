DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "littleneck.pl",
  "contextFraction.pl",
  "PGcourse.pl"
);
# allow the student to change the seed for this problem.

rand_button();


TEXT(beginproblem());

###################################
# Setup
Context()->variables->add(u=>'Real',t=>'Real');
$a=random(1,8,1);$as=$a**2;
$u=Formula("e^(-x)")->reduce;
$fu=Formula("1/($as -u^2)")->reduce;
$g=Formula("e^(-x)");
$f=$fu->substitute(u=>$u);
$df=Formula("$g/($as-e^(-2*x))")->reduce;
$Fu=Formula("ln|(u-$a)/(u+$a)|")->reduce;
$F=$Fu->substitute(u=>$u);
Context("Fraction");
$c=Compute("1/(2*$a)");
Context("Numeric");

$ans=Compute("$c*$F");
if($c==1){$c=""}
if($c==-1){$c="-"}
###################################
# Main text
Context()->texStrings;
BEGIN_TEXT
Evaluate the integral by any method.
$PAR
\( \displaystyle \int  $df \;dx\) \(=\) \{ans_rule(20) \} \(+C\)
END_TEXT

###################################
# Answers

#$showPartialCorrectAnswers = 0;

ANS($ans->cmp);

###################################
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR
For \(u=$u\) we have \( - du = $g dx\) hence using partial fractions 
$PAR
\[ \displaystyle \int $df \;dx = - \int $fu \;du=$c$Fu+C=$c $F+C\]
$BR
END_SOLUTION
Context()->normalStrings;

###################################
ENDDOCUMENT();
