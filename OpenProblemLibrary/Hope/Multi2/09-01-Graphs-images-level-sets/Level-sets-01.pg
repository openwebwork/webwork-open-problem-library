## Section1('9.1')
## Problem1('')


##############################
#  Initialization

DOCUMENT(); 

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
);

TEXT(beginproblem());


#############################
#  Setup

Context("Numeric");

$a = random(2,5,1);
do { $b = random(2,5,1); } until ($b != $a);
$c = random(6,9,1);
$cma = $c - $a;

$f = Formula("$b*x^2+$a");

$answer = List(Compute("sqrt($cma/$b)"),Compute("-sqrt($cma/$b)"));

$k = Compute("1");


######################
#  Main text

Context()->texStrings;
BEGIN_TEXT
Let \( f : \mathbb{R} \to \mathbb{R} \) be defined by
\( f(x) = $f \) and set \( y = f(x) \).
$BR
$BR
(a) The level sets of \( f \) are subsets of \( \mathbb{R}^k \) for 
\( k = \) \{ ans_rule(5) \}
$BR
$BR
(b) Find the level set of \( f \) 
at level \( y = $c \).  If there is more than one answer, 
enter your answers as a comma separated list.
$BR
$BR
\{ ans_rule(30) \}
END_TEXT
Context()->normalStrings;


########################
#  Answer evaluation

$showPartialCorrectAnswers = 1;

ANS( $k->cmp(), $answer->cmp() );


COMMENT("MathObject version");

ENDDOCUMENT();
