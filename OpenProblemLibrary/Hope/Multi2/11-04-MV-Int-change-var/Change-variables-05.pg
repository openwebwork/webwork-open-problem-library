###########################
#  Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"parserMultiAnswer.pl",
"PGmatrixmacros.pl",
"PGcourse.pl",
"PGunion.pl",
);

TEXT(beginproblem());

SRAND($psvn);


###########################
#  Setup

Context("Numeric")->variables->are(u=>'Real',v=>'Real');

$a = random(2,4,1);
$b = random(5,7,1);

$aa = $a * $a;
$bb = $b * $b;

$x = Compute("u^2 / v");
$y = Compute("v / u");

$ulo = Real(1);
$uhi = Real($aa);
$vlo = Real(1);
$vhi = Real($bb);


$multians0 = MultiAnswer( $x, $y )->with(

  singleResult => 1,
  allowBlankAnswers => 1,
  checkTypes => 0,
  format =>  "<table border='0' cellspacing='10'>
  <tr><td> %s </td></tr>
  <tr><td> %s </td></tr>  </table>",
  tex_format =>  "\begin{array}{r} %s \\ %s \end{array}",

  checker => sub {
      my ( $correct, $student, $answerHash ) = @_;
      my @c = @{$correct};
      my @s = @{$student};
      my @score = ();
      foreach my $j (0..1) {
        $score[$j] = 0;
        if ($c[$j]->typeMatch($s[$j]) && $c[$j] == $s[$j]) { $score[$j] = 1; }
      }
      return [ @score ];
  }

);


$multians1 = MultiAnswer( Compute("2 u / v"), Compute("-u^2 / v^2"), Compute("-v / u^2"), Compute("1 / u"), Compute("1 / v") )->with(

  singleResult => 1,
  allowBlankAnswers => 1,
  checkTypes => 0,
  format =>  "<table border='0' cellspacing='10'>
  <tr><td> %s </td><td> %s </td></tr>
  <tr><td> %s </td><td> %s </td></tr>
  </table> %s",
  tex_format =>  "\left\lbrack\begin{array}{rr} %s & %s \\ %s & %s \end{array}\right\rbrack; %s",

  checker => sub {
      my ( $correct, $student, $answerHash ) = @_;
      my @c = @{$correct};
      my @s = @{$student};
      my @score = ();
      foreach my $j (0..4) {
        $score[$j] = 0;
        if ($c[$j]->typeMatch($s[$j]) && $c[$j] == $s[$j]) { $score[$j] = 1; }
      }
      return [ @score ];
  }

);


$area = Compute("($aa - 1) * ln($bb)");







