DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGstandard.pl",
"MathObjects.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
#"parserRadioButtons.pl",
"algebraMacros.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

Context()->strings->add(a=>{});
Context()->strings->add(b=>{});


$n = random(6,12,1);
$i = random(1,$n-1,1);
$j = random(0,1,1);
$k = random(1,$n-1,1);
$g = gcd($i,$n);
$nn = $n/$g;



if($j == 0){
    @inter1 = ();
    for($c=0;$c<$nn;$c++){
        $e = ($k + $g*$c) % $n;
        @inter1[$c] = "($e,0)";
    }
    $ans1 = join(", ",@inter1);
    $ans1 = Compute("$ans1");
}


elsif($j == 1){
    $e1 = "($i,$j)";
    $fill = ($k+$i) % $n;
    $e2 = "($fill,$j)";
    @inter1 = ($e1,$e2);
    $ans1 = join(", ",@inter1);
    $ans1 = Compute("$ans1");
}

BEGIN_TEXT

Find the elements of the left coset \(a^{$k} \langle a^{$i} b^{$j} \rangle\) in \( \{ dihedral( $n ) \} \). $BR
Enter your answer as a comma separated list; make sure each coset representation is of the form \((e_{1}, e_{2})\) where \(e_{1}\) represents the exponent for \(a\) and \(e_{2}\) represents the exponent for \(b\). $BR $BR

\{ans_rule(50)\}



END_TEXT

ANS( $ans1->cmp(unordered=>1) );



ENDDOCUMENT();    