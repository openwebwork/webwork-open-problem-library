DOCUMENT();        

loadMacros(
"PG.pl",
"PGstandard.pl",
"MathObjects.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"algebraMacros.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

@set = (3,4,5,7,11,13,17);

$r1 = random(0,6,1);
$r2 = random(0,6,1);
while($r1 == $r2){
    $r2 = random(0,6,1);
}

$k = $set[$r1];
$a = $set[$r2];
$n = $k*$a;

@ans1 = ();
$count1 = 0;

for($i=2;$i<$n;$i++){
    if($i%$a == 0){
        @ans1[$count1] = $i;
        $count1++;
    }
}

$answer1 = join(", ",@ans1);
$answer1 = Compute("$answer1");

@ans2 = ();
$count2 = 0;

for($i=0;$i<$n;$i++){
    if($i%$k == 0){
        @ans2[$count2] = $i;
        $count2++;
    }
}

$answer2 = join(", ",@ans2);
$answer2 = Compute("$answer2");

BEGIN_TEXT

(a) Find all elements in \( \{ cyclic( $n ) \} \) of order \($k\). $BR $BR

\{ ans_rule(30) \} $BR $BR

(b) Find all elements in the subgroup \(\langle $k \rangle\) of \( \{ cyclic( $n ) \} \). $BR $BR

\{ ans_rule(30) \}


END_TEXT

$modulus = $n;
ANS( $answer1->cmp( checker => ~~&modChecker ) );
ANS( $answer2->cmp( checker => ~~&modChecker ) );

ENDDOCUMENT();       