DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGgraphmacros.pl",
  "PGmatrixmacros.pl",
  "PGmorematrixmacros.pl",
  "PGnumericalmacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

$a11 = random(-3,-2,1);
$a21 = 1;
$a12 = random(-7,-5,1);
$a22 = 9;

$b11 = random(3,7,1);
$b21 = 1;
$b12 = random(2,4,2);
$b22 = random(-5,-1,2);
$b13 = 0;
$b23 = random(1,4,1);

$c11 = 1;
$c21 = 0;
$c31 = 0;
$c12 = 0;
$c22 = 1;
$c32 = 0;
$c13 = random(-4,-2,1);
$c23 = 0;
$c33 = 0;
$c14 = random(2,5,1);
$c24 = random(1,3,1);
$c34 = 1;
$c15 = random(-1,1,1);
$c25 = 0;
$c35 = random(2,4,1);

$mA =new Matrix(2,2);
$mA->assign(1,1,$a11);
$mA->assign(1,2,$a12);
$mA->assign(2,1,$a21);
$mA->assign(2,2,$a22);

$mB = new Matrix(2,3);
$mB->assign(1,1,$b11);
$mB->assign(1,2,$b12);
$mB->assign(1,3,$b13);
$mB->assign(2,1,$b21);
$mB->assign(2,2,$b22);
$mB->assign(2,3,$b23);

$mC = new Matrix(3,5);
$mC->assign(1,1,$c11);
$mC->assign(1,2,$c12);
$mC->assign(1,3,$c13);
$mC->assign(1,4,$c14);
$mC->assign(1,5,$c15);
$mC->assign(2,1,$c21);
$mC->assign(2,2,$c22);
$mC->assign(2,3,$c23);
$mC->assign(2,4,$c24);
$mC->assign(2,5,$c25);
$mC->assign(3,1,$c31);
$mC->assign(3,2,$c32);
$mC->assign(3,3,$c33);
$mC->assign(3,4,$c34);
$mC->assign(3,5,$c35);

$ans1 = 0;
$ans2 = 1;
$ans3 = 2;

BEGIN_TEXT
Find a basis for the null space of each matrix and give the nullity of the set.
$BR
\{ mbox('A = ', display_matrix($mA) ) \}

\{ mbox('B = ',display_matrix($mB) ) \}

\{ mbox('C = ',display_matrix($mC) ) \}
$BR
The nullity of A is \{ans_rule(10) \} .
$BR$BR
The nullity of B is \{ans_rule(10) \} .
$BR$BR
The nullity of C is \{ans_rule(10) \} .


END_TEXT

ANS(num_cmp($ans1));
ANS(num_cmp($ans2));
ANS(num_cmp($ans3));

ENDDOCUMENT()
