DOCUMENT();

loadMacros(
"PGstandard.pl",
"PGchoicemacros.pl",

#"freemanMacros.pl,
#"PGgraders.pl",
);

TEXT(beginproblem());
# Create and use pop up lists 
$tf = new_select_list();
$tf->rf_print_q(~~&pop_up_list_print_q);

# Specify choices presented to students

$tf->ra_pop_up_list( 
[ No_answer => "?", "T"=>"True", "F"=>"False"] ); 

$a = random(2,3,1);
$aa = $a+1;
$b = random(6,9,1);
$c = random(7,10,1);

# Questions and answers

$tf -> qa ( 
"If  \( \  T:R^{$aa} \rightarrow R^{$c} \) is a linear transformation,  then range (\(T\)) is a subspace of \(R^{$c}\).","T");

# How many questions to use
$tf->choose(1);

BEGIN_TEXT
Indicate whether the statement is true or false.
$BR
\{ $tf-> print_q \}
END_TEXT

install_problem_grader(~~&std_problem_grader);
$showPartialCorrectAnswers = 0;

ANS(str_cmp($tf->ra_correct_ans));

SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR
TRUE, from theorem 4.5.
END_SOLUTION

ENDDOCUMENT();