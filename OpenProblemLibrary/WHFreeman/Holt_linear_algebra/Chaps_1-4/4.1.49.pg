DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "parserPopUp.pl",
  "MathObjects.pl",
  "freemanMacros.pl",
  "PGcourse.pl"
);

# make sure we're in the context we want
Context("Numeric");

$d2=random(2,4);
$d1=$d2+random(1,5);

# the arguments of PopUp are [list of answers], 
#    correct answer
  $popup = new_select_list();
  $popup->rf_print_q(~~&pop_up_list_print_q);
  $popup->ra_pop_up_list( [ No_answer => "",
    True => "True", False => "False" ] );

  $popup->qa("This set contains the zero vector and is closed under vector 
    addition and scalar multiplication.", "True",
    "This set is a subset of the codomain.", "False",
    "This set is a subset of the domain.", "True",
     "This set is a subspace of \(\mathbb{R}^$d2\).", "False",);
  $popup->choose(4);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;
Context()->texStrings;
BEGIN_TEXT
\{ textbook_ref_exact("Holt Linear Algebra", "4.1","49") \} 
$PAR
If \(T:{\mathbb R}^$d1\to {\mathbb R}^$d2\) is a linear transformation, 
then select true or false for each statement about
the set \(\ker( T)\).

$PAR
\{ $popup->print_q() \}

END_TEXT
Context()->normalStrings;

ANS
(str_cmp( $popup->ra_correct_ans() )) ;
#(str_cmp( $popup4->ra_correct_ans() )) ;


Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR
The null set is a subset of the domain, \({\mathbb R}^$d1\), not the codomain
\({\mathbb R}^$d2\).

END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
