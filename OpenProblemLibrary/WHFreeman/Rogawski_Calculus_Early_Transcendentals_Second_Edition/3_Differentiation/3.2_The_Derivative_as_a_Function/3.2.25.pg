loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",

  "PGcourse.pl"
);

TEXT(beginproblem());


Context("Numeric");

$a=random(2,14,1);
$f = Compute("$a x^{-3}+x^2+14")->reduce;
$derivf=$f->D;

Context()->texStrings;

BEGIN_TEXT
Find the derivative of the function \( f(x)= $f\).

$BR$BR

\( f'(x)= \) \{ ans_rule() \}

END_TEXT

ANS($derivf->cmp);

SOLUTION(EV3(<<'END_SOLUTION'));
$SOL $PAR
\( \frac {d}{dx} $f = $derivf\).
END_SOLUTION
