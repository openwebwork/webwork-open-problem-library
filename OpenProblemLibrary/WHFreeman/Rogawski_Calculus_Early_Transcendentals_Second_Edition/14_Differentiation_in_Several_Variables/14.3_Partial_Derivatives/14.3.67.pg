## DBsubject('Calculus')
## DBchapter('Differentiation in Several Variables')
## DBsection('Partial Derivatives')
## KEYWORDS('calculus')
## TitleText1('Calculus: Early Transcendentals')
## EditionText1('2')
## AuthorText1('Rogawski')
## Section1('14.3')
## Problem1('67')
## Author('JustAsk - Vladimir Finkelshtein')
## Institution('W.H.Freeman')

DOCUMENT();

loadMacros("PG.pl","PGbasicmacros.pl","PGanswermacros.pl");
loadMacros("Parser.pl");
loadMacros("freemanMacros.pl");
loadMacros("PGauxiliaryFunctions.pl");
loadMacros("PGgraphmacros.pl");
loadMacros("PGchoicemacros.pl");

TEXT(beginproblem());

Context()->texStrings;

$a=non_zero_random(-9,9,1);
$b=non_zero_random(-9,9,1);
$spow=random(2,6,1);
$tpow=random(2,6,1);

$context = Context();
$context->variables->add(r=>'Real');
$context->variables->add(s=>'Real');
$context->variables->add(t=>'Real');

$f=Formula("r($a*s^($spow)+$b*t^($tpow))")->reduce();
$fr=Formula("$a*s^($spow)+$b*t^($tpow)")->reduce();
$frs=Formula("$a*$spow*s^($spow-1)")->reduce();
$frst=Formula("0");

BEGIN_TEXT
\{ textbook_ref_exact("Rogawski ET 2e", "14.3","67") \}
$PAR
Given \(F(r,s,t)=$f\), compute:
$PAR
\(F_{rst}=\)\{ans_rule()\}
$BR
END_TEXT 

Context()->normalStrings;

ANS($frst->cmp);

Context()->texStrings;

SOLUTION(EV3(<<'END_SOLUTION'));
$PAR
$SOL
$BR
For \(F(r,s,t)=$f\) we have
$PAR
\(F_{r}=$fr\) 
$BR
\(F_{rs}=$frs\) 
$BR
\(F_{rst}=$frst\) 

$BR
END_SOLUTION

ENDDOCUMENT();