DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a=random(11,339,1);
$b=random(1,5,1);
$c=$a-$b*360;

BEGIN_TEXT
The angle between 0 degree and 360 degrees 
that is coterminal with the $c degree angle is \{ans_rule(15) \} degrees.
$BR
END_TEXT

$ans1=$a;

ANS(num_cmp($ans1));

