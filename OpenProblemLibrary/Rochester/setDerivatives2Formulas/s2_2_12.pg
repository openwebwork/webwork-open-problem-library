DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(2,7,1);
$b1 = random(2,8,1);
$c1 = random(2,7,1);
$d1 = random(2,6,1);
$n1 = random(3,7,1);
$x1 = random(1,5,1);

$deriv1 = (-2*$x1*($b1+$x1**2) - ($a1-$x1**2)*2*$x1)/(($b1+$x1**2)**2);
$funct1 = "(-2*x*($b1+x**2) - ($a1-x**2)*2*x)/(($b1+x**2)**2)";

TEXT(EV2(<<EOT));
If 
\[ f(x) =  \frac { $a1 - x^2 } { $b1 + x^2 } \]
find \( f'( x ) \).
$BR $BR \{ans_rule(48) \}
$BR
EOT

$ans = $funct1;
ANS(fun_cmp($ans));

TEXT(EV2(<<EOT));
Find \( f'( $x1 ) \).
$BR $BR \{ans_rule(48) \}
$BR $BR

EOT

$ans = $deriv1;
ANS(num_cmp($ans));

ENDDOCUMENT();        # This should be the last executable line in the problem.
