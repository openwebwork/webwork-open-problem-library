DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$tag = random(0,3,1);
@choose = (8,27,64,125);
$a = $choose[$tag];

TEXT(EV2(<<EOT));
Evaluate the expression \($a^{-4/3} \).
$BR $BR \{ans_rule(10) \}
$BR
[NOTE:  Your answer cannot be an algebraic expression. ]
$BR
EOT

$ans = $a**(-4/3);
ANS(num_cmp($ans, mode=>"strict"));

