DOCUMENT();		# This should be the first executable line in the problem.

loadMacros("PG.pl",      
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",      
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl"     
          );      

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(1, 9, 1);

BEGIN_TEXT
Find the volume of the solid bounded by the planes 
x = 0, y = 0, z = 0, and x + y + z = $a.
$PAR
\{ ans_rule(20) \} 
END_TEXT

ANS(num_cmp( $a**3/6 ) );

