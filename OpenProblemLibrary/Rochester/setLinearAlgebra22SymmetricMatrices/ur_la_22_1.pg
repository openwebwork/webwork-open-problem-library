DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGchoicemacros.pl",
"PGcourse.pl",
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

Context('Matrix');

@a = NchooseK(6,6);
$i1 = random(1,2,1);
$j1 = 3 - $i1;
$i2 = random(1,3,2);
$j2 = 4 - $i2;
$i3 = random(2,3,1);
$j3 = 5 - $i3;

$A = Matrix([
[ $a[0], $a[3], $a[4] ],
[ $a[3], $a[1], $a[5] ],
[ $a[4], $a[5], $a[2] ],
]);

Context()->texStrings;
BEGIN_TEXT
Enter a \( 3 \times 3 \) symmetric matrix \(A\) that has entries 
\(a_{11}=$a[0]\), \(a_{22}=$a[1]\), \(a_{33}=$a[2]\), \(a_{$i1 $j1}=$a[3]\), \(a_{$i2 $j2}=$a[4]\), and \(a_{$i3 $j3}=$a[5]\). 
$BR
$BR
\( A = \) \{ $A->ans_array \}
END_TEXT
Context()->normalStrings;

ANS( $A->cmp );
;
