## KEYWORDS('Algebra', 'Exponential', 'Logarithmic', 'Applications','logarithms')

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$tag1 = random(0,2,1);
@choose1 = (2,4,5,8,10);
$a1 = $choose1[$tag1];
$b1 = random(2,4,1);
$b2 = 1/$a1**$b1;
$tag2 = random(3,4,1);
$c1 = $choose1[$tag2];
$d1 = random(2,4,1);
$d2 = 1/($c1**$d1);
$f1 = 4;
$g1 = random(1,2,1);
$g2 = 2*4**$g1;
$h1 = random(4,5,1);
$i1 = random(3,5,1);
$j1 = $h1**$i1;

BEGIN_TEXT;
Evaluate the following expressions.$BR
\( \log_ { $a1 } $b2  \) \{ans_rule(10) \}
END_TEXT

$ans = -$b1;
ANS(num_cmp($ans));

BEGIN_TEXT;
$BR
\( \log_ { $c1 } $d2  \) \{ans_rule(10) \}
END_TEXT

$ans = -$d1;
ANS(num_cmp($ans));

BEGIN_TEXT;
$BR
\( \log_  { $j1 } \sqrt { $h1 } \) \{ans_rule(10) \}
END_TEXT

$ans = 1/(2*$i1);
ANS(num_cmp($ans));

BEGIN_TEXT;
$BR
\( \log_ { $f1 } $g2  \) \{ans_rule(10) \}
END_TEXT
$ans = $g1 + .5;
ANS(num_cmp($ans));
ENDDOCUMENT();        # This should be the last executable line in the problem.

