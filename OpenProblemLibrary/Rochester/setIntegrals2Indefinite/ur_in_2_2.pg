TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a=random(2,9,1); 

BEGIN_TEXT
Evaluate the indefinite integral
\[ \int  $a e^x \,  dx \]
$BR 
\(\int  $a e^x\,  dx = \) \{ans_rule(30)\} \(+C\)
END_TEXT

$answer="$a* e^x";

ANS(fun_cmp($answer, mode=>"antider"));

