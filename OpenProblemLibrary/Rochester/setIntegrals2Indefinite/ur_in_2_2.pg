TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a=random(2,9,1); 

BEGIN_TEXT
Evaluate the indefinite integral
\[ \int  $a e^x  dx \]
$BR \{ans_rule(30)\}
END_TEXT

$answer="$a* $E**x";

ANS(fun_cmp($answer, mode=>"antider"));

