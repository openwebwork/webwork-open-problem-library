DOCUMENT() ;       

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGdiffeqmacros.pl",
  "PGcourse.pl"
);
############
$pp  = random(2,9,1) ;
$qq  = random(2,9,1) ;
$ee  = random(1,5,1) ;
$ff  = $ee + ($ee *$ee)  ;
$a = $pp **2 ;
$b = 2*$pp * $qq  ;
$c = $qq *$qq + $ff ;
$m = random(2,9,1) ;
$n = random(2,9,1) ;
#############


$L = diffop($a,$b,$c,$m,$n) ;
$ans = ivy($a,$b,$c,$m,$n) ;

TEXT(beginproblem()) ;

$showPartialCorrectAnswers = 1 ;

BEGIN_TEXT

Find \(y\) as a function of \(t\) if $BR
\[ $L =  0,\] 
\(y(0) = $m,  \quad  y'(0) = $n .\) $BR
\(y = \) \{ans_rule(80)\}
$BR
Note: This problem cannot interpret complex numbers.  You may need to simplify your answer before submitting it.

END_TEXT

ANS(fun_cmp($ans , var=>"t" )) ;

ENDDOCUMENT() ;



      







