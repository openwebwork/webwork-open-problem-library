DOCUMENT() ;       

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGdiffeqmacros.pl",
  "PGcourse.pl"
);
############
$t  = random(1,3,1) ;
$v = random(0,2,1) ;
$aa = ((2**$t)*(3**$v)) ;
$a = $aa **2  ;
$pp = random( -17,19,6) ;
$qq = $pp  ;
$c = $pp * $qq ;
$b =  $aa *($pp + $qq) ;
$p = 0 ;
$m = random(2,9,1) ;
$n = random(2,9,1) ;
#############

$L = diffop($a,$b,$c,$m,$n) ;
$ans = ivy($a,$b,$c,$m,$n) ;

TEXT(beginproblem()) ;

$showPartialCorrectAnswers = 1 ;

BEGIN_TEXT

Find \(y\) as a function of \(t\) if $BR
\[ $L =  0, \]
\(y(0) = $m,  \quad  y'(0) = $n .\) $BR
\(y = \) \{ans_rule(80)\}

END_TEXT

ANS(fun_cmp($ans , var=>'t' )) ;

ENDDOCUMENT() ;

      

