## DBchapter(Higher order differential equations)
## DBsection(Linear, constant coefficients, homogeneous (complex roots))
## Institution(Rochester)
## Level(3)
## KEYWORDS('differential equation' 'second order' 'linear')

DOCUMENT() ;        

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGdiffeqmacros.pl"
) ;

$aa = random(2,11,1) ;
$a = $aa **2 ;
$B = random(-10,10,1) ;
$e = random(1,9,1) ;
$eps = random(-1,1,1) ;
$b = 2*$B*$aa ;
$c = $B *$B - $eps * ($e **2) ;	
$m = random(1,9,1) ;
$n = random(1,9,1) ;

##############

$k = random(1,5,1) ;
$L = diffop($a,$b,$c,$m,$n) ;
$y = ivy($a,$b,$c,$m,$n) ;
$tk = "(t - $k )" ;
$y =~s~~t~~$tk~~g ;
$ans = $y ;
TEXT(beginproblem()) ;

$showPartialCorrectAnswers = 1 ;

BEGIN_TEXT

Find \(y\) as a function of \(t\) if $BR
\[ $L =  0, \] 
\(y($k) = $m,  \quad  y'($k) = $n .\) $BR
\(y = \) \{ans_rule(80)\}

END_TEXT

ANS(fun_cmp($ans, vars=>"t")) ;

ENDDOCUMENT() ; 
