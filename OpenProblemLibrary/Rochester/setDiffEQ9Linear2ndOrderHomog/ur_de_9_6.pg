DOCUMENT() ;       

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGdiffeqmacros.pl",
  "PGcourse.pl"
);
############
$aexp = random(0,3,1) ;
$aexp5 = random(0,2,1) ;
$b = 0 ;
$a = (4**$aexp)*(25**$aexp5) ;          
$m = random(1,9,1) ;
$n = random(1,9,1) ;
$cc = random(1,3,1) ;
$c = -(9**$cc) ;
#############
$L = diffop ($a, $b ,$c) ;
$ans = ivy ($a,$b,$c,$m,$n) ;
#$ans ="2" ;
TEXT(beginproblem()) ;
$showPartialCorrectAnswers = 1 ;

BEGIN_TEXT

Find \(y\) as a function of \(t\) if 
\[ $L =  0\] with \(  \quad  y(0) = $m,  \quad  y'(0) = $n .\) $BR
\(y = \) \{ans_rule(80)\}

END_TEXT
       
ANS(fun_cmp($ans, vars=>"t")) ;


ENDDOCUMENT() ;




