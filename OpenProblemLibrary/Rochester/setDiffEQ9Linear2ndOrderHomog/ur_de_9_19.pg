DOCUMENT() ;       

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGdiffeqmacros.pl",
) ;
############

$r = random(2,9,1);
$sign = random(-1,1,2);
$r = $r * $sign;

$a = 1 - 2 * $r;
$b = $r**2;

$k1 = random(-10,10,1);
$d = random(-5,5,1);
if ( $d - $r * $k1 == 0 ) { $d = $d+1; }

$k2 = $d - $r * $k1;

$eq = FEQ("x^2 y'' + $a x y' + $b y = 0");
$ans = FEQ("x**$r * ($k1 + $k2 * ln x)");

TEXT(beginproblem()) ;

$showPartialCorrectAnswers = 1 ;

BEGIN_TEXT

Find \(y\) as a function of \(x\) if \[ $eq, \]
$BR
\( y(1) = $k1, \ \ y'(1) = $d. \) 
$BR
\(y=\)\{ans_rule(20)\}

END_TEXT

ANS(fun_cmp($ans));

ENDDOCUMENT() ;       






