DOCUMENT() ;        

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGdiffeqmacros.pl",
  "PGcourse.pl"
);
###
$aa = random(2,11,1) ;
$a = $aa **2 ;
$B = random(-10,10,1) ;
$e = random(1,9,1) ;
$eps = random(-1,1,1) ;
$b = 2*$B*$aa ;
$c = $B *$B - $eps * ($e **2) ;	
$m = random(1,9,1) ;
$n = random(1,9,1) ;

####################
       

$L = diffop($a,$b,$c,$m,$n) ;
$ans = ivy($a,$b,$c,$m,$n) ;

TEXT(beginproblem()) ;

$showPartialCorrectAnswers = 1 ;

BEGIN_TEXT

Find \(y\) as a function of \(t\) if $BR
\[ $L =  0, \] 
\(y(0) = $m,  \quad  y'(0) = $n .\) $BR
\(y = \) \{ans_rule(170)\}
$BR
Note: This problem cannot interpret complex numbers.  You may need to simplify your answer before submitting it.

END_TEXT

ANS(fun_cmp($ans, vars=>"t")) ;

ENDDOCUMENT() ; 






