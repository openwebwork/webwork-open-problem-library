DOCUMENT() ;        

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "parserVectorUtils.pl",
  "PGcourse.pl"
);

$showPartialCorrectAnswers = 1 ;

TEXT(beginproblem());

Context('Matrix');

$L = Matrix([
[1,0,0,0],
[non_zero_random(-5,5,1),1,0,0],
[non_zero_random(-5,5,1),non_zero_random(-5,5,1),1,0],
[non_zero_random(-5,5,1),non_zero_random(-5,5,1),non_zero_random(-5,5,1),1],
]);

$U = Matrix([
[non_zero_random(-5,5,1),non_zero_random(-5,5,1),non_zero_random(-5,5,1),non_zero_random(-5,5,1)],
[0,non_zero_random(-5,5,1),non_zero_random(-5,5,1),non_zero_random(-5,5,1)],
[0,0,non_zero_random(-5,5,1),non_zero_random(-5,5,1)],
[0,0,0,non_zero_random(-5,5,1)],
]);

$A = $L * $U;

Context()->texStrings;
BEGIN_TEXT
Find the \(LU\) factorization of \[ A= $A. \]  That is, write \(A=LU\) where 
\(L\) is a lower triangular matrix with ones on the diagonal, 
and \(U\) is an upper triangular matrix. 
$BR
$BR
$BCENTER
\(A=\) 
\{ $L->ans_array() \}
\{ $U->ans_array() \}
$ECENTER
END_TEXT
Context()->normalStrings;


ANS( $L->cmp() );
ANS( $U->cmp() );

;
ENDDOCUMENT();
