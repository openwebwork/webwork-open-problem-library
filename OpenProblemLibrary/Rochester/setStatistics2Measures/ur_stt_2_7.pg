DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGgraphmacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$d = random(1,9,1);

$a[0] = random(10,15,1);
$a[1] = random(16,25,1);
$a[2] = $a[1];
$c1 = $a[2]+1;
$c2 = $a[2]+3;
$a[3] = random($c1,$c2,1);
$d1 = $a[3]+1;
$d2 = $a[3]+3;
$a[4] = random($d1,$d2,1);
$e1 = $a[4]+1;
$e2 = $a[4]+3;
$a[5] = random($e1,$e2,1);
$f1 = $a[5]+3;
$f2 = $a[5]+6;
$a[6] = random($f1,$f2,1);
$g1 = $a[6]+3;
$g2 = $a[6]+6;
$a[7] = random($g1,$g2,1);
$h1 = $a[7]+3;
$h2 = $a[7]+6;
$a[8] = random($h1,$h2,1);
$n1 = $a[8]+5;
$n2 = $a[8]+8;
$a[9] = random($n1,$n2,1);
$m1 = $a[9]+5;
$m2 = $a[9]+8;
$a[10] = random($m1,$m2,1);
$p1 = $a[10]+5;
$p2 = $a[10]+8;
$a[11] = random($p1,$p2,1);

$N = ($d*10)/100*12;
$M = $N-1;
if ($N == int($N)) {
	$ans = (($a[$N]+$a[$M])/2);
} else  {$ans = $a[int($N)];
}



@set = ($a[0], $a[1], $a[2], $a[3], $a[4], $a[5], $a[6], $a[7], $a[8], $a[9], $a[10], $a[11]);
@slice = NchooseK(12,12);
@sset = @set[@slice];



BEGIN_TEXT

Find the indicated decile of the following data set

\[ $sset[0], \ $sset[1], \ $sset[2], \ $sset[3], \ $sset[4], \ $sset[5], \ $sset[6], \ $sset[7], \ $sset[8], \ $sset[9],
\ $sset[10], \ $sset[11] \]$PAR


\(D_{$d} = \) \{ans_rule(10)\}


END_TEXT
ANS(num_cmp($ans, mode=>"arith"));

ENDDOCUMENT();       # This should be the last executable line in the problem.
