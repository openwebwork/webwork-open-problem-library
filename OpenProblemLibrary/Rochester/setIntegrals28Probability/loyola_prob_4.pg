DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$k=random(1,10,1);
$kd = ( $k == 1 ) ? '' : $k;
$m=random(2,10,2);
$pi = arccos(-1);

BEGIN_TEXT
The density function for the number of times the riders scream on a roller coaster is given by
\[ f(x) = \begin{cases}
 \frac{1}{$kd \pi}( 1 - \cos($m x)) & \text{if}  \;\; 0\leq x \leq $kd \pi\\
 0 & \text{otherwise.}
\end{cases}
\]
Find the mean number of screams over the course of the ride.

$PAR

 \{ans_rule(30)\}
$PAR
END_TEXT

$ans1=$k*$pi/2 ;

ANS(num_cmp($ans1));

