);

$showPartialCorrectAnswers = 1;

$aa = random(3,8,1);

TEXT(beginproblem());
BEGIN_TEXT
Let
\[y=x^{\log_$aa(x)}\]
Then $BR
\( \displaystyle \frac{dy}{dx}\) =  \{ ans_rule(50)\}
$BR
$BBOLD Note:$EBOLD You must express your answer in terms of ln, log, or logten,
as WeBWorK doesn't
understand how to evaluate logarithms to other bases.
END_TEXT

##set $PG_environment{'textbook'} in webworkCourse.ph
if (defined($textbook)) {
   if ($textbook eq "EllisGulick5") {
BEGIN_TEXT
$PAR
END_TEXT
}
}

ANS(fun_cmp("(2*log(x)*x^(log(x)/log($aa)))/(x*log($aa))", limits=>[2,5], vars=>"x"));

ENDDOCUMENT();

