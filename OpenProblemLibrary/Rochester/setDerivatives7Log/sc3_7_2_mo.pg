DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

##############################################################
#
#  Setup
#
#
Context("Numeric");

$a1 = Real( random(2,5,1) );
$b1 = Real(  random(2,8,1) );
$c1 = Real( random(2,4,1) );
$x1 = Real( random(1,5,1) );
$deriv1 = $a1/($b1+$x1);
$funct1 = "$a1/($b1+x)";


$f = Compute(" $a1 ln($b1+x) ");
$fp = $f->D;
$fpatx1 = $fp->eval(x=>$x1);


##############################################################
#
#  Text
#
#

Context()->texStrings;
BEGIN_TEXT
If \( f(x) = $f  \), find \( f'( x ) \).
$BR$BR 
\( f'(x) = \)\{ans_rule(50) \}

$BR$BR
Find \( f'( $x1 ) \).
$BR$BR 
\( f'($x1) = \)\{ans_rule(50) \}

END_TEXT
Context()->normalStrings;


##############################################################
#
#  Answers
#
#
ANS($fp->cmp, $fpatx1->cmp);


;
ENDDOCUMENT();
