TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a=random(140,160,1);

BEGIN_TEXT
The pH reading of a sample of each substances is given. Calculate the hydrogen ion concentration of the substance.
$BR
(a) Vinegar: pH = 3.0.
$BR
Your answer is \{ans_rule(15) \}.
$BR
(b) Milk: pH = 6.5.
$BR
Your answer is \{ans_rule(15) \}.
$BR
END_TEXT

$ans1 = 10**(-3.0);
$ans2 = 10**(-6.5);

ANS(num_cmp($ans1));
ANS(num_cmp($ans2));

