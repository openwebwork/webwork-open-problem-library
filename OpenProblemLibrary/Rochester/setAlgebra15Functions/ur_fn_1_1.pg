  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);$a2=$a*$a;

TEXT(EV2(<<EOT));
Find the domain of the function \( f(x)=\sqrt{x^3-$a2 x} \). What is the least value of \( x \) in the domain?$BR
$BR
Least Value\( = \)\{ans_rule(10) \}
$BR
EOT

$ans = -$a;
ANS(num_cmp($ans));

