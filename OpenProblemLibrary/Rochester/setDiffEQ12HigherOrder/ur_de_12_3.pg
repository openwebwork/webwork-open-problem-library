DOCUMENT() ;       

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGdiffeqmacros.pl",
) ;


$a = random(2,9,1) ;
$b = $a**2;

$m = random(-9,9,1) ;
$n1 = random(-9,9,1) ;
$p1 = random(-4,4,1) ;

$n = $a*$n1;
$p = -($a**2)*$p1;

$c3 = $n1;
$c2 = $p1;
$c1 = $m -$c2;

$ans = "$c1 + $c2*cos($a*x) + $c3*sin($a*x)";

TEXT(beginproblem()) ;

$showPartialCorrectAnswers = 0 ;

BEGIN_TEXT

Find \(y\) as a function of \(x\) if $BR
\[ y''' +$b y' =  0,\]
\( y(0) = $m, \ \ y'(0) = $n, \ \ y''(0) = $p.\) $BR
\(y(x) = \) \{ans_rule(100)\}

END_TEXT

ANS(fun_cmp($ans)) ;

ENDDOCUMENT() ;       




