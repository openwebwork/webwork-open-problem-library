## DBsubject(Differential equations)
## DBchapter(Higher order differential equations)
## DBsection(Undetermined coefficients)
## Institution(Rochester)
## MLT(undet_02)
## MLTleader(1)
## Level(3)
## KEYWORDS('differential equation' 'higher order' 'third order')

DOCUMENT() ;       

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGdiffeqmacros.pl",
  "PGcourse.pl"
);


$a = random(2,9,1) ;
$b = random(2,9,1) ;
while( $a == $b ) {$b = random(2,9,1) ;}
$c = $a+$b;
$d = $a*$b;

$m = random(10,30,1) ;
$n = random(10,30,1) ;
$p = random(10,30,1) ;

$s = 1 - $c + $d;
$mult = random(1,5,1);
$num = $s * $mult;

$m1 = $m - $mult;
$n1 = $n - $mult;
$p1 = $p - $mult;

$c3 = ($p1 - $a * $n1) / ($b**2 - $a * $b);
$c2 = ($n1 - $b * $c3) / $a;
$c1 = $m1 - $c2 - $c3;

$ans = "$c1 + $c2*e^($a*x) + $c3*e^($b*x) + $mult *e^x";

TEXT(beginproblem()) ;

$showPartialCorrectAnswers = 1 ;

BEGIN_TEXT

Find \(y\) as a function of \(x\) if $BR
\[ y'''- $c y'' + $d y' =  $num e^x,\]
\( y(0) = $m, \ \ y'(0) = $n, \ \ y''(0) = $p.\) $BR
\(y(x) = \) \{ans_rule(50)\}

END_TEXT

ANS(fun_cmp($ans)) ;

ENDDOCUMENT() ;       







