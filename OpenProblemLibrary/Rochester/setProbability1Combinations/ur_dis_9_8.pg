DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());


for($i = 0; $i < 6; $i ++) {
	$a[$i] = random(5,12,1);
	$b[$i] = random(1,$a[$i],1);
	$ans[$i] = fact($a[$i])/(fact($b[$i])*fact($a[$i]-$b[$i]));
}

BEGIN_TEXT
Find the value of each of the following quantities: $BR
\( C($a[0] , $b[0] )= \) \{ ans_rule(30) \} $BR
\( C($a[1] , $b[1] )= \) \{ ans_rule(30) \} $BR
\( C($a[2] , $b[2] )= \) \{ ans_rule(30) \} $BR
\( C($a[3] , $b[3] )= \) \{ ans_rule(30) \} $BR
\( C($a[4] , $b[4] )= \) \{ ans_rule(30) \} $BR
\( C($a[5] , $b[5] )= \) \{ ans_rule(30) \} $BR
$PAR
END_TEXT

ANS(num_cmp( $ans[0] ) );
ANS(num_cmp( $ans[1] ) );
ANS(num_cmp( $ans[2] ) );
ANS(num_cmp( $ans[3] ) );
ANS(num_cmp( $ans[4] ) );
ANS(num_cmp( $ans[5] ) );


ENDDOCUMENT();        # This should be the last executable line in the problem.;
