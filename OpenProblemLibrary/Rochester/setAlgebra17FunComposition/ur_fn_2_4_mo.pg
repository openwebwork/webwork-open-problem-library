);

Context('Numeric');

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

###################
#
#  Setup


$a = Real( random(1,9,1) );
$c = $a*($a+1);

###################
#
#  Text

BEGIN_TEXT
Let \(f(x) = \sqrt{$c - x} \) and \(g(x) = x^2 - x \).
$BR
Then the domain of \( f\circ g \) is equal to \( [a,b] \) for
$BR
\( a = \) \{ ans_rule(10)\}
$BR
and
$BR
\( b = \) \{ans_rule(10)\}
END_TEXT 

###################
#
#  Answers


ANS(( -$a)->cmp);
ANS(num_cmp($a+1));

;

;
