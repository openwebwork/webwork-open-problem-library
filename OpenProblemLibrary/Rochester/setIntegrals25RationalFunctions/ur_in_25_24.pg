&DOCUMENT;
loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);


TEXT(beginproblem());

$p=random(2,5,1);
$u=random(1,6,1);
$a=non_zero_random(-3,3,1);
$b=random(2,5,1);
$q=$u + $a*$p;
$h=2*$a;
$j=($b*$b)+($a*$a);
$funct="($p/2)*ln(x*x+$h*x+$j)+($u/$b)*arctan((x+$a)/$b)";

TEXT(EV2(<<EOT));

Evaluate the integral. 
$BR
$BR \[ \int\frac{$p x+$q}{x^2+$h x+$j} dx \]
$BR \{ans_rule( 60) \}
EOT
$ans=$funct;
ANS(fun_cmp($ans, mode=>"antider"));

&ENDDOCUMENT
