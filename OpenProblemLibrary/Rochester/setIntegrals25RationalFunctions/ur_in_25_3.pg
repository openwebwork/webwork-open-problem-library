&DOCUMENT;

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGnumericalmacros.pl",
  "PGcourse.pl"
);


TEXT(beginproblem());

$a=random(-3,0,1);
$b=random(1,4,1);
$c=random(-2,2,1);
$e=random(1,5,1);
$d= ($c**2 + $e**2)/4;

$function = sub { my $x=shift;
                  (1)/(  ($x*$x + $c*$x + $d ) );
                };
$soln=romberg($function, $a, $b);

TEXT(EV3(<<'EOT'));
Evaluate the integral. 
$BR
$BR \[ \int_{$a}^{$b} \frac{1}{(x^2 + ${c}x+ $d)} dx \]
$BR \{ans_rule( 60) \}
EOT

ANS(num_cmp($soln));

&ENDDOCUMENT


