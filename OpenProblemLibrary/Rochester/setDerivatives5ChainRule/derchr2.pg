TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(3,9,1);
$b1 = random(2,15,1);
while ($b1 == $a1) {$b1 = random(2,15,1);}
$b2 = random(2,3,1);
$b4 = random(2,15,1);
$b3 = random(2,15,1);

#while ($b==$a) {
