## DESCRIPTION
##   Greatest Common Divisors
## ENDDESCRIPTION

## Tagged by nhamblet

## DBsubject(Number theory)
## DBchapter(Divisibility)
## DBsection(GCDs and LCMs)
## Institution(Rochester)
## Level(2)
## TitleText1('Discrete Mathematics')
## AuthorText1('Rosen')
## EditionText1('4')
## Section1('2.3')
## Problem1('24')
## KEYWORDS('Integers', 'GCD', 'Greatest Common Divisor')

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers=0;


$a = random(1,5,1);
$b = random(1,5,1);
$c = random(1,5,1);
$d = random(1,5,1);
$e = random(1,5,1);
$f = random(1,5,1);
$g = random(1,12,1);
$h = random(1,12,1);
$j = random(1,12,1);

@prime = ('2','3','5','7','11','13');
@slice = NchooseK(6,3);
@sprime = @prime[@slice];
@p = num_sort(@sprime);
#@q = @*-1;
#@r = num_sort(@b);
 
$a1 = 2**$a*3**$b*5**$c;
$b1 = 2**$d*3**$e*5**$f;
$a2 = 2**$g*3*5*7*11*13;
$b2 = $p[0]**$g*$p[1]**$h*$p[2]**$j*17;

$bb2 = $p[0]**$g*$p[1]*$p[2];

$s2a = ($a==1) ? '2' : "2^$a"; 
$s3b = ($b==1) ? '3' : "3^$b"; 
$s5c = ($c==1) ? '5' : "5^$c"; 
$s5b = ($b==1) ? '5' : "5^$b"; 
$s2d = ($d==1) ? '2' : "2^$d"; 
$s3e = ($e==1) ? '3' : "3^$e"; 
$s5f = ($f==1) ? '5' : "5^$f"; 
$s2g = ($g==1) ? '2' : "2^{$g}"; 
$p0g = ($g==1) ? "$p[0]" : "$p[0]^{$g}"; 
$p1h = ($h==1) ? "$p[1]" : "$p[1]^{$h}"; 
$p2j = ($j==1) ? "$p[2]" : "$p[2]^{$j}"; 

BEGIN_TEXT
What are the greatest common divisors of the following pairs of integers? 
$BR
(a) \( $s2a \cdot $s3b \cdot $s5c \) and \( $s2d \cdot $s3e \cdot $s5f \) 
$BR Answer =
\{ ans_rule(20) \}
$PAR
END_TEXT

ANS(num_cmp( gcd($a1,$b1) ) );

BEGIN_TEXT
(b) \( $s2g \cdot 3 \cdot 5 \cdot 7 \cdot 11 \cdot 13\) and 
\( $p0g \cdot $p1h \cdot $p2j \cdot 17 \)
$BR Answer =
\{ ans_rule(20) \}
$PAR
END_TEXT

ANS(num_cmp( gcd($a2,$bb2) ) );

BEGIN_TEXT
(c) \( $s2a \cdot 7\) and \( $s5b \cdot 13 \) 
$BR Answer =
\{ ans_rule(20) \}
END_TEXT

ANS(num_cmp( 1 ) );

ENDDOCUMENT();        # This should be the last executable line in the problem.;
