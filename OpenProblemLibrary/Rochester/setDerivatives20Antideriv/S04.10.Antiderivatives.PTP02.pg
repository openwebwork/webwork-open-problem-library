DOCUMENT();   

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,10,1);
$b = random(2,10,1);

if ($b == $a) { $b=$b+1; }

BEGIN_TEXT
Let \( \displaystyle f(x) = \frac{$a}{x} - $b e^x \).
$BR Enter an antiderivative of \( f(x) \)
$BR \{ans_rule(40) \}
END_TEXT

$antideriv="$a * ln(abs(x)) -$b * $E^x";

ANS(fun_cmp($antideriv, mode=>"antider"));

ENDDOCUMENT();
