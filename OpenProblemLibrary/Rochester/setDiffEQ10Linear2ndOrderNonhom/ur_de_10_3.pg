## DBsubject(Differential equations)
## DBchapter(Higher order differential equations)
## DBsection(Undetermined coefficients)
## Institution(Rochester)
## MLT(undet_01)
## Level(3)
## KEYWORDS('differential equation' 'second order' 'linear' 'nonhomogeneous')

DOCUMENT() ;       

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGdiffeqmacros.pl"
) ;
$k = random(1,9,1) ;
$p = random(2,9,1) ;
$w = random(2,9,1) ;
$K = 2*$w*$w*$k  ;
$P = 2*$w*$w*$p  ;
$A = 1 ;
$B = $w  ;
$C =  0 ;
$m = random(1,9,1) ;
$n = random(1,9,1) ;
################

$L = diffop($A,$B,$C) ;
$rhs = "$K \,\sin($w t) + $P \, \cos($w t)" ;
$y = undeterminedSin ($A,$B,$C,0,$w,0,$P,0,$K) ;
$y0 = -$p - $k ;
$yprime0 = $w*($p -$k ) ;
$z = ivy($A,$B,$C,$m - $y0,$n - $yprime0 ) ;
$ans = " $y + $z " ;

TEXT(beginproblem()) ;

$showPartialCorrectAnswers = 1 ;

BEGIN_TEXT

Find the solution of  
\[ $L =  $rhs \] 
with \( y(0) = $m \) and \( y'(0) = $n .\)
$BR
\(y = \) \{ans_rule(80)\}

END_TEXT

ANS(fun_cmp($ans, vars=>"t")) ;

ENDDOCUMENT() ;
#######

     








