DOCUMENT() ;       

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGdiffeqmacros.pl",
) ;
############

$r = random(2,9,1);
$sign = random(-1,1,2);
$r = $r * $sign;

$a = 1 - 2 * $r;
$b = $r**2;

$c = random(-10,10,1);
$d = random(-5,5,1);

$m = random(2,9,1);
while ($m * ($m-1) + $m * $a + $b == 0) { $m = random(2,9,1);}

$n = 1/($m * ($m-1) + $m * $a + $b);

$c1 = $c - $n;

if ( $d - $r * $c1 == 0 ) { 
	$d = $d+1; 
	$c1 = $c - $n;
}

$d1 = $d - $m * $n;

$k1 = $c1;
$k2 = $d1 - $r * $c1;

$eq = FEQ("x^2 y'' + $a x y' + $b y = x^$m");
$ans = FEQ("x**$r * ($k1 + $k2 * ln x) + $n x^$m");

TEXT(beginproblem()) ;

$showPartialCorrectAnswers = 1 ;

BEGIN_TEXT

Find \(y\) as a function of \(x\) if 
\[ $eq, \]
\( y(1) = $c, \ \ y'(1) = $d. \) 
$BR
\(y=\) \{ans_rule(80)\}

END_TEXT

ANS(fun_cmp($ans));

ENDDOCUMENT() ;       






