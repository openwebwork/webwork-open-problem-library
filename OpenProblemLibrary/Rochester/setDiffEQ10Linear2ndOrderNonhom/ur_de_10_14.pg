## Level(4)
## KEYWORDS('differential equation' 'second order' 'linear' 'nonhomogeneous')

DOCUMENT() ;       

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGdiffeqmacros.pl",
  "PGcourse.pl"
);
############


TEXT(beginproblem()) ;

$showPartialCorrectAnswers = 1 ;

$r1 = random(2,9,1);
$r2 = random(-9,-1,1);
while(($r1 + $r2 == 0) or ($r1 + $r2 == 1) or ($r1 + $r2 == 2)) { $r2 = random(-9,-1,1); } 

$a = 1 - $r1 - $r2;
$b = $r1 * $r2;

$d = random(-10,10,1);
$c = random(-10,10,1);
while (($c == $r1 * $d) or ($c == $r2 * $d)) { $c = random(-10,10,1); } 

$m = random(2,9,1);
while ($m * ($m-1) + $m * $a + $b == 0) { $m = random(2,9,1);}

$n = 1/($m * ($m-1) + $m * $a + $b);

$eq = FEQ("x^2 y'' + $a x y' + $b y = x^$m");

$d1 = $d - $n;
$c1 = $c - $m * $n;
$k1 = ($c1 - $r2 * $d1)/($r1 - $r2);
$k2 = ($c1 - $r1 * $d1)/($r2 - $r1); 

$ans = FEQ("$k1 * x**$r1 + $k2 * x**$r2 + $n * x**$m");

BEGIN_TEXT

Find \(y\) as a function of \(x\) if 
\[ $eq, \]
\( y(1)=$d, \ \ y'(1)=$c. \) 
$BR
\(y=\) \{ans_rule(80)\}

END_TEXT

ANS(fun_cmp($ans));

ENDDOCUMENT() ;       






