DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(3,9,1);
$power = $a - 1;
$coeff = 1 / $a;
$funct = "$coeff * e^(x^$a)";

TEXT(EV2(<<EOT));
Evaluate the indefinite integral.
$BR \[ \int x^$power e^{x^$a} dx \]
$BR Answer =  \{ans_rule( 50) \} \(+C\)
EOT


ANS(fun_cmp($funct, limits=>[0.75,1.25], mode=>"antider"));

ENDDOCUMENT();


