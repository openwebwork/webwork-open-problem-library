loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = 0;
$b1 = random(2,10,1);
$c1 = random(5,9,1);
$d1 = random(1,4,1);

BEGIN_TEXT
Evaluate the definite integral
\[ \int_{ $d1 } ^ { $c1 } \frac { $b1 } { \sqrt x } dx \]
\{ans_rule(40) \}
$PAR
END_TEXT

$ans = (2*$a1*$c1**2.5/5 + 2*$b1*$c1**.5) -(2*$a1*$d1**2.5/5 + 2*$b1*$d1**.5 );
ANS(num_cmp($ans));

