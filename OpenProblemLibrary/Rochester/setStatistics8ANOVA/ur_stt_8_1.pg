## DESCRIPTION
##   Analysis of Variance
## ENDDESCRIPTION

## Tagged by nhamblet

## DBsubject(Statistics)
## DBchapter(Hypothesis tests)
## DBsection(One-way ANOVA)
## Institution(Rochester)
## Level(3)
## KEYWORDS('ANOVA', 'Analysis', 'Variance')

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGgraphmacros.pl",
  "PGnumericalmacros.pl",
  "PGstatisticsmacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$dft = random(10,20,1);
$tdf = random(40,50,1);
$dfe = $tdf-$dft;

$sst = random(15,20,.1);
$tss = random(40,50,.1);
$sse = $tss-$sst;

$mst = $sst/$dft;
$mse = $sse/$dfe;
$f = $mst/$mse;


BEGIN_TEXT
Complete the ANOVA table for a completely randomized design below. $PAR
\{begintable(5)\}
\{row("Source", "df", "SS", "MS", "\(F\)")\}
\{row("Treatments", "\($dft\)", "\($sst\)", ans_rule(10), ans_rule(10))\}
\{row("Error", ans_rule(10), ans_rule(10), ans_rule(10), "\( \ \)")\}
\{row("Total", "\($tdf\)", "\($tss\)", "\( \ \)", "\( \ \)")\}
\{endtable()\}
$PAR

END_TEXT
ANS(num_cmp($mst));
ANS(num_cmp($f));
ANS(num_cmp($dfe));
ANS(num_cmp($sse));
ANS(num_cmp($mse));

ENDDOCUMENT();       # This should be the last executable line in the problem.

