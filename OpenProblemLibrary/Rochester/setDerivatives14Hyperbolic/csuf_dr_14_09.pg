  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);
$deriv = "x / sqrt(x^2 + $a) / (1 - $a - x^2)";

BEGIN_TEXT
If 
\[ f(x) =  \coth^{-1} \sqrt{x^2 + $a}\]
then \( f'( x ) =\) \{ans_rule(50) \}.
$BR
END_TEXT

ANS(fun_cmp($deriv));

