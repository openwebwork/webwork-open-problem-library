DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(1,9,1);
$b = random(2,9,1);
$deriv = "- ($a+$b)sinh(x)/($b+cosh(x))^2";

BEGIN_TEXT
If 
\[ f(x) =  \frac{$a - \cosh x}{$b + \cosh x} \]
then \( f'( x ) =\) \{ans_rule(50) \}.
$BR
END_TEXT

ANS(fun_cmp($deriv));

ENDDOCUMENT();        # This should be the last executable line in the problem.
