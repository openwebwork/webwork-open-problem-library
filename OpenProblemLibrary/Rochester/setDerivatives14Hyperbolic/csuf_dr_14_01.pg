DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);
$deriv = "$a * (sech($a*x))^2";

BEGIN_TEXT
If 
\[ f(x) =  \tanh ($a x)\]
then \( f'( x ) =\) \{ans_rule(50) \}.
$BR
END_TEXT

ANS(fun_cmp($deriv));

