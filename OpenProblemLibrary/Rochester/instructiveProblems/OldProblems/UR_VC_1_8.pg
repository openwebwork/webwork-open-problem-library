DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"PGcourse.pl",
# "source.pl",
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

###################
#
#  Setup

$a = random(-10, 10);
$b = random(-10, 10);
$c = random(-10, 10);
$d = random(-10, 10);
$e = random(-10, 10);
$f = random(-10, 10);
$g = random(-10, 10);

$abs = sqrt(($a)**2 + ($b)**2 + ($c)**2);

###################
#
#  Text

BEGIN_TEXT
$PAR
Let \( \bf a \) = ($a, $b, $c) and \( \bf b \) = ($d, $e, $f) be vectors.  Compute the following
vectors.

$PAR A. \( \bf a \) + \( \bf b \) = (\{ ans_rule(6) \}, \{ ans_rule(6) \},
\{ ans_rule(6) \})

$PAR
B. ${g}\( \bf{a} \)= (\{ ans_rule(6) \}, \{ ans_rule(6) \}, \{
ans_rule(6) \})

$PAR
C. \( \bf a \) - \( \bf b \)= (\{ ans_rule(6) \}, \{ ans_rule(6) \}, \{
ans_rule(6) \})

$PAR
D. \( \left|{\bf a}\right| \) = \{ ans_rule(40) \}
END_TEXT

###################
#
#  Answers


ANS(num_cmp($a+$d));
ANS(num_cmp($b+$e));
ANS(num_cmp($c+$f));

ANS(num_cmp($g*$a));
ANS(num_cmp($g*$b));
ANS(num_cmp($g*$c));

ANS(num_cmp($a-$d));
ANS(num_cmp($b-$e));
ANS(num_cmp($c-$f));

ANS(num_cmp($abs));

ENDDOCUMENT(); 
