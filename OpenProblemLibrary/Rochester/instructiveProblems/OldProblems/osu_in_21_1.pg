DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

###################
#
#  Setup

$a = random(2,7,1);
$a3 = $a*$a*$a;
$b = random(3,8,1);
$b3 = $b*$b*$b;
$temp1 = 3*$b**4+3*$b**2/32;
$temp2 = 3*$a**4+3*$a**2/32;
$answ = $temp1 + $temp2;

###################
#
#  Text

BEGIN_TEXT
Find the length of the curve
\[
x = 3 y^{4/3}-\frac{3}{32}y^{2/3}, \quad -$a3\le y\le $b3
\]
$BR
\(L\) =  \{ ans_rule()\}
END_TEXT

###################
#
#  Answers

ANS(num_cmp($answ));

ENDDOCUMENT();
