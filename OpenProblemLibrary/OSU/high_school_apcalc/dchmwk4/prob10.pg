DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

Context('Numeric');

$a_gen  = random(2,9,1);
$a_sign = random(-1,1,2);
$a      = $a_gen * $a_sign;

$n = random(2,8,1);

$gen = random(0,3,1);

if ($gen == 0) {
  $trig1 = "\sin";
  $trig2 = "\sin";
  $ans  = "$a*$n*x**($n-1)*cos(sin(x**($n)))*cos(x**($n))";
  }
if ($gen == 1) {
  $trig1 = "\sin";
  $trig2 = "\cos";
  $ans  = FEQ("-$a*$n*x**($n-1)*cos(cos(x**($n)))*sin(x**($n))");
  }
if ($gen == 2) {
  $trig1 = "\cos";
  $trig2 = "\sin";
  $ans  = FEQ("-$a*$n*x**($n-1)*sin(sin(x**($n)))*cos(x**($n))");
  }
if ($gen == 3) {
  $trig1 = "\cos";
  $trig2 = "\cos";
  $ans  = "$a*$n*x**($n-1)*sin(cos(x**($n)))*sin(x**($n))";
  }

BEGIN_TEXT
Let \[ f(x) = $a $trig1 ( $trig2 ( x ^{$n} )) \]
$BR$BR
\( f'( x ) = \) \{ans_rule(30) \}
END_TEXT

ANS(Compute($ans)->cmp);

ENDDOCUMENT();        # This should be the last executable line in the problem.
