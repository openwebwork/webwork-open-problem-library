  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(2,10,1);
$deriv1f= "x * (x^2+$a1)**(-0.5)";
$deriv2f = "(x^2+$a1)**(-0.5) - x^2 * (x^2+$a1)**(-1.5)";


TEXT(EV2(<<EOT));
Let \( f(x) = \sqrt { x^2 + $a1 } \). $PAR
\( f'(x) = \) \{ans_rule(30) \} $PAR
\( f''( x) = \) \{ans_rule(30) \}, $PAR
EOT

$ans1 = $deriv1f;
ANS(fun_cmp($ans1));
$ans3 = $deriv2f;
ANS(fun_cmp($ans3));

