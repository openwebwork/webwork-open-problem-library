"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);
$a2 = $a**2;
$ans1 = 2/$a;

BEGIN_TEXT
Evaluate the following limit:$BR
\( \lim_{x\to  $a2}\frac{x- $a2}{x(\sqrt{x} - $a)} =\) 
\{ans_rule(20)\}$BR


END_TEXT

ANS(num_cmp($ans1));


