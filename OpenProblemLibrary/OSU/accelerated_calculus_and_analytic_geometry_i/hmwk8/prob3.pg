DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

$showPartialCorrectAnswers = 1;


$a = random(2,5,1);
$b = random(2,6,1);
$ans = "$a * 1/2 * (x^2 * ln($b * x) - 1/2 * x^2)";

TEXT(beginproblem());
BEGIN_TEXT
Use integration by parts to evaluate the integral.
$BR \[ \int $a x \ln ($b x)\;  dx \]
$BR $BR \{ans_rule( 60) \}  \(+C\)
$PAR

END_TEXT

ANS(fun_cmp($ans, mode=>"antider"));

ENDDOCUMENT();

