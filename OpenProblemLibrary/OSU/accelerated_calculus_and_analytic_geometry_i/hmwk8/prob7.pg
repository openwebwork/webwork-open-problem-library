$showPartialCorrectAnswers = 1;


$a = random(2,20,1);
$b = random(2,20,1);
$c = $a*$b;
$ans = "($c*3*x)/8 +$b*(cos($a*x))^3*sin($a*x)/4+3*$b*cos($a*x)*sin($a*x)/8";

TEXT(beginproblem());
BEGIN_TEXT

Evaluate the indefinite integral.
$BR \[ \int $c \cos^4($a x) dx \]
$BR $BR \{ans_rule( 60) \}  \(+C\)
$PAR

END_TEXT

ANS(fun_cmp($ans, mode=>"antider"));

