## DESCRIPTION
##   Domain of an Inverse Function
## ENDDESCRIPTION

## Tagged by nhamblet

## DBsubject(Algebra)
## DBchapter(Inverse functions)
## DBsection(Finding the inverse function)
## Institution(OSU)
## MLT(InverseFunctions_FindInverse_14)
## Level(3)
## KEYWORDS('Inverse', 'Domain')

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);


$showPartialCorrectAnswers = 1;
if (!($studentName =~ /PRACTICE/)) {
$seed = random(1,4,1);
if ($studentName =~ /VINCE VERSION1/) {$seed = 1;}
if ($studentName =~ /VINCE VERSION2/) {$seed = 2;}
if ($studentName =~ /VINCE VERSION3/) {$seed = 3;}
if ($studentName =~ /VINCE VERSION4/) {$seed = 4;}
SRAND($seed);}


$AA = random(1,6,1);
$BB = random(3,12,1);
$CC = random(7,25,1);
$DD = random(6,15,1);

$answ1 = -$BB/$DD;
$answ2 = $AA/$CC;
TEXT(beginproblem());
BEGIN_TEXT
\[f(x)=\frac{$AA e^x - $BB}{$CC e^x + $DD}\]
$PAR

\(f^{-1}(x)\)  =  \{ ans_rule()\}
$PAR

The domain of \(f^{-1}(x)\) is the open interval \((a,b)\), where $BR
\(a\)  =  \{ ans_rule()\}
$BR
and
\(b\)  =  \{ ans_rule()\}

END_TEXT

ANS(fun_cmp("ln(($DD*x+$BB)/($AA-$CC*x))", limits=>[$answ1+.0001,$answ2-.0001], vars=>"x"));
ANS(num_cmp($answ1));
ANS(num_cmp($answ2));

ENDDOCUMENT();
