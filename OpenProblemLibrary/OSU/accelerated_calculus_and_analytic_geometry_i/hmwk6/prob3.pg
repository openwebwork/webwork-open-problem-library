## DESCRIPTION
##   Definite Integral of an Inverse Function
## ENDDESCRIPTION

## Tagged by nhamblet

## DBsubject(Calculus - single variable)
## DBchapter(Integrals)
## DBsection(Conceptual understanding of integration)
## Institution(OSU)
## MLT(integral_concept_inverse)
## Level(3)
## KEYWORDS('Definite Integral', 'Inverse', 'Fundamental Theorem')

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);


$showPartialCorrectAnswers = 1;
if (!($studentName =~ /PRACTICE/)) {
$seed = random(1,4,1);
if ($studentName =~ /VINCE VERSION1/) {$seed = 1;}
if ($studentName =~ /VINCE VERSION2/) {$seed = 2;}
if ($studentName =~ /VINCE VERSION3/) {$seed = 3;}
if ($studentName =~ /VINCE VERSION4/) {$seed = 4;}
SRAND($seed);}


$a = random(3,7,1);
$b = $a + random(4,8,1);

$c = random(2,6,1);
$d = $c + random(3,7,1);

$AA = $c*($b-$a);
$BB = $a*($d-$c);
$temp1 = ($b-$a)*($d-$c);
$CC = random(0.000001,$temp1-0.000001,0.000001);
$answ = $BB+$CC;
$temp2 = $AA+$CC;

TEXT(beginproblem());
BEGIN_TEXT
Suppose \(f(x)\) is continuous and decreasing on the closed interval
\($a\le x\le $b\), that \(f($a)=$d\), \(f($b)=$c\) and that
\(\int_{$a}^{$b}f(x)\,dx=$temp2\).
$BR
Then \(\int_{$c}^{$d}f^{-1}(x)\,dx\) = \{ ans_rule()\}

END_TEXT

ANS(num_cmp($answ));
ENDDOCUMENT();
