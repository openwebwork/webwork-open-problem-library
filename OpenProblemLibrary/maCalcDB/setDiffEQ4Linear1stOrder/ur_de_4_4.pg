##  ## Differential equations   
##    dy/dt -  y = a exp(t) + B exp(k t) 
##  
##   URL:http://webhost.math.rochester.edu/mth163lib/discuss/msgReader$377
##  
## ENDDESCRIPTION 
DOCUMENT() ;
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem()) ;
$showPartialCorrectAnswers = 1 ;

$k = random(2,9,1) ;
$a = random(1,9,1) ;
$b = random(1,9,1) ;
$B = ($k - 1)*$b ;
$m = random(1,9,1) ;

BEGIN_TEXT

Solve the initial value problem $BR
\[  \frac{dy}{dt} -  y = $a \exp(t) + $B \exp($k t) \] with \( y(0) = $m. \) $BR
\( y = \) \{ans_rule(70) \}. $BR


END_TEXT

$ans = "($m - $b )* exp(t) + $a * t * exp(t) + $b * exp($k *t)" ;


ANS(fun_cmp($ans, vars=>"t")) ;

ENDDOCUMENT() ;       
