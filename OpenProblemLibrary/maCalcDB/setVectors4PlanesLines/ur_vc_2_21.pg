DOCUMENT();	# This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;
              
$ml = new_match_list();

$ml -> qa(
	"\( x^{2} + y^{2} = 5 \)",
	"circular cylinder",
	"\( z = 2x^{2} + 3y^{2} \)",
	"elliptic paraboloid",
	"\( z = y^{2} - 2x^{2} \)",
	"hyperbolic paraboloid",
	"\( z = x^{2} \)",
	"parabolic cylinder",
	"\( z = 4 \)",
	"horizontal plane",
	"\( z = 2x + 3y \)",
	"nonhorizontal plane",
	"\( x^{2} + 2y^{2} + 3z^{2} = 1 \)",
	"ellipsoid"
);

$ml -> choose(7);

BEGIN_TEXT
Match the surfaces with the appropriate descriptions.
$PAR
\{ $ml -> print_q \}
$PAR
\{ $ml -> print_a \}
END_TEXT

ANS(str_cmp($ml -> ra_correct_ans) );

ENDDOCUMENT();	# This should be the last executable line in the problem.
