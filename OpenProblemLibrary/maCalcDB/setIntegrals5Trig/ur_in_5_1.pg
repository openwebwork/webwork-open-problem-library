DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

$showPartialCorrectAnswers = 1;

$l = random(1,10,1);
$a = random(1,20,1);
$l1 = $l+1;
$l3 = $l+3;
$ans = "-cos($a * x)**($l1)/($l1 *$a)+ cos($a * x)**($l3)/($l3 * $a)";

TEXT(beginproblem());

BEGIN_TEXT
Evaluate the indefinite integral.
$BR \[ \int \sin^3($a x)\cos^{$l}($a x) dx \]
$BR $BR \{ans_rule( 60) \}  \(+C\)
END_TEXT

