$showPartialCorrectAnswers = 1;
ANS(num_cmp($RAx*1000, units=>"N"));
ANS(num_cmp($RAy*1000, units=>"N"));
ANS(num_cmp($RE*1000, units=>"N"));
ANS(num_cmp($TBC*1000, units=>"N"));
ANS(num_cmp($sigmaBC, units=>"Pa"));
ANS(num_cmp($deltaBC, units=>"cm"));
ANS(num_cmp($TCI*1000, units=>"N"));
ANS(num_cmp($sigmaCI, units=>"Pa"));
ANS(num_cmp($deltaCI, units=>"cm"));
ANS(num_cmp($THI*1000, units=>"N"));
ANS(num_cmp($sigmaHI, units=>"Pa"));
ANS(num_cmp($deltaHI, units=>"cm"));
ANS(num_cmp($TCH*1000, units=>"N"));
ANS(num_cmp($sigmaCH, units=>"Pa"));
ANS(num_cmp($deltaCH, units=>"cm"));
ANS(num_cmp($TCD*1000, units=>"N"));
ANS(num_cmp($sigmaCD, units=>"Pa"));
ANS(num_cmp($deltaCD, units=>"cm"));
ANS(num_cmp($TCG*1000, units=>"N"));
ANS(num_cmp($sigmaCG, units=>"Pa"));
ANS(num_cmp($deltaCG, units=>"cm"));
ANS(num_cmp($TGH*1000, units=>"N"));
ANS(num_cmp($sigmaGH, units=>"Pa"));
ANS(num_cmp($deltaGH, units=>"cm"));



