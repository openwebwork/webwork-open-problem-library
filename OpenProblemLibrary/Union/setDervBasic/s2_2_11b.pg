# Edited by N. Srivastava, Union College, 9/11/03

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGunion.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

###################################
# Setup

$an = random(2,8,1);
$as = random(-1,1,2);
$a  = $an * $as;

$bn = random(2,8,1);
$bs = random(-1,1,2);
$b  = $bn * $bs;

$m = random(4,6,1);
$n = random(2,3,1);

$f=Formula("$a x^{$m} sqrt{x} + $b/(x^{$n} sqrt{x}) ");

###################################
# Main text

Context()->texStrings;
BEGIN_TEXT
Let \( f(x) = \displaystyle $f \).
 Find \(f'(x)\).
$PAR
\( f'( x ) = \) \{ans_rule(50) \}
END_TEXT
Context()->normalStrings;

###################################
# Answers

$showPartialCorrectAnswers = 1;
$ans=$f->D('x');
ANS($ans->cmp(limits => [0,4]));

###################################

ENDDOCUMENT();
