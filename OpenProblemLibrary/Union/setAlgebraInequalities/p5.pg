);

TEXT(beginproblem());



###################################
# Setup

Context("Interval");
$a = random(1,5,1);
$b = random(6,10,1);

###################################
# Main text

BEGIN_TEXT
Solve the following inequality. Enter the answer in interval notation.
\[ \frac{$a -x}{x-$b} \ge 0  \]
Answer: \{ans_rule(35)\}
$PAR
$INFINITY_UNION_MESSAGE
END_TEXT

###################################
# Answers

$showPartialCorrectAnswers = 1;
ANS(Interval("[$a,$b)")->cmp);

