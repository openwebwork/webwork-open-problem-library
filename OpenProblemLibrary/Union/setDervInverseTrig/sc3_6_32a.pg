DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGunion.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

###################################
# Setup

$a = random(2,8,1);

@trig=("sin x","cos x");
$i = random(0,1,1);

$f=Formula("$a $trig[$i] arcsin x")->reduce;

###################################
# Main text

Context()->texStrings;
BEGIN_TEXT
Let \( f(x) = \displaystyle $f \).  Find \(f'(x)\).
$PAR
\(f'(x) =\) \{ans_rule(50) \}
END_TEXT
Context()->normalStrings;

###################################
# Answers

$showPartialCorrectAnswers = 1;
$ans=$f->D('x')->with(limits=>[0,1]);
ANS($ans->cmp());

###################################

;
ENDDOCUMENT();
