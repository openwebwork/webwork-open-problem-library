DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGunion.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

###################################
# Setup

$a = random(2,6,1);
$b = random(-11,11,2);
$c = random(1,5,1);
$d = random(6,10,1);

$y="{$a} x^{3/2} + $b";

###################################
# Main text

Context()->texStrings;
BEGIN_TEXT
Find the length of the curve defined by
\[y=$y\]
from \(x = $c\) to \(x = $d\).
$PAR
The length is \{ans_rule(40)\}.

END_TEXT
Context()->normalStrings;

###################################
# Answers

$showPartialCorrectAnswers = 1;

$answer = ((4+9*$a**2*$d)**(3/2)-(4+9*$a**2*$c)**(3/2))/(27*$a**2);

ANS(Real($answer)->cmp);

###################################


;
ENDDOCUMENT();
