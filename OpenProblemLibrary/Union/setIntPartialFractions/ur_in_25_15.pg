DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGunion.pl",            # Union College utilities
  "MathObjects.pl",
  "PGcourse.pl",           # Customization file for the course
);

TEXT(beginproblem());

###################################
# Setup

$a = random(1,4,1);
$b = random(1,4,1);
while($b==$a) {$b=random(1,4,1)};

$A = non_zero_random(-6,6,1);
$coef1 = $a**2 + $a*$b + $b**2;
while($A==$coef1) {$A = non_zero_random(-6,6,1)};
$B = $coef1 - $A;

$coef2 = - $a - $b;
$coef3 = $a*$b;
$coef4 = $coef2*$a*$b + $A*$b + $B*$a;

$integrand= Formula("{x^3 + $coef4}/{x^2 - ${coef2}x + $coef3}")->reduce;

###################################
# Main text

Context()->texStrings;
BEGIN_TEXT
Evaluate the indefinite integral.
$PAR
\( \displaystyle\int $integrand \, dx \)
             = \{ans_rule(50)\} \( + C\).
END_TEXT
Context()->normalStrings;

###################################
# Answers

$showPartialCorrectAnswers = 1;
Context()->flags->set(reduceConstants=>0);

$antideriv = Formula(  "(x^2)/2+${coef2}x+${A}ln(abs(x+$a))+${B}ln(abs(x+$b))")->reduce
                    ->with(limits => [6,10]);

ANS($antideriv->cmp(upToConstant=>1));

###################################


COMMENT('MathObject version');
ENDDOCUMENT();
