DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGunion.pl",            # Union College utilities
  "MathObjects.pl",
  "PGcourse.pl",           # Customization file for the course
);

TEXT(beginproblem());

###################################
# Setup

$a=random(1,3,1);
$b=random(-6,-2,1);
$c=random(1,6,1);
$d=random(2,7,1);
$f=random(-12,-5,1);

#Constants in Numerator
$r=$c+$d;
$s=2*$b*$c + ($a + $b)*$d + $f;
$t=($b**2)*$c + $a*$b*$d +$a*$f;

#Forming denominator
$sum = $a+$b;
$prod=$a*$b;
$quadratic = Formula("x^2 + ${sum}x + $prod")->reduce;

$integrand= Formula("{${r}x^2 +${s}x + $t} / {(x+$b)($quadratic)}")->reduce;

# This problem can also be given without starting the denom's factorization:
#$k=$a+2*$b;
#$l=2*$a*$b + $b**2;
#$m=$a*$b**2;
#
#$integrand=Formula("{$r x^2 + $s x + $t} / {x^3 + $k x^2 + $l x + $m} ")-reduce;

###################################
# Main text

Context()->texStrings;
BEGIN_TEXT
Evaluate the indefinite integral.
$PAR
\( \displaystyle\int $integrand \, dx \)
             = \{ans_rule(50)\} \( + C\).
END_TEXT
Context()->normalStrings;

###################################
# Answers

$showPartialCorrectAnswers = 1;
Context()->flags->set(reduceConstants=>0);

$antideriv = Formula( "${c}ln(abs(x+$a)) + ${d}ln(abs(x+$b)) - $f/(x+$b)")->reduce
                    ->with(limits => [10,20]);

ANS($antideriv->cmp(upToConstant=>1));

###################################


COMMENT('MathObject version');
ENDDOCUMENT();
