# DESCRIPTION
# Asks to list all whole/natural numbers in the given set. The numbers in the set are between two 
# numbers.
# ENDDESCRIPTION

## DBsubject(Algebra)
## DBchapter(Algebra of real numbers and simplifying expressions)
## DBsection(Properties)
## Institution(University of Missouri-Columbia)
## Author(Rick Lynch)
## MLT(alg_real_num_2015_list)
## Level(2)
## KEYWORDS('real numbers')


###########################################################################
# initialization 
###########################################################################
DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;


###########################################################################
# setup contexts and variables 
###########################################################################
Context("Numeric");
$case = random(0,1,1); # 0 - whole numbers, 1 - natural numbers
if ($case == 0) {
  $str1 = "whole";
} else {
  $str1 = "natural";
}
$a = random(4,7);
$b = $a + random(3,7);
@anslist = (($a+1)..($b-1));
$ans = List(@anslist);


###########################################################################
# state the problem 
###########################################################################
Context()->texStrings;
BEGIN_TEXT
List the elements of the set 
$PAR
\[
$LBRACE x | x \mbox{ is a $str1 number between $a and $b} $RBRACE.
\]
$PAR
Separate multiple answers by commas.
$PAR
Answer: \{ans_rule(30)\}
END_TEXT


###########################################################################
# check the answer  
###########################################################################
ANS(Compute($ans)->cmp());

COMMENT('Desc: Asks to list all whole/natural numbers in the given set. The numbers in the set are between two numbers.' );
ENDDOCUMENT();
