#  Initialization

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGunion.pl",
  "MathObjects.pl",
  "AnswerFormatHelp.pl",
  "parserMultiAnswer.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());


###################################
# Setup

$a = random(2,6,1);
do {$b = random(2,6,1)} until (gcd($a,$b) ==1);
$c = random(2,6,1);
$exp = "\frac{$c}{$a - $b x}";

Context("Interval");
$ans = Compute(" R - {$a/$b }" );

###################################
# Main text

Context()->texStrings;
BEGIN_TEXT
Find the domain of the function \(f(x) = 2^{$exp}\).  Write your answer using 
\{AnswerFormatHelp("interval", "interval notation")\}.
$PAR
$SPACE The domain is \{ans_rule(20)\}.
END_TEXT
Context()->normalStrings;

###################################
# Answers

$showPartialCorrectAnswers = 0;

ANS( $ans->cmp() );


ENDDOCUMENT();
