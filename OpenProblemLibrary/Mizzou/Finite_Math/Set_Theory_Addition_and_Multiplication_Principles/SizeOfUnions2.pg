##DESCRIPTION
##  Introduction to Set Theory: Inclusion-Exclusion
##ENDDESCRIPTION


## Author1('David Covert')
## Author2('Jason Aubrey')
## DBsubject(Combinatorics)
## DBchapter(Counting)
## DBsection(Inclusion/exclusion)
## Date(Spring 2012)
## Institution(University of Missouri)
## Level(3)
## KEYWORDS('set theory', 'union', 'intersection', 'inclusion exclusion')

DOCUMENT();      

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

# Show which answers are correct and which ones are incorrect
$showPartialCorrectAnswers = 1;

##############################################################
#
#  Setup
#
#

Context("Numeric");
$A = random(100,200,1);
$B = random(100,200,1);
$AnB = random(25,75);
$AUB = $A + $B - $AnB;
$U = random(550,600,1);
$AnB = $A + $B - $AUB;
$AUBcomplement = $U - $AUB;
$AnotB = $A - $AnB;
$BnotA = $B - $AnB;
$notA = $U - $A;
$notB = $U - $B;
$AornotB = $A + $notB - $AnotB;
$BornotA = $B + $notA - $BnotA;

##############################################################
#
#  Text
#

Context()->texStrings;
BEGIN_TEXT
Suppose that \(A\) and \(B\) are sets with 
$PAR
\[
n(A') = $notA, n(B') = $notB, n((A \cup B)') = $AUBcomplement, \text{ and  } n(U) = $U.
\]
$PAR  Find the following:

$PAR
\(n(A) = \)  \{ANS_RULE(10)\}
$PAR
\(n(B) =  \) \{ANS_RULE(10)\}
$PAR
\(n(A \cup B) = \) \{ANS_RULE(10)\}
$PAR
\(n(A \cap B) = \)  \{ANS_RULE(10)\}
$PAR
\(n(A \cap B') = \) \{ANS_RULE(10)\}
$PAR
\(n(A' \cap B)  = \) \{ANS_RULE(10)\}
$PAR
\(n(A' \cap B') = \) \{ANS_RULE(10)\}
END_TEXT
Context()->normalStrings;


##############################################################
#
#  Answers
#

ANS(Compute($A)->cmp);
ANS(Compute($B)->cmp);
ANS(Compute($AUB)->cmp);
ANS(Compute($AnB)->cmp);
ANS(Compute($AnotB)->cmp);
ANS(Compute($BnotA)->cmp);
ANS(Compute($AUBcomplement)->cmp);

ENDDOCUMENT();
