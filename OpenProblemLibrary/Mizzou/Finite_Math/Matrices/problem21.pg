"PGgraphmacros.pl",
"PGmatrixmacros.pl", 
"PGnumericalmacros.pl",
"PGauxiliaryFunctions.pl"
);
############

TEXT(beginproblem());

foreach $i (1..3) {

   foreach $j (1..3) {
   		$a[$i][$j] = non_zero_random(-10,10,1);
   	}
}

$b[1][1]= non_zero_random(-10,10,1);
$b[1][2]= (0);
$b[1][3]= (0);
$b[2][1]= (0);
$b[2][2]= non_zero_random(-10,10,1);
$b[2][3]= (0);
$b[3][1]= (0);
$b[3][2]= (0);
$b[3][3]= non_zero_random(-10,10,1);

$showPartialCorrectAnswers = 1 ;

BEGIN_TEXT

Compute the following product. 
$BR
\{ mbox( display_matrix([[$a[1][1], $a[1][2], $a[1][3]],
                         [$a[2][1], $a[2][2], $a[2][3]],
                         [$a[3][1], $a[3][2], $a[3][3]]]),
         display_matrix([[$b[1][1], $b[1][2], $b[1][3]],
                         [$b[2][1], $b[2][2], $b[2][3]],
                         [$b[3][1], $b[3][2], $b[3][3]]]), '\(=\)', answer_matrix(3,3,5) ) \}

END_TEXT

foreach $i (1..3) {

   foreach $j (1..3) {
        $sum = 0;
        foreach $k (1..3) {
           $sum = $sum + $a[$i][$k]*$b[$k][$j];
        } 
   		ANS(num_cmp($sum)  );
   	}
}

ENDDOCUMENT() ;        
