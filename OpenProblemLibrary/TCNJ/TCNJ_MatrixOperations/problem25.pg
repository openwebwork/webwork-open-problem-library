DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGcourse.pl",
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

Context('Matrix');

$u = Matrix([
[non_zero_random(-9,9,1)],
[non_zero_random(-9,9,1)],
[non_zero_random(-9,9,1)],
]);

$ut = $u->transpose;

$ans1 = $u * $ut;
$ans2 = $ut * $u;

Context()->texStrings;
BEGIN_TEXT
If 
\[ u = $u \]
then
$BR
$BR
\( u u^T = \) \{ $ans1->ans_array(15) \} and
\( u^T u = \) \{ $ans2->ans_array(15) \}.
END_TEXT
Context()->normalStrings;

ANS($ans1->cmp);
ANS($ans2->cmp);
COMMENT('MathObject version');
ENDDOCUMENT();