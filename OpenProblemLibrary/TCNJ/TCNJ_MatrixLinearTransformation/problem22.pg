"$str1 ", "$ansstr1",
"$str2 ", "$ansstr2",
"$str3 ", "$ansstr4",
); 
$tf->extra( "$ansstr3");
$tf->choose(3);
$tf->choose_extra([0],0);


BEGIN_TEXT

$str
$PAR

