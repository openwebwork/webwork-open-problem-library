TEXT(beginproblem());

$a = random(1, 8, 1);
$b = random(1, 8, 1);
$a3 = 3*$a;
$b2 = 2*$b;
$a3_y2 = clean_scalar_string(3*$a, "y^2");
$b2_x = clean_scalar_string(2*$b,"x");
BEGIN_TEXT
Evaluate the iterated integral 
\( I = \int_{0}^{1}\!\!\int_{1-y}^{1+y} ($a3_y2 + $b2_x )\: dx dy \)

$PAR
\{ ans_rule(40) \}
END_TEXT

ANS(num_cmp( $a*(3/2) + 2*$b ) );



