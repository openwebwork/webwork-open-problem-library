DOCUMENT();
loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl",
           "PGgraphmacros.pl",
           "Dartmouthmacros.pl");
 

## Do NOT show partial correct answers
$showPartialCorrectAnswers = 1;

## Lots of set up goes here
$a = random( 4, 10, 1 );
$b = random(4,10,1);
$b_squared = $b**2;



## Ok, we are ready to begin the problem...
##
TEXT(beginproblem());

BEGIN_TEXT
Find the volume of the ellipsoid 
\(x^2 + y^2 + $a z^2 = $b_squared\).

  $PAR
\{ans_rule(40)\}

END_TEXT

    $ans = 4 * $PI * $b**3 / (3*sqrt($a));
ANS(num_cmp($ans));


ENDDOCUMENT();




