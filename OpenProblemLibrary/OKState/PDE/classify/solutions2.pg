## KEYWORDS('PDE', 'partial', 'differential', 'equation', 'solution')

########################################################################

###########################
# Initialization

DOCUMENT();      

loadMacros(
"PGstandard.pl",
"PGchoicemacros.pl",
"PGcourse.pl",
);

TEXT(beginproblem());

###########################
# Setup

$A = random(2,7,1);
$ASQ = $A**2;
$AD = $A*2;

$mc = new_checkbox_multiple_choice();
$mc -> qa (
"Select all solutions of  
\( $ASQ u_t = u_{xx} \).  There may be more than
one correct answer.", 
"\( u = e^{$A x + t} \)$BR",
"\( u = $A e^{$A x + t} \)$BR",                
"\( u = e^{$A x + t + $AD} \)$BR",
"\( u = 0 \)$BR",
"\( u = e^{$A x + t} + $A x + $AD\)$BR",
);
$mc -> extra(
"\( u = e^{$A x - t} \)$BR",
"\( u = $A e^{x+t} \)$BR",
"\( u = e^{$AD x - $ASQ t} \)$BR",
"\( u = e^{$AD x + t } \)$BR",
"\( u = e^{$A x + t} + $A t + $AD\)$BR",
);
$mc -> makeLast("None of the above");

###########################
# Main text

BEGIN_TEXT
\{ $mc -> print_q() \}
$BR
\{ $mc -> print_a() \}
END_TEXT

###########################
# Answer evaluation


install_problem_grader(~~&std_problem_grader);

$showPartialCorrectAnswers = 0;

ANS( checkbox_cmp( $mc->correct_ans() ) );


ENDDOCUMENT();        
