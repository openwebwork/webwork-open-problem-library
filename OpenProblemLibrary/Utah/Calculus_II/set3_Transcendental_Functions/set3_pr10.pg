TEXT(beginproblem()); 

$showPartialCorrectAnswers = 0;

$A = random(2,9,1);

BEGIN_TEXT

Find \(\displaystyle\int_{-$A}^{$A}\frac{\displaystyle\cosh x}{\displaystyle 1 + \sinh^2 x}\, dx\).

$PAR

Answer: \{ans_rule(40)\}.

END_TEXT

$ans = 2 * atan(sinh($A));
ANS(num_cmp($ans));

