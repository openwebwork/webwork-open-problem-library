DOCUMENT();

loadMacros('PG.pl',
         'PGbasicmacros.pl',
         'PGchoicemacros.pl',
         'PGanswermacros.pl',
         'PGauxiliaryFunctions.pl'
);

TEXT(beginproblem());

$showPartialCorrectAnswers = 0;

$A = random(2,6,1);
$F = random(1,6,1);
$G = random(1,6,1);
$D = random(2,6,1);
$B = $A + $F;
$C = $D + $G;


BEGIN_TEXT

Find
\[\displaystyle\int\frac{\displaystyle dx}{\displaystyle ( $A x + $B) 
\sqrt{ $C x + $D }}\]

$PAR

Answer: \{ans_rule(80)\} \( + C. \)

END_TEXT

$ans = "2/(sqrt($A*$B*$C-$A**2*$D)) * atan(sqrt(($A*$C*x+$A*$D)/($B*$C-$A*$D)))";
ANS(fun_cmp($ans, vars=>"x"));

ENDDOCUMENT();


