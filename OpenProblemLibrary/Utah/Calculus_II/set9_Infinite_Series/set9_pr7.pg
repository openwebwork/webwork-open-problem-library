$showPartialCorrectAnswers = 1;

BEGIN_TEXT

Find the power series representation for 

\[ f(x) = x e^{x^{2}}. \]

$PAR

\[ f(x) = \sum_{n=0}^{\infty} \dfrac{1}{a_{n}!} x^{p_{n}}, \] 

where \( a_{n} = \) \{ans_rule(20)\} and \( p_{n} = \) \{ans_rule(20)\}.

END_TEXT

$ans1 = "n";
$ans2 = "2*n+1";
ANS(fun_cmp($ans1, vars=>"n"));
ANS(fun_cmp($ans2, vars=>"n"));
