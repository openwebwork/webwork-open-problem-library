DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

$showPartialCorrectAnswers = 0;
$A = random(3,9,1);
$B = random(2,5,1);
$C = random(6,9,1);


BEGIN_TEXT

Let  \( f(x) = $A +  ( $B  x + $C   ) ^3 \) .
$PAR

Find  \( f^{-1}(x) = \) \{ans_rule(50) \} 
$PAR

Find  \(  \bigl( f(x) \bigr)^{-1} = \) \{ans_rule(50) \} 
$PAR

END_TEXT

$ans1 = "( ( x - $A )^(1/3) - $C ) / $B";

$ans2 = "1 / ( $A + ( $B * x + $C )^3 )";

ANS(fun_cmp($ans1, limits=>[[10,12]], vars=>["x"]));
ANS(fun_cmp($ans2, vars=>"x"));

ENDDOCUMENT();
