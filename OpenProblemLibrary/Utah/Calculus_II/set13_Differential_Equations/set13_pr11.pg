  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem()); 

$showPartialCorrectAnswers = 1;

BEGIN_TEXT

Solve the following differential equation:

\[ y'' + 9y  = 0; \quad y = 3, \; y'=3 \;\text{at}\; x = \pi/3 \]

Answer: \(y(x) = \) \{ans_rule(80)\}.
END_TEXT

$ans = "-sin(3*x)-3*cos(3*x)";
ANS(fun_cmp($ans, vars=>"x"));

