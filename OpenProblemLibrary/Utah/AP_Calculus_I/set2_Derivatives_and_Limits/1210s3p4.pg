DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

$b=random(2,9,2);
$a=random(3,9,2);
$A=$a+$b;
$B=$a*$b;

TEXT(beginproblem());

TEXT(EV2(<<EOT)); 


$BR
\(\displaystyle\lim_{x\longrightarrow$b} \frac{x^2-$A x + $B}{x-$b} = \)
 \{ ans_rule(20) \}.  
EOT
ANS(num_cmp($b-$a));



