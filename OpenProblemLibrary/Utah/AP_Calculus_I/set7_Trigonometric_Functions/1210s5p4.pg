DOCUMENT();        # This should be the first executable line in the problem.

loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
"PGauxiliaryFunctions.pl");

TEXT(beginproblem());

TEXT(EV2(<<EOT));
Let
\[f(x) = \sin\frac{1}{x}.\]
$BR
\(f'(x) = \) \{&ans_rule(45)\}.
EOT
$ans="-cos(1/x)/(x*x)";
ANS(fun_cmp($ans, limits=>[[0.1,1]], vars=>["x"]));

TEXT(EV2(<<EOT));
$BR
Let
\[g(x) = \frac{1}{\sin x}.\]
$BR
\(g'(x) = \) \{&ans_rule(45)\}.
EOT
$ans="-cos(x)/sin(x)**2";
ANS(fun_cmp($ans, limits=>[[0.1,1]], vars=>["x"]));






