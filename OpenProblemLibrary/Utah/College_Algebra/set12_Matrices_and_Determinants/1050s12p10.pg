DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);


#install_problem_grader(~~&std_problem_grader);
TEXT(beginproblem());
#$showPartialCorrectAnswers = 0;
#$showPartialCredit = 0;


$a11=random(-9,9,1); 
$a12=random(-9,9,1);
$a13=random(-9,9,1);
$a21=random(-9,9,1); 
$a22=random(-9,9,1);
$a23=random(-9,9,1);

$b11=random(-9,9,1); 
$b12=random(-9,9,1);
$b21=random(-9,9,1);
$b22=random(-9,9,1); 
$b31=random(-9,9,1);
$b32=random(-9,9,1);







TEXT(EV2(<<EOT)); 

Let 
\[A = 
\left[\begin{array}{rrr}
$a11 & $a12 & $a13 \\
$a21 & $a22 & $a23  \\
\end{array}\right]\]
 
and

\[B = 
\left[\begin{array}{rr}
$b11 & $b12 \\
$b21 & $b22 \\
$b31 & $b32 \\
\end{array}\right]\]

Then \[
C = AB =
\left[\begin{array}{rrr}
c_{11} & c_{12}  \\
c_{21} & c_{22}  \\
\end{array}\right],
\]

and



\[
D = BA = 
\left[\begin{array}{rrr}
d_{11} & d_{12} & d_{13} \\
d_{21} & d_{22} & d_{23} \\
d_{31} & d_{32} & d_{33} \\
\end{array}\right],
\]

where

$BR
\(c_{11} \) =  \{ ans_rule(10) \},

EOT
ANS(num_cmp($a11*$b11 + $a12*$b21 + $a13*$b31 ));

TEXT(EV2(<<EOT)); 
$BR
\(c_{12} \) =  \{ ans_rule(10) \},
EOT
ANS(num_cmp( $a11*$b12 + $a12*$b22 + $a13*$b32 ));

TEXT(EV2(<<EOT)); 
$BR
\(c_{21} \) =  \{ ans_rule(10) \},
EOT
ANS(num_cmp($a21*$b11 + $a22*$b21 + $a23*$b31 ));

TEXT(EV2(<<EOT)); 
$BR
\(c_{22} \) =  \{ ans_rule(10) \},
EOT
ANS(num_cmp($a21*$b12 + $a22*$b22 + $a23*$b32 ));


TEXT(EV2(<<EOT)); 
$BR
and
$BR
\(d_{11} \) =  \{ ans_rule(10) \},

EOT
ANS(num_cmp($a11*$b11 + $a21*$b12 ));

TEXT(EV2(<<EOT)); 
$BR
\(d_{12} \) =  \{ ans_rule(10) \},
EOT
ANS(num_cmp($a12*$b11 + $a22*$b12 ));

TEXT(EV2(<<EOT)); 
$BR
\(d_{13} \) =  \{ ans_rule(10) \},
EOT
ANS(num_cmp($a13*$b11 + $a23*$b12 ));

TEXT(EV2(<<EOT)); 
$BR
\(d_{21} \) =  \{ ans_rule(10) \},
EOT
ANS(num_cmp($a11*$b21 + $a21*$b22 ));

TEXT(EV2(<<EOT)); 
$BR
\(d_{22} \) =  \{ ans_rule(10) \},
EOT
ANS(num_cmp($a12*$b21 + $a22*$b22 ));

TEXT(EV2(<<EOT)); 
$BR
\(d_{23} \) =  \{ ans_rule(10) \},
EOT
ANS(num_cmp($a13*$b21 + $a23*$b22 ));

TEXT(EV2(<<EOT)); 
$BR
\(d_{31} \) =  \{ ans_rule(10) \},
EOT
ANS(num_cmp($a11*$b31 + $a21*$b32 ));

TEXT(EV2(<<EOT)); 
$BR
\(d_{32} \) =  \{ ans_rule(10) \},
EOT
ANS(num_cmp($a12*$b31 + $a22*$b32 ));

TEXT(EV2(<<EOT)); 
$BR
\(d_{33} \) =  \{ ans_rule(10) \}. 
EOT
ANS(num_cmp($a13*$b31 + $a23*$b32 ));

TEXT(EV2(<<EOT)); 
$BR
The determinant of \(D\) is \{ ans_rule(10) \}.
$BR
EOT
ANS(num_cmp(0));


SOLUTION(EV3(<<'EOT')); 
$BR $BBOLD Solution:$EBOLD Whenever you multiply an \(n\times p\)
matrix with a \(p\times n\) matrix, the resulting \(n\times n\) matrix
has a zero determinant if \(p < n\).
EOT



ENDDOCUMENT();  
