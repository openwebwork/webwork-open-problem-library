DOCUMENT();        # This should be the first executable line in the problem.

loadMacros("PG.pl", "PGbasicmacros.pl", "PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl");

install_problem_grader(~~&std_problem_grader);TEXT(beginproblem());

$showPartialCorrectAnswers = 1;
$showPartialCredit = 0;


# output the text of the problem

$a = random(2,9,1);
$b = random(2,9,1);
$c = random(2,9,1);
$d = random(2,9,1);
$e = random(2,9,1);
$f = random(2,9,1);
$g = random(2,9,1);

BEGIN_TEXT

Let

\[ A=\left(\begin{array}{ccc} 
            $a & 1 & $c \cr
	    $c & -1 & $f
	   \end{array}\right), 
 B=\left(\begin{array}{cc} 
            $b & 0 \cr
	    $d & -$d \cr
	    $e & $g 
	   \end{array}\right) .\]

Then

\[ AB=\left(\begin{array}{cc} 
            a_{11} & a_{12} \cr
	    a_{21} & a_{22} 
	    \end{array}\right) \]

where 
\( a_{11}= \) \{ans_rule(3)\},
\( a_{12}= \) \{ans_rule(3)\},
\( a_{21}= \) \{ans_rule(3)\},
\( a_{22}= \) \{ans_rule(3)\},

\[ BA=\left(\begin{array}{ccc} 
            b_{11} & b_{12} & b_{13} \cr
	    b_{21} & b_{22} & b_{23} \cr
	    b_{31} & b_{32} & b_{33}
	   \end{array}\right) \]

where 
\( b_{11}= \) \{ans_rule(3)\},
\( b_{12}= \) \{ans_rule(3)\},
\( b_{13}= \) \{ans_rule(3)\},
\( b_{21}= \) \{ans_rule(3)\},
\( b_{22}= \) \{ans_rule(3)\},
\( b_{23}= \) \{ans_rule(3)\},
\( b_{31}= \) \{ans_rule(3)\},
\( b_{32}= \) \{ans_rule(3)\},
\( b_{33}= \) \{ans_rule(3)\},

and

\[ B^T A^T=\left(\begin{array}{cc} 
            c_{11} & c_{12} \cr
	    c_{21} & c_{22} 
	   \end{array}\right) \]

where 
\( c_{11}= \) \{ans_rule(3)\},
\( c_{12}= \) \{ans_rule(3)\},
\( c_{21}= \) \{ans_rule(3)\},
\( c_{22}= \) \{ans_rule(3)\}.

END_TEXT

$ans1 = $a*$b+$d+$c*$e ; 
$ans2 = -$d+$c*$g ; 
$ans3 = $c*$b-$d+$f*$e ;
$ans4 = $d+$f*$g ;
$ans5 = $a*$b ; 
$ans6 = $b ; 
$ans7 = $b*$c;
$ans8 = ($a-$c)*$d ;
$ans9 = 2*$d ;
$ans10 = ($c-$f)*$d ;
$ans11 = $a*$e+$c*$g ;
$ans12 = $e-$g ;
$ans13 = $c*$e+$f*$g ;
$ans14 = $a*$b+$d+$c*$e ;
$ans15 = $c*$b-$d+$f*$e ;
$ans16 = -$d+$c*$g ;
$ans17 = $d+$f*$g ;

ANS(num_cmp($ans1));
ANS(num_cmp($ans2));
ANS(num_cmp($ans3));
ANS(num_cmp($ans4));
ANS(num_cmp($ans5));
ANS(num_cmp($ans6));
ANS(num_cmp($ans7));
ANS(num_cmp($ans8));
ANS(num_cmp($ans9));
ANS(num_cmp($ans10));
ANS(num_cmp($ans11));
ANS(num_cmp($ans12));
ANS(num_cmp($ans13));
ANS(num_cmp($ans14));
ANS(num_cmp($ans15));
ANS(num_cmp($ans16));
ANS(num_cmp($ans17));


ENDDOCUMENT();        # This should be the last executable line in the problem.
