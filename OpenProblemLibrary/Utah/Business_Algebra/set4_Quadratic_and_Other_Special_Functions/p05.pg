$BR
 \{ ans_rule(3) \}
EOT
ANS(str_cmp("["));

TEXT(EV2(<<EOT));
 \{ ans_rule(18) \},
EOT
ANS(num_cmp(0));

TEXT(EV2(<<EOT));
 \{ ans_rule(18) \}
EOT
ANS(str_cmp("infinity"));

TEXT(EV2(<<EOT));
 \{ ans_rule(3) \}.  
$BR

EOT
