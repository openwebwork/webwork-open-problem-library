$showPartialCorrectAnswers = 1;


$a = random(2,9,1);
$power = $a + 1;
$coeff = 1 / $power;
$ans = "(ln(x)^$power)/$power ";

TEXT(beginproblem());
BEGIN_TEXT
Evaluate the indefinite integral.
\[ \int \frac{(\ln(x))^$a}{x} dx \]
$BR
Hint: you should let \(u=\ln(x)\), and go from there.
$BR
\{ans_rule( 30) \} \(+C\)
$PAR

END_TEXT

ANS(fun_cmp($ans, limits=>[1,2], mode=>"antider", vars=>'x'));

