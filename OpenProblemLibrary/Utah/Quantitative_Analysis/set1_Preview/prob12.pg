DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

$a = random(1,9,1);
$b = random(1,9,1);
$c = random(1,9,1);
$d = random(1,9,1);

TEXT(EV2(<<EOT));
$BR
Evaluate the expression
$BR \($a \times $b - $c \times $d \) =\{ ans_rule(20) \}
$BR
EOT
$answer = $a *$b - $c* $d ;
ANS(num_cmp($answer, mode=>"strict"));

TEXT(EV2(<<EOT));
$BR
Evaluate the expression
$BR \($a \times ($b - $c) \times $d \) =\{ ans_rule(20) \}
$BR
EOT
$answer = $a * ($b - $c)*  $d ;
ANS(num_cmp($answer, mode=>"strict"));

TEXT(EV2(<<EOT));
$BR
Evaluate the expression
$BR \($a \times ($b - $c \times $d) \) =\{ ans_rule(20) \}
$BR
EOT
$answer = $a * ($b - $c*  $d); 
ANS(num_cmp($answer, mode=>"strict"));




ENDDOCUMENT();        # This should be the last executable line in the problem.
