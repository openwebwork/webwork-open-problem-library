DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGML.pl",
  "MathObjects.pl",
  "parserPopUp.pl",
  "PGcourse.pl"
);
 
$showPartialCorrectAnswers = 1;
TEXT(beginproblem());

Context("Interval"); 

$a = random(2, 6, 1);
$b = random(-3, 2, 1); 
$c = random(3, 7, 1);
$d = $a*$b;
$e = $a*$c;
$f = $b/$a;
$g = $c/$a;

BEGIN_PGML
Let [`f(x) = [$a]x`].  Let [`S = ([$b], [$c]]`].  

What is [`f (S)`]? [____________]{Compute("($d, $e]")}

What is [` f^{-1} (S)`]? [____________]{Compute("($f, $g]")}

END_PGML

    ENDDOCUMENT();
