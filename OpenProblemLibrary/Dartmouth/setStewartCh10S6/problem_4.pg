## Do NOT show partial correct answers
$showPartialCorrectAnswers = 0;

$a = random(2,20);
$b = random($a+1,25);


## Ok, we are ready to begin the problem...
##
TEXT(beginproblem());


BEGIN_TEXT
$BR
Solve the initial value problem
\[ t^{$a}\frac{dy}{dt} + 2t^{\{$a-1\}} y = t^{$b}    \]
with \(t > 0\) and \( y(1) = 0\).
$BR
\{ans_rule(60)\}
$PAR
END_TEXT

    ANS(fun_cmp("(t**($b-$a+1) -1/t**2)/($b-$a+3)", vars=>['t']  ));
ENDDOCUMENT();




