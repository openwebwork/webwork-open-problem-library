DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "contextFraction.pl",
  "answerHints.pl",
  "PGcourse.pl"
);
#for currency use Context("Currency") then Currency($A);
#Then, in the text use $DOLLAR $a
TEXT(beginproblem());
$showPartialCorrectAnswers = 1;
Context("LimitedFraction")->flags->set(
      requireProperFractions=>1,
      showProperFractions =>1,
);
$pd1=random(3,7,2);
$qint1 = random (1,5,1);
$qfnum1= random(1,6,1);
$p1=Compute("1/$pd1");
$q1=Compute("$qint1 $qfnum1/7");

$sd1=random(3,7,2);
$tint1 = random (1,2,1);
$tfnum1= random(1,6,1);
$s1=Compute("1/$sd1");
$t1=Compute("$tint1 $tfnum1/7");
Context("Numeric");
$k1=$p1**2*$q1;
$k2=($t1**2)/$s1;
#for solution
$qnum=7*$qint1+$qfnum1;
Context("LimitedFraction")->flags->set(
      requireProperFractions=>1,
      showProperFractions =>1,
);
$ans1 = Compute("$k1"); 
$ans2=  Compute("$k2");

Context()->texStrings;
BEGIN_TEXT
$PAR Evaluate each of the following expressions. $BR
Your answer must be in simplest form [ a proper fraction or mixed number]
$PAR
If \(p=$p1 \) and \(q=$qint1 \frac{ $qfnum1}{7} \) then \(p^2q=\) \{ans_rule(5)\}
$PAR
If \(s=$s1 \) and \(t=$tint1 \frac{ $tfnum1}{7} \) then \(t^2/s=\) \{ans_rule(5)\}

END_TEXT
Context()->normalStrings;

ANS($ans1->cmp);
ANS($ans2->cmp);
BEGIN_SOLUTION
$PAR Solution $PAR
\(p^2q=(\frac{1}{$pd1^2})($qint1\frac{ $qnum1}{7})=(\frac{1}{$pd1^2})(\frac{$qnum}{7})=  $ans1\)
END_SOLUTION	


;
ENDDOCUMENT();

