# WeBWorK problem written by Carl Yao
# Portland Community College
#
# Template
#
# Evaluate these expressions:
#
#    [`` [$c]([$a]-[$b])^{2} - [$c]([$a]-[$b]^{2}) =  ``]  [________]{$ans} 
# 
# Last updated: Jordan 7/25/13; Hughes, 6/26/13
# ENDDESCRIPTION

## DBsubject('Algebra')
## DBchapter('Basic Algebra')
## DBsection('Algebraic Expressions')
## KEYWORDS('integer','order of operations','integer exponent','subtract','evaluate')
## DBCCSS('6.EE.1')
## TitleText1('')
## EditionText1('')
## AuthorText1('')
## Section1('')
## Problem1('')
## Author('Alex Jordan, Carl Yao, Chris Hughes')
## Institution('PCC')

##############################################



DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGML.pl",
  "PGcourse.pl",
);

##############################################

Context("LimitedNumeric");

$b=random(2,4,1);
$a=$b+random(3,5,1);
$c=random(2,6,1);
while ($c==$b) {$c=random(2,6,1);}

$ans=Compute($c*($a-$b)**2-$c*($a-$b**2));

##############################################

TEXT(beginproblem());

BEGIN_PGML

Evaluate the following.

    [`` [$c]([$a]-[$b])^{2} - [$c]([$a]-[$b]^{2}) =  ``]  [________]{$ans} 

END_PGML

##############################################

$step1 = $a-$b;
$step2 = ($b)**2;
$step3 = $a-$step2;
$step4 = ($step1)**2;
$step5 = $c*$step4;
$step6 = $c*$step3;

BEGIN_PGML_SOLUTION

    [`
       \begin{aligned}
       \quad [$c]([$a]-[$b])^{2} - [$c]([$a]-[$b]^{2})  & = [$c]\cdot[$step1]^{2} - [$c]([$a]-[$b]^{2}) \\
              & = [$c]\cdot[$step1]^{2} - [$c]([$a]-[$step2]) \\
& = [$c]\cdot[$step1]^{2} - [$c]\cdot[$step3] \\
& = [$c]\cdot[$step4] - [$c]\cdot[$step3] \\
& = [$step5] - [$c]\cdot[$step3] \\
& = [$step5] - [$step6] \\
& = [$ans] 
\end{aligned}
`]

END_PGML_SOLUTION

ENDDOCUMENT();


