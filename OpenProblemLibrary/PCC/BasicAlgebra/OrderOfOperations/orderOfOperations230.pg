##############################################

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGML.pl",
  "PGcourse.pl",
);

##############################################

Context("LimitedNumeric");

$a=random(2,10,2);
$b=random(3,9,2);

$ans1= Compute((abs($a-$b))**2-abs($a-$b**2));
$ans2= Compute($a-$b)**2-($a-$b**2);

##############################################

TEXT(beginproblem());

BEGIN_PGML

Evaluate these expressions:

a)    [`` \left\lvert [$a]-[$b]\right\rvert ^{2}-\left\lvert [$a]-[$b]^{2}\right\rvert  =  ``]  [________]{$ans1} 

b)    [`` ([$a]-[$b])^{2}-([$a]-[$b]^{2}) =  ``]  [________]{$ans2} 

END_PGML

##############################################

$step11 = $a-$b;
$step12 = $b**2;
$step13 = $a-$step12;
$step14 = abs($step11);
$step15 = abs($step13);
$step16 = $step14**2;

BEGIN_PGML_SOLUTION

a)   [`\begin{aligned}[t]
 \left\lvert [$a]-[$b]\right\rvert ^{2}-\left\lvert [$a]-[$b]^{2}\right\rvert   & =  \left\lvert [$step11]\right\rvert ^{2}-\left\lvert [$a]-[$b]^{2}\right\rvert  \\
& =  \left\lvert [$step11]\right\rvert ^{2}-\left\lvert [$a]-[$step12]\right\rvert  \\
& =  \left\lvert [$step11]\right\rvert ^{2}-\left\lvert [$step13]\right\rvert  \\
& = [$step14]^{2}-[$step15] \\
& = [$step16]-[$step15] \\
& = [$ans1] 
\end{aligned}`].

b)   [`\begin{aligned}[t]
 ([$a]-[$b])^{2}-([$a]-[$b]^{2}) & =  ([$step11])^{2}-([$a]-[$b]^{2}) \\
& =  ([$step11])^{2}-([$a]-[$step12]) \\
& =  ([$step11])^{2}-([$step13]) \\
& = [$step16]-([$step13]) \\
& = [$ans2] 
\end{aligned}`]

END_PGML_SOLUTION

ENDDOCUMENT();


