# ENDDESCRIPTION

## DBsubject('Algebra')
## DBchapter('Basic Algebra')
## DBsection('Algebraic Expressions')
## KEYWORDS('integer','order of operations','integer exponent','subtract','divide','evaluate')
## DBCCSS('6.EE.1')
## TitleText1('')
## EditionText1('')
## AuthorText1('')
## Section1('')
## Problem1('')
## Author('Alex Jordan, Carl Yao, Chris Hughes')
## Institution('PCC')

##############################################



DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGML.pl",
  "contextFraction.pl",
  "PGcourse.pl",
);

##############################################

Context("LimitedFraction");
Context()->flags->set(showMixedNumbers=>0,requirePureFractions=>1,requireProperFractions=>1);

@multiples = (2, 3, 4, 5, 6);

$a=list_random((1,27));
$b=list_random((2,4));
$c=random(1,10,1);
$d=$a**(1/3)+$b+$c;

$ans=Fraction(($a-(-$b)**3),($c-$d));


##############################################

TEXT(beginproblem());

BEGIN_PGML

Evaluate the following.

    [`` \frac{[$a]-(-[$b])^{3}}{[$c]-[$d]} =  ``]  [________]{$ans}

END_PGML

##############################################

$step1 = $b**3;
$step2 = $a+$step1;
$step3 = $c-$d;

BEGIN_PGML_SOLUTION

    [`
\begin{aligned}
\frac{[$a]-(-[$b])^{3}}{[$c]-[$d]}  & = \frac{[$a]-(-[$step1])}{[$c]-[$d]}  \\
& = \frac{[$a]+[$step1]}{[$c]-[$d]}  \\
& = \frac {[$step2]} {[$step3]}  \\
& = [$ans] 
\end{aligned}
`]

END_PGML_SOLUTION

ENDDOCUMENT();


