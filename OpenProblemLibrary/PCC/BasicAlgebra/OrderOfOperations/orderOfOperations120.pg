# WeBWorK problem written by Carl Yao
# Portland Community College
#
# Template
#
# Evaluate this expression:
#
#    [`` ([$a]\cdot[$b])^{2} - [$a]\cdot[$b]^{2} =  ``]  [________]
# 
# Last updated: Hughes, 6/26/13
# ENDDESCRIPTION

## DBCCSS('6.EE.1')
## DBsubject(Arithmetic)
## DBchapter(Integers)
## DBsection(Multiple operations)
## Institution(PCC)
## Author(Alex Jordan, Carl Yao, Chris Hughes)
## Level(3)
## MO(1)
## KEYWORDS('integer','order of operations','integer exponent','subtract','multiply','evaluate')

##############################################



DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGML.pl",
  "answerHints.pl",
  "PGcourse.pl"
);

##############################################

Context("LimitedNumeric");

$a=random(2,5,1);
$b=random(2,5,1);
while ($b==$a) {$b=random(2,5,1);}

$ans=Compute(($a*$b)**2-($a*$b**2));

##############################################

TEXT(beginproblem());

BEGIN_PGML

Evaluate this expression:

    [`` ([$a]\cdot[$b])^{2} - [$a]\cdot[$b]^{2} =  ``]  [________]

END_PGML

##############################################


$wrong=0;
ANS($ans -> cmp() ->
  withPostFilter(AnswerHints(
    [Compute("$wrong")] =>
    "Note that parentheses make all the difference.")));

$step1 = $a*$b;
$step2 = ($step1)**2;
$step3 = ($b)**2;
$step4 = $a*$step3;

BEGIN_PGML_SOLUTION

    [`\begin{aligned}
          ([$a]\cdot[$b])^{2} - [$a]\cdot[$b]^{2}  & = ([$step1])^{2} - [$a]\cdot[$b]^{2} \\
& = [$step2] - [$a]\cdot[$b]^{2} \\
& = [$step2] - [$a]\cdot[$step3] \\
& = [$step2] - [$step4] \\
& = [$ans] 
\end{aligned}`]

END_PGML_SOLUTION

ENDDOCUMENT();


