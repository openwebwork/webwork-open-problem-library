## DBsubject('Algebra')
## DBchapter('Basic Algebra')
## DBsection('Algebraic Expressions')
## KEYWORDS('integer','negative','operation','add','subtract','evaluate')
## DBCCSS('6.EE.1')
## TitleText1('')
## EditionText1('')
## AuthorText1('')
## Section1('')
## Problem1('')
## Author('Alex Jordan, Carl Yao, Chris Hughes')
## Institution('PCC')


##############################################

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGML.pl",
  "PGcourse.pl",
);

##############################################

Context("LimitedNumeric");

do {
$a = random(1,10,1);
$a2 = $a**2;
$b = random(2,9,1);
$c = random(2,9,1);
$d = $a2-$b*$c;

$den = random(2,9,1)*random(-1,1,2);
$ans = non_zero_random(-9,9,1);
$num = $den*$ans;

$e = random(-20,-10,1);
$f = -($num-$a)-$e;

} until ($f<0);

$g = random(-5,-1,1);
$h = $den+$g**3;


##############################################

TEXT(beginproblem());

BEGIN_PGML

Evaluate the following.

    [`` \frac{ \sqrt{[$d]+[$b]\cdot[$c]} + \left\lvert [$e]+[$f] \right\rvert }{[$h]-([$g])^3}=``][________]{$ans}  

END_PGML

##############################################

$s1 = -$e-$f;
$s2 = -$g**3;

BEGIN_PGML_SOLUTION

    [``
\begin{aligned}[t] 
   \frac{ \sqrt{[$d]+[$b]\cdot[$c]} + \left\lvert [$e]+[$f] \right\rvert }{[$h]-([$g])^3}  & = \frac{ \sqrt{[$d]+[$b*$c]} + \left\lvert [$e+$f] \right\rvert }{[$h]-([$g**3])}  \\
& = \frac{ \sqrt{[$a2]} +[$s1] }{[$h]+[$s2]}  \\
& = \frac{ [$a] +[$s1] }{[$den]}  \\
& = \frac{ [$num] }{[$den]}  \\
   & = [$ans]
\end{aligned}
    ``]

END_PGML_SOLUTION

ENDDOCUMENT();


