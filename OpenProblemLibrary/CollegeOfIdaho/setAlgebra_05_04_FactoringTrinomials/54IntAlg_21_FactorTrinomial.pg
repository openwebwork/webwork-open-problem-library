DOCUMENT(); # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "answerHints.pl",
  "CofIdaho_macros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

######################################
#  Setup
#  Note: This polynomial does not factor.

$b = random(2,4,1);
$a = $b-1;
if ($b==2) {$a=3;}
do {
   $c= random(2,13,1); 
   } until ($b*$b<4*$a*$c);

$p = Formula("$a x^2 + $b x + $c");
$polynomial = $p->reduce->TeX;

######################################
#  Main text

BEGIN_TEXT
Factor:  
$PAR
\( $polynomial = \) \{ ans_rule(30) \} 
$BR$BR
If the polynomial does not factor, enter "Does not factor"
END_TEXT

######################################
#  Answer

$showPartialCorrectAnswers = 1;

$answer = $p; 
ANS(FactoringEvaluator("Does not factor",$answer,"x"));

######################################
# Solution

Context()->texStrings;
BEGIN_SOLUTION
$BR
Answer: Does not factor
END_SOLUTION
Context()->normalStrings;


;
ENDDOCUMENT();






























