DOCUMENT(); # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "CofIdaho_macros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

######################################
#  Setup

$a= list_random(2,3,5,6,7);
do {$b= list_random(2,3,5,6,7);} while ($a==$b);

$polynomial = Formula("x^4 - ($a + $b) x^2 + ($a*$b)")->reduce->TeX;

######################################
#  Main text

BEGIN_TEXT
Factor:  
$PAR
\( $polynomial = \) \{ ans_rule(30) \} 
END_TEXT

######################################
#  Answer

$answer="(x^2 - $a) * (x^2 - $b)";
ANS(FactoringEvaluator($answer,"x")); 

$showPartialCorrectAnswers = 1;

######################################


;
ENDDOCUMENT();



























