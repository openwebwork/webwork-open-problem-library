DOCUMENT(); # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "CofIdaho_macros.pl"
);

TEXT(beginproblem());

######################################
#  Setup

@alphabet = ("a","b","s","t","x","y");
$n = random(0,5,1);
$var = $alphabet[$n];
Context()->variables->are($var=>'Real');

$a= random(2,7,1);
do {$b= random(2,7,1);} while ($a==$b);
$ab= $a * $b;

######################################
#  Main text

BEGIN_TEXT
Factor by grouping:  
$PAR
\(  $var^2 + $a $var + $b $var + $ab =\) \{ ans_rule(30) \} 
END_TEXT

######################################
#  Answer

$answer = "($var + $b) * ($var + $a)";
ANS(FactoringEvaluator($answer,$var)); 

$showPartialCorrectAnswers = 1;

######################################


COMMENT('MathObject version');
ENDDOCUMENT();


























