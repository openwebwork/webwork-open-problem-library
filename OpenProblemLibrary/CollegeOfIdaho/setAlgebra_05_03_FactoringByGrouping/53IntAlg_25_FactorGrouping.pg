DOCUMENT(); # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "CofIdaho_macros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());

######################################
#  Setup

@alphabet = ("a","b","s","t","x","y");
$n = random(0,4,2);
$var1 = $alphabet[$n];
$var2 = $alphabet[$n+1];
Context()->variables->are($var1=>'Real',$var2=>'Real');

$a= random(2,5,1);
do {$b= random(2,7,1);} until (gcd($a,$b)==1);
$ab= $a * $b;

######################################
#  Main text

BEGIN_TEXT
Factor by grouping:  
$PAR
\( $var1^2 + $a $var1 + $b $var1 $var2 + $ab $var2 = \) \{ ans_rule(30) \} 
END_TEXT

######################################
#  Answer

$answer = "($var1 + $b*$var2) * ($var1 + $a)";
ANS(FactoringEvaluator($answer,[$var1,$var2])); 

$showPartialCorrectAnswers = 1;

######################################


;
ENDDOCUMENT();


























