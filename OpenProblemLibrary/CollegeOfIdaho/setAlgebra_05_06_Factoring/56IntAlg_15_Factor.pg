DOCUMENT(); # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "CofIdaho_macros.pl"
#####Doesn't do non-x variables with higher powers well
#  "contextPolynomialFactors.pl",  
#  "contextLimitedPowers.pl"
#####Put back when fixed
);

TEXT(beginproblem());

######################################
#  Setup

@alphabet = ("a","b","s","t","x","y");
$n = random(0,4,2);
$var1= $alphabet[$n];
$var2= $alphabet[$n+1];
Context()->variables->are($var1=>'Real',$var2=>'Real');

$a = random(1,4,1);  #$a and $b must be relatively prime.
do {$b = random(2,5,1);} until (gcd($a,$b)==1);
$c = random(2,10,2);

$polynomial = Formula("($a^2*$c)$var1^3 $var2- ($b^2*$c)$var1 $var2")->reduce->TeX;

######################################
#  Main text

BEGIN_TEXT
Factor completely: 
$PAR
\( $polynomial = \)  \{ ans_rule(30) \} 
END_TEXT

######################################
#  Answer

$answer = "$c*$var1*$var2*($a*$var1 - $b)* ($a*$var1 + $b)";
ANS(FactoringEvaluator($answer,["$var1","$var2"]));
#ANS(FactoringEvaluator($answer,["x","y"]));  

$showPartialCorrectAnswers = 1;

######################################
#  Solution

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR

\($polynomial = $factoredform\)

END_SOLUTION
Context()->normalStrings;

COMMENT('MathObject version');
ENDDOCUMENT();
