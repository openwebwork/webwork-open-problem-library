);

TEXT(beginproblem());
$showpartialcorrectanswers = 1;
Context('Numeric');

$a = non_zero_random(-12, 13, 1);
$b = non_zero_random(-8,7,1);
$c = random(2,7,1);
$d = random(3,9,1);
$f = random(2,12,1);

BEGIN_TEXT
Consider the function \(f(x)\), for which \(f(0) = $c\) and
\( f'(0) = $a\).
$BR$BR
Find \( h'(0) \) for the function \( h(x) = \frac{1}{f(x)} \).
$BR$BR
\( h'(0) \) = \{ans_rule(30) \}
END_TEXT

$ans = Compute("(-$a)/(($c)**2)");
ANS($ans->cmp);


