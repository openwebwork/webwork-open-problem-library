TEXT(beginproblem());
$showpartialcorrectanswers = 1;

$a = random(2, 8, 1);
$b = random(2,7,1);
$c = random(2,7,1);
$r1 = random(1,12,1);

TEXT(EV2(<<EOT));
Let \( f(x) =($b x -$a x^3) ($c+\sqrt{x}) \).
Find \( f'(x) \).
$BR
\( f'(x)\) = \{ans_rule(40) \}
$BR
$BR
EOT

$ans = "($b-3*$a*(x)**2)*($c+sqrt(x))+($b*x -$a*x**3)*(1/(2*sqrt(x)))";
ANS(fun_cmp($ans, vars=>"x"));


