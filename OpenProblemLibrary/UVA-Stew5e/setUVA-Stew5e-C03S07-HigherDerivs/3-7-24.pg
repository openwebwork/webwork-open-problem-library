loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

Context("Numeric");

$a1 = random(2,5);
$b1 = random(2,8);
 while($b1==$a1) {$b1=random(2,8)};

$f1 = Formula(" -12*$a1*$b1^3/($a1+$b1*x)^4 ");

BEGIN_TEXT
Let \( f(x) = \dfrac{$a1 - $b1 x}{$a1 + $b1 x} \). $BR$BR
\( f'''( x ) = \) \{ans_rule(20) \}
END_TEXT

ANS($f1->cmp);

;

