DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,5,1);
$b = random(2,8,1);
$c = random(2,4,1);
$x1 = random(1,5,1);

$deriv1f="$a*(sec($a*x)**2)";
$deriv1= $a*(sec($a*$x1)**2);
$deriv2f="$a*$a*2*sec($a*x)*sec($a*x)*(tan($a*x))";
$deriv2= $a*$a*2*sec($a*$x1)*sec($a*$x1)*(tan($a*$x1));

TEXT(EV2(<<EOT));
Let \( f(x) =  \tan ($a x) \). $BR
Then \( f'(x) \) is \{ans_rule(20) \} $BR
and \( f'($x1) \) is \{ans_rule(20) \} $BR
EOT

$ans1 = $deriv1f;
$ans2 = $deriv1;

ANS(fun_cmp($ans1));
ANS(num_cmp($ans2));
TEXT(EV2(<<EOT));
\( f''( x ) \) is \{ans_rule(20) \} $BR
and \( f''( $x1 ) \) is \{ans_rule(20) \}
EOT

$ans3 = $deriv2f;
$ans4 = $deriv2;
ANS(fun_cmp($ans3));
ANS(num_cmp($ans4));
ENDDOCUMENT();        # This should be the last executable line in the problem.
