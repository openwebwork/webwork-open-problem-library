DOCUMENT();        # This should be the first executable line in the problem.

loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl");

$a = random(2,8,1);

TEXT(beginproblem());

$showPartialCorrectAnswers = 1;


TEXT(EV2(<<EOT));
Find the derivative of
\[
f(t) = \ln(\sinh(t)).
\]
$BR
$BR
\(f'(t)\) = \{ans_rule(30)\}

EOT

ANS(fun_cmp("cosh(t)/sinh(t)", vars=>"t"));

