  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(2,7,1);
$b1 = random(2,12,1);

$x1 = random(1,5,1);

$deriv1 = $a1*exp($x1);
$funct1 = "$a1*exp(x)";

TEXT(EV2(<<EOT));
If \( f(x) =  $a1 e^x+$b1 \), find \( f'( x ) \).
$BR $BR
\( f'(x) \) = \{ans_rule(30) \}
EOT

$ans = $funct1;
ANS(fun_cmp($ans));

