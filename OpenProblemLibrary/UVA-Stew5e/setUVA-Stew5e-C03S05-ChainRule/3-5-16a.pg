TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a1 = random(2,5,1);
$b1 = random(2,8,1);
$c1 = random(-4,-1,1);
$x1 = random(1,5,1);
$deriv1 = $a1*(tan($b1*$x1)/cos($b1*$x1))*$b1;
$funct1 = "$a1*(tan($b1*x)/cos($b1*x))*$b1";

TEXT(EV2(<<EOT));
Let \[ f(x) =  $a1 \sec ($b1 x) \]
$PAR
\( f'( x ) = \) \{ans_rule(40) \}
$BR
EOT

$ans = $funct1;
ANS(fun_cmp($ans));

