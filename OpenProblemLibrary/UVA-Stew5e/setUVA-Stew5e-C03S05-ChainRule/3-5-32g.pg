DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,4,1);
$b = random(2,4,1);
$c = random(2,4,1);
$d = random(2,4,1);

$m = random(2,5,1);
$n = random(2,5,1);

$e = exp(1);

$gen = random(0,3,1);

if ($gen == 0) {
  $trig1 = "\sin";
  $trig2 = "\sin";
  $ans  = "1/2*(sin($e^(x^$m*sin(x))))^(-1/2) * ".
           "cos($e^(x^$m*sin(x))) " . 
           "$e^(x^$m*sin(x)) * " .
           "($m*x^($m-1)*sin(x) + x^$m*cos(x))";
  }
# Prevent < br/> being inserted into the correct answer by using the " . construction.
#FIXME -- rewrite using MathObjects

if ($gen == 1) {
  $trig1 = "\sin";
  $trig2 = "\cos";
  $ans  = "1/2*(sin($e^(x^$m*cos(x))))^(-1/2) * " .
           "cos($e^(x^$m*cos(x))) * " .
           "$e^(x^$m*cos(x)) * " .
           "($m*x^($m-1)*cos(x) - x^$m*sin(x))";
  }
if ($gen == 2) {
  $trig1 = "\cos";
  $trig2 = "\sin";
  $ans  = "-1/2*(cos($e^(x^$m*sin(x))))^(-1/2) * " .
           "sin($e^(x^$m*sin(x))) * " .
           "$e^(x^$m*sin(x)) * " .
           "($m*x^($m-1)*sin(x) + x^$m*cos(x))";
  }
if ($gen == 3) {
  $trig1 = "\cos";
  $trig2 = "\cos";
  $ans  = "-1/2*(cos($e^(x^$m*cos(x))))^(-1/2) * " .
           "sin($e^(x^$m*cos(x))) * " .
           "$e^(x^$m*cos(x)) * " .
           "($m*x^($m-1)*cos(x) - x^$m*sin(x))";
  }

TEXT(EV2(<<EOT));
Let \[ f(x) = \sqrt{$trig1 (e^{x^{$m} $trig2(x)})} \]
$PAR
\( f'( x ) = \) \{ans_rule(60) \}
$BR
EOT

ANS(fun_cmp($ans, limits=>[0,.35], vars=>"x"));

ENDDOCUMENT();        # This should be the last executable line in the problem.
