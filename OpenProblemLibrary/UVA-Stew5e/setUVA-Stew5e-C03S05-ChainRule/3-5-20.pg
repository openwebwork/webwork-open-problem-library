DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a_numb = random(2,9,1);
$a_sign = random(-1,1,2);
$a      = $a_numb * $a_sign;

$b_numb = random(2,9,1);
$b_sign = random(0,0,2);
$b      = $b_numb * $b_sign;

$c_numb = random(2,9,1);
$c_sign = random(-1,1,2);
$c      = $c_numb * $c_sign;

$d = random(2,9,1);

$e = random(2,9,1);

$n = random(3,15,1);

TEXT(EV2(<<EOT));
Let \[ f(x) = ($a x^2 + $c) \sqrt[$n]{$d x^2 + $e}  \]
$PAR
\( f'( x ) = \) \{ans_rule(80) \}
$BR
EOT


$exp_2 = 1/$n - 1;

$ans = "2*$a*x*($d*x**2+$e)^(1/$n)+($a*x^2+$c)*2*$d*x/$n*($d*x^2+$e)^$exp_2";
ANS(fun_cmp($ans));

ENDDOCUMENT();        # This should be the last executable line in the problem.
