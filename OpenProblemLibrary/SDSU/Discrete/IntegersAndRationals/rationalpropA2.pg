# DESCRIPTION
# Identify the correct property related to rational
# numbers
# WeBWorK problem written by Michael E. O'Sullivan
# and Thomas Schmidt
# ENDDESCRIPTION

## DBsubject(Numbers)
## DBchapter(Real numbers)
## DBsection(Properties)
## Institution(SDSU)
## Author(Thomas Schmidt','Michael E. O'Sullivan)
## Level(1)
## KEYWORDS('')


############################
# Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"PGchoicemacros.pl"
);

TEXT(beginproblem());

############################
# Setup

$showPartialCorrectAnswers = 0;

$expr = "\(r(a + b) =\)";

$correctAns1 = "\(ra+rb\)";
@otherAns1 = ("\(\frac{a}{r} + \frac{b}{r}\)","\(r + a + b\)","\(ra - rb\)");

$correctAns2 = "Distributivity";
@otherAns2 = ("Commutativity","Additive Identity","Additive Inverse","Multiplicative Identity","Associativity");

$radio1 = new_multiple_choice();
$radio1->qa("",$correctAns1);
$radio1->extra(@otherAns1);

$radio2 = new_multiple_choice();
$radio2->qa("",$correctAns2);
$radio2->extra(@otherAns2);

############################
# Main Text

Context()->texStrings;
BEGIN_TEXT
$BR
$expr
\{$radio1->print_a()\}
$BR
$BR
Which property does this expression demonstrate?
\{$radio2->print_a()\}
END_TEXT
Context()->normalStrings;

############################
# Answer Evaluation

ANS(radio_cmp($radio1->correct_ans()));
ANS(radio_cmp($radio2->correct_ans()));

############################
# Solution

Context()->texStrings;
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR Solution: $PAR
$expr $correctAns1 $BR
This property is known as the $BBOLD $correctAns2 $EBOLD $BR
END_SOLUTION
Context()->normalStrings;

ENDDOCUMENT();
