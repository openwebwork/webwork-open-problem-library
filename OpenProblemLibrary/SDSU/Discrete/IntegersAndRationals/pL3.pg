############################
# Initialization

DOCUMENT();       

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGchoicemacros.pl",
  "PGgraphmacros.pl",
  "PGcourse.pl"
);
 
TEXT(beginproblem());

############################
# Setup

$showPartialCorrectAnswers = 0;

@set = ("1","2","3","4","6","12");
$tmp = join(",",@set);
$answer = Compute($tmp);

############################
# Main Text

BEGIN_TEXT
$PAR
List all of the positive common divisors of 48 and 60:
$BR \{ans_rule(10)\} $BR $BR
Note: Enter your answers as a comma-separated list. $BR
[Hint: Your list should begin with 1,2,3]
$PAR $PAR
END_TEXT

############################
# Answer Evaluation

ANS($answer->cmp());

############################
# Solution

SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR
The common divisors of 48 and 60 are: 1, 2, 3, 4, 6, 12 $BR $BR
$PAR
END_SOLUTION

ENDDOCUMENT();
