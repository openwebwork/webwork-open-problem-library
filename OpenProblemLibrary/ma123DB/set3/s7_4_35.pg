DOCUMENT();


loadMacros(
  "PGstandard.pl",
  "PGcourse.pl"
);


TEXT(beginproblem());

$p=random(1,5,1);
$u=random(1,6,1);
$r=random(-4,-1,1);
$a=random(-3,3,1);
$l=2*$a;
$q=$u+$p*$a;
$b=random(1,5,1);
$c=random(-3,3,1);
$d=$p+$r;
$e=$q+($c*$p)+($l*$r);
$k=$a*$a+$b*$b;
$f=($r*$k)+($c*$q);
$g=$l+$c;
$h=$l*$c+$k;
$j=$k*$c;
$hp=$p/2;
$v=$u/$b;
$funct="$hp*ln(x*x+$l*x+$k)+$r*ln(abs(x+$c))+$v*arctan((x+$a)/$b)";

$num = nicestring([$d, $e, $f], ['x^2', 'x', '']);
$den = nicestring([1, $g, $h, $j], ['x^3', 'x^2', 'x', '']);
BEGIN_TEXT
Evaluate the integral.
$BR
$BR \[ \int \frac{$num}{$den} dx \]
$BR Answer: \{ans_rule(80) \} \(+C\)
END_TEXT
$ans=$funct;
ANS(fun_cmp($ans, mode=>'antider'));

ENDDOCUMENT();
