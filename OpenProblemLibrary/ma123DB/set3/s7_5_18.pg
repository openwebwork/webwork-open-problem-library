loadMacros(
  "PGstandard.pl",
  "PGcourse.pl"
);

$showPartialCorrectAnswers = 1;

$a = random(3,9,1);
$ans = "log($a+1)";

TEXT(beginproblem());
BEGIN_TEXT

Evaluate the definite integral. $BR$BR
\[ \int_{1}^{e^$a} \frac{dx}{x (1+\ln x)} \] $BR
Answer: \{ans_rule( 30) \}
$BR
END_TEXT

ANS(num_cmp($ans));

