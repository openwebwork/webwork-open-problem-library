DOCUMENT();
loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

COMMENT('');

Context("Numeric");

{
$a = random(2,9,1);
$b = random(2,9,1);
redo if ($a==$b);
}
$aa = $a*$a;
$bb = $b*$b;
$answer = Formula("x^2-2*$a*$b");

TEXT(beginproblem());

Context()->texStrings;
BEGIN_TEXT
Find \( f \) if 
\( g(x) = $b x+\frac{$a}{x} \) and \( (f\circ g)(x)=$bb x^2+\frac{$aa}{x^2} \).
$PAR
\( f(x) =\)\{ ans_rule(35) \}

END_TEXT
Context()->normalStrings;

ANS( $answer->cmp() );

ENDDOCUMENT();
