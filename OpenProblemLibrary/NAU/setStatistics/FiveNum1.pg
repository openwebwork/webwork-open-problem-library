DOCUMENT();     
loadMacros(
  "PGstandard.pl",
  "extraAnswerEvaluators.pl",
  "PGnauStats.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem) ;
$showPartialCorrectAnswers = 1;

for ($i = 0; $i < 15; $i++){
  push @dat, random(25,100,1);
}

@sort_dat=num_sort(@dat);
$Min=$sort_dat[0];
$Max=$sort_dat[$#sort_dat];

$datSize = $#sort_dat + 1;

$c2=$#sort_dat/2;
$M=$sort_dat[$c2];

$c1=$c2/2-.5;
$Q1=$sort_dat[$c1];
$Q3=$sort_dat[$c2+$c1+1];

$DATA=join(",  ",@dat);

BEGIN_TEXT
$BR
\[ $DATA \]
$BR
Consider the data set above.  Give the five number summary listing values in 
numerical order: $BR $BR

\{ans_rule(5)\} , \{ans_rule(5)\} , \{ans_rule(5)\} , \{ans_rule(5)\} , \{ans_rule(5)\} .$BR$BR



END_TEXT

ANS(num_cmp($Min));
ANS(num_cmp($Q1));
ANS(num_cmp($M));
ANS(num_cmp($Q3));
ANS(num_cmp($Max));

ENDDOCUMENT();
