DOCUMENT();     
loadMacros("PGstandard.pl",
           "extraAnswerEvaluators.pl",
           "PGnauStats.pl"
);

TEXT(&beginproblem) ;
$showPartialCorrectAnswers = 1;

for ($i = 0; $i < 10; $i++){
  push @dat, random(-10,25,1);
}

@sort_dat=num_sort(@dat);
$Min=$sort_dat[0];
$Max=$sort_dat[$#sort_dat];

$datSize = $#sort_dat + 1;

$c2=$#sort_dat/2;
$M=($sort_dat[$c2]+$sort_dat[$c2+1])/2;

$c1=$c2/2;
$Q1=$sort_dat[$c1];
$Q3=$sort_dat[$c2+$c1+1];

$DATA=join(",  ",@dat);

BEGIN_TEXT
$BR
\[ $DATA \] 
$BR
Consider the following data set.  Give the five number summary listing values in 
numerical order: $BR $BR
\{ans_rule(5)\} , \{ans_rule(5)\} , \{ans_rule(5)\} , \{ans_rule(5)\} , \{ans_rule(5)\} .
$BR$BR

END_TEXT

ANS(num_cmp($Min));
ANS(num_cmp($Q1));
ANS(num_cmp($M));
ANS(num_cmp($Q3));
ANS(num_cmp($Max));

ENDDOCUMENT();