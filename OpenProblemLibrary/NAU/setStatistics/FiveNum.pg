DOCUMENT();     
loadMacros("PGstandard.pl",
           "extraAnswerEvaluators.pl",
           "PGnauStats.pl"
);

TEXT(&beginproblem) ;
$showPartialCorrectAnswers = 1;

$n=random(10,30,1);

for ($i = 0; $i < $n; $i++){
  push @dat, random(25,100,1);
}

@ans = FiveNum(@dat);

BEGIN_TEXT
$BR
Consider the following data set.  Give the five number summary listing values in 
numerical order: $BR 
\{ans_rule(5)\} , \{ans_rule(5)\} , \{ans_rule(5)\} , \{ans_rule(5)\} , \{ans_rule(5)\} .$BR$BR
 Data set: \{join ", ", @dat \}
$BR$BR

END_TEXT

ANS(num_cmp($ans[0]));
ANS(num_cmp($ans[1]));
ANS(num_cmp($ans[2]));
ANS(num_cmp($ans[3]));
ANS(num_cmp($ans[4]));

ENDDOCUMENT();