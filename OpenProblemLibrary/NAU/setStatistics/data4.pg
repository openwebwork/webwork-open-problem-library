ANS(arith_num_cmp($wee));
ANS(arith_num_cmp($big));
ANS(arith_num_cmp($mean));
ANS(arith_num_cmp($med));
ANS(arith_num_cmp($numean1));
ANS(arith_num_cmp($numean2));
ANS(arith_num_cmp($numed1));
ANS(arith_num_cmp($numed2));




