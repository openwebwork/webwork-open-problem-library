DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

$showCorrectPartialAnswers = 1;

TEXT(beginproblem());

BEGIN_TEXT
Evaluate the expression 
\( (9x)^{2/3}+(2y)^{2/3}+z^{2/3} \)
using \(x=3, y=4, z=-1\).
$BR
Your answer is \{ans_rule(15)\}.
END_TEXT

$num1 = 14;

ANS(num_cmp($num1, mode=>"strict"));

