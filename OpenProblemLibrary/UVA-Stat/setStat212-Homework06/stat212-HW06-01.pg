DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGnumericalmacros.pl",
"PGstatisticsmacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;
install_problem_grader(~~&std_problem_grader);

$mc[1] = new_multiple_choice(); 
$mc[1]->qa('Given that \(Z\) is a standard normal random variable, 
\(P(-1.0 \leq Z \leq 1.5)\) is equal to',
   '0.7745'
);
$mc[1]->extra(
    '0.8413',
    '0.0919',
    '0.9332'
); 

$mc[2] = new_multiple_choice(); 
$mc[2]->qa('Given that \(Z\) is a standard normal random variable, the value
\(z\) for which \(P(Z \leq z) = 0.2580\) is',
    '-0.6495'
);
$mc[2]->extra(
    '0.6018',
    '0.3982',
    '0.6999'
);


$mc[3] = new_multiple_choice(); 
$mc[3]->qa('If a random variable \(X\) is exponentially distributed
with parameter \(\lambda = 2\), then \(P(X \geq 1)\) is equal to',
    '0.1353'
);
$mc[3]->extra(
    '0.8647',
    '0.3935',
    '0.6065'
);


$mc[4] = new_multiple_choice(); 
$mc[4]->qa('If a random variable \(X\) is exponentially distributed
with parameter \(\lambda = 1.5\), then \(P(2 \leq X \leq 4)\) is equal to',
    '0.0473'
);
$mc[4]->extra(
    '0.6617',
    '0.9527',
    '0.3383'
);


$mc[5] = new_multiple_choice(); 
$mc[5]->qa('If \(X\) is a normal random variable with a standard 
deviation of 10, then \(3X\) has a standard deviation equal to',
    '30'
);
$mc[5]->extra(
    '90',
    '10',
    '\(\sqrt{30}\)'    
);


$a = random(1,5,1);
$b = random(1,5,1);
while ($a==$b){
     $b=random(1,5,1);
}


BEGIN_TEXT
$PAR
\{ $mc[$a]->print_q() \}

\{ $mc[$a]->print_a() \}
$PAR
\{ $mc[$b]->print_q() \}

\{ $mc[$b]->print_a() \}
$PAR

END_TEXT

ANS(radio_cmp($mc[$a]->correct_ans));
ANS(radio_cmp($mc[$b]->correct_ans));

ENDDOCUMENT();       # This should be the last executable line in the problem.
