#@pred=predint(@fit,$xnew);
@llim=();
@ulim=();
$in=0;
for($i=0;$i<=9;$i++)
{ @pred=predint(@fit,$xnext[$i]); 
  @llim=(@llim,$pred[0]-$tstar*$pred[1]);
  @ulim=(@ulim,$pred[0]+$tstar*$pred[1]);
  if($llim[$i]<=$ynext[$i] && $ynext[$i]<=$ulim[$i]) 
  { $in=$in+1; }
}

