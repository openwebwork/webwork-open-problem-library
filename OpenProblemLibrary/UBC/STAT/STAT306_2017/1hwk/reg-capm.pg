for($i=0;$i<9;$i++) { $xnextstr=$xnextstr . $xnext[$i] . ", " ; }
$xnextstr=$xnextstr . $xnext[9] . ")";

$ynextstr="ynext=c(";
for($i=0;$i<9;$i++) { $ynextstr=$ynextstr . $ynext[$i] . ", " ; }
$ynextstr=$ynextstr . $ynext[9] . ")";

# variables for answers
