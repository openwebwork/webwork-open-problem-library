BEGIN_HINT
Check the class notes.
END_HINT

#########################################################
$showPartialCorrectAnswers = 1;

ANS( str_cmp($ansa1) );
ANS( str_cmp($ansa2) );
ANS( str_cmp($ansb1) );
ANS( str_cmp($ansb2) );
ANS( radio_cmp($mcquesc->correct_ans()) ); 
ANS( checkbox_cmp($mcquesd->correct_ans()) ); 
ANS( checkbox_cmp($mcquese->correct_ans()) ); 

