## DBsubject(Calculus - single variable)
## DBchapter(Differentiation)
## DBsection(Logarithmic differentiation)
## Level(3)
## MO(1)

DOCUMENT();      

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

# Print problem number and point value (weight) for the problem
TEXT(beginproblem());

# Show which answers are correct and which ones are incorrect
$showPartialCorrectAnswers = 1;


Context("Numeric");

$CC = random(3,7,1);
$f= Formula("x^(ln(x)/$CC)");
$df = $f->D->reduce;

Context()->texStrings;
BEGIN_TEXT
\(\frac{d}{dx}\left( \left( \sqrt[$CC]{x} \right) ^{\ln(x)}\right)\)  =  \{ ans_rule(40)\}
$BR
END_TEXT
Context()->normalStrings;

ANS($df->cmp() );

BEGIN_SOLUTION
$PAR SOLUTIONS $PAR
 If \(y=\left( \sqrt[$CC]{x} \right) ^{\ln(x)}\), then \(\ln(y) = \frac{(ln(x))^2}{$CC}\).
$BR Then, \(\frac{dy}{dx}\frac{1}{y} = \frac{2 \ln(x)}{$CC x}\).
$BR \(\frac{dy}{dx} =\left( \sqrt[$CC]{x} \right) ^{\ln(x)} \left(\frac{2 \ln(x)}{$CC x}\right)\).

END_SOLUTION

ENDDOCUMENT();        
