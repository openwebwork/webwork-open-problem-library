#  Setup

Context("Numeric");
Context()->variables->set(x=>{limits=>[-2,-1]});

$y0 = random(1,5,1);
$den = 2 * $y0**2;

$answer1 = Compute(" (2*(1/$den - x))^(-1/2)");
#$answer2 = Compute("-(2*(1/$den - x))^(-1/2)");


#############################
#  Main text

Context()->texStrings;
BEGIN_TEXT
Solve \( y^{\,\prime} = y^3 \) if \( y(0) = $y0 \).
$BR
$BR
\( y(x) = \)
\{ ans_rule(30) \}
\{ AnswerFormatHelp("formulas") \}
END_TEXT
Context()->normalStrings;


############################
#  Answers

$showPartialCorrectAnswers = 1;

ANS( $answer1->cmp() );

;

