##############################
#  Initialization

DOCUMENT(); 

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"AnswerFormatHelp.pl",
"parserNumberWithUnits.pl",
);

TEXT(beginproblem());


#############################
#  Setup

Context("Numeric");

$a = random(2,6,1);
$b = random(1,6,1);

$d0 = list_random(250,500,750,1000,1250,1500);

$answer = NumberWithUnits("$d0 + $a*(60)^3/3 + $b * 60","km");


#############################
#  Main text

Context()->texStrings;
BEGIN_TEXT
A spaceship is traveling directly away from earth
with speed \( $a t^2 + $b \ \mathrm{km/s} \).  At
time \( t = 0 \) is is \( $d0 \ \mathrm{km} \)
from earth.  How far is it from earth one minute
after time \( t = 0 \)?  Include units in your answer.
$BR
$BR
Distance = 
\{ ans_rule(30) \} 
\{ AnswerFormatHelp("units") \}
END_TEXT
Context()->normalStrings;


############################
#  Answers

$showPartialCorrectAnswers = 1;

ANS( $answer->cmp(tolType=>absolute,tolerance=>0.0001) );

COMMENT("MathObject version.");

ENDDOCUMENT();
