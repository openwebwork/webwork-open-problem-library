##############################
#  Initialization

DOCUMENT(); 

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "AnswerFormatHelp.pl",
  "parserAssignment.pl",
  "unionLists.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());


#############################
#  Setup 1

Context("Numeric");
Context()->variables->are(t=>"Real");
Context()->flags->set(useBaseTenLog=>0);
Context()->variables->set(t=>{limits=>[1,5]});

$a = random(1,4,1);
do { $b = random(2,5,1); } until ($b != $a);
do { $c = random(2,9,1); } until ($c != $a && $c != $b);

$yp = Formula("ln($a + $b t)");
$g = $yp->D('t','t') + $c * $yp;

Context()->texStrings;
BEGIN_TEXT
The function \( y_p(t) = \ln($a + $b t), \ t > -\frac{$a}{$b} \),
is a particular solution to the differential equation
\( y^{\,\prime\prime} + $c y = g(t) \).  Find \( g(t) \).
$BR
$BR
\( g(t) = \) 
\{ ans_rule(30) \}
\{ AnswerFormatHelp("formulas") \}
END_TEXT
Context()->normalStrings;

$showPartialCorrectAnswers = 1;

ANS( $g->cmp() );



ENDDOCUMENT();
