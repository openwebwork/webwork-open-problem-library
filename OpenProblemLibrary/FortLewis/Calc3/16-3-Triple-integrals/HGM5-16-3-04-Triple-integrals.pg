DOCUMENT();        

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl",
);


TEXT(beginproblem());

##############################################
#  Setup

Context("Numeric")->variables->are(x=>"Real",y=>"Real",z=>"Real");

$a = random(2,6,1);
$b = $a + random(1,3,1);
$n = random(2,4,1);
$n1 = $n + 1;

$f = Formula("x**$n * cos(y+z)")->reduce;

$answer = Real("-4*($b**$n1 - $a**$n1)/$n1");


##############################################
#  Main text

Context()->texStrings;
Context()->flags->set(reduceConstants=>0);
BEGIN_TEXT
Find the triple integral of the function \( f(x,y,z) = $f \)
over the cube \( $a \leq x \leq $b \), \( 0 \leq y \leq \pi \), \( 0 \leq z \leq \pi.\)
$BR
$BR
\{ans_rule(40)\}
END_TEXT
Context()->normalStrings;


##################################################
#  Answers

ANS($answer->cmp);

$showPartialCorrectAnswers = 1;


COMMENT('MathObject version');
ENDDOCUMENT();