DOCUMENT(); 

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
#"contextFraction.pl",
#"PGgraphmacros.pl",
#"contextInequalities.pl",
"PGchoicemacros.pl",
#"parserMultiAnswer.pl",
"PGcourse.pl",
);

TEXT(beginproblem());

##############################################
#  Setup

Context("Numeric");
#Context()->variables->add(y=>"Real");
#Context("Fraction-NoDecimals");

$a = random(1,3,1);
$aa = $a * $a;
$b = list_random(5,7);

$cmc = new_checkbox_multiple_choice();
$cmc -> qa (
"Select ${BBOLD}all${EBOLD} expressions that are equivalent to 
\[  \frac{x-$a}{$b x} \cdot  \frac{x}{x^2-$aa}\]
There may be more than one correct answer.",  # question
"\( \displaystyle \frac{1}{$b(x+$a)} \)$BR",
"\( \displaystyle \frac{1}{$b} \cdot \frac{1}{x+$a}\)$BR",
);
$cmc -> extra(
"\( \displaystyle \frac{-1}{$b(x-$a)} \)$BR",
"\( \displaystyle \frac{-1}{$b x-$a} \)$BR",
);
# The next line can be uncommented.
$cmc->makeLast("None of the above");


#Context()->operators->undefine("+","-","*","/","**","^");
#Context()->functions->disable("All");

##############################################
#  Main text

Context()->texStrings;
BEGIN_TEXT

\{ $cmc->print_q() \}
$BR
\{ $cmc->print_a() \}

END_TEXT
Context()->normalStrings;

##################################################
#  Answer evaluation

install_problem_grader(~~&std_problem_grader);

$showPartialCorrectAnswers = 0;

ANS(checkbox_cmp( $cmc->correct_ans() ) );


COMMENT('MathObject version');
ENDDOCUMENT();