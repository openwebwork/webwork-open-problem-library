);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$an = random(2,5,1);
$as = random(-1,1,2);
$a  = $an * $as;
$n  = random(2,8,1);

$p = random(2,4,1);

$funct1 = "$a*x^($n-1)*($n*ln(x)+ 1)";

BEGIN_TEXT
Let \( f(x) = $a x^ {$n} \ln x \).
$BR$BR
\( f'( x ) = \) \{ans_rule(40) \}
END_TEXT

ANS(fun_cmp($funct1));

