##DESCRIPTION
##  Find a derivative of an expression
##  involving trig functions; requires use of chain rule
##ENDDESCRIPTION

## Shotwell cleaned
## tcao , PAID on 11-24-2003

## DBsubject(Calculus - single variable)
## DBchapter(Differentiation)
## DBsection(Derivatives involving multiple rules (all rules))
## Date(6/3/2002)
## Level(3)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('6')
## Section1('3.4')
## Problem1('41')
## MO(1)

DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

Context('Numeric');

$a = random(2,4,1);
$b = random(2,4,1);
$c = random(2,4,1);
$d = random(2,4,1);

$m = random(2,5,1);
$n = random(2,5,1);


$gen = random(0,3,1);

Context()-> variables -> set(x=>{limits=>[0,.6]});
if ($gen == 0) {
  $trig1 = "\sin";
  $trig2 = "\sin";
  $ans  = Compute("1/2*(sin(e^(x^$m*sin(x))))^(-1/2) * 
           cos(e^(x^$m*sin(x))) * 
           e^(x^$m*sin(x)) *
           ($m*x^($m-1)*sin(x) + x^$m*cos(x))");
  }
if ($gen == 1) {
  $trig1 = "\sin";
  $trig2 = "\cos";
  $ans  = Compute("1/2*(sin(e^(x^$m*cos(x))))^(-1/2) * 
           cos(e^(x^$m*cos(x))) * 
           e^(x^$m*cos(x)) *
           ($m*x^($m-1)*cos(x) - x^$m*sin(x))");
  }
if ($gen == 2) {
  $trig1 = "\cos";
  $trig2 = "\sin";
  $ans  = Compute("-1/2*(cos(e^(x^$m*sin(x))))^(-1/2) * 
           sin(e^(x^$m*sin(x))) * 
           e^(x^$m*sin(x)) *
           ($m*x^($m-1)*sin(x) + x^$m*cos(x))");
  }
if ($gen == 3) {
  $trig1 = "\cos";
  $trig2 = "\cos";
  ##Context()-> variables -> set(x=>{limits=>[0,.6]});
  $ans  = Compute("-1/2*(cos(e^(x^$m*cos(x))))^(-1/2) * 
           sin(e^(x^$m*cos(x))) * 
           e^(x^$m*cos(x)) *
           ($m*x^($m-1)*cos(x) - x^$m*sin(x))");
  }

BEGIN_TEXT
Let \[ f(x) = \sqrt{$trig1 (e^{x^{$m} $trig2(x)})} \]
$BR$BR
\( f'( x ) = \) \{ans_rule(100) \}
END_TEXT

ANS($ans -> cmp);

ENDDOCUMENT();        # This should be the last executable line in the problem.
