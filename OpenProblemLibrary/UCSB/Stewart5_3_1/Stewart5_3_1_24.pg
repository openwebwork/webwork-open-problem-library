DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "AnswerFormatHelp.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a=non_zero_random(-10, -2);

Context("Numeric");
Context()->noreduce('(-x)-y');

$f = Compute("(x^2 + $a sqrt(x) ) / ((x))")->reduce;
$df = $f->D;

Context()->texStrings;

BEGIN_TEXT

$PAR
Differentiate the following function:
\[
\displaystyle y = $f
\]

$PAR

\(y'=\) \{ans_rule(40)\}

END_TEXT


ANS($df->cmp(limits=>[.1,4]));

ENDDOCUMENT();

