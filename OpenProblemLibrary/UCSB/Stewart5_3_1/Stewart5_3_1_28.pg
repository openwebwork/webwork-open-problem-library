DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"AnswerFormatHelp.pl",
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;


Context("Numeric");
Context()->variables->add(v=>'Real', a=>'Real', b=>'Real', c=>'Real');
$f= Formula("a * e^v + b / v + c * v^2")->reduce;
$df = $f->D('v');

Context()->texStrings;

BEGIN_TEXT
Differentiate the following function with respect to \(v\):
\[
\displaystyle y = $f
\]
\(y'=\) \{ans_rule(30)\}

END_TEXT


ANS($df->cmp);

ENDDOCUMENT();
