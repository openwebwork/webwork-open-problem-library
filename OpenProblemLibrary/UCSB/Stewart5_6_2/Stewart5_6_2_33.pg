## DBsubject(Calculus - single variable)
## DBchapter(Applications of integration)
## DBsection(Volumes by washers)
## Institution(UCSB)
## MLT(likeThis3)
## Level(5)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('5')
## Section1('6.2')
## Problem1('33')
## KEYWORDS('integration', 'volumes')

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1);
$b=random(1,10,1)*random(-1,1,2);
$c=random(1,10,1)*random(-1,1,2);

BEGIN_TEXT
$PAR
END_TEXT

# Make a new checkbox multiple choice
$cmc = new_checkbox_multiple_choice();

# Insert some  questions and matching answers in the q/a list.
$cmc -> qa (
	"Which of the following integrals represents the volume of the solid obtained by rotating the region bounded by the curves \(y=\sin(x)\) and \(y=0,\) with \(0 \le x \le \pi\) about the line \(y=1?\)",
        "\( \displaystyle \pi \int_{0}^{\,\pi} {[1^2-(1-\sin(x))^2]}\,dx \)",
        );

# Insert some incorrect answers
$cmc -> extra(

        "\( \displaystyle \pi \int_{0}^{\,\pi} {[1-\sin^2(x)]}\,dx \)",
        "\( \displaystyle \pi \int_{0}^{\,\pi} {[(1)-(1-\sin(x))]^2}\,dx \)",
        "\( \displaystyle \pi \int_{0}^{\,1} {[1^2-(1-\sin(x))^2]}\,dx \)",
        "\( \displaystyle \pi \int_{0}^{\,1} {[1-\sin^2(x)]}\,dx \)",
        "\( \displaystyle \pi \int_{0}^{\,1} {[(1)-(1-\sin(x))]^2}\,dx \)",
        );

# Print the text using $cmc->print_q for the questions and
# $cmc->print_a to print the answers.

BEGIN_TEXT
$PAR
\{$cmc -> print_q \}
$PAR
\{$cmc -> print_a \}
$PAR
END_TEXT

ANS(checkbox_cmp( $cmc->correct_ans ));

ENDDOCUMENT();
