## DBsubject(Calculus - single variable)
## DBchapter(Techniques of integration)
## DBsection(Partial fractions)
## Institution(UCSB)
## MLT(PartFracProperSimpleIndef)
## Level(2)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('5')
## Section1('7.4')
## Problem1('29')
## KEYWORDS('integration', 'partial fractions')

DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1)*random(-1,1,2);
$b=random(1,10,1)*random(-1,1,2);
$c=random(1,10,1)*random(-1,1,2);
$d=4*$a;

BEGIN_TEXT

$PAR
Evaluate the integral
\[\int {\frac{$a x + $d}{x^2+2x+5}}\, dx\]

$PAR
Note: Use an upper-case "C" for the constant of integration.

$PAR
\{ans_rule(45)\}

END_TEXT

ANS(fun_cmp("1/2*$a*ln(x^2+2*x+5)-1/2*arctan(1/2*x+1/2)*$a+1/2*arctan(1/2*x+1/2)*$d+C+c", var=>["x","C"], params=>["c"], limits=>[[-6,6],[-10,10]]));

ENDDOCUMENT();
