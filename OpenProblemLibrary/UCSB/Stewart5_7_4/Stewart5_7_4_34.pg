DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1)*random(-1,1,2);
$b=random(1,10,1)*random(-1,1,2);
$c=random(1,10,1)*random(-1,1,2);
$d=(-2)*$a;

BEGIN_TEXT

$PAR
Evaluate the integral
\[\int {\frac{$a x^3}{x^3+1}}\, dx\]

$PAR
Note: Use an upper-case "C" for the constant of integration.

$PAR
\{ans_rule(45)\}

END_TEXT

ANS(fun_cmp("$a*(x-1/3*ln(abs(x+1))+1/6*ln(x^2-x+1)-1/sqrt(3)*atan((2*x-1)/sqrt(3)))+C+c", var=>["x","C"], params=>["c"], limits=>[[1.1,10],[-10,10]]));

ENDDOCUMENT();
