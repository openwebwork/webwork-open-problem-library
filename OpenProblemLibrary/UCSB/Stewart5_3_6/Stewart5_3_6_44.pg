  "PGcourse.pl"
);


TEXT(beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(2,10,1);

###############################################
# text
BEGIN_TEXT
Differentiate \( \displaystyle y=\sqrt{1-x^2}\sin^{-1}{x}. \)

$BR$BR

\(y' =\) \{ans_rule(30)\}

END_TEXT
###############################################
# answer

Context('Numeric');
Context()->variables->set(x=>{limits=>[-0.9,0.9]});
$deriv = Formula(" -x*arcsin(x)/(1-x^2)^(1/2) + 1 ");
ANS($deriv -> cmp);

ENDDOCUMENT();
