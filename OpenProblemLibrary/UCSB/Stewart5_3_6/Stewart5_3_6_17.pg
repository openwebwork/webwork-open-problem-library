DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1);
$c=($a)**2;

BEGIN_TEXT

$PAR

Find \(dy/dx\) by implicit differentiation. $BR
\[ \sqrt{xy}=$a + x^2y \]

$PAR

\(dy/dx =\) \{ans_rule(50)\}

END_TEXT

ANS(fun_cmp(" (4(xy)^(3/2)-y)/(x-2x^2(xy)^(1/2)) ", var=>["x", "y"]));

ENDDOCUMENT();
