$c=random(1,10,1)*random(-1,1,2);

BEGIN_TEXT

$PAR
Find \(f\) if \(f''(x) = 4-6x-40x^3,\,f(0)=$a,\,f'(0)=$b.\)

$PAR
\(f(x) =\) \{ans_rule(50)\}

END_TEXT

ANS(fun_cmp("2*x^2-x^3-2*x^5+($b)*x+($a)", var=>["x"]));

