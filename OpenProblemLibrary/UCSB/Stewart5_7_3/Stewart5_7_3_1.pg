## DBsubject(Calculus - single variable)
## DBchapter(Techniques of integration)
## DBsection(Trigonometric substitution)
## Institution(UCSB)
## MLT(Trig_sec)
## Level(2)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('5')
## Section1('7.3')
## Problem1('1')
## KEYWORDS('trigonometric substitution')

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1)*random(-1,1,2);
$b=random(1,10,1)*random(-1,1,2);
$c=random(1,10,1)*random(-1,1,2);

BEGIN_TEXT

$PAR
Evaluate the integral using the indicated trigonometric substitution.
\[\int {\frac{$a}{x^2 \sqrt{x^2-9}}}\, dx,\quad x=3 \sec(\theta)\]

$PAR
Note: Use an upper-case "C" for the constant of integration.

$PAR
\{ans_rule(45)\}

END_TEXT

ANS(fun_cmp("$a/9*sqrt(x^2-9)/x+C+c", var=>["x","C"], params=>["c"], limits=>[[3.1,10],[-10,10]]));

ENDDOCUMENT();
