## DBsubject(Calculus - single variable)
## DBchapter(Techniques of integration)
## DBsection(Trigonometric substitution)
## Institution(UCSB)
## MLT(Trig_tan)
## Level(3)
## Static(1)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('5')
## Section1('7.3')
## Problem1('32')
## KEYWORDS('trigonometric substitution')

DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1)*random(-1,1,2);
$b=random(1,10,1)*random(-1,1,2);
$c=random(1,10,1)*random(-1,1,2);

BEGIN_TEXT

$PAR
Use trigonometric substitution to evaluate the integral
\[\int {\frac{x^2}{(x^2+a^2)^{3/2}}}\, dx\]

$PAR
Note: Use an upper-case "C" for the constant of integration.

$PAR
\{ans_rule(45)\}

END_TEXT

ANS(fun_cmp("ln(x+sqrt(x^2+a^2))-x/sqrt(x^2+a^2)+C+c", var=>["a","x","C"], params=>["c"], limits=>[[0.1,7],[0.1,7],[-10,10]]));

ENDDOCUMENT();
