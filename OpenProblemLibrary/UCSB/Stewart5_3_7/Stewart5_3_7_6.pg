## updated tbf - Dec 2014

## DBsubject(Calculus - single variable)
## DBchapter(Differentiation)
## DBsection(Higher-order derivatives)
## Institution(UCSB)
## MLT(hod_2nd_poly)
## Level(2)
## MO(1)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('5')
## Section1('3.7')
## Problem1('6')
## KEYWORDS('Differentiation','Product','Quotient')

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
Context("Numeric");
$showPartialCorrectAnswers = 1;

$a=random(8,9);
$b=random(2,9);
$c=random(6,7);
$d=random(2,12);
$e=random(4,5);
Context()->variables->are('t'=>"Real");
$f = Formula(" t^{$a} - $b t^{$c} - $d t^{$e} ");
$f1 = Formula(" $a t^{$a - 1} - $b*$c t^{$c-1} - $d*$e t^{$e-1} ");
$f2 = Formula(" {$a*$a - $a} t^{$a-2} - {$b*$c*$c - $b*$c} t^{$c - 2} - {$d*$e*$e - $d*$e} t^{$e - 2} ");

Context()->texStrings;
BEGIN_TEXT

Find the first and second derivatives of the following function: $BR
\[f(t)=$f\]
\(f'(t)\, =\) \{ans_rule(30)\} $BR$BR
\(f''(t) =\) \{ans_rule(30)\}

END_TEXT
Context()->normalStrings;

ANS($f1->cmp);
ANS($f2->cmp);

;

ENDDOCUMENT();
