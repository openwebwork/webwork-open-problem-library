## MLT(hod_1st_2nd_root)
## Level(3)
## MO(1)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('5')
## Section1('3.7')
## Problem1('16')
## KEYWORDS('Differentiation','Product','Quotient')


DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"MathObjects.pl"
);

TEXT(&beginproblem);
Context("Numeric");
$showPartialCorrectAnswers = 1;
$a=random(1,7,1);
$b=random(1,5,1);
$c=random(1,10,1);

$f= Formula(" {2*$a x}/sqrt{$b x + $c} ");
$f1 = Formula(" {$a*$b x + 2*$a*$c}/($b x + $c)^{3/2} ");
$f2 = Formula(" {-$a*$b*$b x - 4*$a*$b*$c}/{2($b x + $c)^{5/2}} ");

Context()->texStrings;
BEGIN_TEXT

Find the first and second derivatives of the following function: 
\[\displaystyle y= $f\]

\(y'\, =\) \{ans_rule(40)\} $BR$BR
\(y'' =\) \{ans_rule(40)\}
 
END_TEXT
Context()->normalStrings;

ANS($f1->cmp);
ANS($f2->cmp);

;

ENDDOCUMENT();
