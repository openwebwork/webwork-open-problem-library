DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
Context("Numeric");

$a=random(2,10,1)*random(-1,1,2);
$b=random(2,5,1)*random(-1,1,2);
$c=random(1,9,1);
while($b==$c){$c=random(1,9,1);}

$f = Formula(" (-6*$a*$b*$b*$c)/($b*x - $c)^4 " );

BEGIN_TEXT

Find a formula for \(y'''\) if \(\displaystyle y=\frac{$a x}{$b x-$c}\).
$BR$BR
\(y'''=\) \{ans_rule(30)\}

END_TEXT

ANS($f->cmp);

;

ENDDOCUMENT();
