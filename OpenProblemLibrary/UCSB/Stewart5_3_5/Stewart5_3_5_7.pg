DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(2,10,1);
$b=random(2,10,1);

BEGIN_TEXT

$PAR
Differentiate \(F(x) = (x^3+$a x)^{$b}\).

$PAR

\(F'(x) =\) \{ans_rule(50)\}

END_TEXT

ANS(fun_cmp("($b)*(3x^2+$a)*(x^3+$a x)^($b - 1)", var=>"x"));

ENDDOCUMENT();
