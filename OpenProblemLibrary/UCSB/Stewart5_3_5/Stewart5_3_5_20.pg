DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(2,10,1);
$b=random(2,10,1);

BEGIN_TEXT

$PAR
Differentiate \( y = (x^2+$a)\sqrt[3]{x^2+2}\).

$PAR

\(y'=\) \{ans_rule(60)\}

END_TEXT

ANS(fun_cmp(" 2x(x^2+2)^(1/3) + (2x/3)(x^2+$a)(x^2+2)^(-2/3)   ", var=>"x"));

ENDDOCUMENT();
