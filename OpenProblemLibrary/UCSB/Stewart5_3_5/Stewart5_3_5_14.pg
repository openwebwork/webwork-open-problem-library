DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

Context("Numeric");
$a=random(2,10,1);
Context()->texStrings;
BEGIN_TEXT

$PAR
Differentiate \(y(x) = a^3+\cos^{$a}{x}\).

$PAR

\(y'(x)=\) \{ans_rule(50)\}

END_TEXT
Context()->normalStrings;

ANS(Compute("-sin(x)($a)(cos(x))^($a -1)")->cmp());

