DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(3,10,1);
$b=random(2,10,1);

BEGIN_TEXT

$PAR
Differentiate \(F(x) = \sqrt[$a]{$b + 2x + x^3}\).

$PAR

\(F'(x) =\) \{ans_rule(50)\}

END_TEXT

ANS(fun_cmp("(1/$a)($b + 2x + x^3)^((1/$a)-1)(3x^2+2)", var=>"x"));

ENDDOCUMENT();
