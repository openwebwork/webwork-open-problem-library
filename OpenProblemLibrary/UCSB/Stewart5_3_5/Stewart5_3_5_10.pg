DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(2,10,1);
$b=random(2,10,1);

BEGIN_TEXT

$PAR
Differentiate \(F(x) = ($a + x^{$b})^{2/3}\).

$PAR

\(F'(x) =\) \{ans_rule(50)\}

END_TEXT

ANS(fun_cmp("(2/3)($b)(x^($b - 1))($a + x^{$b})^(-1/3)", var=>"x"));

ENDDOCUMENT();
