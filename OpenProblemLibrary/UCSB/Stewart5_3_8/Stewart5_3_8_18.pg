DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(2,10,1);
$b=random(1,10,1);
 while($b==$a) {$b=random(1,10,1)};

BEGIN_TEXT


Differentiate 
\[G(u)= \ln\left(\sqrt{\frac{$a u + $b}{$a u - $b}}\right). \]

$BR$BR

\(G'(u) =\) \{ans_rule(50)\}

END_TEXT

ANS(fun_cmp(" -($b)*($a)/(($a)^2 u^2 - ($b)^2) ", var=>"u"));

ENDDOCUMENT();
