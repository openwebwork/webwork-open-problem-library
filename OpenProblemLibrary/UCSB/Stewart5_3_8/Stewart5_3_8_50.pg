TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(7,11,1);
$b=$a + 1;

BEGIN_TEXT

$PAR

\(\displaystyle \frac{d^{$b}}{dx^{$b}}\left(x^{$a}\ln{x}\right) =\) \{ans_rule(40)\}

END_TEXT

ANS(fun_cmp(" fact($a)/x ",var=>"x"));

