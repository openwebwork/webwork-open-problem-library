DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
Context("Numeric");

$a=random(2,10,1);
$f = Formula("$a x ln{(x)}");
$f1 = Formula("$a (ln(x)+1)");
$f2 = Formula("$a /x ");

Context()->texStrings; 
BEGIN_TEXT

Find \(y'\) and \(y''\) for \( y= $f \). $BR$BR
\(y'= \) \{ans_rule(30)\} $BR$BR
\(y''=\) \{ans_rule(30)\}

END_TEXT
Context()->normalStrings;

ANS($f1->cmp);
ANS($f2->cmp);

;

ENDDOCUMENT();
