$b=random(1,10,1);
 while($b==$a) {$b=random(1,10,1)};

BEGIN_TEXT

$PAR

Find \(y'\) and \(y''\) for \( y=$a x\ln{x} \).

$PAR

\(y'= \) \{ans_rule(30)\} $BR
\(y''=\) \{ans_rule(30)\}

END_TEXT

ANS(fun_cmp(" $a (ln(x)+1) ", var=>"x"));
ANS(fun_cmp(" $a /x ", var=>"x"));

