DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(2,10,1);
$b=random(1,10,1);
 while($b==$a) {$b=random(1,10,1)};

BEGIN_TEXT

$PAR

Differentiate \( y=[\ln(1+e^x)]^{$a} \).

$PAR

\(y'=\) \{ans_rule(30)\}

END_TEXT

ANS(fun_cmp(" ($a)*(ln(1+e^x))^($a -1)*e^x/(1+e^x)  ", var=>"x"));

ENDDOCUMENT();
