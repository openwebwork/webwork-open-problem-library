DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;

$a=random(1,10,1);
$b=random(1,10,1);

BEGIN_TEXT

$PAR
Evaluate the following limit. If the answer is positive infinite, type "I"; if negative infinite, type "N"; and if it does not exist, type "D". $PAR

\[\displaystyle\lim_{x \to \infty} \frac{\sqrt{9x^6-x}}{x^3+ $a}\]
$PAR

\{ans_rule(10)\}

END_TEXT

ANS(fun_cmp("3", var=>["I","N","D"]));

ENDDOCUMENT();
