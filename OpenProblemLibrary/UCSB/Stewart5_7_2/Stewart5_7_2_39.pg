## DBsubject(Calculus - single variable)
## DBchapter(Techniques of integration)
## DBsection(Trigonometric integrals)
## Institution(UCSB)
## MLT(sec)
## MLTleader(1)
## Level(3)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('5')
## Section1('7.2')
## Problem1('39')
## KEYWORDS('integration, trigonometric integrals')

DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1)*random(-1,1,2);
$b=random(1,10,1)*random(-1,1,2);
$c=random(1,10,1)*random(-1,1,2);

BEGIN_TEXT

$PAR
Evaluate the integral 
\[\int {$a \csc(x)}\, dx\]

$PAR
Note: Use an upper-case "C" for the constant of integration.  Also, use 1/tan(x) for cot(x), and use 1/sin(x) for csc(x).


$PAR
\{ans_rule(45)\}

END_TEXT

ANS(fun_cmp("$a*ln(abs(1/sin(x)-cot(x)))+C+c", var=>["x","C"], params=>["c"], limits=>[[0.1,3.1],[-10,10]]));

ENDDOCUMENT();
