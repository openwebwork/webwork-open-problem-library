## DBsection(Partial fractions)
## Institution(UCSB)
## MLT(PartFracProperFactQuad)
## MLTleader(1)
## Level(2)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('5')
## Section1('7.5')
## Problem1('73')
## KEYWORDS('integration')

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1)*random(-1,1,2);
$b=random(1,10,1)*random(-1,1,2);
$c=random(1,10,1)*random(-1,1,2);
$d=2*$a;

BEGIN_TEXT

$PAR
Evaluate the integral
\[\int {\frac{$a}{(x-2)(x^2+4)}}\, dx\]

$PAR
Note: Use an upper-case "C" for the constant of integration.

$PAR
\{ans_rule(60)\}

END_TEXT

ANS(fun_cmp("$a*(1/8*ln(abs(x-2))-1/16*ln(x^2+4)-1/8*atan(x/2))+C+c", var=>["x","C"], params=>["c"], limits=>[[2.1,8],[-10,10]]));

ENDDOCUMENT();
