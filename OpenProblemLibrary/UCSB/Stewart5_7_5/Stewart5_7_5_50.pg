## DBsubject(Calculus - single variable)
## DBchapter(Techniques of integration)
## DBsection(Challenging integrals)
## Institution(UCSB)
## MLT(challenge_01)
## MLTleader(1)
## Level(3)
## TitleText1('Calculus: Early Transcendentals')
## AuthorText1('Stewart')
## EditionText1('5')
## Section1('7.5')
## Problem1('50')
## KEYWORDS('integration')

DOCUMENT();

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(&beginproblem);
$showPartialCorrectAnswers = 1;
$a=random(1,10,1)*random(-1,1,2);
$b=random(1,10,1)*random(-1,1,2);
$c=random(1,10,1)*random(-1,1,2);
$d=2*$a;

BEGIN_TEXT

$PAR
Evaluate the integral
\[\int {\frac{$a}{x^2 \sqrt{4x+1}}}\, dx\]

$PAR
Note: Use an upper-case "C" for the constant of integration.

$PAR
\{ans_rule(90)\}

END_TEXT

ANS(fun_cmp("$a*(2*ln(sqrt(4*x+1)+1)-2/(sqrt(4*x+1)+1)-2*ln(abs(sqrt(4*x+1)-1))-2/(sqrt(4*x+1)-1))+C+c", var=>["x","C"], params=>["c"], limits=>[[1,6],[-10,10]]));

ENDDOCUMENT();
