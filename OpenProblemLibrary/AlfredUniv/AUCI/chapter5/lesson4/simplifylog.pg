DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a=random(2,5,1);
$b=random(2,5,1);
$c=random(2,5,1);

BEGIN_TEXT
\($a \log x - $b \log (x^2+1) +$c \log (x-1) = \log\Big(\)\{ans_rule(35)\}\(\Big )\).
END_TEXT

$ans1 = "x**$a*(x-1)**$c/(x**2+1)**$b";

ANS(fun_cmp($ans1));

