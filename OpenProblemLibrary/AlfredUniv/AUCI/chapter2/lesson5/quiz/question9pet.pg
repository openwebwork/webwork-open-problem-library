## Declare Variables

## Function Definition
$a0 = non_zero_random(-10,10);
$a1 = non_zero_random(-10,10);
$a2 = non_zero_random(-10,10);

$f = Formula("$a0*x^2+$a1*x+$a2")->reduce;
$df = $f->D('x');
$ddf = $df->D('x');

Context()->texStrings;
BEGIN_TEXT
If \(y=$f\) then \(\frac{d^2y}{dx^2} = \) \{$ddf->ans_rule()\}

END_TEXT
Context()->normalStrings;
ANS($ddf->cmp);


