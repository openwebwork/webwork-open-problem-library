# Load whatever macros you need for the problem
loadMacros(
"MathObjects.pl",
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
);
 
## Show partial correct answers
$showPartialCorrectAnswers = 0;

## Display the problem information
TEXT(beginproblem());
$mc = new_multiple_choice();
$mc->qa(
"A student attempts to compute \(f^\prime(5)\) for a quadratic function \(f\) by computing \(\frac{f(7)-f(5)}{2}\). Choose the best answer.", 
"Her computation is incorrect because she computed the slope of a secant line, not a tangent line."
);

$mc->makeLast("Her computation is correct.","Her computation is incorrect because she computed the slope of a secant line, not a tangent line.","Her computation is incorrect because she did not compute a slope.","Her computation is incorrect because she didn't use \(\Delta x\).",
"Not enough information is given.");

Context()->texStrings;
BEGIN_TEXT
\{ $mc->print_q() \}
$BR
\{ $mc->print_a() \}
END_TEXT
Context()->normalStrings;

install_problem_grader(~~&std_problem_grader);
$showPartialCorrectAnswers = 0;

ANS( radio_cmp( $mc->correct_ans() ) );

ENDDOCUMENT();
