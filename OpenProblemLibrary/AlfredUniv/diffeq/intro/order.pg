##########################################################################
##
## Set up the variables to allow derivatives in different notations. For some 
## reason I decided to write these out as formulas instead of texing them, that
## was dumb. A couple of the equations I had to tex anyways to get them to look
## right. The smart thing to do would be to clean all of this up and just tex
## all of the formulas.
##
Context()->variables->add(t=>"Real",y=>"Real",yp=>"Real",y2p=>"Real",y3p=>"Real",ydot=>"Real",yddot=>"Real",dydx=>"Real",d2ydx2=>"Real",d3ydx3=>"Real",dzdx=>"Real",dzdy=>"Real",d2zdx2=>"Real",d2zdy2=>"Real",d2zdxdy=>"Real");

Context()->variables->set(yp => {TeX => 'y^\prime'},y2p => {TeX => 'y^{\prime\prime}'},y3p => {TeX => 'y^{\prime\prime\prime}'},ydot => {TeX => '\dot{y}'}, yddot => {TeX => '\ddot{y}'},dydx => {TeX => '\frac{dy}{dx}'},d2ydx2 => {TeX => '\frac{d^2y}{dx^2}'},d3ydx3 => {TeX => '\frac{d^3y}{dx^3}'},dzdx => {TeX => '\frac{\partial z}{\partial x}'},dzdy => {TeX => '\frac{\partial z}{\partial y}'},d2zdx2 => {TeX => '\frac{\partial^2 z}{\partial x^2}'},d2zdy2 => {TeX => '\frac{\partial^2 z}{\partial y^2}'},d2zdxdy => {TeX => '\frac{\partial^2 z}{\partial x\partial y}'});


##########################################################################
##
## Strings
##

Context()->strings->are(linear=>{},nonlinear=>{},ordinary=>{},partial=>{},"hard to say"=>{}, =>{});
$blank = String("");
$linear = String("linear");
$non = String("nonlinear");
$hts = String("hard to say");
$ode = String("ordinary");
$pde = String("partial");

##########################################################################
##
## Set up the equations
## using the variables 
## x,y,yp,y2p,y3p,ydot,yddot,dydx,d2ydx2,d3ydx3,dzdx,dzdy,d2zdx2,d2zdy2,d2zdxdy
##

$eqn1l = Formula("(1-x)*y2p-4*x*yp+5*y");
$eqn1r = Formula("cos(x)");
$eqn1linearity = $linear;
$eqn1type = $ode;
$eqn1order = Compute("2");

$eqn2l = Formula("x*d3ydx3-((dydx))^4+y");
$eqn2r = Formula("0");
$eqn2linearity = $non;
$eqn2type = $ode;
$eqn2order = Compute("3");

$eqn3l = Formula("d2zdx2+dzdy+y");
$eqn3r = Formula("cos(x+y)");
$eqn3linearity = $linear;
$eqn3type = $pde;
$eqn3order = Compute("2");

$eqn4l = Formula("d2ydx2");
$eqn4r = Formula("sqrt(1+(dydx)^2)");
$eqn4linearity = $non;
$eqn4type = $ode;
$eqn4order = Compute("2");

$eqn5l = Formula("dydx");
$eqn5r = Formula("cos(y)/y");
$eqn5linearity = $non;
$eqn5type = $ode;
$eqn5order = Compute("1");

$eqn6l = Formula("d2zdxdy+(xy)^2");
$eqn6r = Formula("0");
$eqn6linearity = $linear;
$eqn6type = $pde;
$eqn6order = Compute("2");

$eqn7l = Formula("(ydot)^2-4*t*ydot+5*y");
$eqn7r = Formula("t");
$eqn7linearity = $non;
$eqn7type = $ode;
$eqn7order = Compute("1");

$eqn8l = Formula("ydot-(1-yddot)*ydot");
$eqn8r = Formula("t^3-t");
$eqn8linearity = $non;
$eqn8type = $ode;
$eqn8order = Compute("2");




##########################################################################
##
## Problem Text
##

Context()->texStrings;
BEGIN_TEXT
\(
\begin{aligned}
(1-x)y^{\prime\prime}-4xy^\prime+5y&=cos(x) & \hbox{is a  \{ pop_up_list([$blank->string,$linear->string,$non->string]) \}\{ pop_up_list([$blank->string,$ode->string,$pde->string]) \} differential equation with order \{$eqn1order->ans_rule(7)\}.}\\
x\frac{d^3y}{dx^3}-\left(\frac{dy}{dx}\right)^4&=$eqn2r & \hbox{is a  \{ pop_up_list([$blank->string,$linear->string,$non->string]) \}\{ pop_up_list([$blank->string,$ode->string,$pde->string]) \} differential equation with order \{$eqn1order->ans_rule(7)\}.}\\
$eqn3l&=$eqn3r & \hbox{is a  \{ pop_up_list([$blank->string,$linear->string,$non->string]) \}\{ pop_up_list([$blank->string,$ode->string,$pde->string]) \} differential equation with order \{$eqn1order->ans_rule(7)\}.}\\
$eqn4l&=\sqrt{1+\left(\frac{dy}{dx}\right)^2} & \hbox{is a  \{ pop_up_list([$blank->string,$linear->string,$non->string]) \}\{ pop_up_list([$blank->string,$ode->string,$pde->string]) \} differential equation with order \{$eqn1order->ans_rule(7)\}.}\\
$eqn5l&=$eqn5r & \hbox{is a  \{ pop_up_list([$blank->string,$linear->string,$non->string]) \}\{ pop_up_list([$blank->string,$ode->string,$pde->string]) \} differential equation with order \{$eqn1order->ans_rule(7)\}.}\\
$eqn6l&=$eqn6r & \hbox{is a  \{ pop_up_list([$blank->string,$linear->string,$non->string]) \}\{ pop_up_list([$blank->string,$ode->string,$pde->string]) \} differential equation with order \{$eqn1order->ans_rule(7)\}.}\\
$eqn7l&=$eqn7r & \hbox{is a  \{ pop_up_list([$blank->string,$linear->string,$non->string]) \}\{ pop_up_list([$blank->string,$ode->string,$pde->string]) \} differential equation with order \{$eqn1order->ans_rule(7)\}.}\\
$eqn8l&=$eqn8r & \hbox{is a  \{ pop_up_list([$blank->string,$linear->string,$non->string]) \}\{ pop_up_list([$blank->string,$ode->string,$pde->string]) \} differential equation with order \{$eqn1order->ans_rule(7)\}.}\\
\end{aligned}
\)
END_TEXT
