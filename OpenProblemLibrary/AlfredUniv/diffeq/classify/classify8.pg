DOCUMENT();      

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGcourse.pl"
);

# Print problem number and point value (weight) for the problem
TEXT(beginproblem());

# Show which answers are correct and which ones are incorrect
$showPartialCorrectAnswers = 0;

##############################################################
#
#  Setup
#
#
# Make a new checkbox multiple choice
$cmc = new_checkbox_multiple_choice();
$cmc -> qa (
"Check all that apply to the differential equation \(\frac{dy}{dx}-\sin^2(x)y=\cos^2(x)y+x\)", 
"linear",                
"first order",
"a candidate for integrating factors",
"ODE",
);
$cmc -> makeLast(
"ODE",
"PDE",
"first order",
"second order",
"third order",
"linear",
"a candidate for integrating factors",
"separable",
"homogeneous",
"autonomous"
);

##############################################################
#
#  Text
#
#


BEGIN_TEXT
\{ $cmc -> print_q() \}
$PAR
\{ $cmc -> print_a() \}

END_TEXT

##############################################################
#
#  Answers
#
#
install_problem_grader(~~&std_problem_grader);

$showPartialCorrectAnswers = 0;

ANS(checkbox_cmp($cmc->correct_ans));



ENDDOCUMENT();        
