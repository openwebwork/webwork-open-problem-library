$A = non_zero_random(2,11);
$B = non_zero_random(2,11);

$int1 = Compute("(- sqrt($A),sqrt($A))");

  Context("Numeric");
  Context()->flags->set( 
  tolerance => 0,
  tolType => "absolute"
  );


  $a  = Compute("$A");  
  $a2 = Compute("2 * $a");
  $a6 = Compute("6 * $a");

  $mone   = Compute("-1");
  $zero  = Compute("0");
  $one   = Compute("1");
  $two   = Compute("2");
  $three = Compute("3");

  $b  = Compute("$B");
  $bm1 = Compute("$b - 1");


  Context("Numeric");
  Context()->flags->set( 
  tolerance => 0.00001,
  tolType => "absolute"
  );

$c2  = Compute("1/(2*$a)");
$c4  = Compute("$c2*(-1*(1 + 2 * $b)/(12 * $a))");
$c6  = Compute("$c4*(-1*(11 + 4 * $b)/(30*$a))");


$c3  = Compute("(1 - $b) /(6*$a)");
$c5  = Compute("$c3*(-1*(5 + 3 * $b)/(20 * $a))");
$c7  = Compute("$c5*(-1*(19+5*$b)/(42*$a))");



## Declare variables
