DOCUMENT();

# Load whatever macros you need for the problem
loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl",
           "PGgraphmacros.pl",
           "MathObjects.pl");
 
## Show partial correct answers
$showPartialCorrectAnswers = 1;
## Display the problem information
TEXT(beginproblem());

## Declare Variables
Context()->variables->are(x=>'Real',y=>'Real');

## Function Definition
$a0 = random(1,10);
$a1 = non_zero_random(-10,10);


$f = Formula("$a0*cos(x)+$a1*sin(x)")->reduce;
$df = $f->D('x');

Context()->texStrings;
BEGIN_TEXT

Find \(\>f'(x)\>\) if  \(\>f(x) = $f\).
$BR
$BR
Answer:  \{ans_rule(20)\}

END_TEXT
Context()->normalStrings;
ANS($df->cmp);


ENDDOCUMENT();
