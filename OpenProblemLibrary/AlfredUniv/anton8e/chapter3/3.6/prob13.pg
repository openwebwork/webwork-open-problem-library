## Show partial correct answers
$showPartialCorrectAnswers = 1;
## Display the problem information
TEXT(beginproblem());

## Declare Variables
Context()->variables->are(x=>'Real',y=>'Real');

## Function Definition
$a0 = random(2,9);

$f = Formula("x*cos($a0*x) - (sin(x))**2")->reduce;
$df = $f->D('x');
$ddf = $df->D('x');

Context()->texStrings;
BEGIN_TEXT

Find \(\>d^2y/dx^2\>\) if  \(\>y = $f\).
$BR
$BR
Answer: \{ans_rule(20)\}

END_TEXT
Context()->normalStrings;
ANS($ddf->cmp);


