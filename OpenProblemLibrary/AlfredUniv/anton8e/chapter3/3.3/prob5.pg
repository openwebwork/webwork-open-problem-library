## Declare Variables
Context()->variables->are(x=>'Real',y=>'Real');

## Function Definition
$a0 = non_zero_random(-10,10);
$a1 = non_zero_random(-10,10);


$f = Formula("($a0*x**2+$a1)**2")->reduce;
$df = $f->D('x');

Context()->texStrings;
BEGIN_TEXT

Find \(\>f'(x)\>\) if  \(\>f(x) = $f\).
$BR
$BR
Answer:  \{ans_rule(20)\}

END_TEXT
Context()->normalStrings;
ANS($df->cmp);


