## Function Definition
$a0 = non_zero_random(-10,10);
$a1 = non_zero_random(-10,10);
$a2 = random(2,9);

$f = Formula("($a2*$a1-$a0)/$a2**2")->reduce;

Context()->texStrings;
BEGIN_TEXT

Find \(\>g'($a2)\>\) given that  \(\>f($a2) = $a0\>\), \(\>f'($a2) = $a1\>\), and  \(\>g(x)= \frac {f(x)} {x}\). 
$BR
$BR
Answer: \{ans_rule(20)\}

END_TEXT
Context()->normalStrings;
ANS($f->cmp);


