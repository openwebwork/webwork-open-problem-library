## Function Definition;
$a = non_zero_random(-10,10);
$b = non_zero_random(-10,10);


$f = Formula("($a*x-$b)**(1/3)")->reduce;
$ans1 = Formula("(x**3+$b)/$a")->reduce;


Context()->texStrings;
BEGIN_TEXT
Find a formula for \(\> f^{-1} (x)\>\) if \(\>f(x) = \root 3 \of {$a x - $b}\>\).
$BR
$BR
Answer:: \{ans_rule(20)\}

END_TEXT
