loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "PGgraphmacros.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);
 
## Show partial correct answers
$showPartialCorrectAnswers = 1;
## Display the problem information
TEXT(beginproblem());

## Declare Variables
Context()->variables->are(x=>'Real',y=>'Real');

## Function Definition
$a = random(2,9);



$f = Formula("ln((x-1)**3*(x**2+1)**4)")->reduce;
$df = $f->D('x');

Context()->texStrings;
BEGIN_TEXT

Find \(\>dy/dx\>\) if  \(\>y = $f\).
$BR
$BR
Answer: \{ans_rule(20)\}

END_TEXT

BEGIN_HINT
$BR
$BBOLD Hint $EBOLD
Use properties of logarithms
END_HINT

BEGIN_SOLUTION
$BR
$BBOLD Solution $EBOLD
\(f = \ln((x-1)^3(x^2+1)^4)=3\ln(x-1)+4\ln(x^2+1)\)
$BR
So
$BR
\(f^\prime = \frac{3}{x} + \frac{4*2x}{x^2+1}\)
END_SOLUTION
Context()->normalStrings;
ANS($df->cmp);


ENDDOCUMENT();
