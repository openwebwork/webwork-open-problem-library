DOCUMENT();

# Load whatever macros you need for the problem
loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl",
           "PGgraphmacros.pl",
           "MathObjects.pl");
 
## Show partial correct answers
$showPartialCorrectAnswers = 1;
## Display the problem information
TEXT(beginproblem());

## Declare Variables
Context()->variables->are(x=>'Real',y=>'Real');

## Function Definition
$a = random(2,9);
$b = non_zero_random(-10,10);


$f = Formula("x**3*($a*x**2+$b)**(-2/3)")->reduce;
$f1 = Formula("($a*x**2+$b)")->reduce;
$df = $f->D('x');

Context()->texStrings;
BEGIN_TEXT

Find \(\>dy/dx\>\) if  \(\>y = x^3($f1)^{-2/3}\).
$BR
$BR
Answer: \{ans_rule(20)\}

END_TEXT
Context()->normalStrings;
ANS($df->cmp);


ENDDOCUMENT();
