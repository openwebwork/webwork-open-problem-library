Context("Numeric");

$a=non_zero_random(1,10, 1);
$b=non_zero_random(2,10, 1);
$c=non_zero_random(2,10, 1);

$g=Formula("$a*x^9")->reduce;
$h=Formula("$b*x^4")->reduce;
$j=Formula("$c*x")->reduce;
$f=$g*($h+$j);


