###########################
#  Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"AnswerFormatHelp.pl",
"PGML.pl",
"PGcourse.pl",
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

###########################
#  Setup
Context()->noreduce('(-x)-y','(-x)+y');

$a=non_zero_random(1,10);
$b=non_zero_random(1,10);
do { $c = random(1,10); } until ( $c != $a );

$f=Formula("(x+$c)/(x**2+($a-$b)*x-$a*$b)")->reduce;

Context("Interval");
$ans=Compute("(-infinity,-$a)U(-$a,$b)U($b,infinity)");


###########################
#  Main text

BEGIN_PGML

Find the domain of [`` f(x)=[$f] ``]. Enter your solution in interval notation.

[_______________________]{$ans} [@ AnswerFormatHelp("intervals") @]*


END_PGML


