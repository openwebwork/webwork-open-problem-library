###########################
#  Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"AnswerFormatHelp.pl",
"PGML.pl",
"PGcourse.pl",
"contextFraction.pl",
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

###########################
#  Setup

$a = random(1,2);  # different forms of problem

# Parameters

$u = random(1,6);
$v = random(1,10);
do{ $w = random(1,10); } until ($w != $v);  

# Solutions
Context("Interval");

if ($a ==1) {
  if ($v < $w) {
    $x1 = Interval("($v,$w)");
  }
  else {
    $x1 = Interval("($w,$v)");
  }
}
else {
  if ($v < $w) {
    $x1 = Interval("(-$w,-$v)");
  }
  else {
    $x1 = Interval("(-$v,-$w)");
  }
}

###########################
#  Main text
if ($a == 1) 
{
BEGIN_PGML

Solve the rational inequality.  Write your answer in interval notation. 

[`` \dfrac{{[$u]}} {x^2 - {[$v+$w]} x + {[$v*$w]}} < 0 ``]

 [_______________]{$x1} [@ AnswerFormatHelp("interval") @]*

END_PGML
}
else
{
BEGIN_PGML

Solve the rational inequality.  Write your answer in interval notation. 

[`` \dfrac{{[$u]}} {x^2 + {[$v+$w]} x + {[$v*$w]}} < 0 ``]

 [_______________]{$x1} [@ AnswerFormatHelp("interval") @]*

END_PGML
}

