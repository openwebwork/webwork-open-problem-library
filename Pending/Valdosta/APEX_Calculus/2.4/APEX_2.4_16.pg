#Problem APEX 2.4.16

DOCUMENT();

# Load whatever macros you need for the problem
loadMacros(
  "PG.pl",
  "PGbasicmacros.pl",
  "PGchoicemacros.pl",
  "PGanswermacros.pl",
  "PGauxiliaryFunctions.pl",
  "MathObjects.pl"
);

## DBsubject(Calculus - single variable)
## DBchapter(Differentiation)
## DBsection(Product rule (with trigonometric functions))
## Institution('Valdosta State University')
## Author('S. V. Ault')
## TitleText1('APEX Calculus')
## AuthorText1('Hartman')
## EditionText1('3.0')
## Section1('2.4')
## Problem1('16')

$showPartialCorrectAnswers = 1;

$a = random(2,5,1);
$a1 = $a+1;
$b = random(1,10,1)*random(-1,1,2);

$f = "t^{-$a}";
$fp = "-$a t^{-$a1}";

$r = random(1,4,1);

if ($r==1) {
  $g = "\sec t + $b";
  $gp = "\sec t \tan t";
  $g_ans = "sec(t) + $b";
  $gp_ans = "sec(t)*tan(t)";
} elsif ($r==2) {
  $g = "\csc t + $b";
  $gp = "-\csc t \cot t";
  $g_ans = "csc(t) + $b";
  $gp_ans = "-csc(t)*cot(t)";
} elsif ($r==3) {
  $g = "\tan t + $b";
  $gp = "\sec^2 t";
  $g_ans = "tan(t) + $b";
  $gp_ans = "(sec(t))^2";
} else {
  $g = "\cot t + $b";
  $gp = "-\csc^2 t";
  $g_ans = "cot(t) + $b";
  $gp_ans = "-(csc(t))^2";
}
$ans = "($f)($gp_ans) + ($fp)($g_ans)";

TEXT(beginproblem());

BEGIN_TEXT
Compute the derivative of the given function.
$PAR
\[
  f(t) = \frac{1}{t^{$a}} \left( $g \right)
\]
$PAR
\(f'(t) = \)\{ ans_rule(25) \}.
END_TEXT

ANS( fun_cmp( $ans, vars=>['t'] ));

SOLUTION(EV3(<<'END_SOLUTION'));
$BR$BBOLD Solution:$EBOLD 
$PAR
Use the Product Rule.
$PAR
\[
  f'(t) = ($f)($gp) + ($fp)($g).
\]
END_SOLUTION





ENDDOCUMENT();    



