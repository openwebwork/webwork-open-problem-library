##DESCRIPTION
##  Find natural freuquencies a second order system of ODE
##ENDDESCRIPTION


## DBsubject(Differential equations)
## DBchapter(Systems of differential equations)
## DBsection(Applications)
## Institution(Oklahoma State University)
## Author(Jiri Lebl)
## Level(5)
## MO(1)
## TitleText1('Notes on Diffy Qs: Differential Equations for Engineers')
## AuthorText1('Jiri Lebl')
## EditionText1('5')
## Section1('3.6')
## Problem1('')
## KEYWORDS('ODE', 'ordinary', 'differential', 'system', 'second order', 'natural frequencies')

########################################################################

###########################
# Initialization

DOCUMENT();

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"AnswerFormatHelp.pl",
"contextFraction.pl",
"unorderedAnswer.pl",
"PGcourse.pl",
);

TEXT(beginproblem());


###########################
#  Setup

Context("Fraction");

$k1 = random(1,6,1);
$k2 = random(1,6,1);
$m1 = random(1,6,1);
$m2 = random(1,6,1);

#matrix [[a,b],[c,d]]
$Ma = Compute("-($k1+$k2)/$m1");
$Mb = Compute("$k2/$m1");
$Mc = Compute("$k2/$m2");
$Md = Compute("- $k2/$m2");

$eig1 = Compute("-(sqrt(($k2^2+2*$k1*$k2+$k1^2)*$m2^2+(2*$k2^2-2*$k1*$k2)*$m1*$m2+$k2^2*$m1^2)+($k2+$k1)*$m2+$k2*$m1)/(2*$m1*$m2)")->reduce;
$eig2 = Compute("(sqrt(($k2^2+2*$k1*$k2+$k1^2)*$m2^2+(2*$k2^2-2*$k1*$k2)*$m1*$m2+$k2^2*$m1^2)+(-$k2-$k1)*$m2-$k2*$m1)/(2*$m1*$m2)")->reduce;

if ($eig1 < $eig2) {
  $tmp = $eig1;
  $eig1 = $eig2;
  $eig2 = $tmp;
}

$answer1 = Compute("sqrt(-$eig1)")->reduce;
$answer2 = Compute("sqrt(-$eig2)")->reduce;

############################
##  Main text
#

Context()->texStrings;
BEGIN_TEXT
Take the system 
\[
{\vec{x}\,}'' =
\begin{bmatrix}
$Ma & $Mb \\ $Mc & $Md
\end{bmatrix}
\vec{x}
\]
Find the natural frequencies of the system (you might want to use a calculator here)
$BR
$BR
\(\omega_1 = {}
\)
\{ ans_rule(40) \}
$BR
$BR
\(\omega_2 = {}
\)
\{ ans_rule(40) \}
$BR
$BR
\{ AnswerFormatHelp("numbers") \}
END_TEXT
Context()->normalStrings;


#############################
# Answer evaluation

$showPartialCorrectAnswers = 1;

UNORDERED_ANS( $answer1->cmp() , $answer2->cmp() );

#############################
# Solution

Context()->texStrings;
BEGIN_SOLUTION
${PAR}SOLUTION:$PAR
The eigenvalues of the matrix
\(\begin{bmatrix}
$Ma & $Mb \\ $Mc & $Md
\end{bmatrix}\)
are \( $eig1 \) and \( $eig2 \).
$BR
$BR
The natural frequencies are the square roots of the negatives of the eigenvalues, so
\( $answer1 \) and \( $answer2 \).
END_SOLUTION
Context()->normalStrings;

;

ENDDOCUMENT();
