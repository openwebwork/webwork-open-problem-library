END_HINT

ANS((conj($a))->cmp());
ANS((conj($b))->cmp());
ANS((conj($c))->cmp());
ANS((conj($d))->cmp());
ANS((conj($e))->cmp());
ANS(($f)->cmp());
ANS(($g)->cmp());
ANS(Re(($h))->cmp());
ANS(Im(($h))->cmp());
ANS(($i)->cmp());
ANS(($j)->cmp());
ANS(($k)->cmp());

ENDDOCUMENT() ;       
