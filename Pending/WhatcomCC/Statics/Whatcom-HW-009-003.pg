#image scale
$imgScale = .7;

#image aspect ratio
$width = $imgScale*458;
$height = $imgScale*454;

loadMacros(   
	"PGstandard.pl",     # Standard macros for PG language
	"MathObjects.pl",
	"PGML.pl",
	"parserPopUp.pl",
	"parserMultiAnswer.pl",
