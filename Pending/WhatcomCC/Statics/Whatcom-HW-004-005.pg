Context("Numeric");

#given
$Fx = -random(100,200,1);
$Fy = random(50,100,1);
$Fz = random(300,400,1);
$W = random(50,100,5);
$a = random(5,8,0.1);
$b = random(4,6,0.1);
$c = random(3,5,0.1);

#computation 
$Ax = -$Fx;
$Ay = -$Fy;
$Az = $W*$a-$Fz;

$MAx = $W*$a**2/2-$a*$Fz;
$MAy = 0;
$MAz = $a*$Fx;

#set tolerance
Context()->flags->set(
tolerance=>.01);

