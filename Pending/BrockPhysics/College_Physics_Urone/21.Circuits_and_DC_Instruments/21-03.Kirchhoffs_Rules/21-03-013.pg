##DESCRIPTION
## Calculate the effective resistance of a network of resistors
##ENDDESCRIPTION

## DBsubject(Electricity and Magnetism)
## DBchapter(Electric Circuits)
## DBsection(Kirchhoff's laws)
## Date(2 February 2018)
## Institution(Brock University)
## Author(Kyle Winch, Santo D'Agostino)
## TitleText('')
## AuthorText('')
## EditionText('')
## Section('')
## Problem('')
## KEYWORDS(electric circuit)


DOCUMENT();
loadMacros( "PGbasicmacros.pl",
                     "MathObjects.pl",
	             "PGauxiliaryFunctions.pl",
	             "PGchoicemacros.pl",
	             "PGanswermacros.pl",
                     "PG_CAPAmacros.pl",
                     "BrockPhysicsMacros.pl"
);

TEXT(beginproblem());

$showPartialCorrectAnswers = 1;
$showHint =3;

$R1 = random(20,200,5);
$R2 = random(45,195,5);
$R3 = random(26,226,5);
$R4 = random(32,232,5);
$R5 = random(15,180,5);
$R6 = random(17,187,5);
$R7 = random(21,241,5);
$R8 = random(14,164,5);
$R9 = random(12,177,5);
$R = $R1 + $R7 + 1/(1/($R4) + 1/($R2 + $R8 + 1/(1/($R5) + 1/($R3 + $R6 + $R9))));
$emf = random(50,100,1);
$current = $emf/($R);
$I1 = $current;
$I7 = $I1;
$I4 = abs(($emf - ($I1)*($R1) - ($I7)*($R7))/($R4));
$I2 = abs($I1 - $I4);
$I8 = abs($I7 - $I4);
$I5 = abs(($emf - ($I1)*($R1) - ($I2)*($R2) - ($I7)*($R7) - ($I8)*($R8))/($R5));
$I3 = abs($I2 - $I5);
$I6 = $I3;
$I9 = $I3;

BEGIN_TEXT
\{ image( "Circuit_39.png", width=>300, height=>300,   tex_size=>700, extra_html_tags=>'alt="Circuit Example"' ) \}
$PAR
END_TEXT

BEGIN_TEXT
(a) Determine the total resistance of the circuit if R<sub>1</sub> = $R1 \(\Omega\), R<sub>2</sub> = $R2 \(\Omega\), R<sub>3</sub> = $R3 \(\Omega\), R<sub>4</sub> = $R4 \(\Omega\), R<sub>5</sub> = $R5 \(\Omega\), R<sub>6</sub> = $R6 \(\Omega\), R<sub>7</sub> = $R7 \(\Omega\), R<sub>8</sub> = $R8 \(\Omega\), and R<sub>9</sub> = $R9 \(\Omega\).
$PAR

\{ans_rule(20)\} \(\Omega\)
$PAR
END_TEXT

ANS(num_cmp("$R"));

BEGIN_TEXT
(b) Determine the current flowing from the battery if the battery voltage is \(\mathcal{E}\) = $emf V.
$PAR

\{ans_rule(20)\} A
$PAR
END_TEXT

ANS(num_cmp("$current"));

BEGIN_TEXT
(c) Determine the current through each resistor.
$PAR
Current through resistor R<sub>1</sub> = \{ans_rule(20)\} A
$PAR
Current through resistor R<sub>2</sub> = \{ans_rule(20)\} A
$PAR
Current through resistor R<sub>3</sub> = \{ans_rule(20)\} A
$PAR
Current through resistor R<sub>4</sub> = \{ans_rule(20)\} A
$PAR
Current through resistor R<sub>5</sub> = \{ans_rule(20)\} A
$PAR
Current through resistor R<sub>6</sub> = \{ans_rule(20)\} A
$PAR
Current through resistor R<sub>7</sub> = \{ans_rule(20)\} A
$PAR
Current through resistor R<sub>8</sub> = \{ans_rule(20)\} A
$PAR
Current through resistor R<sub>9</sub> = \{ans_rule(20)\} A
$PAR

END_TEXT

ANS(num_cmp("$I1"));

ANS(num_cmp("$I2"));

ANS(num_cmp("$I3"));

ANS(num_cmp("$I4"));

ANS(num_cmp("$I5"));

ANS(num_cmp("$I6"));

ANS(num_cmp("$I7"));

ANS(num_cmp("$I8"));

ANS(num_cmp("$I9"));


ENDDOCUMENT()
