TEXT(beginproblem());

$showPartialCorrectAnswers = 1;

$mCi = 7.5;
$BqCi = 3.7*10**10;
$m = 98.906;
$time = 6.02*3600;
$denom = 0.693*(6.02*10**23);
$mass = (($mCi*10**-3)*$BqCi*$m*$time)/$denom;

BEGIN_TEXT
\($mCi\) mCi of \(^{99m}\textrm{Tc}\) is used in a brain scan. What is the mass of technetium?
$PAR

\{ans_rule(40)\} g

$PAR
END_TEXT

ANS(num_cmp("$mass"));


