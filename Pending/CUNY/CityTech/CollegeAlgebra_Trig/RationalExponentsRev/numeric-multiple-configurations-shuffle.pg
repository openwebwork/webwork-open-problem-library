## MO(1)
########################################################################

DOCUMENT();      

loadMacros(
   "PGstandard.pl",     # Standard macros for PG language
   "MathObjects.pl",
   "contextFraction.pl",
   "PGchoicemacros.pl"
);

# Print problem number and point value (weight) for the problem
TEXT(beginproblem());

# Show which answers are correct and which ones are incorrect
$showPartialCorrectAnswers = 1;

##############################################################
#
#  Setup
#
#
Context("LimitedFraction");

Context()->strings->are(nonreal => {}, imaginary => {alias => "nonreal"});

$r = random(4,12,1);
$r2 = $r**2;
$r3 = $r**3;

@q = ( "\($r2^{3/2}\)", "\(-$r2^{3/2}\)", "\((-$r2)^{3/2}\)", "\($r2^{-3/2}\)", "\(-$r2^{-3/2}\)", "\((-$r2)^{-3/2}\)" ); 

@a = ( Real($r3), Real(-$r3), String("nonreal"), Fraction(1,$r3), Fraction(-1,$r3), String("nonreal") );

@perm = NchooseK(6,6);

##############################################################
#
#  Text
#
#

Context()->texStrings;
BEGIN_TEXT
$PAR
Compute the following. Use "nonreal" or "imaginary" for non-real results.
$PAR
a) $q[$perm[0]] = \{ans_rule(10)\}
$PAR
b) $q[$perm[1]] = \{ans_rule(10)\}
$PAR
c) $q[$perm[2]] = \{ans_rule(10)\}
$PAR
d) $q[$perm[3]] = \{ans_rule(10)\}
$PAR
e) $q[$perm[4]] = \{ans_rule(10)\}
$PAR
f) $q[$perm[5]] = \{ans_rule(10)\}
END_TEXT
Context()->normalStrings;

##############################################################
#
#  Answers
#
#

ANS($a[$perm[0]]->cmp);
ANS($a[$perm[1]]->cmp);
ANS($a[$perm[2]]->cmp);
ANS($a[$perm[3]]->cmp);
ANS($a[$perm[4]]->cmp);
ANS($a[$perm[5]]->cmp);

ENDDOCUMENT();        
