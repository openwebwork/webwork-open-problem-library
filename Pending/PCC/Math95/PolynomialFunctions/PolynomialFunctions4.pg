##############################################

DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",


);

TEXT(beginproblem());

##############################################


Context("Numeric");

$a = random(10,14,.1);
$b = random(10,100,.1);
$d = random(10,100,1);
$e = random(10,100,.1);

$c = random(2,4,1)*random(-1,1,2);

$f = Formula("$a x^3-$b x^2 + $d x + $e")->reduce;


$answer = $f->eval(x=>$c);



##############################################

Context()->texStrings;

BEGIN_TEXT
Evaluate \(f\) at the given value of \(x\). $BR

\[f(x)=$f \] $PAR

\(f($c) = \) \{ans_rule(5)\}.






END_TEXT

Context()->normalStrings;

##############################################

ANS($answer->cmp());




ENDDOCUMENT();
