DOCUMENT();      

loadMacros(
   "PGstandard.pl",     # Standard macros for PG language
   "MathObjects.pl",
   "PGchoicemacros.pl",

);

# Print problem number and point value (weight) for the problem
TEXT(beginproblem());

# Show which answers are correct and which ones are incorrect
$showPartialCorrectAnswers = 1;

##############################################################
#
#  Setup
#
#
Context("Numeric");

$mc = new_multiple_choice();
$mc->qa(
"Which of the following is the probability density function of \( Y = e^X \)?",
"\[
    f_Y(y) =
        \begin{cases}
           \frac{1}{2y} & \frac{1}{e} \le y \le e\\
                          0 & \text{otherwise.}\\
        \end{cases} 
\]");
$mc->extra(
"\[
    f_Y(y) =
        \begin{cases}
           0.0729e^y & \frac{1}{e} \le y \le e \\
                          0 & \text{otherwise.}\\
        \end{cases} 
\]",
"\[
    f_Y(y) =
        \begin{cases}          
           \frac{2e^2}{e^4 - 1}y & \frac{1}{e} \le y \le e \\
                                          0 & \text{otherwise.}\\
        \end{cases} 
\]",
"\[
    f_Y(y) =
        \begin{cases}
           \frac{e}{1 - e^2} & \frac{1}{e} \le y \le e \\
                                  0 & \text{otherwise.} \\
        \end{cases} 
\]",
"\[
    f_Y(y) =
        \begin{cases}
         \frac{1}{2} \ln \, y +  \frac{1}{2} & \frac{1}{e} \le y \le e \\
                                                     0 & \text{otherwise.}\\
        \end{cases} 
\]",
);


##############################################################
#
#  Text
#
#

Context()->texStrings;
BEGIN_TEXT
\( X \) is uniformly distributed over the interval \( [-1, 1] \).

$PAR
\{ $mc->print_q() \}
$BR
\{ $mc->print_a() \}
END_TEXT
Context()->normalStrings;

##############################################################
#
#  Answers
#
#

ANS( radio_cmp( $mc->correct_ans() ) );

ENDDOCUMENT();