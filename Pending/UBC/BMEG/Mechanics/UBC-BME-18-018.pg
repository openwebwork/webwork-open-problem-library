DOCUMENT();

#image scale
$imgScale = .4;

#image aspect ratio
$width = $imgScale*737;
$height = $imgScale*875;

loadMacros(   
	"PGstandard.pl",     # Standard macros for PG language
	"MathObjects.pl",
	"PGML.pl",
	"parserPopUp.pl",
	"parserMultiAnswer.pl",
	"parserRadioButtons.pl",
