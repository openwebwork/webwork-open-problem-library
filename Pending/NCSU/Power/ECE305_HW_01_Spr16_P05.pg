$showPartialCorrectAnswers = 1;
ANS($modvl1v->cmp());
ANS($argvl1dv->cmp());
ANS($modvrv->cmp());
ANS($argvrdv->cmp());
ANS($modvl2v->cmp());
ANS($argvl2dv->cmp());
ANS($modvcv->cmp());
ANS($argvcdv->cmp());
ANS($modisv->cmp());
ANS($argisdv->cmp());
ANS($modi1v->cmp());
ANS($argi1dv->cmp());
ANS($modi2v->cmp());
ANS($argi2dv->cmp());
ANS($popupia->cmp());
