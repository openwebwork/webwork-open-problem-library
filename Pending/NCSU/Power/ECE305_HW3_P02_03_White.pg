## Debugging Code Parameters From Textbook
#$CS=40;$Lnt=100;$Lnb=35;$Lg1=0.1;$Lg2=0.1;$N1=400;$N2=300;$Phi=$CS/10;$I1=5; $Omg=377;

#Calculations
$Ac=$CS/10000;
$Lct=$Lnt/100;
$Lcb=$Lnb/100;
$Lgm1=$Lg1/100;
$Lgm2=$Lg2/100;
$Phin=$Phi/1000;
$B=$Phin/$Ac;
$Hg1=$B/4/pi*10000000;
$Hg2=$Hg1;
$Fg12=$Hg1*$Lgm1;
$Hct=200;
$Fct=$Hct*$Lct;

$Hcb=700;
$Fcb=$Hcb*$Lcb;

$Ftot=$Fg12*2+$Fcb+$Fct;
$I=Real($Ftot/($N1+$N2));

$Rct=$Lct*200/$Ac;
$Rg1=$Lgm1/4/pi*10000000/$Ac;
$Rg2=$Rg1;
$Rcb=$Lcb*700/$Ac;
$Rtot=$Rcb+$Rct+$Rg1+$Rg2;
$L11=$N1**2/$Rtot;
$L22=$N2**2/$Rtot;
$L12=$N1*$N2/$Rtot;
$W1=1/2*$L11*$I**2;
$W2=1/2*$L22*$I**2;
$W12=$L12*$I**2;
$Wtot=$W12+$W1+$W2;

$emf1=($L11+$L12)*$I1*$Omg*Formula("cos($Omg*t)");
$emf2=($L22+$L12)*$I1*$Omg*Formula("cos($Omg*t)");











