# Answer calculation
$Ampv=$Ampv0/sqrt(2);
$Rlcv=i*$Lcv/1000*2*pi*1/$Tv;
$icv=$Ampv*(1/($Rcv+$Rccv)+1/$Rlcv);
$modicv=mod($icv);
$argicv=arg($icv);
$argicdv=$argicv*180/pi;
$formula_ic=Formula("$modicv*sqrt(2)*cos(2*pi/$Tv*t+$argicv)")->reduce;

$Vlcv=$Ampv;
