## Problem1('5')
## KEYWORDS('derivative')


DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",            
  "MathObjects.pl","littleneck.pl","contextFraction.pl"          
);
# allow the student to change the seed for this problem.

rand_button();


TEXT(beginproblem());

###################################
# Setup
Context()->variables->add(u=>'Real',t=>'Real');
$a=random(2,8,1);
$b=random(1,8,1);
$u=Formula("$b+cos($a*x)")->reduce;
$fu=Formula("1/u")->reduce;
$g=Formula("sin($a*x)");
$f=$fu->substitute(u=>$u);
$df=Formula("$g/$u")->reduce;
$Fu=Formula("ln|u|")->reduce;
$F=$Fu->substitute(u=>$u);
Context("Fraction");
$c=Compute("-1/($a)");
Context("Numeric");

$ans=Compute("$c*$F");
if($c==1){$c=""}
if($c==-1){$c="-"}
###################################
# Main text
Context()->texStrings;
BEGIN_TEXT
Evaluate the integral by any method.
$PAR
\( \displaystyle \int  $df \;dx\) \(=\) \{ans_rule(20) \} \(+C\)
END_TEXT

###################################
# Answers

#$showPartialCorrectAnswers = 0;

ANS($ans->cmp);

###################################
SOLUTION(EV3(<<'END_SOLUTION'));
$PAR SOLUTION $PAR
For \(u=$u\) we have \($c du = $g dx\) hence;
$PAR
\[ \displaystyle \int $df \;dx = $c \int $fu \;du=$c$Fu+C=$c $F+C\]
$BR
END_SOLUTION
Context()->normalStrings;

###################################
ENDDOCUMENT();
